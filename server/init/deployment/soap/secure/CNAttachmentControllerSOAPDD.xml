<?xml version='1.0' encoding='UTF-8'?>
<isd:service xmlns:isd="http://xml.apache.org/xml-soap/deployment" 
     id='http://ksoe.org/CNAttachmentController/message/'>
<isd:provider type="org.apache.soap.providers.EnergyAuthorizationStatelessEJBProvider" scope="Session" methods="create">
 <isd:java class="com.ksoe.energynet.ejb.CNAttachmentController"/>
 <isd:option key="FullHomeInterfaceName" value="com.ksoe.energynet.ejb.CNAttachmentControllerHome"/>
 <isd:option key="JNDIName" value="ksoe/energynet/CNAttachmentController"/>
 <isd:option key="ContextProviderURL" value="iiop://localhost:900"/>
 <isd:option key="FullContextFactoryName" value="com.ibm.websphere.naming.WsnInitialContextFactory"/>
 <isd:option key="KsoeClientSessionContextBeanURIName" value="http://ksoe.org/AuthorizationSessionStatelessController/message/" />
</isd:provider>

<isd:faultListener>org.apache.soap.server.DOMFaultListener</isd:faultListener>

<isd:mappings>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
     xmlns:x="http://www.w3.org/2001/XMLSchema" qname="x:date"
     javaType = "java.util.Date"
     java2XMLClassName="org.apache.soap.encoding.soapenc.DateSerializer"
     xml2JavaClassName="org.apache.soap.encoding.soapenc.DateSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:CNAttachment" 
     javaType="com.ksoe.energynet.valueobject.CNAttachment" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:CNAttachmentShortList" 
     javaType="com.ksoe.energynet.valueobject.lists.CNAttachmentShortList" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:CNAttachmentShort" 
     javaType="com.ksoe.energynet.valueobject.brief.CNAttachmentShort" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:CNAttachmentFilter" 
     javaType="com.ksoe.energynet.valueobject.filter.CNAttachmentFilter" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENDocAttachment" 
     javaType="com.ksoe.energynet.valueobject.ENDocAttachment" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/> 
 </isd:mappings>
</isd:service>
