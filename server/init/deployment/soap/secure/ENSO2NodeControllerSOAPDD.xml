<?xml version='1.0' encoding='UTF-8'?>
<isd:service xmlns:isd="http://xml.apache.org/xml-soap/deployment" 
     id='http://ksoe.org/ENSO2NodeController/message/'>
<isd:provider type="org.apache.soap.providers.EnergyAuthorizationStatelessEJBProvider" scope="Session" methods="create">
 <isd:java class="com.ksoe.energynet.ejb.ENSO2NodeController"/>
 <isd:option key="FullHomeInterfaceName" value="com.ksoe.energynet.ejb.ENSO2NodeControllerHome"/>
 <isd:option key="JNDIName" value="ksoe/energynet/ENSO2NodeController"/>
 <isd:option key="ContextProviderURL" value="iiop://localhost:900"/>
 <isd:option key="FullContextFactoryName" value="com.ibm.websphere.naming.WsnInitialContextFactory"/>
 <isd:option key="KsoeClientSessionContextBeanURIName" value="http://ksoe.org/AuthorizationSessionStatelessController/message/" />
</isd:provider>

<isd:faultListener>org.apache.soap.server.DOMFaultListener</isd:faultListener>

<isd:mappings>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
     xmlns:x="http://www.w3.org/2001/XMLSchema" qname="x:dateTime"
     javaType = "java.util.Date"
     java2XMLClassName="org.apache.soap.encoding.soapenc.DateSerializer"
     xml2JavaClassName="org.apache.soap.encoding.soapenc.DateSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENSO2Node" 
     javaType="com.ksoe.energynet.valueobject.ENSO2Node" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENSO2NodeShortList" 
     javaType="com.ksoe.energynet.valueobject.lists.ENSO2NodeShortList" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENSO2NodeShort" 
     javaType="com.ksoe.energynet.valueobject.brief.ENSO2NodeShort" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENSO2NodeFilter" 
     javaType="com.ksoe.energynet.valueobject.filter.ENSO2NodeFilter" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENServicesObjectRef" 
     javaType="com.ksoe.energynet.valueobject.references.ENServicesObjectRef" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENSO2NodeTypeRef" 
     javaType="com.ksoe.energynet.valueobject.references.ENSO2NodeTypeRef" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 </isd:mappings>
</isd:service>
