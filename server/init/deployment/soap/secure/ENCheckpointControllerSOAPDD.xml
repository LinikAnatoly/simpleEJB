<?xml version='1.0' encoding='UTF-8'?>
<isd:service xmlns:isd="http://xml.apache.org/xml-soap/deployment" 
     id='http://ksoe.org/ENCheckpointController/message/'>
<isd:provider type="org.apache.soap.providers.EnergyAuthorizationStatelessEJBProvider" scope="Session" methods="create">
 <isd:java class="com.ksoe.energynet.ejb.ENCheckpointController"/>
 <isd:option key="FullHomeInterfaceName" value="com.ksoe.energynet.ejb.ENCheckpointControllerHome"/>
 <isd:option key="JNDIName" value="ksoe/energynet/ENCheckpointController"/>
 <isd:option key="ContextProviderURL" value="iiop://localhost:900"/>
 <isd:option key="FullContextFactoryName" value="com.ibm.websphere.naming.WsnInitialContextFactory"/>
 <isd:option key="KsoeClientSessionContextBeanURIName" value="http://ksoe.org/AuthorizationSessionStatelessController/message/" />
</isd:provider>

<isd:faultListener>org.apache.soap.server.DOMFaultListener</isd:faultListener>

<isd:mappings>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENCheckpoint" 
     javaType="com.ksoe.energynet.valueobject.ENCheckpoint" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENCheckpointShortList" 
     javaType="com.ksoe.energynet.valueobject.lists.ENCheckpointShortList" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENCheckpointShort" 
     javaType="com.ksoe.energynet.valueobject.brief.ENCheckpointShort" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENCheckpointFilter" 
     javaType="com.ksoe.energynet.valueobject.filter.ENCheckpointFilter" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENTransportDepartmentRef" 
     javaType="com.ksoe.energynet.valueobject.references.ENTransportDepartmentRef" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 </isd:mappings>
</isd:service>
