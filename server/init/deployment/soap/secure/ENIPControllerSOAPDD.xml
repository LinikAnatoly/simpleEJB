<?xml version='1.0' encoding='UTF-8'?>
<isd:service xmlns:isd="http://xml.apache.org/xml-soap/deployment" 
     id='http://ksoe.org/ENIPController/message/'>
<isd:provider type="org.apache.soap.providers.EnergyAuthorizationStatelessEJBProvider" scope="Session" methods="create">
 <isd:java class="com.ksoe.energynet.ejb.ENIPController"/>
 <isd:option key="FullHomeInterfaceName" value="com.ksoe.energynet.ejb.ENIPControllerHome"/>
 <isd:option key="JNDIName" value="ksoe/energynet/ENIPController"/>
 <isd:option key="ContextProviderURL" value="iiop://localhost:900"/>
 <isd:option key="FullContextFactoryName" value="com.ibm.websphere.naming.WsnInitialContextFactory"/>
 <isd:option key="KsoeClientSessionContextBeanURIName" value="http://ksoe.org/AuthorizationSessionStatelessController/message/" />
</isd:provider>

<isd:faultListener>org.apache.soap.server.DOMFaultListener</isd:faultListener>

<isd:mappings>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
     xmlns:x="http://www.w3.org/2001/XMLSchema" qname="x:dateTime"
     javaType = "java.util.Date"
     java2XMLClassName="org.apache.soap.encoding.soapenc.DateSerializer"
     xml2JavaClassName="org.apache.soap.encoding.soapenc.DateSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENIP" 
     javaType="com.ksoe.energynet.valueobject.ENIP" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENIPShortList" 
     javaType="com.ksoe.energynet.valueobject.lists.ENIPShortList" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENIPShort" 
     javaType="com.ksoe.energynet.valueobject.brief.ENIPShort" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENIPFilter" 
     javaType="com.ksoe.energynet.valueobject.filter.ENIPFilter" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENIPStatusRef" 
     javaType="com.ksoe.energynet.valueobject.references.ENIPStatusRef" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENIPRef" 
     javaType="com.ksoe.energynet.valueobject.references.ENIPRef" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 </isd:mappings>
</isd:service>
