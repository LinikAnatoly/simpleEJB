<?xml version='1.0' encoding='UTF-8'?>
<isd:service xmlns:isd="http://xml.apache.org/xml-soap/deployment" 
     id='http://ksoe.org/ENOwnerController/message/'>
<isd:provider type="org.apache.soap.providers.EnergyAuthorizationStatelessEJBProvider" scope="Session" methods="create">
 <isd:java class="com.ksoe.energynet.ejb.ENOwnerController"/>
 <isd:option key="FullHomeInterfaceName" value="com.ksoe.energynet.ejb.ENOwnerControllerHome"/>
 <isd:option key="JNDIName" value="ksoe/energynet/ENOwnerController"/>
 <isd:option key="ContextProviderURL" value="iiop://localhost:900"/>
 <isd:option key="FullContextFactoryName" value="com.ibm.websphere.naming.WsnInitialContextFactory"/>
 <isd:option key="KsoeClientSessionContextBeanURIName" value="http://ksoe.org/AuthorizationSessionStatelessController/message/" />
</isd:provider>

<isd:faultListener>org.apache.soap.server.DOMFaultListener</isd:faultListener>

<isd:mappings>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENOwner" 
     javaType="com.ksoe.energynet.valueobject.ENOwner" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENOwnerShortList" 
     javaType="com.ksoe.energynet.valueobject.lists.ENOwnerShortList" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENOwnerShort" 
     javaType="com.ksoe.energynet.valueobject.brief.ENOwnerShort" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
 <isd:map encodingStyle="http://schemas.xmlsoap.org/soap/encoding/" 
     xmlns:x="http://ksoe.org/EnergyproControllerService/type/" qname="x:ENOwnerFilter" 
     javaType="com.ksoe.energynet.valueobject.filter.ENOwnerFilter" 
     java2XMLClassName="org.apache.soap.encoding.soapenc.BeanSerializer" 
     xml2JavaClassName="org.apache.soap.encoding.soapenc.BeanSerializer"/>
	 
