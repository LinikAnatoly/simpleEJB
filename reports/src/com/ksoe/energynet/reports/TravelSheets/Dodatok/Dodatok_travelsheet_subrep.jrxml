	<parameter name="TravelSheetItem" class="java.lang.Double">
		<defaultValueExpression><![CDATA[new Double(1)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
tk.name as tkname,
fm.finmolname as foname
from
entravelsheetitem as si
left join
(entravlshttm2trnsprttm as siti
left join
(entransportitem as ti
left join
((enplanworkitem as pi
left join
(enplanwork as pw
left join
(enworkorder2enplanwork as wopw
left join
finmoldata as fm
on
fm.workordercode = wopw.workordercode)
on
wopw.plancode = pw.code)
on
pi.planrefcode = pw.code)
left join
tktechcard as tk
on
tk.code = pi.kartarefcode)
on
pi.code = ti.planitemrefcode)
on
ti.code = siti.transportitemrefcode)
on
si.code = siti.travelsheetitemrefcode
where
fm.moltyperefcode = 1
and
si.code = $P{TravelSheetItem}]]>
	</queryString>
