	<parameter name="TravelSheet" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
sh.code as code /* Код */,
ma.name as mark /* Марка автокрана */,
to_char(cast(sh.datestart as date), 'dd.mm.yyyy') as datestart /*Начальная дата*/,
 /*(case when sh.statusrefcode = 3 or sh.statusrefcode = 4 or sh.statusrefcode = 5 then to_char(cast(sh.datefinal as date), 'dd.mm.yyyy') else null end)*/ to_char(sh.datefinal , 'dd.mm.yyyy')  as datefinal /*Дата конца*/,
sh.numbergen as numbergen /* № дорожного листа */,
(tr.gosnumber) as gosnumber /* номер транспорта */ ,
to_char(cast(sh.datestart as date),'dd') as DayOfTravelSheet /* День*/,
to_char(cast(sh.datestart as date),'MM') as MonthOfTravelSheet /* Месяц*/,
to_char(cast(sh.datestart as date),'yy')  as YearOfTravelSheet /*Год*/,
wm.name as WorkMode /*Режим работы*/,
fw.name as finworkername /*ФИО водителя*/,
fw.tabnumber as tabnumber /*Таб номер*/,
' '||tkdrive.sertificatenumber as sertificatenumber /*код удостоверения */,
de.name as department /*В чье распоряжение*/,
sh.speedometerstart /* Спидометер старт */,
(case when sh.statusrefcode = 3 or sh.statusrefcode = 4 or sh.statusrefcode = 5 then sh.speedometerfinal else null end) as speedometerfinal /* Спидометер финиш */,
(case when sh.statusrefcode = 3 or sh.statusrefcode = 4 or sh.statusrefcode = 5 then (sh.speedometerfinal - sh.speedometerstart) else null end) as kilometrage /* Километраж */,
(case when sh.workmoderefcode = 1 then CAST('ненорм' as varchar) else to_char(cast(sh.timefinal as timestamp), 'HH24:mi') end) as str_timefinal /* Время прибытия 1*/,

(case when sh.statusrefcode = 3 or sh.statusrefcode = 4 or sh.statusrefcode = 5
 then to_char(sh.timestart, 'HH24:mi') else null end) as timestart /* Время отправки */,
(case when sh.statusrefcode = 3 or sh.statusrefcode = 4 or sh.statusrefcode = 5
 then to_char(sh.timefinal, 'HH24:mi') else null end) as timefinal /* Время прибытия */,

fu.name as fuelmark /* Марка топлива */,
(case when sh.statusrefcode = 3 or sh.statusrefcode = 4 or sh.statusrefcode = 5 then (select sum(si1.summachinehours) from entravelsheetitem as si1 where
si1.travelsheetrefcode = sh.code) else null end) as summachinehours /* Час у наряді */,
(select sum(sf1.countgen) from entravelsheetfuel as sf1 where
sf1.travelsheetrefcode = sh.code) as fuelcountgen /* Выдано топлива */,
(select sum(shr.countgenstart) from entravelsheetfuelremns as shr where
shr.travelsheetrefcode = sh.code) as remnants_before /* Залишок при виїзді */,
(case when sh.statusrefcode = 3 or sh.statusrefcode = 4 or sh.statusrefcode = 5 then (select sum(shr.countgenfinal) from entravelsheetfuelremns as shr where
shr.travelsheetrefcode = sh.code) else null end) as remnants_after /* Залишок при поверненні */,
/* Витрати палива за нормою */
(select sum(countgen) from
(select distinct es.code, es.countgen from
entravelsheetitem as si left join entravlshttm2trnsprttm as siti on si.code = siti.travelsheetitemrefcode
left join entransportitem as ti on ti.code = siti.transportitemrefcode
left join entransport2enestimate as ties on ties.transportrefcode = ti.code
left join enestimateitem as es on es.code = ties.estimaterefcode
where es.kindrefcode = 2
and si.travelsheetrefcode = sh.code
and si.kindrefcode = 1 ) as NormativeFuelConsumption1) as NormativeFuelConsumption,
/* Фактичні витрати палива */
(case when sh.statusrefcode = 3 or sh.statusrefcode = 4 or sh.statusrefcode = 5 then (select sum(quantity) from
(select distinct fm.code, fm.quantity from
entravelsheetitem as si left join entravlshttm2trnsprttm as siti on si.code = siti.travelsheetitemrefcode
left join entransportitem as ti on ti.code = siti.transportitemrefcode
left join entransport2enestimate as ties on ties.transportrefcode = ti.code
left join enestimateitem as es on es.code = ties.estimaterefcode
left join finmaterials as fm on fm.estimateitemrefcode = es.code
where es.kindrefcode = 2
and si.travelsheetrefcode = sh.code) as RealFuelConsumption1) else null end) as RealFuelConsumption,
trh.reg_id
from
entravelsheet as sh
--left join entravelsheetitem as si on si.travelsheetrefcode = si.code
--left join entravlshttm2trnsprttm as siti on siti.travelsheetitemrefcode = si.code
--left join entransportitem as ti on ti.code = siti.transportitemrefcode
inner join endepartment as de on de.code = sh.departmentcode
inner join finworker as fw on fw.code = sh.finworkercode
left  join tkdriversertificate as tkdrive on fw.tabnumber = tkdrive.tabnumber
inner join entravelworkmode as wm on wm.code = sh.workmoderefcode
inner join tktransportreal as tr on tr.code = sh.transportrealcode
left join tktransportmark as ma on ma.code = tr.transportmarkcode
inner join tkfueltype as fu on fu.code = ma.fueltypecode
inner join tktransportrealhistory as trh on (trh.transportrealrefcode = tr.code and trh.datestart <= sh.datefinal and coalesce(trh.datefinal, to_date('31.12.9999', 'dd.mm.yyyy')) >= sh.datefinal)
where
sh.code = $P{TravelSheet}]]>
	</queryString>
