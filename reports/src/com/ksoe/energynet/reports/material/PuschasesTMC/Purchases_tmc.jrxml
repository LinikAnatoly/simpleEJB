	<parameter name="isGroupByIdentid2010" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="monthStart" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="monthFinal" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="yearStart" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="yearFinal" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="contractNumber" class="java.lang.Integer"/>
	<parameter name="materialGroupCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="porgcode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="materialCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="isGroupByIdentid2015" class="java.lang.Integer" isForPrompting="false"/>
	<queryString>
		<![CDATA[ select
bi.code as bicode ,
bi.contractnumber ,
to_char(bi.contractdate,'dd.mm.yyyy') as contractdate ,
org.name as orgname ,
m.name as matname ,
meas.name as measname ,
bi.countfact::numeric(15,6) as countfact ,
bi.sumgen as billitemsuma ,
(bi.sumgen/bi.countfact)::numeric(15,2) as billpricewithnds ,
b.numberdoc  as billnumber ,
to_char(b.dategen,'dd.mm.yyyy') as billdate
,(select  sum((fi2bi.countfact * fi.pricewithoutnds ) * (1+coalesce(f.ndspercent/100)))::numeric(15,2)
 from rqfkorderitem2billitem fi2bi , rqfkorderitem fi  , rqfkorder f
where fi2bi.billitemrefcode = bi.code
and fi2bi.fkorderitemrefcode = fi.code
and f.code = fi.fkorderrefcode
and f.kindcode in (1,15)) as sum_postavka ,

 case when $P{isGroupByMaterialGroups} = 1 then ( select mm.name from tkmaterials mm where mm.code = m.rootgrouprefcode )
 else '-1' end as rootgrouprefname ,
  ( select mm.name from tkmaterials mm where mm.code = m.rootgrouprefcode ) as namerootmaterial ,

case when $P{isGroupByIdentid2010}= 1 then coalesce(m.identid2010,'Без кода ДК') else '-1' end as identid2010 ,

case when $P{isGroupByIdentid2015} = 1 then coalesce(m.identid2015,'Без кода ДК') else '-1' end as identid2015

, ( select string_agg(budg.shortname, ',')
    from rqbillitem2orderitem bi2oi,
         rqorderitem oi,
         endepartment budg
    where bi2oi.billitemrefcode = bi.code and
          bi2oi.orderitemrefcode = oi.code and
          oi.budgetrefcode = budg.code) as budgetnames
, coalesce((select sum(coalesce(pi.summagen, 0)) ::numeric(10, 2) as pay_sum
   from rqpaydocitem pi,
        rqpaydoc p
   where pi.paydocrefcode = p.code and
         pi.code in (
                      select pib.paydocitemcode
                      from rqpaydocitem2billitem pib
                      where pib.billitemcode = bi.code
         )),0) as pay_sum
from
rqbill b  left join rqorg org on b.orgcode = org.code
, rqbillitem bi
, tkmaterials m
, tkmeasurement meas
where b.code = bi.billrefcode
and bi.materialcode = m.code
and b.billtypecode = 1
and m.measurementcode = meas.code

/* номер договора */
and (bi.findocid = $P!{contractNumber} or $P!{contractNumber} = 0)

/* группа материалов */
and bi.materialcode in
(select tm.code from tkmaterials tm
where tm.rootgrouprefcode = $P{materialGroupCode} or $P{materialGroupCode}= 0)
/*постачальник */
and (org.id = $P{porgcode} or  $P{porgcode} = 0 )

/*материал */
and  ( bi.materialcode = $P{materialCode} or $P{materialCode}= 0 )


and b.dategen between
      to_date('01.'|| $P{monthStart} ||'.'|| $P{yearStart}, 'dd.mm.yyyy')
     and
      last_day(to_date('01.'|| $P{monthFinal} ||'.'|| $P{yearFinal}, 'dd.mm.yyyy'))
order by identid2010  , identid2015 ,  rootgrouprefcode ,  b.dategen]]>
	</queryString>
