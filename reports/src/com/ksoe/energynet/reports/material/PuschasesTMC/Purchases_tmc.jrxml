	<subDataset name="dataset1" whenResourceMissingType="Empty">
		<parameter name="billitemcode" class="java.lang.Double"/>
		<queryString>
			<![CDATA[select coalesce(pi.summagen::numeric(10,2) ,0)  as pay_sum ,
       to_char(p.dategen , 'dd.mm.yyyy') as pay_date
          from rqpaydocitem pi , rqpaydoc p
           where pi.paydocrefcode = p.code
           and pi.code in (
          select pib.paydocitemcode from rqpaydocitem2billitem pib
          where pib.billitemcode = $P{billitemcode} )

union all /*клеим пустую строку если нет оплат */

select 0 as pay_sum , '' as pay_date
where (select count(q.code) from rqpaydocitem2billitem q where q.billitemcode = $P{billitemcode} ) = 0

order by pay_date]]>
		</queryString>
		<field name="pay_sum" class="java.math.BigDecimal"/>
		<field name="pay_date" class="java.lang.String">
			<fieldDescription><![CDATA[Дата оплаты (расчетная)]]></fieldDescription>
		</field>
	</subDataset>
