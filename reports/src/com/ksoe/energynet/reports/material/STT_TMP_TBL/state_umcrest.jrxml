	<queryString>
		<![CDATA[select ' '||codecfo as codecfo,
namecfo ,
' '||div_code as div_code,
div_name,
' '||nn as nn,
mat_name,
' '||bal_sch as bal_sch,
mu_name,
nameostatok,
aq,
sm
from (
select l.code as codecfo, l.name as namecfo , v.div_code, v.div_name, v.nn, v.mat_name, v.bal_sch, v.mu_name, t.name as nameostatok, sum(quantity) aq, sum(cost) sm
  from umc_dba.v_calc_rest_detail v, sprav.budget_core b, sprav.frc_list l, umc_dba.rest_purpose_type t
 where b.frc_list_id = l.id
   and b.id = v.budget_core_id
   and v.rest_purpose_type_id = t.id
   and v.rest_purpose_type_id not in (3, 5)
   and  (($P{showRestMaterial} = 1 and l.code in ($P!{fincfocode} )) or ( $P{showRestMaterial} = 2 )  )
 group by l.code, l.name, v.div_code, v.div_name, v.nn, v.mat_name, v.bal_sch, v.mu_name, t.name
 )

 Where aq <> 0

 order by codecfo, namecfo , div_code, div_name, nn, mat_name, bal_sch, mu_name, nameostatok]]>
