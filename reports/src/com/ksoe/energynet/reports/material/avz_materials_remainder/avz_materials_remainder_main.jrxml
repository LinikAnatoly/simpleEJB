	<variable name="sumavz" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[($F{countrequired}.subtract($V{quantity}.add($V{order_and_not_in}))
 ).doubleValue() >= 0.0
 ? $F{countrequired}.subtract($V{quantity}.add($V{order_and_not_in})).multiply($F{cost}).setScale(2, java.math.BigDecimal.ROUND_HALF_UP)
:
  new BigDecimal(0)]]></variableExpression>
		<initialValueExpression><![CDATA[new BigDecimal(0)]]></initialValueExpression>
	</variable>
