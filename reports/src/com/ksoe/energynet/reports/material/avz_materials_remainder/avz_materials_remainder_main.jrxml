	<variable name="order_and_not_in" class="java.math.BigDecimal">
		<variableExpression><![CDATA[($F{avz_in_order}.subtract($F{avz_in_fkorder})).compareTo(new BigDecimal(0)) == -1 ? new BigDecimal(0)
:
$F{avz_in_order}.subtract($F{avz_in_fkorder})]]></variableExpression>
