	<queryString>
		<![CDATA[select
t.name as nameetalon
, m.name
, ai.materialrefcode
, sum(ai.countgen) as countgen /*нормативна кількість материала по хое вцелом */
, sum(ai.countrequired) as countrequired
, (select string_agg(nn,',') from tempnomenclatures where materialrefcode = ai.materialrefcode ) as nn
, coalesce(t.cost,0) as cost
, coalesce((select sum(i.countfact) from rqorderitem i
					 where i.orderrefcode in ( select o.code from rqorder o where o.creationmethodrefcode = 2  and o.statusrefcode = 2 )
					   and i.statusrefcode = 1
					   and i.materialcode = ai.materialrefcode ),0) as avz_in_order
, coalesce((select sum(fkitem.countgen) from rqfkorderitem fkitem, rqfkorder fkord
						where fkitem.code in (
                                              select f2e.fkorderitemrefcode from rqfkorderitem2enstmttm f2e
                                              where f2e.estimateitemcode in (
                                                                            select e.estimateitemcode from rqorderitem2enestimttm e
                                                                            where e.orderitemcode in (  select i.code from rqorderitem i
                                                                                                         where i.orderrefcode in ( select o.code from rqorder o where o.creationmethodrefcode = 2 )
                                                                                                           and i.statusrefcode = 1
                                                                                                           and i.materialcode = ai.materialrefcode
                                                                                                     )
                                                                            )
                                             )
						and fkitem.fkorderrefcode = fkord.code
						and fkord.kindcode = 1
                        and fkord.statuscode in (3,4,5)),0) as avz_in_fkorder
 from ennormvolumeavzitem ai, tkmaterials t, tkmeasurement m
where (ai.countrequired <> 0 or ai.countgen <> 0)
and t.code = ai.materialrefcode
and m.code = t.measurementcode
group by ai.materialrefcode, t.name, m.name, t.cost
order by 1]]>
	</queryString>
