	<queryString>
		<![CDATA[select dat.deliveryday, dat.monthgen ,  dat.yeargen, dat.mname, dat.meas,
  Cast(sum(dat.countfact) as numeric(15,4)) as countfact ,
  Cast(dat.price as numeric(15,4)) as price ,
  Cast(sum(dat.cost) as numeric(15,4)) as cost ,

  dat.objname ,
  dat.dname ,
  dat.budgetrefname,
  dat.materialrefcode as mcode
  ,

  --- start col group
(case when  $P{pr_par_mat} = 1 then (  select distinct(tmm.name) from tkmaterials tmm where tmm.code =
                   (select rootgrouprefcode from tkmaterials tmm2
                                            where tmm2.code = dat.materialrefcode )
                 )
     when  $P{pr_par_mat} = 0 then '1' end )
as parent_material
 ,   case when $P{pr_identid} = 1 then
     case when identid is null then 'Без класіфікатора' else identid end
     when $P{pr_identid} = 0 then '1' end
as identid

from
(
select
coalesce(es.deliverytime, coalesce(sm.deliverydate,60)) as deliveryday ,
es.materialrefcode,
p.monthgen,
(p.monthgen- floor(coalesce(es.deliverytime, coalesce(sm.deliverydate,60))/30)) as monthgen1,
p.yeargen,
case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end  as mname,
 coalesce(es.countfact,0) as countfact, ei.name as meas,
 coalesce(coalesce(sm.cost,0) * coalesce(es.countfact,0), 0) as cost,
 coalesce(sm.cost, 0) as price,  $P!{dNameField} as dname ,
 $P!{dbNameField} As budgetrefname ,
 (select ename || ' инв.№'  || invnumber from enelementdata where ecode = e.code)  as objname
,  cast(sm.identid as varchar ) as identid
 from enplanwork p,
enelement e ,
tkmeasurement ei,
enestimateitem es,
tkmaterials sm,
endepartment d ,
endepartment db
where p.code = es.planrefcode
and p.elementrefcode = e.code
and p.departmentrefcode = d.code
and p.budgetrefcode = db.code
and es.materialrefcode = sm.code
and ei.code = sm.measurementcode
and p.kindcode = $P{pKindCode} /*годовой(1) или текущий(2)*/
-- and p.formrefcode = 1 /*только по плановым планам */

and (select getdeliveryperiod(p.yeargen,p.monthgen, coalesce(es.deliverytime, coalesce(sm.deliverydate,60)),1)) = $P{yearGen}
and (
     ((select getdeliveryperiod(p.yeargen,p.monthgen, coalesce(es.deliverytime, coalesce(sm.deliverydate,60)),0)) = $P{monthGen} and $P{wholeYear}  = 0  ) Or
     ((select getdeliveryperiod(p.yeargen,p.monthgen, coalesce(es.deliverytime, coalesce(sm.deliverydate,60)),0)) between 1 and 12 and $P{wholeYear}  = 1  )
    )

and (p.departmentrefcode = $P{renCode} or $P{renCode} = 0)
and (e.code = $P{elementCode} or $P{elementCode} = 0)
And ((p.budgetrefcode = $P{budgCode} ) or $P{budgCode} = 0)
and p.statuscode not in (2,6)
and es.countfact <> 0
And (( es.kindrefcode  = $P{kindrefcodemat}  AND $P{kindrefcodemat} = 1 ) OR
       ( es.kindrefcode  In (1,2) AND $P{kindrefcodemat}  = 2 ))

and ( p.formrefcode = $P{planform} or $P{planform}  = 0 )   /* 1- плановых или 2 - внеплановых планов или и тех и тех  */
And ( p.staterefcode = $P{stateworkcode} or $P{stateworkcode} = 0 )
And ( e.typerefcode = $P{elementtypecode} or $P{elementtypecode} = 0 )

/* группа материалов */
and sm.code in
(select tm.code from tkmaterials tm
where tm.rootgrouprefcode = $P!{materialGroupCode} or $P!{materialGroupCode} = 0)

and ( es.statusrefcode = $P{estimatestatus} or $P{estimatestatus} = 0 )  /*только запланированые материалы или все материалы */

 and (
/*бюджетные 1 */
         (  ( p.typerefcode not in (5, 100, 106,111,112) and $P{pBudgOrInv} = 1 ) or ($P{pBudgOrInv} = 0) )

/*инвест  программа 2  */
     or  (  ( p.typerefcode in (5, 100, 106,111,112) and $P{pBudgOrInv} = 2 ) or ( $P{pBudgOrInv} = 0 ))
      )

) dat
group by identid ,  dname , budgetrefname , objname ,  monthgen,  mname,  meas, deliveryday, yeargen, /*cost,*/ price,  materialrefcode
order by identid , parent_material , dname , budgetrefname, mname ,   objname]]>
