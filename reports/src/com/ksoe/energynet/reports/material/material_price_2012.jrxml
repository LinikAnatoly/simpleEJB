	<parameter name="dateFrom" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
( select gr.name from tkmaterials gr where gr.code = m.rootgrouprefcode ) as  rootgrouprefname ,
  m.name as materialnametxt /*bi.materialnametxt*/,
  case b.vat when 20 then bi.pricewithoutnds*(1+b.vat/100)
  else bi.pricewithoutnds end as pricewithoutnds
, to_char(b.dategen,'dd.MM.yyyy') as dategen , rorg.name
, bi.measurementnametxt
, bi.pricewithoutnds as cenabeznds
from tkmaterials m, rqbillitem bi, rqbill b, rqorg rorg
where bi.materialcode = m.code
  and bi.billrefcode = b.code
  and bi.staterefcode = 1
  and rorg.code = b.orgcode
  and b.dategen =
  (
select max(b1.dategen)
from rqbillitem bi1, rqbill b1
where bi1.materialcode = m.code
  and bi1.billrefcode = b1.code
  and bi1.staterefcode = 1
  and b1.dategen between '$P!{dateFrom}' and '$P!{dateTo}'
  )
  and b.code =
  (
select max(b1.code)
from rqbillitem bi1, rqbill b1
where bi1.materialcode = m.code
  and bi1.staterefcode = 1
  and bi1.billrefcode = b1.code
  and b1.dategen between '$P!{dateFrom}' and '$P!{dateTo}'
  )
and m.rootgrouprefcode in ($P!{strGroupmaterials})
group by  m.name , bi.pricewithoutnds, b.dategen , rorg.name, b.vat , m.rootgrouprefcode , bi.measurementnametxt
order by  m.rootgrouprefcode   ,   m.name]]>
