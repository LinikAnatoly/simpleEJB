  nd.Post_id        pereopred_Post_id,
  ps.post_nazv      pereopred_post_nazv

FROM
  kadry.Prom            prom,
  kadry.Kategory        k,
  kadry.v_post          vp,
  (SELECT gp2.Grafik_Id, gp2.Norma
     FROM  kadry.Grafik_Period  gp2
     WHERE TO_DATE ($P{pdate2}, 'dd.mm.yyyy') BETWEEN gp2.S AND gp2.Po
       AND rownum<=1) g,
  kadry.Doljnost      d,
  kadry.Doljnost_Day  dd,
  (SELECT p.Id             Podr_Id,
