				        )
				               Then graf.rday8 else 0 end
				      +
				       case when ((to_date('09.'||to_char(to_date($P{pdate2},'dd.mm.yyyy'),'mm.yyyy'),'dd.mm.yyyy') between dat.s and dat.po )
				        )
