			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-32" positionType="Float" isPrintRepeatedValues="false" mode="Transparent" x="0" y="15" width="539" height="15" forecolor="#000000" backcolor="#FFFFFF" />
				<box>
