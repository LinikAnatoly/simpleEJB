	<parameter name="servicesObjectCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1017006179]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
case when ts.contractdate > '02.06.2015' then '300506' else '320627' end as mfobanka, ts.*,
textgrivna(coalesce(ts.costworks::numeric(15,2),0)) as txt_costworks,
agreewarrant.code as warrantrefcode,
agreewarrant.warrantshortfio as enwarrant_warrantshortfio,
agreewarrant.genitivefio,
agreewarrant.genitiveposition,
trim(replace(agreewarrant.address,chr(10),'')) as enwarrant_address,
agreewarrant.warrantfio as enwarrant_warrantfio,
agreewarrant.warrantposition as enwarrant_warrantposition,
agreewarrant.numbergen as enwarrant_numbergen,
to_char(agreewarrant.datestart,'dd.mm.yyyy') as enwarrant_datestart,
so.contractnumber as fincontractnumber,
to_char(so.contractdate, 'dd.MM.yyyy') as fincontractdate,
so.contragentname as contragentname,

coalesce((select string_agg(distinct m.identid2015, ',') from tkmaterials m where m.code in (
 select ei.materialrefcode from enestimateitem ei, enplanwork pl
 where pl.code = ei.planrefcode
 and pl.code in (select distinct pl.code
   from entechagrmnt2srvcsbjct tas, enservicesfromsidebjct sfs, enplanwork pl
  where tas.servicesfromsiderefcod = sfs.code
    and tas.techagrkindrefcode = 3
    and (sfs.elementcode = pl.elementrefcode or pl.servicesfsidefinid = tas.findocid)
    and tas.code = ts.code)
 and pl.kindcode = 2
 and ei.kindrefcode = 6 )), 'НЕ ВИЗНАЧЕНО') as kod_dk,

coalesce((select string_agg(distinct trim(replace(pi.commentgen, chr(10), ' ')), ', ')
from enplanworkitem pi where pi.planrefcode in (select distinct pl.code
   from entechagrmnt2srvcsbjct tas, enservicesfromsidebjct sfs, enplanwork pl
  where tas.servicesfromsiderefcod = sfs.code
    and tas.techagrkindrefcode = 3
    and (sfs.elementcode = pl.elementrefcode or pl.servicesfsidefinid = tas.findocid)
    and tas.code = ts.code
    and pl.kindcode = 2 )), 'ДОДАЙТЕ ПОСЛУГУ В ПЛАНІ') as pwi_commentgen,

coalesce((select tc.address from entechconditionsobjcts tc
where so.techconobjectscode = tc.code), '') as ty_address,

coalesce((select tc.building from entechconditionsobjcts tc
where so.techconobjectscode = tc.code), '') as ty_building
, so.contractdateservices

from entechagrmnt2srvcsbjct ts, enwarrant as agreewarrant, enservicesobject so
where so.code = ts.servicesobjectrefcode
and agreewarrant.code = ts.agreementwarrantrefcod
and ts.techagrkindrefcode = 3
and ts.servicesobjectrefcode = $P{servicesObjectCode}]]>
	</queryString>
