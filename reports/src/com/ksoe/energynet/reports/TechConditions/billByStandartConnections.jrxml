	<parameter name="agreeCode" class="java.lang.Integer"/>
	<parameter name="billCode" class="java.lang.Integer">
		<parameterDescription><![CDATA[код рахунку]]></parameterDescription>
		<defaultValueExpression><![CDATA[null]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
	CASE
		WHEN to_date(tudate,'dd.mm.yyyy') > '02.06.2015'
		THEN '300506'
		ELSE '320627'
	END AS mfobanka
	, countgen
	, okpo
	, contragentname
	, CASE
		WHEN tunumber LIKE '%EWF%'
		THEN '__________'
		ELSE tunumber
	END AS tunumber
	, contractnumber
	, tudate
	, dategen
	, itogo
	, vat
	, total
	, tubuilding
	, tuaddress
	, textgrivna(cast (total AS numeric)) AS totaltext
	, tutype
	, coalesce(fincontractnumber, '') AS fincontractnumber
	, coalesce(fincontractdate,'') AS fincontractdate
	, coalesce(departmentcode, 500000019) AS departmentcode
	, contragenttyperefcode
	, bill_numbergen
	, bill_dategen
FROM (
	SELECT
		1 AS countgen
		, coalesce(ecnt.contragentokpo,'') AS okpo
		, ecnt.contragentname
		, ts.departmentcode AS departmentcode
		, tu.numbergen AS tunumber
		, ts.contractnumber
		, to_char(tu.dategen,'dd.mm.yyyy') AS tudate
		, tu.dategen
		, coalesce(bill.sumgen, ts.tysummagen - (coalesce(ts.tysummavat, (cast(ts.tysummagen/6 AS numeric(10,2)))))) AS itogo
		, coalesce(bill.sumvat, ts.tysummavat, (cast(ts.tysummagen/6 AS numeric(10,2)))) AS vat
		, coalesce(bill.sumtotal, ts.tysummagen) AS total
		, tu.building AS tubuilding
		, trim(replace(tu.address,chr(10),'')) AS tuaddress
		, ts.techcondservicestypecd AS tutype
		, 0 AS sumpay
		, ts.fincontractnumber
		, to_char(ts.fincontractdate,'dd.mm.yyyy') AS fincontractdate
		, so.contragenttyperefcode
		, bill.numbergen AS bill_numbergen
		, bill.dategen AS bill_dategen
	FROM
		entechconditionsservcs ts
		INNER JOIN encontragent ecnt ON ecnt.techcondservicesrefcod = ts.code
		INNER JOIN entechconditionsobjcts tu ON ecnt.techconobjectscode = tu.code
		INNER JOIN enservicesobject2techcondtnsservices AS sotc ON sotc.techcondservrefcode = ts.code
		INNER JOIN enservicesobject AS so ON sotc.servicesobjectrefcode = so.code
		/*SUPP-88724 Если параметр billCode задан то подключается связка со счетами и суммы в отчет
		берутся со счетов*/
		LEFT JOIN ensobill AS bill ON (so.code = bill.servicesobjectrefcode AND $P{billCode} IS NOT NULL)
where ts.code = $P{agreeCode}
OR bill.code = $P{billCode}
LIMIT 1 ) AS s1]]>
	</queryString>
