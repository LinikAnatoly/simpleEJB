					<paragraph lineSpacing="Single"/>
				</textElement>
				<textFieldExpression><![CDATA[" в особі   " + new String(($F{warrantrefcode}.intValue() == 500000023 || $F{warrantrefcode}.intValue() == 1017000140 ) ? " Голови правління " : $F{genitiveposition} )
+ " " +
( ($F{genitivefio} != "") ? $F{genitivefio}  : "______________________________" )
