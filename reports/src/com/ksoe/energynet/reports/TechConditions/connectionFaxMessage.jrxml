	<queryString>
		<![CDATA[select enso.cnpackcode, enso.cnsubsystemtyperefcode, cnsub.name as cnsubsystemtypere,
encon.contragentname, encon.contragentaddress, ento.building, powrt.name as powerreliability, ento.tyservicespower,
ento.voltageservices, endt.name as departmentname
from net.encontragent encon, --Замовник
entechconditionsobjcts ento, --Технічні умови
entechconditionsservcs ents, --Договір про виконання технічних умов
net.enservicesobject2techcondtnsservices so2tcs, --Зв’язок договору про послуги з договором на Технічні умови
net.enservicesobject enso, 	--Об"єкти - послуги на сторону
net.cnsubsystemtype cnsub,
net.enpowerreliabilityctgr powrt, --Категорія надійності електропостачання
net.endepartment endt
where enso.code=$P{agreeCode}
and so2tcs.servicesobjectrefcode = enso.code
and so2tcs.techcondservrefcode = ents.code
and encon.techconobjectscode=ento.code
and encon.techcondservicesrefcod=ents.code
and enso.cnsubsystemtyperefcode=cnsub.code
and ento.categoryrefcode= powrt.code
and enso.departmentcode=endt.code;]]>
