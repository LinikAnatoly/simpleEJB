from
  ensheets4so s, ensheets4sotype st,
  enservicesobject so, ensigner sg,
  entechconditionsservcs tcs, entechconditionsobjcts tc, encontragent ecnt
where s.sheet4sotypecode = st.code
  and s.servicesobjectcode = so.code
  and st.signerrefcode = sg.code
  and s.code = $P{sheetCode}
  and so.techconobjectscode = tc.code
  and tc.code = ecnt.techconobjectscode
  and tcs.code = ecnt.techcondservicesrefcod
  and ecnt.techconobjectscode = tc.code]]>
