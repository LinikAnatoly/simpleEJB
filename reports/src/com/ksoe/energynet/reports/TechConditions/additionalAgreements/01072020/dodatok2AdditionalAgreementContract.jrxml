	<parameter name="agreeCode" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select 
(select cast(v.dateval as date) from ensovaluestype vt , ensovalues v
 where vt.code = 12	/*Дата та номер реєстрації заяви*/
   and vt.code=v.sovaluestypecode
   and v.servicesobjectcode in (select qq.servicesobjectrefcode from enservicesobject2techcondtnsservices qq where qq.techcondservrefcode = entechconditionsservcscode )
     ) as DateRestatementDeclare ,
     *,
case connectionkindcode
when 1
then 'Пст = Рзаявл х С = ' || tyservicespower || ' x ' || tar1value || ' = ' || tar1summa || ' тис.грн (без ПДВ)'
when 4
then 'Пнст = Рзаявл х С + (l1 + l2) х Слін =' || tyservicespower|| ' x ' || tar1value ||
          ' + (' || closestdistance || ' + ' || closestdistance2 || ') x ' || tar2value ||
          ' = ' || (tyservicessumma-tysummavat)::numeric(15,5) || ' тис.грн (без ПДВ)'
 when 2
 then 'Пнст = Рзаявл х С + lкошторис =' || tyservicespower|| ' x ' || tar1value ||
          ' + ' || (calculationsum+substationsum)::numeric(15,5) ||
 ' = ' || (tyservicessumma-tysummavat)::numeric(15,5) || ' тис.грн (без ПДВ)'
end as formula
from
(select
so.contractnumberservices, so.contractnumber, to_char(so.contractdate,'dd.mm.yyyy') as contractdate,
coalesce(so.contragentposition,'') as contragentposition,
techco.building, techco.customer, techco.dategen,
tcs.tyServicesPower::numeric(15,2) as tyservicespower,
coalesce(techco.tycurrentpower,0)::numeric(15,2) as tycurrentpower,
coalesce(sotp.infodistance2,0)::integer  as infodistance,
coalesce(sotp.closestdistance,0)::integer as closestdistance,
coalesce(sotp.closestdistance2,0)::integer as closestdistance2,
coalesce(sotp.object4closestdistncnm,'') as object4closestdistncnm,
 coalesce(sotp.object4closestdstnc2nm,'') as object4closestdstnc2nm,
tcs.tariffentryrefcode, c2ctf.tariffentry1refcode, c2ctf.tariffentry2refcode,
coalesce(c2ctf.tariff1value,contarf.value)::numeric(15,5) as tar1value ,
coalesce(c2ctf.tariff2value)::numeric(15,5) as tar2value,
(coalesce(c2ctf.summa1tariff,tcs.tyservicessumma - tcs.tysummavat)/1000)::numeric(15,5) as tar1summa ,
(coalesce(c2ctf.summa2tariff,0)/1000)::numeric(15,5) as tar2summa,
(coalesce(tcs.tyservicessumma,0)/1000)::numeric(15,5) as tyservicessumma,
(coalesce(tcs.tysummavat,0)/1000)::numeric(15,5) as tysummavat,
(coalesce(tcs.tyservicessumma - tcs.tysummavat)/1000)::numeric(15,5) as tyservicessummawithoutvat ,
(coalesce(sotp.calculationsum, 0)/1000)::numeric(15,5) as calculationsum,
(coalesce(sotp.substationbuildsum, 0)/1000)::numeric(15,5) as substationsum,
case when cnt.contragentaddress <> '' then trim(replace(cnt.contragentaddress,chr(10),' ')) else '' end as contragentaddress,
coalesce(trim(replace(cnt.contragentaddresswork,chr(10),' ')),' ') as contragentaddresswork ,
case when cnt.warrantaddress <> '' then trim(replace(cnt.warrantaddress,chr(10),' ')) else '' end as warrantaddress  ,
case when cnt.contragentokpo <> '' then 'Ідентифікаційний номер ' || cnt.contragentokpo else '' end as contragentokpo ,
coalesce(cnt.contragentbankaccount,'') as contragentbankaccount,
coalesce(cnt.contragentbankname,'') as contragentbankname,
coalesce(cnt.contragentbankmfo,'') as contragentbankmfo,
cnt.contragentname,
case when trim(replace(coalesce(cnt.contragentbossname,''),chr(10),'')) = '' then  cnt.contragentname  else trim(replace(cnt.contragentbossname,chr(10),'')) end as contragentbossname,
case when cnt.contragentpassport <> '' then  case when bt.code = 6 then 'Паспорт '
     when bt.code = 5 then 'Свідоцтво про державну реєстрацію '
     else ' '
end || trim(replace(cnt.contragentpassport,chr(10),' ')) else null end as contragentpassport ,
coalesce(powrt.name,'') as categoryRefName,
coalesce(conpp.name,'') as powerPointRefName,
coalesce(conitp.name,'') as InstallationType,
wr.warrantposition as enwarrant_warrantposition ,
wr.warrantshortfio as enwarrant_warrantshortfio,
coalesce(stt.name,'') as settlename,
COALESCE(conph.name,'') as phasityname,
 conkind.name as connectionkindname,
 conkind.code as connectionkindcode,
 coalesce(concitytype.name,'') || ' ' || coalesce(sotp.citytypename,' не вказано ') as connectioncityname,
 tcodep.name as depName , 
 tcs.code as entechconditionsservcscode
from
entechconditionsobjcts techco,
endepartment tcodep,
enbasistype bt,
enpowerreliabilityctgr powrt,
enwarrant wr,
ensettletype stt,
enconnectionkind conkind,
enservicesobject so
left join enservicesobject2techcondtnsservices as so2tcs on (so2tcs.servicesobjectrefcode=so.code)
left join entechconditionsservcs tcs on (so2tcs.techcondservrefcode = tcs.code)
left join encalc2connecttariff c2ctf on (c2ctf.techcondservrefcode = tcs.code)
left join enconnectiontariffentr as contarf on (tcs.tariffentryrefcode=contarf.code)
left join encontragent cnt on (cnt.techcondservicesrefcod = tcs.code)
left join ensotechparams sotp on (sotp.servicesobjectcode=so.code)
left join enconnectionpowerpoint conpp on (sotp.powerpointrefcode=conpp.code)
left join enconnectioninstlltntp conitp on (sotp.installationtyperefcod=conitp.code)
left join enconnectionphasity conph on (sotp.phasityrefcode = conph.code)
left join enconnectioncitytype concitytype on (sotp.citytyperefcode = concitytype.code)
where
tcs.code=$P{agreeCode}
and so.techconobjectscode=techco.code
and tcs.connectionkindrefcode = conkind.code
and cnt.techconobjectscode = techco.code
and cnt.basistypecode = bt.code
and sotp.categoryrefcode=powrt.code
and tcs.warrantrefcode=wr.code
and powrt.settletyperefcode=stt.code
and techco.departmentcode = tcodep.code
) as base_q]]>
	</queryString>
