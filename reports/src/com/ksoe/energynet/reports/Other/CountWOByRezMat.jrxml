	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="pdatestart" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("01.06.2011")]]></defaultValueExpression>
	</parameter>
	<parameter name="pdatefinal" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("30.06.2011")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select  dep.name as depname , count(distinct wo.workordernumber) as workordercount
from enplanwork p, enestimateitem e, finmaterials f,
enworkorder wo, enworkorder2enplanwork wo2pw, endepartment dep
where p.code = e.planrefcode and f.estimateitemrefcode = e.code
and p.datestart between to_date($P{pdatestart},'dd.mm.yyyy') and to_date($P{pdatefinal},'dd.mm.yyyy')
and p.kindcode = 4 and p.statuscode = 1
and wo.code = wo2pw.workordercode
and wo2pw.plancode = p.code
and p.departmentrefcode = dep.code
and f.statusrefcode <> 2
group by dep.name]]>
	</queryString>
	<field name="depname" class="java.lang.String"/>
	<field name="workordercount" class="java.math.BigDecimal"/>
