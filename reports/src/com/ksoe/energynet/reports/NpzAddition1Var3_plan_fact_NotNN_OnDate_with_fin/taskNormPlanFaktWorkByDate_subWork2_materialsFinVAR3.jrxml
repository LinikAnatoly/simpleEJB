	<parameter name="materialcode" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select fm.mat_name, fm.mu_name, fm.quantity
  from finmaterials fm, enestimateitem ei
where fm.estimateitemrefcode = ei.code
  and fm.statusrefcode = 1
  and ei.planitemrefcode =
    (case when /* если Задание-план черновое (т.е. Задания-факта нет), то берем из плана, иначе - из факта */
      (select p.statuscode
         from enplanwork p, enplanworkitem pwi
        where pwi.planrefcode = p.code
          and pwi.code = $P{penplanworkitem}
      ) = 1
      then $P{penplanworkitem}
      else $P{penfactworkitem}
    end)
  and ei.materialrefcode = $P{materialcode}]]>
	</queryString>
	<field name="mat_name" class="java.lang.String"/>
	<field name="mu_name" class="java.lang.String"/>
	<field name="quantity" class="java.lang.Double"/>
