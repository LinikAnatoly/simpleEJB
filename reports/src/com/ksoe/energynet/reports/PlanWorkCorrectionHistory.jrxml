	<queryString>
		<![CDATA[select
ch.dategen as dg, ch.dateedit as de,
cr.name as reason,
 ( case el.typerefcode when 1 then (select '(ПЛ 6-10) '||name from enline10 where elementcode = p.elementrefcode )
 when 3 then (select '(ПC 6-10/0.4) '||name  from ensubstation04 where elementcode= p.elementrefcode )
 when 2 then (select '(ПЛ 0.4) '||name from enline04 where elementcode = p.elementrefcode )
 end )
 as objname,
 r.name as rname,
ch.commentgen
from
enplancorrecthistory ch,
enplancorrectreason cr,
enplanwork p, enelement el, epren r
where ch.reasoncode = cr.code
and ch.dategen between $P{startDate} and $P{finalDate}

and p.code = ch.planrefcode
and p.renrefcode = r.code and el.code = p.elementrefcode
order by r.name, p.elementrefcode, ch.dategen desc]]>
	</queryString>
