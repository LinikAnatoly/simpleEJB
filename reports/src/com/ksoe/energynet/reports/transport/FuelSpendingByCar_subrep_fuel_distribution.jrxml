<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="FuelSpendingByCar_subrep_fuel_distribution" pageWidth="150" pageHeight="200" whenNoDataType="NoDataSection" columnWidth="150" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" isIgnorePagination="true">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<property name="ireport.zoom" value="1.5"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="travelSheetCode" class="java.lang.Double"/>
	<queryString>
		<![CDATA[select
tf.code as code
, coalesce(tf.name, '') as fuel_mark
, sum(fu.countgen) as fuel_qty
from
entravelsheet as ts left join
entravelsheetfuel as fu on ts.code = fu.travelsheetrefcode
left join tkfueltype as tf on fu.fueltypecode = tf.code
where ts.code = $P{travelSheetCode}
group by tf.code, tf.name]]>
	</queryString>
	<field name="code" class="java.lang.Double"/>
	<field name="fuel_mark" class="java.lang.String"/>
	<field name="fuel_qty" class="java.lang.Double"/>
	<background>
		<band splitType="Stretch"/>
	</background>
	<title>
		<band splitType="Stretch"/>
	</title>
