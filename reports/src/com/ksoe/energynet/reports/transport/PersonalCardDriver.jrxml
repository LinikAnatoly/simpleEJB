	<parameter name="dateFinal" class="java.util.Date"/>
	<queryString>
		<![CDATA[select numberday
, entravelsheetnumber
, gosnumber
, tabnumber
, history_koef
, name
, sum(coalesce(viratinorm,0)) as   viratinorm
, planmonth
, case when planmonth = '01' then 'Січень'
              when planmonth = '02' then 'Лютий'
              when planmonth = '03' then 'Березень'
              when planmonth = '04' then 'Квітень'
              when planmonth = '05' then 'Травень'
              when planmonth = '06' then 'Червень'
              when planmonth = '07' then 'Липень'
              when planmonth = '08' then 'Серпень'
              when planmonth = '09' then 'Вересень'
              when planmonth = '10' then 'Жовтень'
              when planmonth = '11' then 'Листопад'
              when planmonth = '12' then 'Грудень'
            end  as mm
 , rashodwork, rashodprobeg
from (
select to_char(pw.datestart,'dd.mm.yyyy') as numberday
, t.numbergen as entravelsheetnumber
, trreal.gosnumber
, fw.tabnumber
, fw.name
, ( select sum(coalesce(ei.countfact,0)) from entravlshttm2trnsprttm trav2tran  ,
           entransportitem tran , enestimateitem ei , entransport2enestimate tran2ei
            where trav2tran.travelsheetitemrefcode = tit.code
              and trav2tran.transportitemrefcode = tran.code
              and tran.code = tran2ei.transportrefcode
              and tran2ei.estimaterefcode = ei.code
              ) as viratinorm
  , to_char(pw.datestart,'mm') as planmonth
, trreal.rashodwork
, trreal.rashodprobeg
/*Коэффициент расчета топлива из истории на дату конца путевого*/
,(select
	coalesce(trh1.koef, 1)
    from tktransportrealhistory as trh1
    where t.datefinal between trh1.datestart and coalesce(trh1.datefinal, to_date('31.12.9999','dd.mm.yyyy'))
    and trh1.transportrealrefcode = trreal.code) as history_koef
 from

entravelsheetitem tit  , enplanwork pw , entravelsheet t , tktransportreal trreal , finworker fw
where tit.planrefcode = pw.code
  and pw.datestart between $P{dateStart}
                       and  $P{dateFinal}
  and pw.kindcode = 4
  and tit.travelsheetrefcode = t.code
  and t.transportrealcode = trreal.code
  and t.finworkercode = fw.code
  and fw.tabnumber::integer = $P{tabnumber}
) as sel1
group by numberday, entravelsheetnumber , gosnumber , tabnumber , name  , planmonth, rashodwork, rashodprobeg, history_koef
  order by numberday , gosnumber]]>
	</queryString>
