	</parameter>
	<parameter name="molCode" class="java.lang.String">
		<parameterDescription><![CDATA[Код МОЛ механика]]></parameterDescription>
		<defaultValueExpression><![CDATA[new String("0000")]]></defaultValueExpression>
	</parameter>
	<parameter name="fuelTypeRefCode" class="java.lang.Integer"/>
	<parameter name="molName" class="java.lang.String"/>
	<parameter name="fuelTypeName" class="java.lang.String"/>
	<parameter name="dateFinal" class="java.util.Date">
		<parameterDescription><![CDATA[Дата для выборки путевых]]></parameterDescription>
		<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="additionalCondition" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
ma.name as model
,re.gosnumber
,ts.numbergen
,ts.datestart as dategen
,fw.name as fio
,fw.tabnumber
, sum(fu.countgen) as fuel_qty
, textnumber(cast(sum(fu.countgen) as numeric),'g','літр ','літри ','літрів ')
, ft.name as fueltypename
, coalesce((select distinct molname from enspravmol where molkod = $P{molCode}), 'Невідомо') as molname
,textnumber(cast((select sum(fu1.countgen)
	from
	entravelsheet as ts1
	inner join entravelsheetfuel as fu1 on ts1.code = fu1.travelsheetrefcode
	inner join tktransportreal as re1 on ts1.transportrealcode = re1.code
	inner join tktransportmark as ma1 on re1.transportmarkcode = ma1.code
	inner join finworker as fw1 on ts1.finworkercode = fw1.code
	inner join tkfueltype as ft1 on fu1.fueltypecode = ft1.code
	where
	ts1.datestart between $P{dateStart} and $P{dateFinal}
	and get_transport_real_mol(re1.code, fu1.dategen) = $P{molCode}
	and ft1.code = $P{fuelTypeRefCode}
	and fu1.countgen > 0) as numeric),'g','літр ','літри ','літрів ') as txt_sum
from
entravelsheet as ts
inner join entravelsheetfuel as fu on ts.code = fu.travelsheetrefcode
inner join tktransportreal as re on ts.transportrealcode = re.code
inner join tktransportmark as ma on re.transportmarkcode = ma.code
inner join finworker as fw on ts.finworkercode = fw.code
inner join tkfueltype as ft on fu.fueltypecode = ft.code
where
ts.datestart between $P{dateStart} and $P{dateFinal}
and get_transport_real_mol(re.code, fu.dategen) = $P{molCode}
and ft.code = $P{fuelTypeRefCode}
and fu.countgen > 0
$P!{additionalCondition}
group by
ma.name
,re.gosnumber
,ts.numbergen
,ts.datestart
,fw.name
,fw.tabnumber
,ft.name
order by ts.datestart]]>
	</queryString>
