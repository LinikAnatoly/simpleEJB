tc."name" as transportclassificationname ,
trdep."name" as trrdepname ,
trr.invnumber as invnumber ,
trr.buhname as buhname ,
trr.gosnumber as gosnumber,
to_char(repr.datestart,'dd.mm.yyyy') as datestart,
to_char(repr.datefinal,'dd.mm.yyyy') as datefinal,
repr.commentgen
from tktransportreal trr, tktransportclassifictn tc, tktransport tr,  entransportdepartment trdep , entransportrealrepair repr
where trr.transportcode = tr.code
and tc.code = tr.transportclassifictncd
and trr.transportdepartmntrfcd = trdep.code
and trr.transportstatuscode = 75000000
and trr.code = repr.realtransportcode
and to_date( $P{datestart} ,'dd.mm.yyyy') between repr.datestart and repr.datefinal
and ( tc.code =  $P{transportClassificationcode}  or  $P{transportClassificationcode}  = -1 )
and  $P!{transportRenCode}
order by trdep."name" , tc."name"]]>
