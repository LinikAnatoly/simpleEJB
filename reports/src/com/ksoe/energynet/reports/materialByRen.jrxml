	<queryString>
		<![CDATA[select mname, meas,
cast(sum(countfact) as numeric(15,4)) as countfact,
cast(price as numeric(15,4)),
cast(sum(cost) as numeric(15,4)) as cost,
dname, monthgen from
(
select sm.name as mname, es.countfact as countfact, ei.name as meas,
 /*es.cost*/ coalesce((sm.cost * es.countfact), 0) as cost,
 /*es.price*/ coalesce(sm.cost, 0) as price, d.name as dname,
 p.monthgen as monthgen
 from enplanwork p,
tkmeasurement ei,
enestimateitem es,
tkmaterials sm
, endepartment d
where p.code = es.planrefcode and p.departmentrefcode = d.code
and es.materialrefcode = sm.code
and ei.code = sm.measurementcode
and p.yeargen = $P{yearGen}
and ((p.monthgen = $P{monthGen} and $P{wholeYear} = 0) or $P{wholeYear} <> 0)
and (p.renrefcode = $P{renCode} or $P{renCode} = 0)
and p.statuscode not in (2,6)
and es.countfact <> 0
) dat
group by monthgen, mname, meas, /*cost,*/ price, dname
order by dname, monthgen]]>
