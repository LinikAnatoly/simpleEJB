	<queryString>
		<![CDATA[Select distinct  enp.datestart ,
       coalesce(eld.ename,'') as tu ,
       coalesce(eld.invnumber,'') as accountnumber
       , o.workordernumber , o.dategen as dateworkorder
       , enpstu.name as statusplan
       , case when ehi.finworkercode is null then 'Х' else '' end as pr_human
       , case when
        coalesce((select count(*) from finmaterials fin where fin.estimateitemrefcode = eni.code ),0) = 0
              then 'Х' else '' end as pr_tmc


 from (select distinct p.code from enplanwork p , enestimateitem e
        where p.datestart between to_date($P{datestart},'dd.mm.yyyy') and to_date($P{datefinal},'dd.mm.yyyy')
          and p.budgetrefcode = 240000001
          and p.kindcode = 4
          and p.code = e.planrefcode
          and e.accountingtyperefcode = 2
          and e.countfact <> 0

      ) enp_f  ,

 enplanwork enp /*on enp.code = enp_f.code */  ,  enworkorder2enplanwork o2p , enworkorder o ,
  enplanworkitem enpi left join enhumenitem ehi on (ehi.planitemrefcode = enpi.code   and ehi.countfact <> 0 )
  , enestimateitem eni , enelementdata eld , enplanworkstatus enpstu
where enp.code = enp_f.code
  and enp.code = o2p.plancode
  and o2p.workordercode = o.code
  and o.finmolcode = $P{finmolcode}
  and enp.code = enpi.planrefcode
  and enpstu.code = enp.statuscode
  and enpi.countgen <> 0
  and enp.elementrefcode = eld.ecode
  and enpi.code = eni.planitemrefcode
  and eni.accountingtyperefcode = 1
  and eni.countfact <> 0
  and eni.kindrefcode = 1 /*материалы*/
  and (
        (
         coalesce((select count(*) from finmaterials fin where fin.estimateitemrefcode = eni.code ),0) = 0
         and
         eni.code is not null
        )

        or
         ehi.finworkercode is null
      )


order by o.workordernumber]]>
	</queryString>
