 , (select count(distinct pp.calculatedate) from ENAverageCountPersonal pp
 where pp.calculatedate between  first_day(to_date($P{datastart},'dd.mm.yyyy')) and last_day(to_date($P{dataend},'dd.mm.yyyy')) ) as countrashetmonth
 from ENAverageCountPersonal p
where p.calculatedate between  first_day(to_date($P{datastart},'dd.mm.yyyy')) and last_day(to_date($P{dataend},'dd.mm.yyyy'))
group by p.nameceh , p.personalvidname
) aaa
