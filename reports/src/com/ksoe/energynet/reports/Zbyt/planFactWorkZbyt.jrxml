	<queryString>
		<![CDATA[select  case when q_month.geoname is null then q_fact.geoname   else q_month.geoname end as district
         , 0 as isservices ,
        case when q_month.podrcode is null then q_fact.podrcode else q_month.podrcode end as monthpodrcode,
        case when q_month.podrname is null then q_fact.podrname else q_month.podrname end as monthpodrname,
        q_month.plancodes as monthplancodes,
        case when q_month.kartarefcode is null then  q_fact.kartarefcode  else q_month.kartarefcode end as monthkartarefcode ,
        (select tty.name from tktechcard tt , tkclassificationtype tty
                                    where case when q_month.kartarefcode is null then tt.code = q_fact.kartarefcode  else tt.code = q_month.kartarefcode end
                                      and tt.classificationtypecode = tty.code
                                      limit 1  ) as tkclassificationtype_name ,
        case when q_month.tktechcardname is null then q_fact.tktechcardname else q_month.tktechcardname end as monthtktechcardname,
        case when q_month.tktechcardtkmeasurement is null then q_fact.tktechcardtkmeasurement else q_month.tktechcardtkmeasurement end as monthtktechcardtkmeasurement,
        q_month.countgen as monthcountgen,
        coalesce(q_month.timegen,0)::numeric(15,2) as monthtimegen,
        to_char(q_month.mindate,'dd.mm.yyyy') as monthmindate,
        to_char(q_month.maxdate,'dd.mm.yyyy') as monthmaxdate,
        case when q_month.finexecutor_name is null
            then q_fact.finexecutor_name else
            q_month.finexecutor_name end  as finexecutor_name ,
        q_fact.podrcode as factpodrcode,
        q_fact.podrname as factpodrname,
        q_fact.plancodes as factplancodes,
        q_fact.kartarefcode as factkartarefcode,
        q_fact.tktechcardname as facttktechcardname,
        q_fact.tktechcardtkmeasurement as facttktechcardtkmeasurement ,
        q_fact.countgen as factcountgen ,
        coalesce(q_fact.timegen,0)::numeric(15,2) as facttimegen ,
        to_char(q_fact.mindate,'dd.mm.yyyy') as factmindate,
        to_char(q_fact.maxdate,'dd.mm.yyyy') as factmaxdate,
        q_fact.materials as factmaterials ,
        -- колонка додатки работы енергосбыт
        (select a.name  from enreportadditnzbtmtrlg a where
        ( case when q_month.kartarefcode is null then q_fact.kartarefcode else q_month.kartarefcode end ) in (select array2items(string_to_array(a.workcode,','))::double precision)
                                                       and a.isservices = 0
                                                       and a.zbytormetrology = 'zbyt'
        limit 1) as info_adition ,
        q_fact.finexecutor_name as finexecutor_name_fact ,
        case when q_month.enplanworktypename is null then q_fact.enplanworktypename else q_month.enplanworktypename end as enplanworktypename
 from
 (   --не услуги
    select eg.name as geoname ,  dep.code as podrcode,  dep.shortname as podrname , string_agg(pw.code::text,',') as plancodes , pwi.kartarefcode ,
     tc.techkartnumber ||' ' || tc.name as tktechcardname
      , tmeas.name as tktechcardtkmeasurement, sum(pwi.countgen) as countgen , sum(pwi.timegen) as timegen , min(pw.datestart) as mindate ,
      max(pw.datestart) maxdate,fw.name  as finexecutor_name , pt.name as enplanworktypename
    from enplanwork pw , endepartment dep , enplanworkitem pwi , tktechcard tc , tkmeasurement tmeas , finexecutor fw ,
    enplanworktype pt , enelement e , engeographicdepartment eg
    where pw.elementrefcode = e.code
    and e.geodepartmentrefcode = eg.code
    and pw.typerefcode=pt.code
    and pw.finexecutorcode = fw.code
    and pw.departmentrefcode = dep.code
    and pw.code = pwi.planrefcode
    and pwi.kartarefcode = tc.code
    and tc.measurementcode = tmeas.code
    and pwi.countgen <> 0
    and pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown($P{podr} ),','))::double precision)
    and case when $P{ruta_rukoee} = 'rukoee' then
         (       pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002010 ),','))::double precision)
	         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002007 ),','))::double precision)
	         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002013 ),','))::double precision)
	         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002016 ),','))::double precision)
	         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002001 ),','))::double precision)
         ) else 1=1
         end
		 and case when $P{ruta_rukoee} = 'ruta' then
(       pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002021 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002022 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002023 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002024 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002025 ),','))::double precision)
) else 1=1
end
    and pw.budgetrefcode = 240000001 --только енергосбыт
    and pw.kindcode = $P{pwkindcode}
    and pw.datestart between to_date($P{datestart},'dd.mm.yyyy') and to_date($P{datefinal} ,'dd.mm.yyyy')
    -- and pw.typerefcode <> 101 --ЕЗ - Плановий обхід  --- не берем в репорт
    and pw.typerefcode <> 8 --услуги отдельно
	and tc.techcardsourcecode not  in (500000039,1000000004,1000000003) -- не берем работы взятые из калькуляций
    and case when $P{finexecutorcode}= ''
     then 1=1
     else ( ($P{finexecutorcode} in
             (select distinct f.fincode::text from enplanwork p , finexecutor f where p.code = pw.code and p.finexecutorcode = f.code   )
             )
             or
             ($P{finexecutorcode} in
             (select distinct f.axorgid from enplanwork p , finexecutor f where p.code = pw.code and p.finexecutorcode = f.code   )
             )
          )
     end
    and ( pw.elementrefcode = $P{obj} or $P{obj}= 0 )
    and ( pw.typerefcode = $P{worktypecode} or $P{worktypecode} = 0   ) /*подвид работ*/
    and ( pw.formrefcode = $P{formplan} or $P{formplan} = 0   ) /*работы плановые позаплановые*/
	/*не берем месячные планы которые со статусом работы завершены и с них не делались задания планы(типа не выполнялись)*/
    and case when (select ch.plannewrefcode from enplancorrecthistory ch
                    where ch.planoldrefcode = pw.code
                      and ch.reasoncode = 4 limit 1 ) is null          and pw.statuscode = 8 then 1=2 else 1=1 end
    group by eg.name , dep.code , dep.name,  pwi.kartarefcode, tc.name, tc.techkartnumber, tmeas.name , fw.name , pt.name
) q_month
full join
(   --не услуги
    select eg.name as geoname , dep.code as podrcode,  dep.shortname as podrname , string_agg(pw.code::text,',') as plancodes , pwi.kartarefcode ,
        tc.techkartnumber ||' ' || tc.name as tktechcardname , tmeas.name as tktechcardtkmeasurement,
     		 case when pwi.kartarefcode = 2017041445 then
        coalesce(sum((select sum(coalesce(pi2el.countgen,0)) from  enpwi2element pi2el where pi2el.planworkitemrefcode = pwi.code )),0)
        else
        sum(pwi.countgen) end as countgen ,
		  case when pwi.kartarefcode = 2017041445 then
	        coalesce(sum((select sum(coalesce(pi2el.countgen,0)) from  enpwi2element pi2el where pi2el.planworkitemrefcode = pwi.code )),0) * coalesce(tc.normoftime,0)
	        else
	        sum((select sum(coalesce(hi.countfact,0)) from  net.enhumenitem hi , net.finworker fw
	               where hi.planitemrefcode = pwi.code
	               and fw.code = hi.finworkercode)) end
        as timegen
			   , min(pw.datestart) as mindate , max(pw.datestart) maxdate ,
           ''::text as materials
           , fw.name  as finexecutor_name  , pt.name as enplanworktypename
    from enplanwork pw , endepartment dep , enplanworkitem pwi , tktechcard tc , tkmeasurement tmeas , finexecutor fw , enplanworktype pt
 , enworkorder2enplanwork ord2enp , enelement e , engeographicdepartment eg
    where pw.elementrefcode = e.code
    and e.geodepartmentrefcode = eg.code
    and pw.typerefcode=pt.code
    and pw.finexecutorcode = fw.code
    and ord2enp.plancode = pw.code
    and pw.departmentrefcode = dep.code
    and pw.code = pwi.planrefcode
    and pwi.kartarefcode = tc.code
    and tc.measurementcode = tmeas.code
    and pwi.countgen <> 0
    and ( pw.statuscode = $P{statusplanfact}  or $P{statusplanfact} = 0   )
	and tc.techcardsourcecode not  in (500000039,1000000004,1000000003) -- не берем работы взятые из калькуляций
    -- если плановая часть с месячных планов
    and case when $P{pwkindcode} =2 then  pw.code in ( select eh2.plannewrefcode from enplanwork pw , enplancorrecthistory eh1 , enplancorrecthistory eh2
                    where pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown($P{podr}),','))::double precision)
                      and case when $P{ruta_rukoee} = 'rukoee' then
				         (       pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002010 ),','))::double precision)
					         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002007 ),','))::double precision)
					         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002013 ),','))::double precision)
					         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002016 ),','))::double precision)
					         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002001 ),','))::double precision)
				         ) else 1=1
				         end
						 and case when $P{ruta_rukoee} = 'ruta' then
(       pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002021 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002022 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002023 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002024 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002025 ),','))::double precision)
) else 1=1
end
                      and pw.budgetrefcode = 240000001 --только енергосбыт
                      and pw.kindcode = $P{pwkindcode}
                      and pw.datestart between to_date($P{datestart},'dd.mm.yyyy') and to_date($P{datefinal},'dd.mm.yyyy')
                     -- and pw.typerefcode <> 101 --ЕЗ - Плановий обхід  --- не берем в репорт
                      and pw.typerefcode <> 8 --услуги отдельно
                       and case when $P{finexecutorcode}= ''
				     then 1=1
				     else ( ($P{finexecutorcode} in
				             (select distinct f.fincode::text from enplanwork p , finexecutor f where p.code = pw.code and p.finexecutorcode = f.code   )
				             )
				             or
				             ($P{finexecutorcode} in
				             (select distinct f.axorgid from enplanwork p , finexecutor f where p.code = pw.code and p.finexecutorcode = f.code   )
				             )
				          )
				     end
                      and ( pw.elementrefcode = $P{obj}  or $P{obj}= 0 )
                      and ( pw.typerefcode = $P{worktypecode} or $P{worktypecode} = 0   ) /*подвид работ*/
                      and ( pw.formrefcode = $P{formplan}  or $P{formplan} = 0   ) /*работы плановые позаплановые*/
                      and eh1.planoldrefcode = pw.code
                      and eh1.reasoncode = 4
                      and eh2.planoldrefcode = eh1.plannewrefcode
                      and eh2.reasoncode = 5
                   )
             when $P{pwkindcode} =1 then pw.code in ( select eh2.plannewrefcode from enplanwork pw ,
              enplancorrecthistory eh1 , enplancorrecthistory eh2 , enplancorrecthistory eh3
                    where pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown($P{podr}),','))::double precision)
                     and case when $P{ruta_rukoee} = 'rukoee' then
				         (       pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002010 ),','))::double precision)
					         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002007 ),','))::double precision)
					         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002013 ),','))::double precision)
					         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002016 ),','))::double precision)
					         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002001 ),','))::double precision)
				         ) else 1=1
				         end
						 and case when $P{ruta_rukoee} = 'ruta' then
(       pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002021 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002022 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002023 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002024 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002025 ),','))::double precision)
) else 1=1
end
                      and pw.budgetrefcode = 240000001 --только енергосбыт
                      and pw.kindcode = $P{pwkindcode}
                      and pw.datestart between to_date($P{datestart},'dd.mm.yyyy') and to_date($P{datefinal},'dd.mm.yyyy')
                     -- and pw.typerefcode <> 101 --ЕЗ - Плановий обхід  --- не берем в репорт
                      and pw.typerefcode <> 8 --услуги отдельно
                       and case when $P{finexecutorcode}= ''
					     then 1=1
					     else ( ($P{finexecutorcode} in
					             (select distinct f.fincode::text from enplanwork p , finexecutor f where p.code = pw.code and p.finexecutorcode = f.code   )
					             )
					             or
					             ($P{finexecutorcode} in
					             (select distinct f.axorgid from enplanwork p , finexecutor f where p.code = pw.code and p.finexecutorcode = f.code   )
					             )
					          )
					     end
                      and ( pw.elementrefcode = $P{obj}  or $P{obj}= 0 )
                      and ( pw.typerefcode = $P{worktypecode} or $P{worktypecode} = 0   ) /*подвид работ*/
                      and ( pw.formrefcode = $P{formplan}  or $P{formplan} = 0   ) /*работы плановые позаплановые*/
                      and eh1.planoldrefcode = pw.code
                      and eh1.reasoncode = 3
                      and eh2.planoldrefcode = eh1.plannewrefcode
                      and eh2.reasoncode = 4
                      and eh3.planoldrefcode = eh2.plannewrefcode
                      and eh3.reasoncode = 5
                   )
             end
    group by eg.name , dep.code ,  dep.name,  pwi.kartarefcode, tc.name, tc.techkartnumber , tmeas.name , fw.name , tc.normoftime , pt.name
) q_fact
 on  (q_month.podrcode = q_fact.podrcode  and q_month.geoname = q_fact.geoname
  and q_month.kartarefcode = q_fact.kartarefcode
  and lower(trim(q_month.finexecutor_name)) = lower(trim(q_fact.finexecutor_name))
  and q_month.enplanworktypename = q_fact.enplanworktypename )

/*zzzzzzzz*/
  UNION ALL
    select
   case when q_month.geoname is null then q_fact.geoname   else q_month.geoname end as district
         , 1 as isservices ,
        case when q_month.podrcode is null then q_fact.podrcode else q_month.podrcode end as monthpodrcode,
        case when q_month.podrname is null then q_fact.podrname else q_month.podrname end as monthpodrname,
        q_month.plancodes as monthplancodes,
        case when q_month.kartarefcode is null then q_fact.kartarefcode else q_month.kartarefcode end as monthkartarefcode ,
        case when q_month.tktechcardname is null then q_fact.tktechcardname else q_month.tktechcardname end as tkclassificationtype_name ,
        case when q_month.tktechcardname is null then q_fact.tktechcardname else q_month.tktechcardname end as monthtktechcardname,
        case when q_month.tktechcardtkmeasurement is null then q_fact.tktechcardtkmeasurement else q_month.tktechcardtkmeasurement end as monthtktechcardtkmeasurement,
        q_month.countgen as monthcountgen,
        coalesce(q_month.timegen,0) as monthtimegen,
        to_char(q_month.mindate,'dd.mm.yyyy') as monthmindate,
        to_char(q_month.maxdate,'dd.mm.yyyy') as monthmaxdate,
        case when q_month.finexecutor_name is null
            then q_fact.finexecutor_name else
            q_month.finexecutor_name end  as finexecutor_name ,
        q_fact.podrcode as factpodrcode,
        q_fact.podrname as factpodrname,
        q_fact.plancodes as factplancodes,
        q_fact.kartarefcode as factkartarefcode,
        q_fact.tktechcardname as facttktechcardname,
        q_fact.tktechcardtkmeasurement as facttktechcardtkmeasurement ,
        q_fact.countgen as factcountgen ,
        coalesce(q_fact.timegen,0)::numeric(15,2) as facttimegen ,
         to_char(q_fact.mindate,'dd.mm.yyyy') as factmindate,
        to_char(q_fact.maxdate,'dd.mm.yyyy') as factmaxdate,
        q_fact.materials as factmaterials ,
        -- колонка додатки услуги енергосбыт
        (select a.name  from enreportadditnzbtmtrlg a where
        ( case when q_month.kartarefcode is null then q_fact.kartarefcode else q_month.kartarefcode end ) in (select array2items(string_to_array(a.workcode,','))::double precision)
                                                        and a.isservices = 1
                                                        and a.zbytormetrology = 'zbyt'
        limit 1 ) as info_adition ,
        q_fact.finexecutor_name as  finexecutor_name_fact ,
        case when q_month.enplanworktypename is null then q_fact.enplanworktypename else q_month.enplanworktypename end as enplanworktypename
 from
 (   -- услуги - картарефкоде - это код классификации
    select geoname , podrcode,  podrname , string_agg(plancode::text,',') as plancodes, kartarefcode , tktechcardname , tktechcardtkmeasurement,
           sum(countgen) as countgen , sum(timegen) as timegen , min(mindate) as mindate , max(maxdate) as maxdate , ''::text as materials
           ,  finexecutor_name , enplanworktypename
            from (
              select eg.name as geoname, dep.code as podrcode,  dep.shortname as podrname , pw.code as plancode, tkcl.code as kartarefcode ,
                     tkcl.kod ||' '|| tkcl.name as tktechcardname , 'ШТ'::text as tktechcardtkmeasurement,
                    -- если услуга то кол-во берем со связки калькуляция-план
                    case when pw.typerefcode = 8 and ( select count(ph.planoldrefcode) from enplancorrecthistory ph where ph.plannewrefcode = pw.code and ph.reasoncode = 13 ) > 0 then
                     ( select p2c.countgen from enplanwork2classfctntp p2c
                        where p2c.classificationtyperfcd = tkcl.code
                          and p2c.planrefcode in ( select ph.planoldrefcode from enplancorrecthistory ph where ph.plannewrefcode = pw.code and ph.reasoncode = 13 )
                          limit 1
                        )
                    else
                    ( select max(pi.countgen) from enplanwork p , enplanworkitem pi , tktechcard tkd
                       where p.code = pi.planrefcode
                       and   pi.kartarefcode = tkd.code
                       and   tkd.classificationtypecode = tkcl.code
                       and   p.code = pw.code  ) end as countgen ,

                     sum(pwi.timegen) as timegen ,
                     pw.datestart as mindate , pw.datestart maxdate , ''::text as materials
                     , fw.name  as finexecutor_name , pt.name as  enplanworktypename
              from enplanwork pw , endepartment dep , enplanworkitem pwi , tktechcard tc , tkmeasurement tmeas , tkclassificationtype tkcl
             , finexecutor  fw , enplanworktype pt , enelement e , engeographicdepartment eg
              where pw.elementrefcode = e.code
              and e.geodepartmentrefcode = eg.code
              and pw.typerefcode = pt.code
              and pw.finexecutorcode = fw.code
              and pw.departmentrefcode = dep.code
              and pw.code = pwi.planrefcode
              and pwi.kartarefcode = tc.code
              and tc.measurementcode = tmeas.code
              and pwi.countgen <> 0
              and tc.classificationtypecode = tkcl.code
              and pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown($P{podr}),','))::double precision)
               and case when $P{ruta_rukoee} = 'rukoee' then
		         (       pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002010 ),','))::double precision)
			         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002007 ),','))::double precision)
			         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002013 ),','))::double precision)
			         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002016 ),','))::double precision)
			         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002001 ),','))::double precision)
		         ) else 1=1
		         end
				 and case when $P{ruta_rukoee} = 'ruta' then
(       pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002021 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002022 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002023 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002024 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002025 ),','))::double precision)
) else 1=1
end
              and pw.budgetrefcode = 240000001 --только енергосбыт
              and pw.kindcode = $P{pwkindcode}
              and pw.datestart between to_date($P{datestart},'dd.mm.yyyy') and to_date($P{datefinal},'dd.mm.yyyy')
           --   and pw.typerefcode <> 101 --ЕЗ - Плановий обхід  --- не берем в репорт
              and ( pw.typerefcode = 8 or ( pw.typerefcode <> 8 and  tc.techcardsourcecode in (500000039,1000000004,1000000003) )  ) -- услуги или работы из источника калькуляции
               and case when $P{finexecutorcode}= ''
			     then 1=1
			     else ( ($P{finexecutorcode} in
			             (select distinct f.fincode::text from enplanwork p , finexecutor f where p.code = pw.code and p.finexecutorcode = f.code   )
			             )
			             or
			             ($P{finexecutorcode} in
			             (select distinct f.axorgid from enplanwork p , finexecutor f where p.code = pw.code and p.finexecutorcode = f.code   )
			             )
			          )
			     end
              and ( pw.elementrefcode = $P{obj}  or $P{obj}= 0 )
              and ( pw.typerefcode = $P{worktypecode} or $P{worktypecode} = 0   ) /*подвид работ*/
              and ( pw.formrefcode = $P{formplan} or $P{formplan}  = 0   ) /*работы плановые позаплановые*/
			  /*не берем месячные планы которые со статусом работы завершены и с них не делались задания планы(типа не выполнялись)*/
              and case when (select ch.plannewrefcode from enplancorrecthistory ch
                    where ch.planoldrefcode = pw.code
                      and ch.reasoncode = 4 limit 1  ) is null          and pw.statuscode = 8 then 1=2 else 1=1 end
              group by eg.name , dep.code, dep.name ,pw.code, tkcl.code,tkcl.name,tkcl.kod , pw.datestart ,pw.datestart  ,fw.name , pt.name
              order by tkcl.name
            ) q_month_in
     group by geoname,  podrcode,  podrname , kartarefcode , tktechcardname , tktechcardtkmeasurement  , finexecutor_name , enplanworktypename
) q_month
full join
(   select geoname , podrcode,  podrname , string_agg(plancode::text,',') as plancodes, kartarefcode , tktechcardname , tktechcardtkmeasurement,
           sum(countgen) as countgen , sum(timegen) as timegen , min(mindate) as mindate , max(maxdate) as maxdate, ''::text as materials
           , finexecutor_name , enplanworktypename
            from (
              select eg."name" as geoname , dep.code as podrcode,  dep.shortname as podrname , pw.code as plancode, tkcl.code as kartarefcode ,
                    tkcl.kod ||' '|| tkcl.name as tktechcardname , 'ШТ'::text as tktechcardtkmeasurement,
                    -- если услуга, то по факту кол-во = кол-во работ по калькуляции делить на кол-фактов (типа когда сделают два факта по услуге
               -- то выполнена будет равна всеравно кому количеству работ скоко заказывали)
                    case when pw.typerefcode = 8 and ( select count(ph.planoldrefcode) from enplancorrecthistory ph where ph.plannewrefcode = pw.code and ph.reasoncode = 13 ) > 0 then
                    /*( select p2c.countgen from enplanwork2classfctntp p2c
                        where p2c.classificationtyperfcd = tkcl.code
                          and p2c.planrefcode in
                          (select pcalc.code from enplanwork p , enplanwork pcalc
                            where p.code = pw.code --заходит код факта
                            and p.elementrefcode = pcalc.elementrefcode
                            and pcalc.kindcode = 5 )
                            limit 1
                        )
                        /
                        (
                        select count(pf.code) from enplanwork p , enplanwork pcalc , enplanwork pf
                        where p.code = pw.code --заходит код факта
                        and p.elementrefcode = pcalc.elementrefcode
                        and pcalc.kindcode = 5
                        and pf.elementrefcode = pcalc.elementrefcode
                        and pf.elementrefcode = p.elementrefcode
                        and pf.kindcode = 4
                        )*/
						(select * from get_countservices_by_planfact_and_cltype(pw.code::integer/*factcode*/
                                                                          , tkcl.code::integer/*tkclassificationtypecode*/ ))
                   else
                   ( select max(pi.countgen) from enplanwork p , enplanworkitem pi , tktechcard tkd
                       where p.code = pi.planrefcode
                       and   pi.kartarefcode = tkd.code
                       and   tkd.classificationtypecode = tkcl.code
                       and   p.code = pw.code  ) end as countgen ,
                     /*sum(pwi.timegen)*/
                    sum((select sum(coalesce(hi.countfact,0)) from  net.enhumenitem hi , net.finworker fw
                   where hi.planitemrefcode = pwi.code
                     and fw.code = hi.finworkercode)) as timegen ,
                     pw.datestart as mindate , pw.datestart maxdate , ''::text as materials
                     , fw.name  as finexecutor_name , pt.name as enplanworktypename
              from enplanwork pw , endepartment dep , enplanworkitem pwi , tktechcard tc , tkmeasurement tmeas , tkclassificationtype tkcl
              , finexecutor fw , enplanworktype pt , enelement e , engeographicdepartment eg
              where pw.elementrefcode = e.code
              and e.geodepartmentrefcode = eg.code
              and pt.code = pw.typerefcode
              and pw.finexecutorcode = fw.code
              and  pw.departmentrefcode = dep.code
              and pw.code = pwi.planrefcode
              and pwi.kartarefcode = tc.code
              and tc.measurementcode = tmeas.code
              and pwi.countgen <> 0
              and tc.classificationtypecode = tkcl.code
              and ( pw.statuscode = $P{statusplanfact} or $P{statusplanfact} = 0   )
			  and ( pw.typerefcode = 8 or ( pw.typerefcode <> 8 and  tc.techcardsourcecode in (500000039,1000000004,1000000003) )  ) -- услуги или работы из источника калькуляции
              and pw.code in ( select eh2.plannewrefcode from enplanwork pw , enplancorrecthistory eh1 , enplancorrecthistory eh2
                    where pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown($P{podr}),','))::double precision)
                     and case when $P{ruta_rukoee} = 'rukoee' then
			         (       pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002010 ),','))::double precision)
				         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002007 ),','))::double precision)
				         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002013 ),','))::double precision)
				         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002016 ),','))::double precision)
				         or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002001 ),','))::double precision)
			         ) else 1=1
			         end
					 and case when $P{ruta_rukoee} = 'ruta' then
(       pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002021 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002022 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002023 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002024 ),','))::double precision)
or  pw.departmentrefcode in  (select array2items(string_to_array(getdepartmentcodesdown(500002025 ),','))::double precision)
) else 1=1
end
                      and pw.budgetrefcode = 240000001 --только енергосбыт
                      and pw.kindcode = $P{pwkindcode}
                      and pw.datestart between to_date($P{datestart},'dd.mm.yyyy') and to_date($P{datefinal} ,'dd.mm.yyyy')
                    --  and pw.typerefcode <> 101 --ЕЗ - Плановий обхід  --- не берем в репорт
                      and pw.typerefcode = 8 -- только услуги
                       and case when $P{finexecutorcode}= ''
				     then 1=1
				     else ( ($P{finexecutorcode} in
				             (select distinct f.fincode::text from enplanwork p , finexecutor f where p.code = pw.code and p.finexecutorcode = f.code   )
				             )
				             or
				             ($P{finexecutorcode} in
				             (select distinct f.axorgid from enplanwork p , finexecutor f where p.code = pw.code and p.finexecutorcode = f.code   )
				             )
				          )
				     end
                      and ( pw.elementrefcode = $P{obj} or $P{obj} = 0 )
                      and ( pw.typerefcode = $P{worktypecode} or $P{worktypecode} = 0   ) /*подвид работ*/
                      and ( pw.formrefcode = $P{formplan} or $P{formplan} = 0   ) /*работы плановые позаплановые*/
                      and eh1.planoldrefcode = pw.code
                      and eh1.reasoncode = 4
                      and eh2.planoldrefcode = eh1.plannewrefcode
                      and eh2.reasoncode = 5
                   )
              group by eg.name , dep.code, dep.name ,pw.code, tkcl.code,tkcl.name, tkcl.kod, pw.datestart ,pw.datestart , fw.name , pt.name
              order by tkcl.name
            ) q_fact_in
     group by geoname , podrcode,  podrname , kartarefcode , tktechcardname , tktechcardtkmeasurement  , finexecutor_name , enplanworktypename
) q_fact
 on  (q_month.geoname = q_fact.geoname and q_month.podrcode = q_fact.podrcode and q_month.kartarefcode = q_fact.kartarefcode
      and lower(trim(q_month.finexecutor_name)) = lower(trim(q_fact.finexecutor_name))
      and q_month.enplanworktypename = q_fact.enplanworktypename  )
order by isservices , monthpodrname, district , monthkartarefcode , finexecutor_name]]>
