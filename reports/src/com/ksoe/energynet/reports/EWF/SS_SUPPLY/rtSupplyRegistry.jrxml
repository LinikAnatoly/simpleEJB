	<subDataset name="pp_set">
		<queryString language="SQL">
			<![CDATA[/* Реестр поставки.
Выборка из подсистемы ПОСТАВКА ЭЛЕКТРОЭНЕРГИИ ПОТРЕБИТЕЛЯМ БЫТОВОГО СЕКТОРА.
Списки текущих состояний договоров поставки электроэнергии.
*/
select 1 as status, 4 as id_subsystem, p.id,
  (select name from cn.cn_ren where id = p.id_ren) as renname,
  'Фізична особа' as name, p.adres, p.reg_num_spl_contract,
  cn."GetPackCurrentStatePP"(cast(p.id as integer)) as cur_state,
  (select cn."GetPackCurrentStateIDPP" (p.id)) as cur_state_id,
  to_char(case
    when p.date_spl_contract is Null then
      case
        when p.date_end_order_spl is Null then
          case
            when (select count(id) from cn.pp2cn
              where id_pp_pack = p.id and id_subsystem_cn = 20) > 0
            then (select date_end_order_spl from cn.eap_packages
              where id = (select max(id_cn_pack) from cn.pp2cn
                where id_pp_pack = p.id and id_subsystem_cn = 20))
            else
              case
                when (select count(id) from cn.pp2cn
                  where id_pp_pack = p.id and id_subsystem_cn = 18) > 0
                then (select date_end_order_spl from cn.cn_20110314_packages
                  where id = (select max(id_cn_pack) from cn.pp2cn
                    where id_pp_pack = p.id and id_subsystem_cn = 18))
                else
                  case
                    when (select count(id) from cn.pp2cn
                      where id_pp_pack = p.id and id_subsystem_cn = 13) > 0
                    then (select date_end_order_spl from cn.ncn_packages
                      where id = (select max(id_cn_pack) from cn.pp2cn
                        where id_pp_pack = p.id and id_subsystem_cn = 13))
                    else
                      case
                        when (select count(id) from cn.pp2cn
                          where id_pp_pack = p.id and id_subsystem_cn = 1) > 0
                        then (select date_end_order_spl from cn.cn_packages
                          where id = (select max(id_cn_pack) from cn.pp2cn
                            where id_pp_pack = p.id and id_subsystem_cn = 1))
						/*else
						  (select cast (min(m.startdate) as date) + 30
					      from cn.pp_movement m where m.id_pack = p.id)*/
                      end
                  end
              end
          end
        else
          p.date_end_order_spl
      end
    else p.date_spl_contract
  end, 'dd.MM.yyyy') as date_end_order_spl,
  to_char(case
    when (select count(id) from cn.pp2cn
      where id_pp_pack = p.id and id_subsystem_cn = 20) > 0
    then (select buildingdate from cn.eap_packages
      where id = (select max(id_cn_pack) from cn.pp2cn
	where id_pp_pack = p.id and id_subsystem_cn = 20))
    else
      case
	when (select count(id) from cn.pp2cn
	  where id_pp_pack = p.id and id_subsystem_cn = 18) > 0
	then (select buildingdate from cn.cn_20110314_packages
	  where id = (select max(id_cn_pack) from cn.pp2cn
	    where id_pp_pack = p.id and id_subsystem_cn = 18))
	else
	  case
	    when (select count(id) from cn.pp2cn
	      where id_pp_pack = p.id and id_subsystem_cn = 13) > 0
	    then (select buildingdate from cn.ncn_packages
	      where id = (select max(id_cn_pack) from cn.pp2cn
		where id_pp_pack = p.id and id_subsystem_cn = 13))
	    else
	      case
		when (select count(id) from cn.pp2cn
		  where id_pp_pack = p.id and id_subsystem_cn = 1) > 0
		then (select buildingdate from cn.cn_packages
		  where id = (select max(id_cn_pack) from cn.pp2cn
		    where id_pp_pack = p.id and id_subsystem_cn = 1))
					/*else
					  (select cast (min(m.startdate) as date) + 30
				      from cn.pp_movement m where m.id_pack = p.id)*/
	      end
	  end
      end
  end, 'dd.MM.yyyy') as buildingdate
from cn.pp_packages p
where p.id_pack_status not in (2, 3, 4, 1000)

order by status desc, id_subsystem desc, renname asc, name asc, adres asc;]]>
		</queryString>
		<field name="renname" class="java.lang.String"/>
		<field name="id" class="java.lang.Double"/>
		<field name="adres" class="java.lang.String">
			<fieldDescription><![CDATA[Физический адрес пациента(адрес объекта пациента)]]></fieldDescription>
		</field>
		<field name="name" class="java.lang.String"/>
		<field name="reg_num_spl_contract" class="java.lang.String">
			<fieldDescription><![CDATA[Регистрационный номер договора о поставке электроэнергии]]></fieldDescription>
		</field>
		<field name="cur_state" class="java.lang.String"/>
		<field name="buildingdate" class="java.lang.String">
			<fieldDescription><![CDATA[Дата выполнения строительства]]></fieldDescription>
		</field>
		<field name="date_end_order_spl" class="java.lang.String">
			<fieldDescription><![CDATA[Предположительная дата заключения связанного с Техническими Условиями договора об использовании электроэнергии бытовым потребителем или о поставке юридическому]]></fieldDescription>
