	<queryString>
		<![CDATA[select
mat_name  ,
mu_name ,
sum(quantity) as  quantity ,
sum(cost) as cost,
depname, budgname , pwform.name as formname,
pwstate.name as statename, pwtype.name as typename
From (
select
finfin.mat_name  ,
finfin.mu_name ,
finfin.quantity ,
finfin.price ,
finfin.cost,  depname, budgname ,
finmate.staterefcode, finmate.formrefcode, finmate.typerefcode

 from (

select fin.code as fincode , 'NPZP' as prizn,
   case when $P{dNameField} = 0 then '0' else d.name end as depname
 , case when $P{dbNameField} = 0 then '0' else db.name end As budgname ,
enp.staterefcode, enp.formrefcode, enp.typerefcode
 from finmaterials fin , enestimateitem eni ,  enplanwork enp , endepartment d , endepartment db
  where eni.code = fin.estimateitemrefcode
    and eni.planrefcode = enp.code
    and fin.statusrefcode = 1
    and enp.departmentrefcode = d.code
    and enp.budgetrefcode = db.code
    and enp.kindcode = 3
    and enp.yeargen = $P{yearGen}
    and enp.monthgen = $P{monthGen}
    And (enp.departmentrefcode = $P{renCode}  or $P{renCode} = 0)
    And (enp.budgetrefcode = $P{budgCode} or $P{budgCode} = 0)
    And (( eni.kindrefcode  = $P{kindrefcodemat}  AND $P{kindrefcodemat} = 1 ) OR
     ( eni.kindrefcode  In (1,2) AND $P{kindrefcodemat}  = 2 ))
    And ( enp.staterefcode = $P{stateworkcode} or $P{stateworkcode} = 0 )
    And ( enp.formrefcode = $P{planform} or $P{planform}  = 0 )   /* 1- плановых или 2 - внеплановых планов или и тех и тех  */
    And ( enp.typerefcode = $P{typerefcode} or $P{typerefcode}  = 0 )

    and (select coalesce(sum(en2en.countgen),0) from enestimateitem2nstmttm en2en
         where en2en.estimateiteminrefcode = eni.code  ) = 0

UNION

select fin.code as fincode , 'NPZF' as prizn,
   case when $P{dNameField} = 0 then '0' else d.name end as depname
 , case when $P{dbNameField} = 0 then '0' else db.name end As budgname ,
enp.staterefcode, enp.formrefcode, enp.typerefcode
 from finmaterials fin , enestimateitem eni ,  enplanwork enp ,  endepartment d , endepartment db
  where eni.code = fin.estimateitemrefcode
    and fin.statusrefcode = 1
    and enp.departmentrefcode = d.code
    and enp.budgetrefcode = db.code
    and eni.planrefcode = enp.code
    and enp.kindcode = 4
    and enp.yeargen = $P{yearGen}
    and enp.monthgen = $P{monthGen}
    And (enp.departmentrefcode = $P{renCode} or $P{renCode} = 0)
    And (enp.budgetrefcode = $P{budgCode} or $P{budgCode} = 0)
    And (( eni.kindrefcode  = $P{kindrefcodemat}  AND $P{kindrefcodemat} = 1 ) OR
     ( eni.kindrefcode  In (1,2) AND $P{kindrefcodemat}  = 2 ))
    And ( enp.staterefcode = $P{stateworkcode} or $P{stateworkcode} = 0 )
    And ( enp.formrefcode = $P{planform} or $P{planform} = 0 )   /* 1- плановых или 2 - внеплановых планов или и тех и тех  */
    And ( enp.typerefcode = $P{typerefcode} or $P{typerefcode} = 0 )


    ) finmate , enestimateitem enit ,  enplanwork enp1 left join  enact2enplanwork act2work on ( act2work.plancode = enp1.code )
    left join enact act  on (  act2work.actrefcode = act.code)  , finmaterials finfin

    where finmate.fincode = finfin.code
      and enit.planrefcode = enp1.code
      and finfin.estimateitemrefcode = enit.code
      and case when $P{actstatusrefcode} = 1   then  act.statusrefcode is null -- зарезервированые и нет в акте
               when $P{actstatusrefcode} = 2   then  act.statusrefcode = 1 -- есть в акте черновом
               when $P{actstatusrefcode} = 3   then  act.statusrefcode = 3 -- есть в акте проведенном
               when $P{actstatusrefcode} = 4   then  (act.statusrefcode is null or  act.statusrefcode = 1 ) -- зарезервированые и в черновом акте
               when $P{actstatusrefcode} = 5   then  act.statusrefcode in (1,3) -- в черновых и в проведеном акте
               when $P{actstatusrefcode} = 6   then  (act.statusrefcode is null or  act.statusrefcode = 3 ) -- зарезервированы и в проведеных актах
               when $P{actstatusrefcode} = 7   then  (act.statusrefcode is null or  act.statusrefcode = 1 or act.statusrefcode = 3 ) -- зарезервированы в черновых а ктах и в проведеных
          end


    ) selfinest , enplanworkstate pwstate, enplanworktype pwtype, enplanworkform pwform
    where
    selfinest.staterefcode = pwstate.code
    and selfinest.formrefcode = pwform.code
    and selfinest.typerefcode = pwtype.code

    group by mat_name  , mu_name, budgname , depname , pwform.name, pwstate.name, pwtype.name
    order by  budgname, depname, pwform.name, pwstate.name, pwtype.name , mat_name]]>
