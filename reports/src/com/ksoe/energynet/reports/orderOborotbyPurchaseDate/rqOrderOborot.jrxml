      and rqfkorderitem.fkorderrefcode = rqfkorder.code
      and rqfkorder.kindcode IN (1,15)
     /*равно проведеным   23,11,2010 добавили  чек что бы выбирать все статусы 23.01.2012 */
      and (( rqfkorder.statuscode in (3,4,5) and $P{fkorderstatus} = 0 ) or ( 1=1 and $P{fkorderstatus} = 1))
      and rqfkorder.statuscode = rqfkorderstatus.code

