,  ord.rqorgname
, ord.contractdate ,
  ord.ddscodetext,
 ( select rqorderstatus.name from rqorder , rqorderitem , rqorderstatus
   where rqorder.code = rqorderitem.orderrefcode
     and rqorderitem.code = ord.oicode
     and rqorderstatus.code = rqorder.statusrefcode limit 1  ) as rqorderstatus ,
    (
select
totalcode
from
(select
mainoi.code as TotalCode,
/*Кол-во по заявке*/
coalesce(mainoi.countfact,0) as OrderQty,
/* Кол-во по счету - естимейты */
coalesce((select
Qty
from
(select
Bills1.TotalCode as TotalCode,
Sum(Bills1.Qty) as Qty
from
(select distinct
oi.code as TotalCode,
bies.code as biescode,
coalesce(bies.countgen,0) as Qty
from
(enestimateitem as es
left join
(rqbillitem2enestimattm as bies
left join
rqbillitem as bi
on
bi.code = bies.billitemcode)
on
es.code = bies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi
on
oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0))
on
oies.estimateitemcode = es.code
where oi.code = mainoi.code) as Bills1
group by TotalCode) as Bills),0) as BIESQty,
/* Количество по приходу - естимейты без остатков с типом 1 */
coalesce((select
Qty
from
(select
FKOrders1.TotalCode,
sum(FKOrders1.Qty+FKOrders1.ostQty) as Qty
from
(select distinct
oi.code as TotalCode,
fies.code as ficode,
(coalesce(fies.countgen,0)) as Qty,
(case when fir.fkorderitemrefcode = fi.code then coalesce(fir.countgen,0) else 0 end) as ostQty
from
((enestimateitem as es
left join
rqfkorderitemremainder as fir
on
fir.estimateitemrefcode = es.code)
left join
(rqfkorderitem2enstmttm as fies
left join
(rqfkorderitem as fi
left join
rqfkorder as fo
on
fo.code = fi.fkorderrefcode and fo.statuscode in (3,4,5)  )
on
fi.code = fies.fkorderitemrefcode)
on
es.code = fies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi on oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) ) on oies.estimateitemcode = es.code
where oi.code = mainoi.code
and fo.statuscode <> 1
and fo.kindcode = 1) as FKOrders1
group by TotalCode) as FKOrders),0) as FIESQty,
/* Проверка есть ли на эту строку остаток типа 1 */
(select
TotalCode from
(select distinct
oi.code as TotalCode
from
(enestimateitem as es
left join
(rqfkorderitem2enstmttm as fies
left join
((rqfkorderitem as fi
left join
rqfkorderitemremainder as fir
on
fir.fkorderitemrefcode = fi.code)
left join
rqfkorder as fo
on
fo.code = fi.fkorderrefcode and fo.statuscode in (3,4,5) )
on
fi.code = fies.fkorderitemrefcode)
on
es.code = fies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi on oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) ) on oies.estimateitemcode = es.code
where oi.code = mainoi.code
and fo.statuscode <> 1
and fo.kindcode = 1
and fir.typerefcode = 1) as FKOrders1) as IsThereTypeOne,
/* Сумма строк прихода где есть тип 1 */
coalesce((select
sum(FIQty)
from
(select distinct
oi.code as TotalCode,
fi.countgen as FIQty
from
(enestimateitem as es
left join
(rqfkorderitem2enstmttm as fies
left join
((rqfkorderitem as fi
left join
rqfkorderitemremainder as fir
on
fir.fkorderitemrefcode = fi.code)
left join
rqfkorder as fo
on
fo.code = fi.fkorderrefcode and fo.statuscode in (3,4,5)  )
on
fi.code = fies.fkorderitemrefcode)
on
es.code = fies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi on oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) ) on oies.estimateitemcode = es.code
where oi.code = mainoi.code
and fo.statuscode <> 1
and fo.kindcode = 1) as FKOrders1
group by TotalCode),0) as FIQty,
/* Сумма строк по счету для сравнения с суммой строк прихода где есть остатки типа 1 */
coalesce((select
Qty
from
(select
Bills1.TotalCode as TotalCode,
Sum(Bills1.Qty) as Qty
from
(select distinct
oi.code as TotalCode,
bi.code as Bicode,
bi.countfact as Qty
from
(enestimateitem as es
left join
(rqbillitem2enestimattm as bies
left join
rqbillitem as bi
on
bi.code = bies.billitemcode)
on
es.code = bies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi
on
oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0))
on
oies.estimateitemcode = es.code
where oi.code = mainoi.code) as Bills1
group by TotalCode) as Bills),0) as BIQty
from
rqorderitem as mainoi
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
where  (mainoi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) ) as IncompleteDelivery
where
IncompleteDelivery.Totalcode = oicode
and
case when $P{IsComplete} > 3  then BIESQty is not null else 0=0 end
and
case
when $P{IsComplete} = 0
then 0 = 0
when $P{IsComplete} = 1
then
   case
       when IsThereTypeOne is not null
       then round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 2
then
   case
       when IsThereTypeOne is not null
       then round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 3
then
   case
       when IsThereTypeOne is not null
       then round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
           or round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
           or round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 4
then
   case
       when IsThereTypeOne is not null
       then round(cast(BIQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(BIESQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 5
then
   case
       when IsThereTypeOne is not null
       then round(cast(BIQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(BIESQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 6
then
   case
       when IsThereTypeOne is not null
       then round(cast(BIQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
           or round(cast(BIQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(BIESQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
           or round(cast(BIESQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
end
) as priznak
,
/*анализ условия по счетам ( все 0 ,  невыставленные 1 , выставленые 2 , выставлен и неоплачен 3, выставлен и  оплачен 4 )  */
( select
case when $P!{paccount} = 0 then 1 /*показываем все строки невзирая на счета  */
     when $P!{paccount} = 1 then   /*показываем строки с невыставлеными счетами*/
       case when (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                   where rqb2ordit.orderitemrefcode =  ord.oicode
                   limit 1 ) > 0 then 0 else 1 end
     when $P!{paccount} = 2 then   /*показываем строки с выставлеными счетами*/
       case when (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                   where rqb2ordit.orderitemrefcode =  ord.oicode
                   limit 1 ) > 0 then 1 else 0 end
     when (($P!{paccount} = 3) or ($P{pstatusbill} = 0 )) then /*16.10.2014 pstatusbill c нулем значиттоже самое что показывать неоплач строки*/  /*показываем строки с выставлеными счетами и неоплачеными*/
       case when  (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                    where rqb2ordit.orderitemrefcode =  ord.oicode
                    limit 1 ) > 0
               and coalesce(( /*select coalesce(sum(rqp2bilit.summafact),0) from rqpaydocitem2billitem rqp2bilit
  where rqp2bilit.billitemcode in (select rqb2ordit.billitemrefcode from rqbillitem2orderitem rqb2ordit
                                    where rqb2ordit.orderitemrefcode =  ord.oicode  )*/
                    select coalesce(bi.staterefcode,0) from rqbillitem2orderitem bi2oi , rqbillitem bi
                             where bi2oi.orderitemrefcode = ord.oicode
                             and bi2oi.billitemrefcode = bi.code
                             and bi.staterefcode = 1
                             limit 1
                    ),0) <> 1   then 1 else 0 end
     when $P!{paccount} = 4 then   /*показываем строки с выставлеными счетами и оплачеными*/
       case when  (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                    where rqb2ordit.orderitemrefcode =  ord.oicode
                    limit 1 ) > 0
               and ( select coalesce(sum(rqp2bilit.summafact),0) from rqpaydocitem2billitem rqp2bilit
  where rqp2bilit.billitemcode in (select rqb2ordit.billitemrefcode from rqbillitem2orderitem rqb2ordit
                                    where rqb2ordit.orderitemrefcode =  ord.oicode  )
                    ) > 0     then 1 else 0 end

end /*если все счета */

) as priznak_account
 from
tkmeasurement ei,
tkmaterials sm,
enestimateitem es left join
(select rqddscodes.txtcode as ddscodetext,  rqorg.name as rqorgname , rqorg.id as rqorgcode ,   o2e.estimateitemcode, ro.code, ro.numberdoc, ro.orderperiod,
roi.countfact, roi.pricewithoutnds, ro.dategen  as orderdategen ,
 roi.sumwithoutnds, roi.planneddatepays, roi.planneddatedelivery, roi.findocid, roi.contractnumber,
 ro.rqorderformcode, ro.kindrefcode, ro.rqordertypecode, roi.code as oicode ,  roi.contractdate , ro.budgetrefcode
from rqorderitem2enestimttm o2e, rqorderitem roi left join rqorg on (roi.orgcode = rqorg.code )
                                                 left join rqddscodes on (roi.ddscodescode = rqddscodes.code)
                                                 left join enresponsibles on (roi.responsiblesrefcode = enresponsibles.code)
                                                 , rqorder ro
where o2e.orderitemcode = roi.code
and ro.code = roi.orderrefcode
/*статус заявки по умолчанию только "в работе ОМТС" , если угодно то берем еще и "попереді" */
and  (
      ( ro.statusrefcode = 2 and $P{orderstatus} = 2  ) /* только в работе ОМТС */
    or
      ( ro.statusrefcode in(2,3) and $P{orderstatus}  = 23  ) /*в работе ОМТС и попередні*/
    )
and ( rqddscodes.code = $P{ddscode} or $P{ddscode}  = 0 )

/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (roi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0)
/* ответственное лицо */
and ( enresponsibles.code = $P{responsiblesCode} or $P{responsiblesCode}  = 0 )

) as ord on ord.estimateitemcode = es.code


,rqordertype tp, rqorderkind kn
,enplanwork p, enelementdata ed
,endepartment db, endepartment r
where p.code = es.planrefcode
and p.elementrefcode = ed.ecode
and p.budgetrefcode = db.code
and p.departmentrefcode = r.code
and es.materialrefcode = sm.code
and ei.code = sm.measurementcode
and es.countfact <> 0
and (( es.kindrefcode  = 1  AND 1 = 1 ) OR
     ( es.kindrefcode  In (1,2) AND 1  = 2 ))

and ord.kindrefcode = kn.code
and ord.rqordertypecode = tp.code


/*and db.code = 75000016
and ord.code = 500000494*/

/*and sm.code = 500000486*/

/*and ord.code = 500000423*/

/* типы заявок */
/* ХОЕ планова = 4 , ХОЕ позапланова = 5 */
and  (ord.kindrefcode = $P!{orderform} or $P!{orderform} = 0)
/*1 - бюджетна заявка , 2 - инвестпрограмма*/
and  (ord.rqordertypecode = $P!{ordertype} or $P!{ordertype} = 0)

/* SUPP-91442 выборка строк заявок по периоду плановых закупок  */
and ord.oicode in
( SELECT DISTINCT oi2ei.orderitemcode from rqplanpurchase pl , rqpurchaseitem pli , rqpurchaseitem2estmttm pli2ei , rqorderitem2enestimttm oi2ei
		where pl.dateadd between to_date($P{startDate},'dd.MM.yyyy') and to_date($P{endDate},'dd.MM.yyyy')
		and pl.code = pli.purchaserefcode
		and pli.code = pli2ei.purchaseitemrefcode
		and pli2ei.estimateitemrefcode=oi2ei.estimateitemcode
)

/* группа материалов */
and sm.code in
(select tm.code from tkmaterials tm
where $P!{strGroupmaterials})

/* номер договора */
and (ord.findocid = $P!{contractNumber} or $P!{contractNumber} = 0)

/* бюджетодержатель */
and (db.code = $P!{budgCode} or $P!{budgCode} = 0)

/* подразделение */
and (r.code = $P!{renCode} or $P!{renCode} = 0)

/* заявка */
and (ord.code = $P!{orderCode} or $P!{orderCode} = 0)

/* объект планирования */
and (ed.ecode = $P!{elementCode} or $P!{elementCode} = 0)

/*План */
and (p.code = $P!{planCode} or $P!{planCode} = 0)

/*постачальник */
 and (ord.rqorgcode = $P!{porgcode} or  $P!{porgcode} = 0 )


group by sm.code ,  (select name from tkmaterials tm
where tm.code in
(select rootgrouprefcode
from tkmaterials
where code = sm.code)),
case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end,
 ei.name,
 sm.numkatalog ,
  ord.numberdoc, ord.orderperiod,
 ord.pricewithoutnds,
 ord.countfact, ord.sumwithoutnds,
 ord.planneddatedelivery, ord.planneddatepays,
 ord.oicode,
 ord.contractnumber, /*db.name, */ /*r.name, ed.ename,*/ tp.name, kn.name ,
 ord.rqorgname ,
 ord.contractdate ,
 ord.ddscodetext ,
 ord.budgetrefcode ,
 ord.orderdategen

order by  (select name from tkmaterials tm
where tm.code in
