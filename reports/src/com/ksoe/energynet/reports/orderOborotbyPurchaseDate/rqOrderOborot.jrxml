 ,
 /*кол-во которое попало в счета */
 coalesce((select sum(bb.countfact) from rqbillitem2orderitem bb where bb.orderitemrefcode=ord.oicode) ,0) as count_in_bills ,

 /*количество строк для движения */
(
SELECT count(countfact) from (
