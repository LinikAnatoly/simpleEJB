	<queryString>
		<![CDATA[select case when distorder>1 then iefact else bifact end as countfact,
  numberdoc, dategen, billprice,
  case when distorder>1 then (billprice*iefact) else (billprice*bifact) end as billlsum,
  billitem, billitemrefcode,
  case when vat>0 then (billprice*(case when distorder>1 then iefact else bifact end))/5 else 0 end as vat,
  orderitemrefcode, pcount, invcount , contractnumber
from (
select sum(be.countgen) as iefact,
  (select bbb.countfact from rqbillitem bbb where bbb.code = bi.code) as bifact,
  b.numberdoc as numberdoc,
  b.dategen as dategen, bi.pricewithoutnds as billprice,
  bi.code as billitem, bi.code as billitemrefcode,
  b.vat as vat,
  bo.orderitemrefcode,

(select distinct count(bb.orderitemrefcode) from rqbillitem2orderitem bb
 where bb.billitemrefcode = bi.code
 group by bb.billitemrefcode) as distorder,

(select coalesce(count(p2b.billitemcode),0)
from rqpaydoc p , rqpaydocitem pi, rqpaydocitem2billitem p2b
where pi.paydocrefcode = p.code
and p2b.paydocitemcode = pi.code
and p2b.billitemcode = bi.code) as pcount,

(select coalesce(count(ii.code),0)
from rqfkorderitem2billitem i2e, rqfkorderitem ii, rqfkorder i
where i2e.fkorderitemrefcode = ii.code
and i.code = ii.fkorderrefcode
and i2e.billitemrefcode = bi.code) as invcount
, bi.contractnumber

from rqorderitem2enestimttm oe,  rqbillitem2enestimattm be,
     rqbill b, rqbillitem bi, rqbillitem2orderitem bo
where be.estimateitemcode = oe.estimateitemcode
 and be.billitemcode = bo.billitemrefcode
 and bo.orderitemrefcode = oe.orderitemcode
 and bi.code = bo.billitemrefcode
 and b.code = bi.billrefcode
 and bo.orderitemrefcode = $P{oicode}
group by b.numberdoc, b.dategen, bi.pricewithoutnds,
  bi.sumwithoutnds, bi.code, b.vat, bo.orderitemrefcode , bi.contractnumber) w]]>
	</queryString>
