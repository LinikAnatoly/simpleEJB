	<parameter name="pordercode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select

 /*выбор наряда */
  ( select enwork.workordernumber || ' / ' || enwork.dategen from enworkorder enwork , finmoldata fm
                     where fm.code = finfin.moldatarefcode
                       and enwork.code  =  fm.workordercode

  )   as naryad ,
 /*выбор ордера*/
 (
   select rqup.numberdoc || ' / ' ||to_char(rqup.dategen , 'dd.mm.yyyy') from rqfkorderitem2enstmttm ieup
                                        , rqfkorderitem rqiup
                                        , rqfkorder rqup
                                          where ieup.finmaterialsrefcode = finfin.code
                                            and ieup.fkorderitemrefcode = rqiup.code
                                            and rqup.code = rqiup.fkorderrefcode
  ) as order
 ,
 /*выбор акта */
 ( select distinct act.numbergen || ' / ' || act.dategen from enact act , enact2enplanwork ena2w , enestimateitem eni , enplanwork enp
                     where enp.code = eni.planrefcode
                       and eni.code = finfin.estimateitemrefcode
                       and ena2w.plancode = enp.code
                       and ena2w.actrefcode = act.code


  )   as actt ,
 finfin.mat_name , finfin.nn , finfin.mu_name  , finfin.div_code , finfin.div_name , finfin.party_id , finfin.quantity
from (
-- Проверка по расходам
select * from finmaterials f where f.statusrefcode = 1
and party_id in
(
 select f.party_id from finmaterials f where f.code in
 (
  select ie.finmaterialsrefcode from rqfkorderitem2enstmttm ie where ie.fkorderitemrefcode in
 (select oi.code from rqfkorderitem oi where oi.fkorderrefcode = $P{pordercode} /*500002772*/)
 )
)
and f.code not in
(
 select f.code from finmaterials f where f.code in
 (
 select ie.finmaterialsrefcode from rqfkorderitem2enstmttm ie where ie.fkorderitemrefcode in
 (select oi.code from rqfkorderitem oi where oi.fkorderrefcode = $P{pordercode} /*500002772*/)
 )
 and f.statusrefcode = 1
)
-- and f.div_code = '5110'
and f.div_code = (select o.moloutcode from rqfkorder o where o.code = $P{pordercode} /*500002772*/)
and f.modify_time > (select o.modify_time from rqfkorder o where o.code = $P{pordercode} /*500002772*/)

UNION
-- Проверка по приходам (выбираем по партии приходного ордера расходы )
Select * from finmaterials fin
  where fin.statusrefcode = 1
  and party_id in  ( /* выбор кода партии по приходному ордеру */
                   select distinct RQFKORDERDATA2FKPARTY.PARTYCODE from RQFKORDERDATA2FKPARTY where RQFKORDERDATA2FKPARTY.FKORDERREFCODE = $P{pordercode} /*500002772*/
                   )


) finfin
order by finfin.modify_time]]>
	</queryString>
