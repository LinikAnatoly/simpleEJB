	<parameter name="group_enestimateitem" class="java.lang.String"/>
	<queryString>
		<![CDATA[select distinct   e2c.findocid, o.id , e2c.contractnumber , o.name
from  enestimateitem ei , tkmaterials tm , enestimateitem2contrct e2c left join rqorg o on e2c.orgcode = o.code

      where e2c.estimateitemcode = ei.code
      and ei.materialrefcode = tm.code
      and e2c.estimateitemcode in(  $P!{group_enestimateitem} )
Union

select distinct  oi.findocid , o.id , oi.contractnumber , o.name
 from rqorderitem2enestimttm qqq , rqorderitem oi  , rqorg o
where qqq.estimateitemcode in( $P!{group_enestimateitem} )
and qqq.orderitemcode = oi.code
and oi.orgcode = o.code]]>
	</queryString>
