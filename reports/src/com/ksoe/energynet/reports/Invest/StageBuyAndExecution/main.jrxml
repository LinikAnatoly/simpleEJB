<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="main" pageWidth="2300" pageHeight="595" orientation="Landscape" whenNoDataType="AllSectionsNoDetail" columnWidth="2280" leftMargin="10" rightMargin="10" topMargin="10" bottomMargin="10" isIgnorePagination="true">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="ipcode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="kvartal" class="java.lang.Integer"/>
	<parameter name="datereport" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
( case
when $P{kvartal} = 1 then 'I'
when $P{kvartal} = 2 then 'II'
when $P{kvartal} = 3 then 'III'
when $P{kvartal} = 4 then 'IV'
else '' end ) as kvartal_rm
,code
,eninvestprogramgroups_name
,eninvestprogramgroups_concat
,itemnumber
,enippurposeprogram
,enippurposeprogramcode
,name_product
,measurementname
,price
,countgen
,sumgen
,invgrouprefcode
,period_count
,period_sum
,financing
,commentgen
,yeargen
,rep_kvartal
,datereport
,itcodesandchildcodes
,profinans_count
,(profinans_sum/1000)::numeric(15,2) as profinans_sum
,osvoeno_count
,(osvoeno_sum/1000)::numeric(15,2) as osvoeno_sum
,osvoeno_docs
,contragent
---
, (countgen-profinans_count)::numeric(15,2) as not_financed_count
, (sumgen-profinans_sum/1000)::numeric(15,2) as not_financed_sum
, case when profinans_count > 0 and profinans_sum > 0
          then ((((profinans_sum/1000/profinans_count) / price ) *100 )-100)::numeric(15,2)
       when (profinans_count <= 0 or profinans_sum <= 0 ) and  (osvoeno_count > 0 and osvoeno_sum > 0 )
          then ((((osvoeno_sum/1000/osvoeno_count) / price ) *100 )-100)::numeric(15,2)

else null
end
as persent
,infotenders
from (
select
ipi.code
,ipgro.name as eninvestprogramgroups_name
,ipgro.name || ' ' || ipgro.commentgen as eninvestprogramgroups_concat
,ipi.itemnumber
,purp.name as enippurposeprogram
,purp.code as enippurposeprogramcode
,ipi.name as name_product
,meas.name as measurementname
,ipi.price
,ipi.countgen
,ipi.sumgen
,ipi.invgrouprefcode
, case when rep_kvartal = 1 then coalesce(ipi.quarter1count,0)
       when rep_kvartal = 2 then coalesce(ipi.quarter1count,0) + coalesce(ipi.quarter2count,0)
       when rep_kvartal = 3 then coalesce(ipi.quarter1count,0) + coalesce(ipi.quarter2count,0)+ coalesce(ipi.quarter3count,0)
       when rep_kvartal = 4 then coalesce(ipi.quarter1count,0) + coalesce(ipi.quarter2count,0)+ coalesce(ipi.quarter3count,0)+ coalesce(ipi.quarter4count,0)
     end as period_count
, case when rep_kvartal = 1 then coalesce(ipi.quarter1sum,0)
       when rep_kvartal = 2 then coalesce(ipi.quarter1sum,0) + coalesce(ipi.quarter2sum,0)
       when rep_kvartal = 3 then coalesce(ipi.quarter1sum,0) + coalesce(ipi.quarter2sum,0)+ coalesce(ipi.quarter3sum,0)
       when rep_kvartal = 4 then coalesce(ipi.quarter1sum,0) + coalesce(ipi.quarter2sum,0)+ coalesce(ipi.quarter3sum,0)+ coalesce(ipi.quarter4sum,0)
     end as period_sum

,ipi.financing /*джерело фынансування */
,ipi.commentgen
,ip.yeargen
,rep_kvartal
,datereport
,(select string_agg(code::varchar, ',')from
(select it.code from enipitem it
where it.code = ipi.code
union all
select it.code from enipitem it
where it.parentrefcode = ipi.code ) selitcode) as itcodesandchildcodes
,

COALESCE(
case when (select count(enipitem2tkmaterials.code) from enipitem2tkmaterials
             where enipitem2tkmaterials.ipitemrefcode in (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code )
                                       ) > 0    then
        (/* кол-во профинансировано если на пункте ИП проставлен материал */
        select sum(bi2ei.countgen)
        from enplanwork p , enestimateitem ei , rqbillitem2enestimattm bi2ei , rqbillitem bi ,
          enipitem2plan ipit2pl
         , enipitem2tkmaterials iti2tm , (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code ) dataenipitem
        where   ipit2pl.ipitemrefcode = dataenipitem.code
        and p.kindcode = 2
        and bi2ei.billitemcode = bi.code
        and bi.ddscodestxt = '312 100' -- выполнено по ддс ип
        and ei.kindrefcode in (1,6)
        and p.code = ei.planrefcode
        and ei.code = bi2ei.estimateitemcode
        and ipit2pl.planrefcode = p.code

        and ipit2pl.ipitemrefcode = iti2tm.ipitemrefcode
        and ei.materialrefcode = iti2tm.materialrefcode

        and bi.code in ( select pi2bi.billitemcode from rqpaydocitem2billitem pi2bi , rqpaydocitem pi , rqpaydoc pay
                               where pi2bi.billitemcode = bi.code
                                  and pi2bi.paydocitemcode = pi.code
                                  and pi.paydocrefcode = pay.code
                                  --and pay.dategen <= datereport
                                  and pay.dategen between to_date('01.01.'||ip.yeargen ,'dd.mm.yyyy')  and datereport
                                  )
         )
 else /*если на пункте материал не проставлен то выбирается кол-во с поля investWorksAmount
        с утвержденных заданий факт которые относятся к этому пункту*/
  coalesce( (select sum(p.investWorksAmount)
    from enipitem iti , enipitem2plan ipit2pl , enplancorrecthistory chp ,  enplancorrecthistory chpfact ,enplanwork p
    , (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code ) dataenipitem
    where   ipit2pl.ipitemrefcode = dataenipitem.code
    and iti.code = ipit2pl.ipitemrefcode
    and ipit2pl.planrefcode = chp.planoldrefcode
    and chp.reasoncode = 4
    and chp.plannewrefcode = chpfact.planoldrefcode
    and chpfact.reasoncode = 5
    and p.code = chpfact.plannewrefcode
    and p.statuscode = 3

             ),0)

 end
 ,0) as profinans_count
 --------------------
 /*сумма профинансировано без НДС */
 , coalesce( ( select sum(summagen)::numeric(15,2) from
(
select sum(
  (
  bi2ei.countgen
  *
  (bi.pricewithoutnds*pi.summagen/bi.sumwithoutnds  )
  )

   )  / (1::numeric+(b.vat::numeric/100::numeric) )  as summagen

  from enplanwork p , enestimateitem ei , rqbillitem2enestimattm bi2ei ,  rqpaydocitem2billitem pi2bi ,
  rqpaydocitem pi , rqpaydoc pay , rqbillitem bi , rqbill b , enipitem2plan ipit2pl
  , (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code ) dataenipitem
where ipit2pl.ipitemrefcode = dataenipitem.code
and p.kindcode = 2
and bi.ddscodestxt = '312 100' -- выполнено по ддс ип
and ei.kindrefcode in (1,6)
and p.code = ei.planrefcode
and ei.code = bi2ei.estimateitemcode
and pi2bi.billitemcode = bi2ei.billitemcode
and pi2bi.paydocitemcode = pi.code
and pi.paydocrefcode = pay.code
and bi2ei.billitemcode = bi.code
-- and pay.dategen <= datereport
and pay.dategen between to_date('01.01.'||ip.yeargen ,'dd.mm.yyyy')  and datereport
and bi.billrefcode = b.code
and ipit2pl.planrefcode = p.code

/* если на пункте указаны материалы то сумму оплат только по этим материалам из плана
   иначе берется сумма оплат по материалам и услугам скопом */
and case when (select count(enipitem2tkmaterials.code) from enipitem2tkmaterials
             where enipitem2tkmaterials.ipitemrefcode in (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code )
                                       ) > 0 then
        ei.materialrefcode in  (select enipitem2tkmaterials.materialrefcode from enipitem2tkmaterials
             where enipitem2tkmaterials.ipitemrefcode in (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code )
                                       )
     else 1=1
     end

group by b.vat
) s1 ),0) as profinans_sum
------------------------------- osvoeno count
, COALESCE(
case when (select count(enipitem2tkmaterials.code) from enipitem2tkmaterials
             where enipitem2tkmaterials.ipitemrefcode in (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code )
                                       ) > 0    then
        (/* кол-во освоено если на пункте ИП проставлен материал */
        select sum(fi2ei.countgen)
        from enplanwork p , enestimateitem ei , rqbillitem2enestimattm bi2ei , rqbillitem bi ,
         enipitem2plan ipit2pl
         , enipitem2tkmaterials iti2tm , rqfkorderitem2enstmttm fi2ei , rqfkorderitem fi , rqfkorder f
         , (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code ) dataenipitem
        where   ipit2pl.ipitemrefcode = dataenipitem.code
        and p.kindcode = 2
        and bi2ei.billitemcode = bi.code
        and bi.ddscodestxt = '312 100' -- выполнено по ддс ип
        and ei.kindrefcode in (1,6)
        and p.code = ei.planrefcode
        and ei.code = bi2ei.estimateitemcode
        and ipit2pl.planrefcode = p.code
        and fi2ei.estimateitemcode = ei.code
        and fi2ei.fkorderitemrefcode = fi.code
        and fi.fkorderrefcode = f.code
        and f.kindcode in (1,14)
        and f.statuscode in (3,4,5)
        and f.dategen <= datereport /*дата освоения материалов */

        and bi.code in ( select pi2bi.billitemcode from rqpaydocitem2billitem pi2bi , rqpaydocitem pi , rqpaydoc pay
                               where pi2bi.billitemcode = bi.code
                                  and pi2bi.paydocitemcode = pi.code
                                  and pi.paydocrefcode = pay.code
                                  )

        and ipit2pl.ipitemrefcode = iti2tm.ipitemrefcode
        and ei.materialrefcode = iti2tm.materialrefcode
         )
 else /*если на пункте материал не проставлен то выбирается кол-во с поля investWorksAmount
        с утвержденных заданий факт которые относятся к этому пункту*/
  coalesce( (select sum(p.investWorksAmount)
    from enipitem iti , enipitem2plan iti2pl , enplancorrecthistory chp ,  enplancorrecthistory chpfact ,enplanwork p
    where iti.code = iti2pl.ipitemrefcode
    and iti2pl.planrefcode = chp.planoldrefcode
    and chp.reasoncode = 4
    and chp.plannewrefcode = chpfact.planoldrefcode
    and chpfact.reasoncode = 5
    and p.code = chpfact.plannewrefcode
    and p.statuscode = 3
    and iti2pl.ipitemrefcode in (
          select it.code from enipitem it
          where it.code = ipi.code
          union all
          select it.code from enipitem it
          where it.parentrefcode = ipi.code
                                )
             ),0)

 end
 ,0) as osvoeno_count
 ------------------------------- osvoeno summa
, COALESCE(
        (select sum(summwithoutnds) from
        (/* кол-во освоено если на пункте ИП проставлен материал */
        select fi2ei.countgen * fi.pricewithoutnds as summwithoutnds
        from enplanwork p , enestimateitem ei , rqbillitem2enestimattm bi2ei ,
          enipitem2plan ipit2pl
         , rqfkorderitem2enstmttm fi2ei , rqfkorderitem fi , rqfkorder f , rqbillitem bi
         , (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code ) dataenipitem
        where   ipit2pl.ipitemrefcode = dataenipitem.code
        and ipit2pl.planrefcode = p.code
        and p.kindcode = 2
        and bi2ei.billitemcode = bi.code
        and bi.ddscodestxt = '312 100' -- выполнено по ддс ип
        and ei.kindrefcode in (1,6)
        and p.code = ei.planrefcode
        and ei.code = bi2ei.estimateitemcode
        and fi2ei.estimateitemcode = ei.code
        and fi2ei.fkorderitemrefcode = fi.code
        and fi.fkorderrefcode = f.code
        and f.kindcode in (1,14)
        and f.statuscode in (3,4,5)
        and f.dategen <= datereport /*дата освоения материалов */

        and bi.code in ( select pi2bi.billitemcode from rqpaydocitem2billitem pi2bi , rqpaydocitem pi , rqpaydoc pay
                               where pi2bi.billitemcode = bi.code
                                  and pi2bi.paydocitemcode = pi.code
                                  and pi.paydocrefcode = pay.code
                                  )

        /* если на пункте указаны материалы то сумму  только по этим материалам из плана
   иначе берется сумма по материалам и услугам скопом */
     and case when (select count(enipitem2tkmaterials.code) from enipitem2tkmaterials
             where enipitem2tkmaterials.ipitemrefcode in (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code )
                                       ) > 0 then
        ei.materialrefcode in  (select enipitem2tkmaterials.materialrefcode from enipitem2tkmaterials
             where enipitem2tkmaterials.ipitemrefcode in (select it.code from enipitem it
                                       where it.code = ipi.code
                                        union all
                                       select it.code from enipitem it
                                       where it.parentrefcode = ipi.code )
                                       )
     else 1=1
     end
         ) as qqq )

 ,0) as osvoeno_sum
 ------------------------------- osvoeno dokumenti
,

        (/* перечень документов по освоению */
        select string_agg(distinct(f.numberdoc) ,',')
        from enplanwork p , enestimateitem ei , rqbillitem2enestimattm bi2ei ,
         rqpaydocitem2billitem pi2bi , rqpaydocitem pi , rqpaydoc pay , enipitem2plan ipit2pl , rqbillitem bi
         , rqfkorderitem2enstmttm fi2ei , rqfkorderitem fi , rqfkorder f
                                      , (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code ) dataenipitem
        where   ipit2pl.ipitemrefcode = dataenipitem.code
        and p.kindcode = 2
        and bi2ei.billitemcode = bi.code
        and bi.ddscodestxt = '312 100' -- выполнено по ддс ип
        and ei.kindrefcode in (1,6)
        and p.code = ei.planrefcode
        and ei.code = bi2ei.estimateitemcode
        and pi2bi.billitemcode = bi2ei.billitemcode
        and pi2bi.paydocitemcode = pi.code
        and pi.paydocrefcode = pay.code
        and ipit2pl.planrefcode = p.code
        and fi2ei.estimateitemcode = ei.code
        and fi2ei.fkorderitemrefcode = fi.code
        and fi.fkorderrefcode = f.code
        and f.kindcode in (1,14)
        and f.statuscode in (3,4,5)
        and f.dategen <= datereport /*дата освоения материалов */

        /* если на пункте указаны материалы то сумму  только по этим материалам из плана
   иначе берется сумма по материалам и услугам скопом */
     and case when (select count(enipitem2tkmaterials.code) from enipitem2tkmaterials
             where enipitem2tkmaterials.ipitemrefcode in (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code )
                                       ) > 0 then
        ei.materialrefcode in  (select enipitem2tkmaterials.materialrefcode from enipitem2tkmaterials
             where enipitem2tkmaterials.ipitemrefcode in (select it.code from enipitem it
                                       where it.code = ipi.code
                                        union all
                                       select it.code from enipitem it
                                       where it.parentrefcode = ipi.code  )
                                       )
     else 1=1
     end
          )

  as osvoeno_docs

  ------------------------------- postavchiki
,

        (/* перечень поставщиков */
        select string_agg(distinct(o.name) ,',')
        from enplanwork p , enestimateitem ei , rqbillitem2enestimattm bi2ei ,
         rqpaydocitem2billitem pi2bi , rqpaydocitem pi , rqpaydoc pay , enipitem2plan ipit2pl
         , rqbillitem bi , rqbill b , rqorg o
         , (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code ) dataenipitem
        where   ipit2pl.ipitemrefcode = dataenipitem.code
        and p.kindcode = 2
        and ei.kindrefcode in (1,6)
        and bi.ddscodestxt = '312 100' -- выполнено по ддс ип
        and p.code = ei.planrefcode
        and ei.code = bi2ei.estimateitemcode
        and pi2bi.billitemcode = bi2ei.billitemcode
        and pi2bi.paydocitemcode = pi.code
        and pi.paydocrefcode = pay.code
        and ipit2pl.planrefcode = p.code
        and bi2ei.billitemcode = bi.code
        and bi.billrefcode = b.code
        and b.orgcode = o.code

        /* если на пункте указаны материалы то сумму  только по этим материалам из плана
   иначе берется сумма по материалам и услугам скопом */
     and case when (select count(enipitem2tkmaterials.code) from enipitem2tkmaterials
             where enipitem2tkmaterials.ipitemrefcode in (select it.code from enipitem it
                                       where it.code = ipi.code
                                       union all
                                      select it.code from enipitem it
                                       where it.parentrefcode = ipi.code )
                                       ) > 0 then
        ei.materialrefcode in  (select enipitem2tkmaterials.materialrefcode from enipitem2tkmaterials
             where enipitem2tkmaterials.ipitemrefcode in (select it.code from enipitem it
                                       where it.code = ipi.code
                                        union all
                                       select it.code from enipitem it
                                       where it.parentrefcode = ipi.code )
                                       )
     else 1=1
     end
          )

  as contragent
,ipi.infotenders
from net.enip ip ,
net.enipitem ipi ,
net.enippurposeprogram purp, /*целевая программа*/
net.eninvestprogramgroups ipgro , /*разделы ИП*/
net.tkmeasurement meas ,
( select $P{kvartal} as rep_kvartal ) as kvartal ,
( select to_date($P{datereport},'dd.mm.yyyy') as datereport ) as datereport
where ip.code = $P{ipcode}

and ip.code = ipi.iprefcode
and ipi.parentrefcode is null
and ipi.purposeprogramrefcode = purp.code
and ipgro.code = ipi.invgrouprefcode
and ipi.measurementcode = meas.code
order by ipi.invgrouprefcode , func_order_(itemnumber) , purp.code
) sel1

order by invgrouprefcode , func_order_(itemnumber) , enippurposeprogramcode]]>
