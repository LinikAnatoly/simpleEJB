from
epren r,
enelementdata ed,
enplanwork p, enestimateitem ei , tkmaterials tkm left join tkmeasurement tkms on tkm.measurementcode = tkms.code
where p.code = ei.planrefcode
and ed.ecode = p.elementrefcode
and ei.planitemrefcode is null
and ei.materialrefcode = tkm.code
and p.renrefcode = r.code
