	</parameter>
	<queryString>
		<![CDATA[SELECT tr.name as transportname, tr.gosnumber as gosnumber,
tr.invnumber as invnumber,
substring(fw.name from 1 for position(' ' in fw.name)) ||
substring(trim(substring(fw.name from position(' ' in fw.name)+1)), 1, 1) ||'.'||
substring(trim(substring(fw.name from position(' ' in fw.name)+1)),
  position(' ' in substring(fw.name from position(' ' in fw.name)+1))+1, 1) ||'.'
as voditel,
fw.tabnumber as tabnumber,
sum(ti.countworkfact) as mashinogodyny,
sum(coalesce((Select sum(distance) from endistance where transportitemrefcode = ti.code), 0)) as distance
from entransportitem ti
inner join enplanworkitem pwi on pwi.code = ti.planitemrefcode
inner join tktransportreal tr on ti.transportrealcode = tr.code
left join finworker fw on ti.finworkercode = fw.code
where ti.planrefcode = $P{planworkcode}
and pwi.countgen <> 0
group by tr.name, tr.gosnumber, tr.invnumber, fw.name, fw.tabnumber]]>
	</queryString>
