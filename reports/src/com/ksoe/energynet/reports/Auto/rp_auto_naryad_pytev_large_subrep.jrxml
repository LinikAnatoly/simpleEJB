	<parameter name="datestart" class="java.lang.String" isForPrompting="false"/>
	<parameter name="datefinal" class="java.lang.String" isForPrompting="false"/>
	<parameter name="entravelsheetcode" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="enworkordercode" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="elementcode" class="java.lang.Integer" isForPrompting="false"/>
	<queryString>
		<![CDATA[Select fin.nn , fin.mat_name , fin.mu_name  , sum(fin.quantity) as quantity
from entransportitem ti left join entravlshttm2trnsprttm entt on ( ti.code = entt.transportitemrefcode)
                        left join entravelsheetitem enti on  entt.travelsheetitemrefcode = enti.code
, entransport2enestimate tr2eni , enestimateitem eni ,
     finmaterials fin , enplanwork enp left join enworkorder2enplanwork enw2 on  enp.code = enw2.plancode
                                       left join enworkorder enw on enw.code = enw2.workordercode
 where ti.transportrealcode = $P{transportrealcode}
   and ti.code = tr2eni.transportrefcode
   and tr2eni.estimaterefcode = eni.code
   and eni.accountingtyperefcode = 1
   and eni.kindrefcode = 2
   and eni.code = fin.estimateitemrefcode
   and fin.statusrefcode <> 2 /*не равно отмененным*/
   and ti.planrefcode = enp.code
   and enp.datestart between to_date($P{datestart},'dd.mm.yyyy') and to_date($P{datefinal},'dd.mm.yyyy')
   and enti.travelsheetrefcode = $P{entravelsheetcode}
   and enw.code =  $P{enworkordercode}
   and enp.elementrefcode = $P{elementcode}
   group by fin.nn , fin.mat_name , fin.mu_name]]>
	</queryString>
