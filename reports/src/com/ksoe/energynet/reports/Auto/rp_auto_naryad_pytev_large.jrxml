	<parameter name="strdepartment" class="java.lang.String" isForPrompting="false"/>
	<parameter name="datestart" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[new String("01.05.2011")]]></defaultValueExpression>
	</parameter>
	<parameter name="datefinish" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[new String("31.05.2011")]]></defaultValueExpression>
	</parameter>
	<parameter name="transportrealcode" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="plankind" class="java.lang.Integer" isForPrompting="false"/>
	<parameter name="molcode" class="java.lang.String">
		<parameterDescription><![CDATA[код мола]]></parameterDescription>
		<defaultValueExpression><![CDATA[new String("5110")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[Select
depname,  markauto , gosnumber ,
workorderdate, workordernumber ,
typeact ,
object ,
elementcode ,
numbergen, datestart ,
mhours_npz ,
distance_npz ,
mhours_travsheet ,
distance_travsheet   ,
transportrealcode ,
entravelsheetcode ,
enworkordercode ,
mol
, coalesce(( select to_char(min(chp.dateevent),'dd.mm.yyyy HH24:MI') as mindateouttransport
from encheckpointpasslisttm chp
where chp.transportrealrefcode = transportrealcode
and to_char(chp.dateevent,'dd.mm.yyyy') = to_char(workorderdate,'dd.mm.yyyy')
and chp.eventtyperefcode = 1 /*выезд*/ ),'_____') as mindateouttransport

, coalesce(( select to_char(max(chp.dateevent),'dd.mm.yyyy HH24:MI') as maxdateouttransport
from encheckpointpasslisttm chp
where chp.transportrealrefcode = transportrealcode
and to_char(chp.dateevent,'dd.mm.yyyy') = to_char(workorderdate,'dd.mm.yyyy')
and chp.eventtyperefcode = 2 /*въезд*/ ),'_____') as   maxdateouttransport

From (

Select
depname,  markauto , gosnumber ,
workorderdate, workordernumber ,
typeact ,
object ,
elementcode ,
numbergen, datestart ,
coalesce(sum(mhours),0) as mhours_npz ,
coalesce(sum(distance),0) as distance_npz ,
coalesce(summachinehours,0) as mhours_travsheet ,
coalesce(distance_travsheet,0) as distance_travsheet   ,
transportrealcode ,
entravelsheetcode ,
enworkordercode ,
mol
from
(
select epd.name as depname,  tkt.name as markauto , tkt.gosnumber ,
       enw.dategen as workorderdate, enw.workordernumber,
       coalesce(eld.ename,'') || ' Інв.№' || coalesce(eld.invnumber,'') as object ,
       eld.ecode as elementcode ,
       pst.shortname as typeact ,
       env.numbergen, env.datestart ,
       ent.code  ,
       coalesce((select sum(entransportitem.countworkfact) from entransportitem where code = ent.code
                                                                                   and tktransporttypecode <> 1 /*не равно бригаднім авто*/   ),0) as mhours ,
       coalesce((select sum(coalesce(dist.distance,0)) from endistance dist where dist.transportitemrefcode = ent.code ),0) as distance
 , eni.summachinehours
 , eni.speedometerfinal - eni.speedometerstart as distance_travsheet
 , tkt.code as transportrealcode
 , env.code as entravelsheetcode
 , enw.code as enworkordercode
 , tkt.finmolcode || ' ' || tkt.finmolname as mol
 from entransportitem ent left join
entravlshttm2trnsprttm entt on ent.code = entt.transportitemrefcode
left join entravelsheetitem eni on  entt.travelsheetitemrefcode = eni.code
left join entravelsheet env on eni.travelsheetrefcode = env.code
 , tktransportreal tkt ,
enplanwork enp left join enworkorder2enplanwork enw2 on  enp.code = enw2.plancode

left join enworkorder enw on enw.code = enw2.workordercode,endepartment epd , enelementdata eld , enplanworkstate pst
, enplanworkitem enpi
where enpi.code = ent.planitemrefcode and
      ent.transportrealcode = tkt.code and
      tkt.departmentrefcode = epd.code and
      enp.elementrefcode = eld.ecode
      $P!{strdepartment}
      and
      enp.datestart between to_date($P{datestart},'dd.mm.yyyy') And to_date($P{datefinish},'dd.mm.yyyy')  And
     ( enp.kindcode = $P{plankind} or $P{plankind} = 0 )  And
     ( tkt.code = $P{transportrealcode} or $P{transportrealcode} = 0 )


     and enp.staterefcode = pst.code
     and enp.code = enpi.planrefcode
     and enpi.countgen <> 0
     and (
     ( tkt.finmolcode = $P{molcode} and trim($P{molcode}) <> '' ) or
     ( 1 = 1 and trim($P{molcode}) = '' )
    )
Group by ent.code , epd.name, enw.workordernumber, enw.dategen, env.numbergen, env.datestart, tkt.gosnumber, tkt.name ,
         eld.ename ,eld.invnumber , pst.shortname , eni.summachinehours , eni.speedometerfinal ,  eni.speedometerstart
         , eld.ecode , tkt.code , env.code ,  enw.code  , tkt.finmolcode , tkt.finmolname
) a1
Group by depname,  markauto , gosnumber , workorderdate, workordernumber, object , elementcode ,  numbergen, datestart , typeact
 , summachinehours , distance_travsheet , transportrealcode , entravelsheetcode ,  enworkordercode , mol
) a2
Where
  (
    ( coalesce(mhours_npz) + coalesce(distance_npz) <> 0 )
   or
    ( coalesce(mhours_travsheet) + coalesce(distance_travsheet) <> 0 )
  )
Order by markauto , gosnumber , workorderdate, workordernumber]]>
