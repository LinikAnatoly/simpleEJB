	<parameter name="TravelSheet" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[Select
	matname
	, sum(countgen) as countgen
	, numberdoc
	, weight
	, sum(countgen) * weight as sumweight

from
(
	Select
		tm.name as matname
		, fki.countgen as  countgen
		, fkk.numberdoc
		, tm.weight
		, fki.countgen * tm.weight as sumweight
	from entravelsheet as tw
		inner join entravelsheetitem as twi on tw.code = twi.travelsheetrefcode
		inner join entransportroute as rou on rou.planrefcode = twi.planrefcode
		inner join entransportrot2rqfkrdr as rou2fk on rou.code = rou2fk.transportrouterefcode
		inner join rqfkorder as fkk on rou2fk.fkorderrefcode = fkk.code
		inner join rqfkorderitem fki on fkk.code = fki.fkorderrefcode
		inner join tkmaterials as tm on fki.materialcode = tm.code
	where tw.code = $P{TravelSheet}
		and twi.kindrefcode = 1
	group by fkk.code ,tm.name, fkk.numberdoc  ,fki.countgen  ,tm.weight  , fki.code

	union all

	select
		tm.name as matname
		, tii.countfact as  countgen
		, t.numberdoc
		, tm.weight
		, tii.countfact * tm.weight as sumweight
	from rqtransportinvoiceitem as tii
		inner join rqtransportinvoice as t on t.code = tii.transportinvoicerefcod
		inner join ntrnsprtrt2rqtrnsprtnv as tr2rqt on tr2rqt.invoicerefcode = t.code
		inner join entransportroute as rou on tr2rqt.transportrouterefcode = rou.code
		inner join entravelsheetitem as twi on rou.planrefcode = twi.planrefcode
		inner join entravelsheet tw on twi.travelsheetrefcode = tw.code
		inner join tkmaterials as tm on tii.materialrefcode = tm.code

	where tw.code = $P{TravelSheet}
	and twi.kindrefcode = 1
) aaa
group by matname  , numberdoc  , weight
order by numberdoc , matname]]>
	</queryString>
