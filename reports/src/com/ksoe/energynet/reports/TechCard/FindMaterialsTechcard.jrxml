	<queryString>
		<![CDATA[select tks.name as tktechcardsourcename  ,
tct.name as tkclassificationtypename   ,
' '||tk.techkartnumber as techkartnumber ,
tk.name as tktechcardname  ,
tm.name as materialname ,
tmmeas.name as measurementname   ,
te2tk.kolvo
 from tktechcard tk  , tktechcardsource tks , tkclassificationtype tct , tkmeasurement meas
 , tkelement2techcard te2tk , tkelement te , tkmaterials tm  , tkmeasurement tmmeas
where tk.techcardsourcecode = tks.code
and tk.classificationtypecode = tct.code
and tk.measurementcode = meas.code
and te2tk.techkartcode = tk.code
and te.code = te2tk.elementcode
and te.elementtypecode = 240000004 /*материалы*/
and tm.elementcode = te.code
and tm.measurementcode = tmmeas.code
and (tks.code =  $P{tktechcardsourcecode} or $P{tktechcardsourcecode} = -1 )

and ( ( tct.code in  ( $P!{tkclassificationtypecodes} ) and $P{tkclassificationtypecodes} <> '0') or $P{tkclassificationtypecodes} = '0' )
and ( ( tm.code in  ( $P!{materialscodes} ) and $P{materialscodes} <> '0') or $P{materialscodes} = '0' )
order by tks.code  , tct.kod , tk.techkartnumber ,  tk.name , tm.name]]>
