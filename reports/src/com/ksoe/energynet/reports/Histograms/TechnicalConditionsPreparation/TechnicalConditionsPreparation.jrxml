	<queryString>
		<![CDATA[SELECT p.code
, p.name AS contractnumberservices
, p.customername AS contragentname
, p.description AS connection_object
, p.customeraddress AS address
, p.power AS capacity
, dep.name AS department_name
, info.*
, CASE WHEN info.startdate1 IS NOT NULL THEN (info.finaldate1 - info.startdate1 + 1)
	ELSE NULL END AS duration1
, CASE WHEN info.startdate2 IS NOT NULL THEN (info.finaldate2 - info.startdate2 + 1)
	ELSE NULL END AS duration2
, CASE WHEN info.startdate3 IS NOT NULL THEN (info.finaldate3 - info.startdate3 + 1)
	ELSE NULL END AS duration3
, CASE WHEN info.startdate4 IS NOT NULL THEN (info.finaldate4 - info.startdate4 + 1)
	ELSE NULL END AS duration4
, CASE WHEN info.startdate5 IS NOT NULL THEN (info.finaldate5 - info.startdate5 + 1)
	ELSE NULL END AS duration5
, CASE WHEN info.startdate6 IS NOT NULL THEN (info.finaldate6 - info.startdate6 + 1)
	ELSE NULL END AS duration6
, (SELECT string_agg(DISTINCT gps1.wfgroupcode::varchar,';') FROM current_states AS cs1
	INNER JOIN wfgroup2state AS gps1 ON cs1.statecode = gps1.statecode
	WHERE $X{IN, gps1.wfgroupcode, COL_GROUPS}
	AND cs1.packcode = p.code) AS current_state_group
, string_agg(DISTINCT cs.statename, ';') AS current_state_name
, string_agg(DISTINCT to_char(cs.startdate,'dd.mm.yyyy'), ';') AS current_state_start
, string_agg(DISTINCT cs.rolename, ';') AS current_state_role
FROM wfpack AS p
LEFT JOIN current_states AS cs ON p.code = cs.packcode
INNER JOIN wfdepartment AS dep ON p.departmentcode = dep.code
INNER JOIN
(SELECT DISTINCT ON (packcode)
packcode
, nth_value(startdate, 1) OVER(PARTITION BY packcode) AS startdate1
, nth_value(startdate, 2) OVER(PARTITION BY packcode) AS startdate2
, nth_value(startdate, 3) OVER(PARTITION BY packcode) AS startdate3
, nth_value(startdate, 4) OVER(PARTITION BY packcode) AS startdate4
, nth_value(startdate, 5) OVER(PARTITION BY packcode) AS startdate5
, nth_value(startdate, 6) OVER(PARTITION BY packcode) AS startdate6
, nth_value(finaldate, 1) OVER(PARTITION BY packcode) AS finaldate1
, nth_value(finaldate, 2) OVER(PARTITION BY packcode) AS finaldate2
, nth_value(finaldate, 3) OVER(PARTITION BY packcode) AS finaldate3
, nth_value(finaldate, 4) OVER(PARTITION BY packcode) AS finaldate4
, nth_value(finaldate, 5) OVER(PARTITION BY packcode) AS finaldate5
, nth_value(finaldate, 6) OVER(PARTITION BY packcode) AS finaldate6
FROM
(SELECT DISTINCT ON (p.code, g.code)
	p.code AS packcode
	, g.code AS groupcode
	, first_value(mo.startdate::date)
				OVER(PARTITION BY p.code, g.code ORDER BY p.code, g.code, EXISTS
					(SELECT FROM wfbaseprocessitems AS bpi2
						WHERE bpi2.statetocode = mo.statecode
						AND bpi2.statefromcode IS NULL) DESC
						, mo.statecode ASC, mo.startdate DESC) AS startdate
	, coalesce(mo.finaldate, CASE WHEN mo.code IS NOT NULL THEN now() ELSE NULL END)::date AS finaldate
FROM
wfpack AS p
CROSS JOIN wfgroup AS g
LEFT JOIN(SELECT DISTINCT ON (mo1.packcode) mo1.packcode
	, mo1.startdate::date AS startdate
		FROM
			wfprocessmovement AS mo1
			INNER JOIN wfgroup2state AS gps1 ON mo1.statecode = gps1.statecode
			INNER JOIN wfprocessstate AS st1 ON gps1.statecode = st1.code
		WHERE gps1.wfgroupcode = $P{COL_GROUP1}
		ORDER BY mo1.packcode, EXISTS (SELECT FROM wfbaseprocessitems AS bpi2
						WHERE bpi2.statetocode = st1.code
						AND bpi2.statefromcode IS NULL) DESC, st1.code, mo1.startdate DESC) AS first_mov ON p.code = first_mov.packcode
LEFT JOIN (
wfgroup2state AS gps
INNER JOIN wfprocessmovement AS mo ON gps.statecode = mo.statecode
) ON (g.code = gps.wfgroupcode AND p.code = mo.packcode AND (CASE WHEN g.code <> $P{COL_GROUP1} THEN (mo.startdate >= first_mov.startdate OR first_mov.packcode IS NULL) ELSE TRUE END))

WHERE $X{IN, g.code, COL_GROUPS}
AND EXISTS (SELECT FROM wfprocessmovement AS mo1
		INNER JOIN wfgroup2state AS gps1 ON mo1.statecode = gps1.statecode
		WHERE $X{IN, gps1.wfgroupcode, COL_GROUPS}
			AND coalesce(mo1.iscompleted, $P{NO}) = $P{NO}
			AND mo1.packcode = p.code)
AND p.packstatuscode <> $P{PACK_STATUS_DELETED}
ORDER BY p.code, g.code, mo.startdate DESC) AS info1) AS info ON p.code = info.packcode
GROUP BY p.code, dep.name, info.packcode
, info.startdate1, info.finaldate1
, info.startdate2, info.finaldate2
, info.startdate3, info.finaldate3
, info.startdate4, info.finaldate4
, info.startdate5, info.finaldate5
, info.startdate6, info.finaldate6
ORDER BY p.code DESC]]>
	</queryString>
