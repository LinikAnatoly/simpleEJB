	pli.materialname as nomenclaturename
	, pli.measurementname as nomenclatureunitname
	, pli.countfact, sz.name as rqstoragename
	, pl.moltocode as moloutcode
	, pl.moltoname as moloutname
	, pl.molfromcode as molincode
	, pl.molfromname as molinname
	, dep.name as title_dep_name
	, to_char(pl.datestart,'dd.MM.yyyy') as datestart
	, to_char(pl.datefinal,'dd.MM.yyyy') as datefinal
	, pl.numbergen, to_char(pl.dategen,'dd.MM.yyyy') as dategen
	, pl.packerfio, pl.packerposition
