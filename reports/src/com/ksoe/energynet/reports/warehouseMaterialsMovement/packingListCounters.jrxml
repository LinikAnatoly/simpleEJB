	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="packingListCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="palletNumber" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
  txtgen,
  numberdoc,
  palletnumber,
  molincode,
  moloutcode,
  moloutname,
  bal_sch, nn, mat_name, mu_name,
  calc_price as price, frc_code, frc_name,
  sum(quantity) as cnt, sum(cost) as sumgen,
  numkatalog, commentgen
  , group_concat(distinct zonename, '; ') as zones

FROM (

SELECT
ok.txtgen,
o.numberdoc,
coalesce(o.palletnumber, 'Невідомо') as palletnumber,
o.molincode, o.molinname,
o.moloutcode, o.moloutname,
o.dategen, o.dateedit,
co.account as bal_sch, cast('' as varchar) as nn, ma.name as mat_name, me.name as mu_name
, co.cost as price, co.cost as calc_price, cast('' as varchar) as frc_code, cast('' as varchar) as frc_name
, 1 as quantity , co.cost
, case when (select pl.budgetrefcode  from enestimateitem ei, enplanwork pl
                     where ei.planrefcode = pl.code
                         and ei.code = oi2.estimateitemcode
                         limit 1
 ) = 75000016 then coalesce( (select '('||tkm.numkatalog||')  ' from tkmaterials tkm where tkm.code = oi.materialcode  limit 1 ) ,'') else '' end as numkatalog
 ,oi2.estimateitemcode
 , oi.materialcode, oi.commentgen as commentgen
 , coalesce(zo.name,'') as zonename
from
rqfkorder o
inner join rqpackinglist2fkorder as plfo on o.code = plfo.fkorderrefcode
inner join rqpackinglist as pl on plfo.packinglistrefcode = pl.code
inner join rqpackinglistitem as pli on pli.packinglistrefcode = pl.code
inner join rqpli2oi as plioi on pli.code = plioi.packinglistitemrefcode
inner join rqfkorderitem oi on (o.code = oi.fkorderrefcode and oi.code = plioi.fkorderitemrefcode)
inner join scinvoice as inv on inv.fkorderitemrefcode = oi.code
inner join scorder as ord on inv.code = ord.invoicerefcode
inner join scorder2counter as coco on ord.code = coco.scorderrefcode
left join rqstoragezone zo on oi.zonerefcode = zo.code
inner join rqfkorderitem2enstmttm oi2 on (oi.code = oi2.fkorderitemrefcode and coco.counterrefcode = oi2.sccounterrefcode)
inner join sccounter as co on coco.counterrefcode = co.code
inner join enestimateitem as es on (es.code = oi2.estimateitemcode and co.estimateitemrefcode = es.code)
inner join tkmaterials as ma on es.materialrefcode = ma.code
inner join tkmeasurement as me on ma.measurementcode = me.code
inner join rqfkorderkind ok on o.kindcode = ok.code
inner join rqpackinglistitemclmns as plic on pli.code = plic.packinglistitemrefcode
where pl.code = $P{packingListCode}
and co.statusrefcode in (1)
and ok.code <> 30
and plic.namepallet = $P{palletNumber}

) sel1
group by
txtgen,
numberdoc,
palletnumber,
molincode,
moloutcode,
moloutname,
bal_sch, nn, mat_name, mu_name,
calc_price, frc_code, frc_name, numkatalog, commentgen]]>
