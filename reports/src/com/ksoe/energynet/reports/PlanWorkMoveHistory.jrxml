	<queryString>
		<![CDATA[select
 ( case el.typerefcode when 1 then (select '(ПЛ 6-10) '||name from enline10 where elementcode = p.elementrefcode )
 when 3 then (select '(ПC 6-10/0.4) '||name  from ensubstation04 where elementcode= p.elementrefcode )
 when 2 then (select '(ПЛ 0.4) '||name from enline04 where elementcode = p.elementrefcode )
 end )
 as objname ,
pcode, reason, oldyear,oldmonth, dg, de, dat.commentgen,
r.name as rname

 from (
select
h.planrefcode as pcode,
mr.name as reason,
h.yeargen as oldyear,
h.monthgen as oldmonth,
h.dategen as dg,
h.dateedit as de,
h.commentgen as commentgen
from
enplanworkmovehistory h,
enplanworkmovereason mr
where mr.code = h.reasoncode
and h.dategen between '01.01.2000' and '01.01.2010'
union all
select
p.code as pcode,
null as reason,
yeargen as oldyear,
monthgen as oldmonth,
CURRENT_DATE as dg,
CURRENT_DATE as de,
'Поточний період виконання' as commentgen
from enplanwork p
where p.code in (
select planrefcode from enplanworkmovehistory where dategen between $P{startDate} and $P{finalDate}
)) dat,
enplanwork p, enelement el, epren r
where p.code = dat.pcode
and p.renrefcode = r.code and el.code = p.elementrefcode

order by r.name, p.elementrefcode, p.code, dat.dg desc]]>
	</queryString>
