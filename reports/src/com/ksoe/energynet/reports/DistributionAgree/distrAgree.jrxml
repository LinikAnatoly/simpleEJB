	</title>
	<detail>
		<band height="50">
			<textField isStretchWithOverflow="true" isBlankWhenNull="true">
				<reportElement key="textField-34" positionType="Float" mode="Transparent" x="1" y="0" width="547" height="11" isPrintWhenDetailOverflows="true" forecolor="#000000" backcolor="#FFFFFF"/>
				<box>
