	<parameter name="orderCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
findoccode,
txtgen,
numberdoc,
molincode, molinname, edrpo,
moloutcode, moloutname,
dategen, dateedit,
bal_sch,
nn,
mat_name,
 mu_name,
price,
' '||frc_code as frc_code ,
frc_name,
docdate,docnumber
,warrantdate,warrantnumber
, warrantfio
, sum(dat.cnt) as cnt, sum(dat.sumgen) as sumgen
, snds
, coalesce(numkatalog,'') as numkatalog
, (select distinct a1.numbergen||' від '||to_char(a1.dategen, 'dd.mm.yyyy') from enact as a1 inner join enact2rqfkorder as ao1 on a1.code = ao1.actrefcode where ao1.fkorderrefcode = $P{orderCode} and ao1.typerefcode = 2) as act_number_and_date
, exists (select 1 from enact2rqfkorder as ao1 where ao1.fkorderrefcode = $P{orderCode} and ao1.typerefcode = 2) as is_from_act_produced
,  zonename as zones
, case
   when (fundingtype = 1 or rqfkorderkindcode = 15 )  then 'Б'
    when fundingtype = 2 then 'ІП'
     when fundingtype = 3 then 'П' else '' end as fundingtype
, in_plan     
 from
(
select
cast(o.code as varchar(10)) as findoccode,
ok.txtgen,
o.numberdoc,
coalesce(org.codeorg, o.molincode) as molincode, coalesce(org.name,o.molinname) as molinname, coalesce(org.okpo,'') as edrpo,
o.moloutcode, o.moloutname,
o.dategen, o.dateedit,
oi.nomenclaturebalsch as bal_sch,
/*SUPP-62352 Для ордера по счетчикам номенклатур может и не быть*/
case when o.accountingtyperefcode = 2 then '' else oi.nomenclaturenum end as nn,
case when o.accountingtyperefcode = 2 then ma.name else oi.nomenclaturename end as mat_name,
case when o.accountingtyperefcode = 2 then mu.name else oi.nomenclatureunitname end as mu_name,
oi.pricewithoutnds as price
, d2r.fincfocode as frc_code
, dp.shortname as frc_name
, o.dategen as docdate, o.numberdoc as docnumber
, o.warrantdate as warrantdate, coalesce(o.warrantnumber, '') as warrantnumber
, coalesce(o.warrantfio,'') as warrantfio
, (oi2.countgen ) as cnt, (oi2.sumwithoutnds ) as sumgen
, o.sumnds as snds
, case when pl.budgetrefcode = 75000016 then '  ('||ma.numkatalog||')' else '' end as numkatalog
, coalesce(zo.name,'') as zonename
, coalesce(oi.fundingtype,0) as fundingtype
, ok.code as rqfkorderkindcode
,(case when e.etype in(19, 20) then 0          when e.etype = 35 then 35          when e.etype = 76 then 76          when e.etype = 79 then 79       
 when pl.elementrefcode  = 1017386539         then 1017386539         when pl.statuscode = 8 then 3 else 1 end )  as in_plan
from
rqfkorder as o
inner join rqfkorderitem as oi on o.code = oi.fkorderrefcode
inner join rqfkorderkind as ok on o.kindcode = ok.code
left join rqorg as org on org.code = o.orgcode
inner join rqfkorderitem2enstmttm as oi2 on oi2.fkorderitemrefcode = oi.code
inner join enestimateitem as ei on oi2.estimateitemcode = ei.code
inner join enplanwork as pl on ei.planrefcode = pl.code
inner join enelementdata e on e.ecode=pl.elementrefcode
inner join endepartment2epren as d2r on d2r.departmentrefcode = pl.budgetrefcode
inner join endepartment as dp on pl.budgetrefcode = dp.code
inner join tkmaterials as ma on ei.materialrefcode = ma.code
inner join tkmeasurement as mu on ma.measurementcode = mu.code
left join rqstoragezone zo on oi.zonerefcode = zo.code
where
o.code = $P{orderCode}

union all


select
cast(o.code as varchar(10)) as findoccode,
ok.txtgen,
o.numberdoc,
coalesce(org.codeorg,'') as molincode, coalesce(org.name,'') as molinname, coalesce(org.okpo,'') as edrpo,
o.moloutcode, o.moloutname,
o.dategen, o.dateedit,
oi.nomenclaturebalsch as bal_sch,
/*SUPP-62352 Для ордера по счетчикам номенклатур может и не быть*/
case when o.accountingtyperefcode = 2 then '' else oi.nomenclaturenum end as nn,
case when o.accountingtyperefcode = 2 then ma.name else oi.nomenclaturename end as mat_name,
case when o.accountingtyperefcode = 2 then mu.name else oi.nomenclatureunitname end as mu_name,
oi.pricewithoutnds as price
, d2r.fincfocode as frc_code
, dp.shortname as frc_name
, o.dategen as docdate, o.numberdoc as docnumber
, o.warrantdate as warrantdate, coalesce(o.warrantnumber, '') as warrantnumber
, coalesce(o.warrantfio,'') as warrantfio
, (rr.countgen ) as cnt, (rr.sumwithoutnds ) as sumgen
, o.sumnds as snds
 , case when rr.budgetcode = 75000016 then '  ('||ma.numkatalog||')' else '' end as numkatalog
, coalesce(zo.name,'') as zonename
, coalesce(oi.fundingtype,0) as fundingtype
, ok.code as rqfkorderkindcode
,(case when rr.typerefcode = 1 then 4                   when (select distinct enplanwork.elementrefcode                         from enplanwork, enestimateitem              
 where enplanwork.code = enestimateitem.planrefcode                         and rr.estimateitemrefcode = enestimateitem.code) = 1017386539                  then 1017386539    
 else 2 end ) as in_plan
from
rqfkorder as o
inner join rqfkorderitem as oi on o.code = oi.fkorderrefcode
inner join rqfkorderkind as ok on o.kindcode = ok.code
left join rqorg as org on org.code = o.orgcode
inner join rqfkorderitemremainder as rr on rr.fkorderitemrefcode = oi.code
inner join tkmaterials as ma on oi.materialcode = ma.code
inner join tkmeasurement as mu on ma.measurementcode = mu.code
left join endepartment2epren d2r on  d2r.departmentrefcode = rr.budgetcode
left join endepartment dp on rr.budgetcode = dp.code
left join rqstoragezone zo on oi.zonerefcode = zo.code
where
o.code =   $P{orderCode}

) dat

group by


findoccode,
txtgen,
numberdoc,
molincode, molinname, edrpo,
moloutcode, moloutname,
dategen, dateedit,
bal_sch,
nn,
mat_name,
 mu_name,
price,
frc_code,
frc_name,
docdate,docnumber
,warrantdate,warrantnumber
, warrantfio
, snds
,coalesce(numkatalog,'')
, fundingtype
, zonename
 , rqfkorderkindcode
 , in_plan
having sum(cnt) > 0]]>
