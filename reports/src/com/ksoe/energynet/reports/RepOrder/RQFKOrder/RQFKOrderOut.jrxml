	<queryString>
		<![CDATA[select
  txtgen,
  numberdoc,
'' as findoccode,
  molincode, molinname,
  moloutcode, moloutname,
  dategen, dateedit,
  bal_sch, nn, mat_name, mu_name,
  calc_price as price, frc_code, frc_name,
  sum(quantity) as cnt, sum(cost) as sumgen,
  numkatalog, commentgen
  , zonename as zones
  , kindcode
FROM (

SELECT
ok.txtgen,
o.numberdoc,
o.kindcode,
o.molincode, o.molinname,
o.moloutcode, o.moloutname,
o.dategen, o.dateedit,
f.bal_sch, f.nn, f.mat_name, f.mu_name
, f.calc_price , f.frc_code, f.frc_name
, f.quantity , f.cost
, case when (select pl.budgetrefcode  from net.enestimateitem ei, net.enplanwork pl
                     where ei.planrefcode = pl.code
                         and ei.code = oi2.estimateitemcode
                         limit 1
 ) = 75000016 then coalesce( (select '('||tkm.numkatalog||')  ' from net.tkmaterials tkm where tkm.code = oi.materialcode  limit 1 ) ,'') else '' end as numkatalog
 ,oi2.estimateitemcode
 , oi.materialcode, oi.commentgen as commentgen
 , coalesce(zo.name,'') as zonename
from
net.rqfkorder o
inner join net.rqfkorderitem oi on o.code = oi.fkorderrefcode
left join net.rqstoragezone zo on oi.zonerefcode = zo.code
inner join net.rqfkorderitem2enstmttm oi2 on oi2.fkorderitemrefcode = oi.code
inner join net.finmaterials f on (oi2.estimateitemcode = f.estimateitemrefcode and oi2.finmaterialsrefcode = f.code)
inner join net.rqfkorderkind ok on o.kindcode = ok.code

where o.code = $P{orderCode}
 and f.statusrefcode in (1, 3, 5)
) sel1
group by
txtgen,
numberdoc,
molincode, molinname,
moloutcode, moloutname,
dategen, dateedit,
bal_sch, nn, mat_name, mu_name,
calc_price, frc_code, frc_name, numkatalog, commentgen, zonename,
kindcode]]>
