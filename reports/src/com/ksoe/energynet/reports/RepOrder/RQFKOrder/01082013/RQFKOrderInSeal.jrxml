	<parameter name="orderCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
findoccode,
txtgen,
numberdoc,
molincode, molinname, edrpo,
moloutcode, moloutname,
dategen, dateedit,
bal_sch,
nn,
mat_name,
 mu_name,
price,
' '||frc_code as frc_code ,
frc_name,
docdate,docnumber
,warrantdate,warrantnumber
, warrantfio
, sum(dat.cnt) as cnt, sum(dat.sumgen) as sumgen
, snds
, coalesce(numkatalog,'') as numkatalog
, '' as act_number_and_date
, false as is_from_act_produced
,  zonename as zones
, case
   when fundingtype = 1 then 'Б'
    when fundingtype = 2 then 'ІП'
     when fundingtype = 3 then 'П' else '' end as fundingtype

 from
(
select
cast(o.code as varchar(10)) as findoccode,
ok.txtgen,
o.numberdoc,
coalesce(org.codeorg, o.molincode) as molincode, coalesce(org.name,o.molinname) as molinname, coalesce(org.okpo,'') as edrpo,
o.moloutcode, o.moloutname,
o.dategen, o.dateedit,
'2013' as bal_sch, /* для пломб будем всегда ставить 2013 */
oi.sealseriesstart || ' ' || oi.sealnumberstart as nn, /* вместо номенклатуры - серия и серийник */
(select tkmaterials.name from tkmaterials where code = oi.materialcode) || ' ' ||
oi.sealname || ' ' || oi.sealcolor as mat_name, /* вместо наименования номенклатур - материал и тип материала*/
'ШТ' as mu_name,
oi.pricewithoutnds as price
, d2r.fincfocode as frc_code
, dp.shortname as frc_name
, o.dategen as docdate, o.numberdoc as docnumber
, o.warrantdate as warrantdate, coalesce(o.warrantnumber, '') as warrantnumber
, coalesce(o.warrantfio,'') as warrantfio
, (oi2.countgen ) as cnt, (oi2.sumwithoutnds ) as sumgen
, o.sumnds as snds
, case when pl.budgetrefcode = 75000016 then  (select ' ('||tkm.numkatalog||')' from tkmaterials tkm where tkm.code = ei.materialrefcode limit 1 ) else '' end as numkatalog
, coalesce(zo.name,'') as zonename
, coalesce(oi.fundingtype,0) as fundingtype

from
rqfkorder o left join rqorg as org on org.code = o.orgcode, rqfkorderitem oi left join rqstoragezone zo on oi.zonerefcode = zo.code
, rqfkorderkind ok, rqfkorderitem2enstmttm oi2 , enestimateitem ei
, enplanwork pl , endepartment2epren d2r, endepartment dp
where
o.code = $P{orderCode}
and o.code = oi.fkorderrefcode
and oi2.fkorderitemrefcode = oi.code
and o.kindcode = ok.code
and oi2.estimateitemcode = ei.code and ei.planrefcode = pl.code
 and d2r.departmentrefcode = pl.budgetrefcode
 and pl.budgetrefcode = dp.code

union all


select
cast(o.code as varchar(10)) as findoccode,
ok.txtgen,
o.numberdoc,
coalesce(org.codeorg,'') as molincode, coalesce(org.name,'') as molinname, coalesce(org.okpo,'') as edrpo,
o.moloutcode, o.moloutname,
o.dategen, o.dateedit,
'2013' as bal_sch, /* для пломб будем всегда ставить 2013 */
oi.sealseriesstart || ' ' || oi.sealnumberstart as nn, /* вместо номенклатуры - серия и серийник */
(select tkmaterials.name from tkmaterials where code = oi.materialcode) || ' ' ||
oi.sealname || ' ' || oi.sealcolor as mat_name, /* вместо наименования номенклатур - материал и тип материала*/
'ШТ' as mu_name,
oi.pricewithoutnds as price
, d2r.fincfocode as frc_code
, dp.shortname as frc_name
, o.dategen as docdate, o.numberdoc as docnumber
, o.warrantdate as warrantdate, coalesce(o.warrantnumber, '') as warrantnumber
, coalesce(o.warrantfio,'') as warrantfio
, (rr.countgen ) as cnt, (rr.sumwithoutnds ) as sumgen
, o.sumnds as snds
, case when rr.budgetcode = 75000016 then  (select '('||tkm.numkatalog||') ' from tkmaterials tkm where tkm.code = oi.materialcode limit 1 ) else '' end as numkatalog
, coalesce(zo.name,'') as zonename
, coalesce(oi.fundingtype,0) as fundingtype

from
rqfkorder o left join rqorg org on org.code = o.orgcode, rqfkorderitem oi left join rqstoragezone zo on oi.zonerefcode = zo.code
, rqfkorderkind ok,
rqfkorderitemremainder rr left join endepartment2epren d2r
on  d2r.departmentrefcode = rr.budgetcode
 left join endepartment dp on rr.budgetcode = dp.code
where
o.code = $P{orderCode}
and o.code = oi.fkorderrefcode
and rr.fkorderitemrefcode = oi.code
and o.kindcode = ok.code

) dat

group by


findoccode,
txtgen,
numberdoc,
molincode, molinname, edrpo,
moloutcode, moloutname,
dategen, dateedit,
bal_sch,
nn,
mat_name,
 mu_name,
price,
frc_code,
frc_name,
docdate,docnumber
,warrantdate,
warrantnumber
, warrantfio
, snds
,coalesce(numkatalog,'')
, fundingtype
, zonename

having sum(cnt) > 0]]>
