	<queryString>
		<![CDATA[select
all_dat.level_, all_dat.l0code, all_dat.l0name
, all_dat.l1code, all_dat.l1name
, all_dat.l2code, all_dat.l2name
, all_dat.l3code, all_dat.l3name
, all_dat.l4code, all_dat.l4name
, all_dat.l5code, all_dat.l5name
,all_dat.measurement
,sum(all_dat.m1) as m1, sum(all_dat.m2) as m2, sum(all_dat.m3) as m3, sum(all_dat.m4) as m4
,sum(all_dat.m5) as m5, sum(all_dat.m6) as m6, sum(all_dat.m7) as m7, sum(all_dat.m8) as m8
,sum(all_dat.m9) as m9, sum(all_dat.m10) as m10, sum(all_dat.m11) as m11, sum(all_dat.m12) as m12
 from (
select
mat_dat.level_, mat_dat.l0code, mat_dat.l0name
, mat_dat.l1code, mat_dat.l1name
, mat_dat.l2code, mat_dat.l2name
, mat_dat.l3code, mat_dat.l3name
, mat_dat.l4code, mat_dat.l4name
, mat_dat.l5code, mat_dat.l5name
, rq_dat.countfact
,tu.name as measurement
, rq_dat.monthgen
, case when rq_dat.monthgen = 1 then rq_dat.countfact else 0 end as m1
, case when rq_dat.monthgen = 2 then rq_dat.countfact else 0 end as m2
, case when rq_dat.monthgen = 3 then rq_dat.countfact else 0 end as m3
, case when rq_dat.monthgen = 4 then rq_dat.countfact else 0 end as m4
, case when rq_dat.monthgen = 5 then rq_dat.countfact else 0 end as m5
, case when rq_dat.monthgen = 6 then rq_dat.countfact else 0 end as m6
, case when rq_dat.monthgen = 7 then rq_dat.countfact else 0 end as m7
, case when rq_dat.monthgen = 8 then rq_dat.countfact else 0 end as m8
, case when rq_dat.monthgen = 9 then rq_dat.countfact else 0 end as m9
, case when rq_dat.monthgen = 10 then rq_dat.countfact else 0 end as m10
, case when rq_dat.monthgen = 11 then rq_dat.countfact else 0 end as m11
, case when rq_dat.monthgen = 12 then rq_dat.countfact else 0 end as m12
from
(
select 0 as level_, l0.code as l0code, l0.name as l0name,
 cast(null as double precision) as l1code, null as l1name,
  cast(null as double precision) as l2code, null as l2name
  ,cast(null as double precision) as l3code, null as l3name
 ,cast(null as double precision) as l4code, null as l4name
 ,cast(null as double precision) as l5code, null as l5name
from
tkmaterials l0
where
l0.parentrefcode is null
and ( l0.code = $P{groupCode} or -1 = $P{groupCode} )
union all

select 1 as level_,l0.code as l0code, l0.name, l1.code as l1code, l1.name as l1name,
cast(null as double precision) as l2code, null as l2name
 ,cast(null as double precision) as l3code, null as l3name
 ,cast(null as double precision) as l4code, null as l4name
  ,cast(null as double precision) as l5code, null as l5name
from
tkmaterials l0 , tkmaterials l1
where
l0.parentrefcode is null
and  l0.code = l1.parentrefcode
and ( l0.code = $P{groupCode} or -1 = $P{groupCode} )
union all
select 2 as level_, l0.code as l0code, l0.name, l1.code as l1code , l1.name as l1name
, l2.code as l2code, l2.name as l2name
 ,cast(null as double precision) as l3code, null as l3name
  ,cast(null as double precision) as l4code, null as l4name
   ,cast(null as double precision) as l5code, null as l5name
 from
tkmaterials l0, tkmaterials l1
, tkmaterials l2
where
l0.parentrefcode is null
and  l0.code = l1.parentrefcode
and  l1.code = l2.parentrefcode
and ( l0.code = $P{groupCode} or -1 = $P{groupCode} )
union all

select 3 as level_, l0.code as l0code, l0.name, l1.code as l1code , l1.name as l1name
, l2.code as l2code, l2.name as l2name
, l3.code as l3code, l3.name as l3name
 ,cast(null as double precision) as l4code, null as l4name
  ,cast(null as double precision) as l5code, null as l5name

 from
tkmaterials l0, tkmaterials l1
, tkmaterials l2
, tkmaterials l3

where
l0.parentrefcode is null
and  l0.code = l1.parentrefcode
and  l1.code = l2.parentrefcode
and  l2.code = l3.parentrefcode
and ( l0.code = $P{groupCode} or -1 = $P{groupCode} )

union all

select 4 as level_, l0.code as l0code, l0.name, l1.code as l1code , l1.name as l1name
, l2.code as l2code, l2.name as l2name
, l3.code as l3code, l3.name as l3name
 ,cast(l4.code as double precision) as l4code, l4.name as l4name
  ,cast(null as double precision) as l5code, null as l5name
 from
tkmaterials l0, tkmaterials l1
, tkmaterials l2
, tkmaterials l3
, tkmaterials l4
where
l0.parentrefcode is null
and  l0.code = l1.parentrefcode
and  l1.code = l2.parentrefcode
and  l2.code = l3.parentrefcode
and  l3.code = l4.parentrefcode
and ( l0.code = $P{groupCode} or -1 = $P{groupCode} )
union all

select 5 as level_, l0.code as l0code, l0.name, l1.code as l1code , l1.name as l1name
, l2.code as l2code, l2.name as l2name
, l3.code as l3code, l3.name as l3name
 ,cast(l4.code as double precision) as l4code, l4.name as l4name
 , l5.code as l5code, l5.name as l5name
 from
tkmaterials l0, tkmaterials l1
, tkmaterials l2
, tkmaterials l3
, tkmaterials l4
, tkmaterials l5
where
l0.parentrefcode is null
and  l0.code = l1.parentrefcode
and  l1.code = l2.parentrefcode
and  l2.code = l3.parentrefcode
and  l3.code = l4.parentrefcode
and l4.code = l5.parentrefcode
and ( l0.code = $P{groupCode} or -1 = $P{groupCode} )
) as mat_dat
left join
(

select ei.materialrefcode as mcode,
 sum(ei.countfact) as countfact
, pl.monthgen
from enplanwork pl, enestimateitem ei
where pl.code = ei.planrefcode
and pl.kindcode = 1 and ei.kindrefcode = 1 and
pl.yeargen = $P{yearGen}
and (pl.departmentrefcode = $P{departmentCode} or -1 = $P{departmentCode})
and (pl.budgetrefcode = $P{budgCode} or -1 = $P{budgCode})
and ei.countfact<>0

group by ei.materialrefcode, pl.monthgen
/*
select
oi.materialcode as mcode, sum(oi.countfact) as countfact
 from
rqorder o, rqorderitem oi
where
o.code = oi.orderrefcode
and o.kindrefcode = 2 and o.orderperiod = '01.10.2010'
group by oi.materialcode
*/
) as rq_dat

on mat_dat.l1code = rq_dat.mcode or mat_dat.l2code = rq_dat.mcode
or mat_dat.l3code = rq_dat.mcode or mat_dat.l4code = rq_dat.mcode
or mat_dat.l5code = rq_dat.mcode
,
tkmaterials tm
left join tkmeasurement tu on tu.code = tm.measurementcode


where rq_dat.mcode is not null
and tm.code = rq_dat.mcode

order by l0name, l1name, l2name, l3name, l4name, l5name
) all_dat
group by all_dat.level_, all_dat.l0code, all_dat.l0name
, all_dat.l1code, all_dat.l1name
, all_dat.l2code, all_dat.l2name
, all_dat.l3code, all_dat.l3name
, all_dat.l4code, all_dat.l4name
, all_dat.l5code, all_dat.l5name
,all_dat.measurement
order by
all_dat.l0code, all_dat.l0name
, all_dat.l1code, all_dat.l1name
, all_dat.l2code, all_dat.l2name
, all_dat.l3code, all_dat.l3name
, all_dat.l4code, all_dat.l4name
, all_dat.l5code, all_dat.l5name
,all_dat.measurement]]>
	</queryString>
