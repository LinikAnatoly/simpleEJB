	<queryString>
		<![CDATA[select deliveryday, monthgen ,  yeargen, mname, meas,
  sum(countfact) as countfact ,
  Cast(price as numeric(15,4)) as price ,
  sum(Cast(cost as numeric(15,4))) as cost ,
  objname ,
  dname ,
  budgetrefname,
  materialrefcode as mcode
    ,


(case when  $P{pr_par_mat} = 1 then (  select distinct(tmm.name) from tkmaterials tmm where tmm.code =
                   (select rootgrouprefcode from tkmaterials tmm2
                                            where tmm2.code = materialrefcode )
                 )
     when  $P{pr_par_mat} = 0 then '1' end )
as parent_material
 ,   case when $P{pr_identid} = 1 then
     case when identid is null then 'Без класіфікатора' else identid end
     when $P{pr_identid} = 0 then '1' end
as identid ,
order_number ,
order_period ,
enresponsiblesname

from
(
select
coalesce(es.deliverytime, coalesce(sm.deliverydate,60)) as deliveryday ,
ri.materialcode as materialrefcode,
-- cast(to_char(ro.orderperiod,'mm') as numeric) as monthgen ,
-- cast (to_char(ro.orderperiod,'yyyy') as numeric ) as yeargen ,
p.monthgen  ,
p.yeargen ,

 case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end  as mname,
 coalesce(ri2es.countgen ,0) as countfact, ei.name as meas,
 coalesce(coalesce(ri.pricewithnds,0) * coalesce(ri2es.countgen,0), 0) as cost,
 COALESCE(ri.pricewithnds, 0) as price,
 $P!{dNameField}  as dname ,
 $P!{dbNameField} As budgetrefname ,
  (case when $P{objNameField} = 1 then '1' else (select ename || ' инв.№'  || invnumber from enelementdata where ecode = e.code) end) as objname
  ,    cast(sm.identid as varchar ) as identid
  , case when $P{priznshoworder} = 1 then ' '||ro.numberdoc else '1' end as order_number ,
    case when $P{priznshoworder} = 1 then to_char(ro.orderperiod,'MM.yyyy') else '1' end as order_period
  , rs.fio ||' ' || 'таб.№' ||' '|| rs.tabnumber as enresponsiblesname
 from enplanwork p,
enelement e ,
rqorderitem ri,
rqorder ro,
rqorderitem2enestimttm ri2es,
tkmeasurement ei,
enestimateitem es,
tkmaterials sm left join enresponsibles rs on sm.responsiblesrefcode = rs.code ,
endepartment d ,
endepartment db
where
ri2es.estimateitemcode = es.code
and ri2es.orderitemcode = ri.code
and ro.code = ri.orderrefcode
and p.code = es.planrefcode
and p.elementrefcode = e.code
and p.departmentrefcode = d.code
and p.budgetrefcode = db.code
and es.materialrefcode = sm.code
and ei.code = sm.measurementcode
and (
      ( cast(to_char(ro.orderperiod,'mm') as numeric) = cast($P{monthGen} as numeric)
        and to_char(ro.orderperiod,'yyyy') = cast($P{yearGen} as varchar)
        and $P{priznperiod} = 0 /*если выборку делаем за месяц и год заявки */
       )
       or
       (  ro.orderperiod between to_date($P{datestart} ,'dd.mm.yyyy')
                          and to_date($P{datefinal} ,'dd.mm.yyyy')

        and $P{priznperiod} = 1 /*если выборку делаем за месяц и год заявки */
       )
    )
and  ( ro.rqorderformcode = $P{orderform} or $P{orderform}  = 0 )   /* 1- плановых или 2 - внеплановых заявок или и тех и тех  */
and  ( ro.rqordertypecode = $P{ordertype} or $P{ordertype} = 0  )  /*1 - бюджетна заявка , 2 - инвестпрограмма*/
and  ro.statusrefcode in ( 2,3) -- утвержденные составленые
and (p.departmentrefcode = $P{renCode} or $P{renCode} = 0)
and (e.code = $P{elementCode}  or $P{elementCode} = 0)
And ((p.budgetrefcode = $P{budgCode}  ) or $P{budgCode} = 0)
and ri.countfact <> 0
And (( es.kindrefcode  = $P{kindrefcodemat}  AND $P{kindrefcodemat} = 1 ) OR
       ( es.kindrefcode  In (1,2) AND $P{kindrefcodemat}  = 2 ))

/* группа материалов */
and sm.code in
(select tm.code from tkmaterials tm
where tm.rootgrouprefcode = $P!{materialGroupCode} or $P!{materialGroupCode} = 0)

/* 14.01.2010 добавили условие что б выбирались толко из ХОЕ планова или ХОЕ позапланова*/
and ((ro.kindrefcode = 4) or (ro.kindrefcode = 5))

) dat
group by identid ,  deliveryday, monthgen ,  yeargen, mname, meas,
         price ,
         objname ,
         dname ,
         budgetrefname,
         materialrefcode ,
         parent_material ,
         order_number ,
         order_period , enresponsiblesname

order by identid ,  parent_material ,dname , budgetrefname , objname ,  mname , enresponsiblesname]]>
