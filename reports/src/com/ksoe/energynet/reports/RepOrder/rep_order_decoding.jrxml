	</parameter>
	<queryString>
		<![CDATA[select rcode ,   mname, meas,
   Cast(sum(countfact) as numeric(15,6)) as countfact ,



         pricewithnds , -- плановая стоимость  единицы материала
         sum(countfact) * pricewithnds as summa  , -- сумма материалов
         dname ,
         dcode ,
         budgetrefname ,
         dbcode ,
         materialcode ,
         datestart  ,

( select  case when $P{MonthRaznar} = 1 then 'Січня'
               when $P{MonthRaznar} = 2 then 'Лютого'
               when $P{MonthRaznar} = 3 then 'Березня'
               when $P{MonthRaznar} = 4 then 'Квітня'
               when $P{MonthRaznar} = 5 then 'Травня'
               when $P{MonthRaznar} = 6 then 'Червня'
               when $P{MonthRaznar} = 7 then 'Липня'
               when $P{MonthRaznar} = 8 then 'Серпня'
               when $P{MonthRaznar} = 9 then 'Вересня'
               when $P{MonthRaznar} = 10 then 'Жовтня'
               when $P{MonthRaznar} = 11 then 'Листопада'
               when $P{MonthRaznar} = 12 then 'Грудня'
             end  ) as period_raznar
            ,
  (select case
              when datestart::int = 1 then 'Січень'
              when datestart::int = 2 then 'Лютий'
              when datestart::int = 3 then 'Березень'
              when datestart::int = 4 then 'Квітень'
              when datestart::int = 5 then 'Травень'
              when datestart::int = 6 then 'Червень'
              when datestart::int = 7 then 'Липень'
              when datestart::int = 8 then 'Серпень'
              when datestart::int = 9 then 'Вересень'
              when datestart::int = 10 then 'Жовтень'
              when datestart::int = 11 then 'Листопад'
              when datestart::int = 12 then 'Грудень'
              end  )  as month_vvod
        ,
         objname ,
        /*elementtypename , */
        numberdoc ,
        txtcode  ,
        orgcode ,
        (select ro.name from rqorg ro where  ro.code = orgcode  ) as orgname ,
rqorderformcode ,  /* 1- плановых или 2 - внеплановых заявок или и тех и тех  */
kindrefcode , /* заявка рес = 1 , заявка бюджетодержатель = 2 , заявка ХОЕ = 3 */
rqordertypecode   /*1 - бюджетна заявка , 2 - инвестпрограмма*/
, deliverytime

  --- start col group
, (case when  $P{pr_par_mat} = 1 then (  select distinct(tmm.name) from tkmaterials tmm where tmm.code =
                   (select rootgrouprefcode from tkmaterials tmm2
                                            where tmm2.code = materialcode )
                 )
     when  $P{pr_par_mat} = 0 then '1' end )
as parent_material

From
(
select
rdds.txtcode ,
r.numberdoc ,
r.code as rcode  ,
ent.name  as elementtypename ,
ri.pricewithnds ,
ri.orgcode ,
to_char(p.datestart,'mm') as datestart ,
p.monthgen ,
p.yeargen ,
sm.code  as materialcode ,
case when r.kindrefcode in (7,8) then
                                (select '('|| tii.techkartnumber || ') ' || coalesce(pii.commentgen,'Невизначено')
                                   from enestimateitem iii , enplanworkitem pii , tktechcard tii
                                  Where iii.planitemrefcode = pii.code
                                    and iii.code = es.code
                                    and pii.kartarefcode = tii.code
                                    limit 1   )
else
case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end end  as mname,
coalesce(es.countfact,0) as countfact,



 ei.name as meas,
 d.name  As dname ,
 d.code as dcode ,
 db.name  As budgetrefname ,
 db.code as dbcode ,
  ( Select ename||' Інв.№ '|| invnumber from enelementdata where ecode = e.code)  as objname ,
 r.rqorderformcode ,  /* 1- плановых или 2 - внеплановых заявок или и тех и тех  */
 r.kindrefcode , /* заявка рес = 1 , заявка бюджетодержатель = 2 , заявка ХОЕ = 3 */
 r.rqordertypecode   /*1 - бюджетна заявка , 2 - инвестпрограмма*/
  , COALESCE(ri.deliverytime , sm.deliverydate , 0 ) as deliverytime
 From enplanwork p left join net.enmol2planwork en2 on (p.code = en2.planrefcode) ,
      enelement e ,
      enelementtype ent ,
      tkmeasurement ei,
      enestimateitem es,
      tkmaterials sm,
      endepartment d ,
      endepartment db ,
      RQORDERITEM2ENESTIMTTM rrr ,
      rqorder r ,
      rqorderitem ri left join rqddscodes rdds on ri.ddscodescode = rdds.code
where p.code = es.planrefcode
  and p.elementrefcode = e.code
  and p.departmentrefcode = d.code
  and p.budgetrefcode = db.code
  and es.materialrefcode = sm.code
  and ei.code = sm.measurementcode
  and e.typerefcode = ent.code
  and ( r.code = $P{PcodeOrder} or $P{PcodeOrder} = 0 )
  and ( p.yeargen = $P{YearVvod} or $P{YearVvod} = 0 )
  and ( p.monthgen = $P{MonthVvod} or $P{MonthVvod}  = 0 )
  and (p.departmentrefcode = $P{renCode} or $P{renCode}  = 0)
  And ((p.budgetrefcode = $P{budgCode} ) or  $P{budgCode}  = 0 )
and p.statuscode not in (2,6)
and es.countfact <> 0
And es.kindrefcode in (1,6) /* материалы и услуги*/
--AND e.typerefcode not in (select code from enelementtype where coalesce(managementrefcode, 0) = 3  )

/*rq*/

and  ( r.orderperiod = to_date('01.'||$P{MonthRaznar}||'.'||$P{YearRaznar},'dd.mm.yyyy')
        or
       '01.01.3000' = to_date('01.'||$P{MonthRaznar}||'.'||$P{YearRaznar},'dd.mm.yyyy')
     )

and  r.statusrefcode in (2,3) -- утвержденные составленые
and  ( r.rqorderformcode = $P{orderform} or $P{orderform}  = 0 )   /* 1- плановых или 2 - внеплановых заявок или и тех и тех  */
 /* ХОЕ планова = 4 , ХОЕ позапланова = 5 , (только плановая + позаплановые  ) = 0 */
and  ( ( $P{zayakind} = 0 and r.kindrefcode in (4 , 5 , 7 , 8)  )
    or ( $P{zayakind} = 4 and r.kindrefcode in (4 , 7 )  ) /*плановые заявки тмц и плановые заявки усоуги*/
    or ( $P{zayakind} = 5 and r.kindrefcode in (5 , 8 )  ) /*неплановые заявки тмц и неплановые заявки услуги*/
     )
and  ( r.rqordertypecode = $P{ordertype} or $P{ordertype} = 0  )  /*1 - бюджетна заявка , 2 - инвестпрограмма*/
and  r.code = ri.orderrefcode
and  ri.code = rrr.orderitemcode
and  rrr.estimateitemcode = es.code
 /*and  es.statusrefcode = 2*/  /*замовлений */
and ri.statusrefcode <> 2

) dat
group by budgetrefname , dbcode , mname,  materialcode , meas ,  dname , dcode , datestart
           , rcode , pricewithnds , objname , /*elementtypename ,*/ numberdoc ,txtcode ,
           orgcode , rqorderformcode ,  kindrefcode ,   rqordertypecode , deliverytime
order by parent_material ,  budgetrefname ,  mname , materialcode ,meas ,  dname ,  dcode]]>
	</queryString>
