				<textElement textAlignment="Center">
					<font fontName="Times New Roman" size="10" pdfFontName="times.ttf" pdfEncoding="Cp1251" isPdfEmbedded="true"/>
				</textElement>
				<textFieldExpression><![CDATA[$P{PcodeOrder}.doubleValue() == 0.0 ?
(" Звіт по заявкам з розшифровками причин закупівлі " +
 "\n" +
( $P{zayakind}.intValue() == 1   ? " по заявкам підрозділів " :
  $P{zayakind}.intValue() == 2  ? " по заявкам бюджетотримачів " :
  $P{zayakind}.intValue() == 3   ? " по заявкам ХОЕ " :
  $P{zayakind}.intValue() == 4   ? " Заявки ХОЕ планові " :
  $P{zayakind}.intValue() == 5   ? " Заявки ХОЕ позапланові " :
" ___ " ) +
" на " + $P{YearRaznar}.toString() + "р." +
 new String(
  $P{MonthRaznar} == new Integer(1) ? "Січня" :
 $P{MonthRaznar} == new Integer(2) ? "Лютого" :
 $P{MonthRaznar} == new Integer(3) ? "Березня" :
 $P{MonthRaznar} == new Integer(4) ? "Квітня" :
 $P{MonthRaznar} == new Integer(5) ? "Травня" :
 $P{MonthRaznar} == new Integer(6) ? "Червня" :
 $P{MonthRaznar} == new Integer(7) ? "Липня" :
 $P{MonthRaznar} == new Integer(8) ? "Серпня" :
 $P{MonthRaznar} == new Integer(9) ? "Вересня" :
 $P{MonthRaznar} == new Integer(10) ? "Жовтня" :
 $P{MonthRaznar} == new Integer(11) ? "Листопада" :
 $P{MonthRaznar} == new Integer(12) ? "Грудня" : " " )

+ " місяця" +
"\n" + " Вид заявки : " +
( $P{orderform}.intValue() == 1   ? " планові " :
  $P{orderform}.intValue() == 2  ? " позапланові " :
    " планові та позапланові " ) +

"\n" + " Тип заявки : " +
( $P{ordertype}.intValue() == 1   ? " бюджетна " :
  $P{ordertype}.intValue() == 2  ? " інвестпрограмма " :
    " бюджетна та інвестпрограмма   " )
)
:
(" Звіт по заявці № " + $F{numberdoc} + "  з розшифровками причин закупівлі " +
 "\n" +
( $F{kindrefcode}.intValue() == 1   ? " заявка підрозділів " :
  $F{kindrefcode}.intValue() == 2  ? " заявка бюджетотримачів " :
  $F{kindrefcode}.intValue() == 3   ? " заявка ХОЕ " :
  $F{kindrefcode}.intValue() == 4   ? " Заявки ХОЕ планові " :
  $F{kindrefcode}.intValue() == 5   ? " Заявки ХОЕ позапланові "
: " ___ " ) +
/* " на " + $P{YearRaznar}.toString() + "р." + $F{period_raznar} + " місяця" + */
"\n" + " Вид заявки : " +
( $F{rqorderformcode}.intValue() == 1   ? " планова " :
  $F{rqorderformcode}.intValue() == 2  ? " позапланова " :
    " планові та позапланові " ) +

"\n" + " Тип заявки : " +
( $F{rqordertypecode}.intValue() == 1   ? " бюджетна " :
  $F{rqordertypecode}.intValue() == 2  ? " інвестпрограмма " :
    " бюджетна та інвестпрограмма   " ))]]></textFieldExpression>
			</textField>
			<textField pattern="" isBlankWhenNull="false">
				<reportElement key="textField-27" x="1285" y="0" width="76" height="22"/>
				<box>
					<topPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<leftPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<bottomPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
					<rightPen lineWidth="0.0" lineStyle="Solid" lineColor="#000000"/>
