	<queryString>
		<![CDATA[SELECT
	code
	,numberdoc
	,dategen
	,expeditorname
	,molinname
	,moloutname
	,palletnumber
FROM
(
	select
		fo.code as code
		, fo.numberdoc as numberdoc
		, fo.dategen as dategen
		, fo.expeditorname as expeditorname
		, coalesce(fo.molinname, 'Невідомо') as molinname
		, coalesce(fo.moloutname, 'Невідомо') as moloutname
		, fo.palletnumber as palletnumber
	from 
    	rqfkorder as fo
	where
		fo.code = $P{orderCode}
) as subquery]]>
	</queryString>
