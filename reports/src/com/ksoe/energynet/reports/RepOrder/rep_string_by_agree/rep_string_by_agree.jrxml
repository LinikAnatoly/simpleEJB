	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="PcodeOrder" class="java.lang.String">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="orderItemCodes" class="java.lang.String">
		<defaultValueExpression><![CDATA[""]]></defaultValueExpression>
	</parameter>
	<parameter name="findocid" class="java.lang.String">
		<parameterDescription><![CDATA[id договора с фин. коллекции]]></parameterDescription>
		<defaultValueExpression><![CDATA["null"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[Select
tm.name as matname ,
tmm.name as measname ,
ri.contractnumber,
ri.contractdate,
sum(ri.countfact) as countfact,
r.numberdoc ,
ri.pricewithnds ,
sum(ri.sumgen) as sumgen ,
case when ri.deliverytime is null then tm.deliverydate else ri.deliverytime end as deliverytime ,
ri.planneddatepays ,
ri.planneddatedelivery ,
dategen
, rf.name as orderform
 ,
        case when to_char(r.orderperiod ,'MM') = '01' then 'Січень'
              when to_char(r.orderperiod ,'MM') = '02' then 'Лютий'
              when to_char(r.orderperiod ,'MM') = '03' then 'Березень'
              when to_char(r.orderperiod ,'MM') = '04' then 'Квітень'
              when to_char(r.orderperiod ,'MM') = '05' then 'Травень'
              when to_char(r.orderperiod ,'MM') = '06' then 'Червень'
              when to_char(r.orderperiod ,'MM') = '07' then 'Липень'
              when to_char(r.orderperiod ,'MM') = '08' then 'Серпень'
              when to_char(r.orderperiod ,'MM') = '09' then 'Вересень'
              when to_char(r.orderperiod ,'MM') = '10' then 'Жовтень'
              when to_char(r.orderperiod ,'MM') = '11' then 'Листопад'
              when to_char(r.orderperiod ,'MM') = '12' then 'Грудень'
            end  as fperiod ,  substr(to_char( r.orderperiod,'dd.mm.yyyy') ,7,4) as fyear
, coalesce(ro.name, 'Невідомо') as orgname
, coalesce(rb.fio, '') as fio
 from rqorder r
 inner join rqorderitem ri on r.code = ri.orderrefcode
 inner join tkmaterials tm on ri.materialcode = tm.code
 inner join tkmeasurement tmm on tm.measurementcode = tmm.code
 inner join rqordertype rt on r.rqordertypecode = rt.code
 inner join rqorderform rf on r.rqorderformcode = rf.code
 left join endepartment edep on r.budgetrefcode = edep.code
 left join  rqorg ro on ri.orgcode = ro.code
 left join rqddscodes rdds on ri.ddscodescode = rdds.code
 left join enresponsibles rb on ri.responsiblesrefcode = rb.code
Where r.code = $P!{PcodeOrder}
$P!{orderItemCodes}
and ri.findocid = $P!{findocid}
and ri.statusrefcode <> 2
group by
tm.name,
tmm.name,
ri.contractnumber,
ri.contractdate,
r.numberdoc ,
ri.pricewithnds ,
ri.deliverytime ,
ri.planneddatepays ,
ri.planneddatedelivery ,
dategen
, rf.name
 ,
        case when to_char(r.orderperiod ,'MM') = '01' then 'Січень'
              when to_char(r.orderperiod ,'MM') = '02' then 'Лютий'
              when to_char(r.orderperiod ,'MM') = '03' then 'Березень'
              when to_char(r.orderperiod ,'MM') = '04' then 'Квітень'
              when to_char(r.orderperiod ,'MM') = '05' then 'Травень'
              when to_char(r.orderperiod ,'MM') = '06' then 'Червень'
              when to_char(r.orderperiod ,'MM') = '07' then 'Липень'
              when to_char(r.orderperiod ,'MM') = '08' then 'Серпень'
              when to_char(r.orderperiod ,'MM') = '09' then 'Вересень'
              when to_char(r.orderperiod ,'MM') = '10' then 'Жовтень'
              when to_char(r.orderperiod ,'MM') = '11' then 'Листопад'
              when to_char(r.orderperiod ,'MM') = '12' then 'Грудень'
            end,  substr(to_char( r.orderperiod,'dd.mm.yyyy') ,7,4)
, coalesce(ro.name, 'Невідомо')
, coalesce(rb.fio, '')
, tm.deliverydate
Order by tm.name]]>
