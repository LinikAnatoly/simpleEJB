	<queryString>
		<![CDATA[select
 tm.name as tmaterialname, tmeas.name as tmeasurename,  oi.pricewithnds as price,
 rm.name as rmaterialname, rmeas.name as rmeasurementnam
from rqorderitem oi, rqorder o, tkmeasurement tmeas,
tkmaterials tm left join rqmaterials2tkmaterils rm2tm on tm.code = rm2tm.tkmaterialscode
left join rqmaterials rm on rm2tm.rqmaterialscode = rm.code
left join rqmeasurement rmeas on rm.measurementcode = rmeas.code
where o.kindrefcode = 2
  and oi.orderrefcode = o.code
  and oi.materialcode = tm.code
  and tmeas.code = tm.measurementcode

  group by tm.name, tmeas.name, rm.name, rmeas.name, oi.pricewithnds
 order by tm.name]]>
	</queryString>
