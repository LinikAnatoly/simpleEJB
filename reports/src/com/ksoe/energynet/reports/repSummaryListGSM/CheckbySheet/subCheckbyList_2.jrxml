	<queryString>
		<![CDATA[/*проверочная выборка маршрутных листов не подвязаных в акты или акт не проведен */
select trsh.numbergen as numbersheet ,     trsh.datestart as datesheet  ,
       sum(finm.quantity) as litrinorm , ft.name as fueltype ,
      (Select sss.name from enplanwork  aaa , enplanworkstate sss where aaa.code = trshi.planrefcode  and aaa.staterefcode = sss.code  ) as acttypename ,
      (Select su.name  from enact  aaa , enactstatus su where aaa.code = act2plan.actrefcode  and aaa.statusrefcode = su.code  ) as actstatus ,
      (Select ' '||aaa.numbergen  from enact  aaa where aaa.code = act2plan.actrefcode  ) as actnumber
 from entravelsheet trsh ,
      entravelsheetitem trshi ,
      entravlshttm2trnsprttm trshi2entri ,
      enestimateitem eni  ,
      entransport2enestimate entri2eni  ,
      finmaterials finm  ,
      enact2enplanwork act2plan ,
      enact act,
      tkfueltype ft ,
      enplanwork pw
Where  trsh.transportrealcode = $P{transportrealcode}
  and  trsh.code = trshi.travelsheetrefcode
  and  trshi.kindrefcode = 2
  and  trshi.code = trshi2entri.travelsheetitemrefcode
  and  trshi2entri.transportitemrefcode = entri2eni.transportrefcode
  and  eni.code = entri2eni.estimaterefcode
  and  ft.materialrefcode = eni.materialrefcode
  and  finm.estimateitemrefcode = entri2eni.estimaterefcode
  and  finm.statusrefcode = 1
  and  finm.div_code = $P{molcode}
  and  trshi.planrefcode = act2plan.plancode
  and  act.code = act2plan.actrefcode
  and  act2plan.plancode = pw.code
  and ((act.statusrefcode = 3 /*проведеные*/ and 1 = $P{actstatus} ) or
       (act.statusrefcode in (1,3,4) /*проведеные и черновые и на подписи */ and 2 = $P{actstatus} ))
  /* and  act.dategen between  to_date( DateStart , 'dd.mm.yyyy') and  to_date( DateEnd , 'dd.mm.yyyy')       */
  and pw.datestart between to_date( $P{DateStart} , 'dd.mm.yyyy') and  to_date( $P{DateEnd} , 'dd.mm.yyyy')
  group by trsh.numbergen  ,     trsh.datestart , ft.name , trshi.planrefcode,  act2plan.actrefcode
  order by trsh.datestart]]>
	</queryString>
