	<parameter name="DATE_FORMAT" class="DateFormat" isForPrompting="false">
		<parameterDescription><![CDATA[Формат дати для звіту]]></parameterDescription>
		<defaultValueExpression><![CDATA[new SimpleDateFormat("dd.MM.yyyy")]]></defaultValueExpression>
	</parameter>
	<parameter name="Psingers" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
matcode,
nn,
name,
namemeasure,
price,
sum(quantity) as quantity,
/* (sum(quantity)*price) as cost, */
sum(cost) as cost,
finmolcode,
finmolname,
dategen,
dateedit,
numbergen,
bal_sch,
fullcost,
finmolcode_os,
finmolname_os,
acttyperefcode,
dateact,
finmolcode_TRANSFER,
partnerName_TRANSFER
from
(
select distinct
	ma.code,
	ma.nn,
	mat.code as matcode,
	me.name as namemeasure,
	coalesce(ma.mat_name,mat.name) as name,
	ma.price,
	ma.quantity,
	ac.finmolname,
	ac.finmolcode,
	ac.dategen,
	ac.dateedit,
	ac.numbergen,
	ma.bal_sch,
	ma.fullcost,
        ma.cost ,
    coalesce(( case when ac.acttyperefcode = 16 then
                   (select COALESCE(q.molcode,'') from enmol2planwork q where q.planrefcode = acpw.plancode )
               else '' end
              ),'') as finmolcode_os /*мол с которого списывается ос (если акт по списанию ОС , иначе пусто )*/
    ,coalesce(( case when ac.acttyperefcode = 16 then
                   (select COALESCE(q.molname,'') from enmol2planwork q where q.planrefcode = acpw.plancode )
               else '' end
              ),'') as finmolname_os /*мол с которого списывается ос (если акт по списанию ОС , иначе пусто )*/
    , ac.acttyperefcode
    , ac.dateact
    ,(select gn.partnercode from enplanwork2gnrlcntrcts p2g  , enact2enplanwork a2p , engeneralcontracts gn
       where p2g.planrefcode=a2p.plancode
       and p2g.generalcontractrefcode = gn.code
       and a2p.actrefcode =  $P{PcodeAkt} ) as finmolcode_TRANSFER
   ,(select gn.partnerName from enplanwork2gnrlcntrcts p2g  , enact2enplanwork a2p , engeneralcontracts gn
   where p2g.planrefcode=a2p.plancode
   and p2g.generalcontractrefcode = gn.code
   and a2p.actrefcode =  $P{PcodeAkt} ) as partnerName_TRANSFER
from
((enestimateitem as es
	inner join
	(tkmaterials as mat
		inner join
		tkmeasurement as me
		on
		me.code = mat.measurementcode
	)
	on
	mat.code = es.materialrefcode)
	inner join
	finmaterials as ma
	on
	ma.estimateitemrefcode = es.code)
	inner join
	(enact2enplanwork as acpw
		inner join
		enact as ac
		on
		ac.code = acpw.actrefcode)
	on
	acpw.plancode = es.planrefcode
where
es.kindrefcode = 3
and ac.code = $P{PcodeAkt}) as Demontaged_materials
group by
	matcode,
	nn,
	name,
	namemeasure,
	price,
	finmolname,
	finmolcode,
	dategen,
	dateedit,
	numbergen,
	bal_sch,
	fullcost,
    finmolcode_os,
    finmolname_os,
    acttyperefcode,
    finmolcode_TRANSFER,
    partnerName_TRANSFER,


    dateact]]>
