	<queryString>
		<![CDATA[select sc.estimateitemrefcode , ' '||sc.invnumber::varchar as invnumber , ' '||sc.buildnumber::varchar as buildnumber

, (
case when el.typerefcode = 7 then
(select rpb.address from enrecordpointbyt rpb where rpb.elementcode = el.code)
when  el.typerefcode = 8 then
(select rpp.recordpointaddr from enrecordpointprom rpp where rpp.elementcode = el.code)
end
) as installPlace ,
enp.datestart ,
sc.sccode
,
 ' '|| (
case when el.typerefcode = 7 then
(select rpb.accountnumber from enrecordpointbyt rpb where rpb.elementcode = el.code)
when  el.typerefcode = 8 then
(select rpp.accountnumber from enrecordpointprom rpp where rpp.elementcode = el.code)
end
) as accountnumber

 from sccounter sc , enestimateitem eni , enplanwork enp , enelement el
 Where ( sc.invnumber = $P{invnumber}  or  $P{invnumber} = '' )
and ( sc.buildnumber = $P{buildnumber} or $P{buildnumber} = '' )
and sc.kindrefcode  = 1 /*только те которые для перемещения */
and sc.estimateitemrefcode = eni.code
and eni.planrefcode = enp.code
and el.code = enp.elementrefcode]]>
	</queryString>
