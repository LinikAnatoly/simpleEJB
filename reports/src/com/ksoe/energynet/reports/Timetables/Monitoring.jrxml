	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="DepartmentCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="DepartmentName" class="java.lang.String"/>
	<parameter name="pdatestart" class="java.lang.String"/>
	<parameter name="pdatefinal" class="java.lang.String"/>
	<queryString>
		<![CDATA[select
contractnumberservices
, case when sel_1.servicesCode = 'S1.3' and contragenttyperefcode in (2,3,5) then 'S1.3'
       when sel_1.servicesCode = 'S1.4.1' and contragenttyperefcode in (1,4) and regionaltype = 1  then 'S1.4.1'
       when sel_1.servicesCode = 'S1.4.2' and contragenttyperefcode in (1,4) and regionaltype = 2  then 'S1.4.2'
       else 'Невизначено' end as servicesCode
       , contragentname
       , contragentaddress
       , contragentphonenumber
       , contractdateservices
       , executeworkdate
       , contragentaddresswork
       , maxdatestartnaryad
        ,plancode
        ,depname
from (
    select so.contractnumberservices
     ,  so.contragenttyperefcode
     , so.regionaltype
     , so.contragentname
     , so.contragentaddress
     , so.contragentphonenumber
     , to_char(so.contractdateservices,'dd.mm.yyyy') as contractdateservices
     , to_char(so.executeworkdate,'dd.mm.yyyy') as  executeworkdate
      , so.contragentaddresswork
     , (
     select distinct skod2class.serviceskod  from enservicesobject so_in  , enplanwork p , enplanwork2classfctntp p2cl , tksrvcskd2tkclssfctntp skod2class
     where so_in.code  = so.code::double precision
     and so_in.elementcode = p.elementrefcode
     and p.kindcode = 5
     and p.code = p2cl.planrefcode
     and p2cl.classificationtyperfcd = skod2class.classificationtyperfcd
     and (   ( skod2class.serviceskod = 'S1.3' and contragenttyperefcode in (2,3,5) )
          or ( skod2class.serviceskod = 'S1.4.1' and contragenttyperefcode in (1,4) and regionaltype = 1 )
          or ( skod2class.serviceskod = 'S1.4.2' and contragenttyperefcode in (1,4) and regionaltype = 2 )
         )

     limit 1
     ) as servicesCode
     , /* макс  дата факта услуг на сторону  */
     ( select to_char(max(p.datestart),'dd.mm.yyyy')  from enservicesobject s  , enplanwork p
     where s.code = so.code
     and s.elementcode = p.elementrefcode
     and p.kindcode = 4
     limit 1 ) as maxdatestartnaryad
     ,   (  select pw.code from enplanwork pw
    where pw.elementrefcode = so.elementcode
      and pw.kindcode = 5
      limit 1 ) as plancode
      , dep.name as depname
    from enservicesobject so , endepartment dep
    where so.contractdateservices between to_date($P{pdatestart},'dd.mm.yyyy') and to_date($P{pdatefinal},'dd.mm.yyyy')
    and ((so.departmentcode = $P{DepartmentCode} ) or ($P{DepartmentCode} = 0 ))
    and so.code in (
     select so_in.code  from enservicesobject so_in  , enplanwork p , enplanwork2classfctntp p2cl , tksrvcskd2tkclssfctntp skod2class
     where so_in.code  = so.code::double precision
     and so_in.elementcode = p.elementrefcode
     and p.kindcode = 5
     and p.code = p2cl.planrefcode
     and p2cl.classificationtyperfcd = skod2class.classificationtyperfcd
     and (   ( skod2class.serviceskod = 'S1.3' and contragenttyperefcode in (2,3,5) )
          or ( skod2class.serviceskod = 'S1.4.1' and contragenttyperefcode in (1,4) and regionaltype = 1 )
          or ( skod2class.serviceskod = 'S1.4.2' and contragenttyperefcode in (1,4) and regionaltype = 2 )
         )
     )
     and so.departmentcode = dep.code
     /*только утвержденные факты что б были*/
     and so.code in (
     select so_in.code  from enservicesobject so_in  , enplanwork p
     where so_in.code  = so.code::double precision
     and so_in.elementcode = p.elementrefcode
     and p.kindcode = 4
     and p.statuscode = 3
     )
 ) sel_1

order by depname , servicesCode]]>
	</queryString>
