	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="sheetCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select i.defectcode, i.defectname,
	case when i.isdetecting = 1 then 'виявлено'
            when i.isdetecting = 0 and (select case when max(tkc.parentrefcode) is not null then null else 1 end
                from tkclassificationtype tkc where tkc.parentrefcode = tk.code) is not null then 'не виявлено' else '' end as result,
	coalesce(i.commentgen, '') as commentgen
from eninspectionsheetitem i, tkclassificationtype tk
where i.classificationtyperfcd = tk.code
and i.sheetrefcode = $P!{sheetCode}
order by i.code]]>
	</queryString>
	<field name="defectcode" class="java.lang.String">
		<fieldDescription><![CDATA[Шифр дефекту]]></fieldDescription>
	</field>
	<field name="defectname" class="java.lang.String">
