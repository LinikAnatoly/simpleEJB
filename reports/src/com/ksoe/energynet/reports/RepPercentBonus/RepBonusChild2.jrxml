	<parameter name="v_getAllTabnInBrigada" class="java.lang.String"/>
	<queryString>
		<![CDATA[select period ,
      fio_worker ,
      name_position_fin ,

sum(realizac_invest_progr) as realizac_invest_progr ,
sum(vikonan_work_priconection) as vikonan_work_priconection ,
sum(work_kr) as work_kr ,
sum(fact_zagr_all_work) as fact_zagr_all_work ,
sum(coalesce(deliverytimefact , 0)) as deliverytimefact ,
sum(realizac_invest_progr) + sum(vikonan_work_priconection) + sum(work_kr) as fact_zagr_kr_pr_inv_work ,
sum(delivery_realizac_invest_progr) + sum(delivery_vikonan_work_priconection) + sum(delivery_work_kr) as delivery_fact_zagr_kr_pr_inv_work
,
/*>>>>выборка отработаных человеко часов вообщем по бригаде по всем работам */
coalesce((select  sum(br_w_all.countfact)  from
(SELECT  enhumenitem.countfact ,
        enplanwork.typerefcode as enp_typerefcode ,
        enplanwork.staterefcode ,
        enelement.typerefcode as element_typerefcode ,
        finworker.name as fio_worker ,
        finworker.positionname ,
        enhumenitem.code as humanitemcode  ,
        sum( endeliverytime.deliverytimefact ) as deliverytimefact
           from enhumenitem   left join endeliverytime on (endeliverytime.humenitemrefcode = enhumenitem.code  )
           , enplanworkitem , finworker , enplanwork
           ,  enelement , tkposition
                   Where  enhumenitem.planitemrefcode = enplanworkitem.code
                     and  enplanworkitem.planrefcode = enplanwork.code
                     and  coalesce(enhumenitem.countfact  , 0 ) <> 0
                     and  enhumenitem.finworkercode = finworker.code
                     and  enplanwork.datestart between to_date($P{pdate1}, 'dd.mm.yyyy') and to_date($P{pdate2},'dd.mm.yyyy')
                     and  enplanwork.statuscode NOT IN (2,6)
                     and  enplanwork.kindcode = $P{plankind}  /* задание факт */
                     and  enplanwork.elementrefcode = enelement.code
                     and  enplanworkitem.countgen <> 0
                     and  enhumenitem.positiongencode = tkposition.code
                     and  finworker.kindrefcode = 1
                     and  finworker.tabnumber in $P!{v_getAllTabnInBrigada}
GROUP BY enhumenitem.countfact ,
        enplanwork.typerefcode ,
        enplanwork.staterefcode ,
        enelement.typerefcode ,
        finworker.name ,
        finworker.positionname ,
        enhumenitem.code


) br_w_all ),0) as brig_work_all
/*<<<<конец выборки отработаных человеко часов вообщем по бригаде по всем работам*/
,
/*>>>>выборка отработаных человеко часов вообщем по бригаде по КР , Приссооединению Инвест программе */
coalesce((select  sum(br_w_all.countfact)  from
(SELECT

      case when enplanwork.typerefcode = 5 then coalesce(enhumenitem.countfact,0)  /* инвест программа */
           when enplanwork.typerefcode = 7 then coalesce(enhumenitem.countfact,0) /* присоединения */
/* работы КР плановые или КР АВР */
 when (
          (
           (enplanwork.staterefcode = 1 )
      and  (
                (enplanwork.typerefcode in (1 /*, 10*/)   and  ($P{Ppodrkod} <> 755) )
                or
                (enplanwork.typerefcode in (1, 10)   and  ($P{Ppodrkod} = 755) )  /*если СПС дильниця з ремонту трансформ то учитываем и неплановые работы*/
                or
                ($P{Ppodrkod} = 605) /*если бригада кабельніх линий  ХГЕС то берем все кап ремонты в процент по кап ремонтам*/
              )
          )
          or /* NET-3595 если    Дільниця з ремонту  силових трансформаторів код = 755
                             или Майстерня з ремонту трансформаторів код 437  то в капремонты учитываем капремонты + доробку + демонтаж */
          (
           ( $P{Ppodrkod} = 755 or $P{Ppodrkod} = 437 )  and enplanwork.staterefcode in ( 1,8,19)
           )
           or /*NET-3838 если код подр 755 или 437 то в КР кидаем реконстр можернизац по неплановым работам*/
          (
           ( $P{Ppodrkod} = 755 or $P{Ppodrkod} = 437 )  and enplanwork.staterefcode = 2 and  enplanwork.typerefcode = 10
           )
          or
           /*SUPP-22235  доробку тип акта и доробку под вид работ относим как КР для спс  */
            ( ( $P{Ppodrkod} = 73 )  and enplanwork.staterefcode in ( 1,8) )
        )

        then coalesce(enhumenitem.countfact,0) else 0 end as countfact ,
        enplanwork.typerefcode as enp_typerefcode ,
        enplanwork.staterefcode ,
        enelement.typerefcode as element_typerefcode ,
        finworker.name as fio_worker ,
        finworker.positionname ,
        enhumenitem.code as humanitemcode   ,
        sum( endeliverytime.deliverytimefact) as deliverytimefact ,
        enplanwork.datestart

           from enhumenitem   left join endeliverytime on (endeliverytime.humenitemrefcode = enhumenitem.code  )
           , enplanworkitem , finworker , enplanwork
           , enelement , tkposition
                   Where  enhumenitem.planitemrefcode = enplanworkitem.code
                     and  enplanworkitem.planrefcode = enplanwork.code
                     and  coalesce(enhumenitem.countfact  , 0 ) <> 0
                     and  enhumenitem.finworkercode = finworker.code


                     and  enplanwork.datestart between to_date($P{pdate1}, 'dd.mm.yyyy') and to_date($P{pdate2},'dd.mm.yyyy')
                     and  enplanwork.statuscode NOT IN (2,6)
                     and  enplanwork.kindcode = $P{plankind}     /* задание факт */
                     and  enplanwork.elementrefcode = enelement.code
                     and  enplanworkitem.countgen <> 0
                     and  enhumenitem.positiongencode = tkposition.code

                     and  finworker.kindrefcode = 1
                     and  finworker.tabnumber in $P!{v_getAllTabnInBrigada}


      GROUP BY enhumenitem.countfact ,
               enplanwork.typerefcode  ,
               enplanwork.staterefcode ,
               enelement.typerefcode ,
               finworker.name ,
               finworker.positionname ,
               enhumenitem.code ,
               enplanwork.datestart  ,
               finworker.tabnumber




) br_w_all ),0) as brig_work_kr_pr_inv
/*<<<<конец выборки отработаных человеко часов вообщем по бригаде по КР , Приссооединению Инвест программе*/





,
/*>>>>выборка времени доставки бригады по всем работам */
coalesce((select  sum(br_w_all.deliverytimefact)  from
(SELECT  enhumenitem.countfact ,
        enplanwork.typerefcode as enp_typerefcode ,
        enplanwork.staterefcode ,
        enelement.typerefcode as element_typerefcode ,
        finworker.name as fio_worker ,
        finworker.positionname ,
        enhumenitem.code as humanitemcode  ,
        sum( endeliverytime.deliverytimefact ) as deliverytimefact
           from enhumenitem   left join endeliverytime on (endeliverytime.humenitemrefcode = enhumenitem.code  )
           , enplanworkitem , finworker , enplanwork
           ,  enelement , tkposition
                   Where  enhumenitem.planitemrefcode = enplanworkitem.code
                     and  enplanworkitem.planrefcode = enplanwork.code
                     and  coalesce(enhumenitem.countfact  , 0 ) <> 0
                     and  enhumenitem.finworkercode = finworker.code


                     and  enplanwork.datestart between to_date($P{pdate1}, 'dd.mm.yyyy') and to_date($P{pdate2},'dd.mm.yyyy')
                     and  enplanwork.statuscode NOT IN (2,6)
                     and  enplanwork.kindcode = $P{plankind}  /* задание факт */
                     and  enplanwork.elementrefcode = enelement.code
                     and  enplanworkitem.countgen <> 0
                     and  enhumenitem.positiongencode = tkposition.code

                     and  finworker.kindrefcode = 1
                     and  finworker.tabnumber in $P!{v_getAllTabnInBrigada}
GROUP BY enhumenitem.countfact ,
        enplanwork.typerefcode ,
        enplanwork.staterefcode ,
        enelement.typerefcode ,
        finworker.name ,
        finworker.positionname ,
        enhumenitem.code

) br_w_all ),0) as delivery_brig_work_all
/*<<<<конец выборки доставки бригады  по всем работам*/
,
/*>>>>выборка доставки  бригаде по КР , Приссооединению Инвест программе */
coalesce((select  sum(br_w_all.deliverytimefact)  from
(SELECT  enhumenitem.countfact ,
         enplanwork.typerefcode as enp_typerefcode ,
         enplanwork.staterefcode ,
         enelement.typerefcode as element_typerefcode ,
         finworker.name as fio_worker ,
         finworker.positionname ,
         enhumenitem.code as humanitemcode   ,
         sum( endeliverytime.deliverytimefact) as deliverytimefact

           from enhumenitem  left join endeliverytime on (endeliverytime.humenitemrefcode = enhumenitem.code  )
           , enplanworkitem , finworker , enplanwork
           ,  enelement , tkposition
                   Where  enhumenitem.planitemrefcode = enplanworkitem.code
                     and  enplanworkitem.planrefcode = enplanwork.code
                     and  coalesce(enhumenitem.countfact  , 0 ) <> 0
                     and  enhumenitem.finworkercode = finworker.code
                     and  enplanwork.datestart between to_date($P{pdate1}, 'dd.mm.yyyy') and to_date($P{pdate2},'dd.mm.yyyy')
                     and  enplanwork.statuscode NOT IN (2,6)
                     and  enplanwork.kindcode = $P{plankind}   /* задание факт */
                     and  enplanwork.elementrefcode = enelement.code
                     and  enplanworkitem.countgen <> 0
                     and  enhumenitem.positiongencode = tkposition.code
                     and  finworker.tabnumber in $P!{v_getAllTabnInBrigada}
                     and  finworker.kindrefcode = 1
                     and  (
                               (enplanwork.typerefcode in (5,7)) or
                              (
                                ( enplanwork.staterefcode in (1 /*, 8*/ ) )   and ( (enplanwork.typerefcode = 1 and $P{Ppodrkod} <> 755 ) or  (enplanwork.typerefcode in (1,10) and $P{Ppodrkod}  = 755) or ($P{Ppodrkod} = 605)   )
                               )
                              or /* NET-3595 если    Дільниця з ремонту  силових трансформаторів код = 755
                             или Майстерня з ремонту трансформаторів код 437  то в капремонты учитываем капремонты + доробку + демонтаж */
                             (
                             ( $P{Ppodrkod} = 755 or $P{Ppodrkod} = 437 )  and enplanwork.staterefcode in ( 1,8,19)
                             )
                             or /*NET-3838 если код подр 755 или 437 то в КР кидаем реконстр можернизац по неплановым работам*/
                            (
                             ( $P{Ppodrkod} = 755 or $P{Ppodrkod} = 437 )  and enplanwork.staterefcode = 2 and  enplanwork.typerefcode = 10
                             )
                             or
                            /*SUPP-22235  доробку тип акта и доробку под вид работ относим как КР для спс  */
                             ( ( $P{Ppodrkod} = 73 )  and enplanwork.staterefcode in ( 1,8) )
                           ) /*по КР попадают тока плановые работы*/

      GROUP BY enhumenitem.countfact ,
        enplanwork.typerefcode  ,
        enplanwork.staterefcode ,
        enelement.typerefcode ,
        finworker.name ,
        finworker.positionname ,
        enhumenitem.code



) br_w_all ),0) as delivery_brig_work_kr_pr_inv
/*<<<<конец выборки доставки  бригады по КР , Приссооединению Инвест программе*/


      From (


select (select  case when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '01' then 'Січень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '02' then 'Лютий'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '03' then 'Березень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '04' then 'Квітень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '05' then 'Травень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '06' then 'Червень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '07' then 'Липень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '08' then 'Серпень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '09' then 'Вересень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '10' then 'Жовтень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '11' then 'Листопад'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '12' then 'Грудень'
            end  ) as period ,
 $P{fio_fin}  as fio_worker ,  $P{post_fin} as name_position_fin  ,


sum(realizac_invest_progr) as realizac_invest_progr ,
sum(vikonan_work_priconection) as vikonan_work_priconection ,
sum(work_kr)  as work_kr ,
sum(fact_zagr_all_work) as fact_zagr_all_work ,
sum(deliverytimefact)   as  deliverytimefact ,
sum(delivery_realizac_invest_progr) as delivery_realizac_invest_progr ,
sum(delivery_vikonan_work_priconection) as delivery_vikonan_work_priconection ,
sum(delivery_work_kr) as delivery_work_kr

from (
select
baz_sel.fio_worker , baz_sel.positionname ,

/* инвест программа */ case when baz_sel.enp_typerefcode = 5 then coalesce(baz_sel.countfact,0) else 0 end as realizac_invest_progr ,
/* присоединения */ case when baz_sel.enp_typerefcode = 7 then coalesce(baz_sel.countfact,0) else 0 end as vikonan_work_priconection ,
/* работы КР  */
case when (
          (
           (baz_sel.staterefcode in (1 /*, 8*/ ) )
      and  (
                (baz_sel.enp_typerefcode in (1 /*, 10*/)   and  ($P{Ppodrkod} <> 755) )
                or
                (baz_sel.enp_typerefcode in (1, 10)   and  ($P{Ppodrkod} = 755) )  /*если СПС дильниця з ремонту трансформ то учитываем и неплановые работы*/
                or
                ($P{Ppodrkod} = 605 ) /*если бригада кабельніх линий  ХГЕС то берем все кап ремонты в процент по кап ремонтам*/

              )
          )

          or /* NET-3595 если    Дільниця з ремонту  силових трансформаторів код = 755
                             или Майстерня з ремонту трансформаторів код 437  то в капремонты учитываем капремонты + доробку + демонтаж */
          (
           ( $P{Ppodrkod} = 755 or $P{Ppodrkod} = 437 )  and baz_sel.staterefcode in ( 1,8,19)
           )
           or /*NET-3838 если код подр 755 или 437 то в КР кидаем реконстр можернизац по неплановым работам*/
          (
           ( $P{Ppodrkod} = 755 or $P{Ppodrkod} = 437 )  and baz_sel.staterefcode = 2 and  baz_sel.enp_typerefcode = 10
           )
          or
           /*SUPP-22235  доробку тип акта и доробку под вид работ относим как КР для спс  */
            ( ( $P{Ppodrkod} = 73 )  and baz_sel.staterefcode in ( 1,8) )

        )
        then coalesce(baz_sel.countfact,0) else 0 end as work_kr ,

/*доставка инвест программа */ case when baz_sel.enp_typerefcode = 5 then coalesce(baz_sel.deliverytimefact,0) else 0 end as delivery_realizac_invest_progr ,
/*доставка присоединения */ case when baz_sel.enp_typerefcode = 7 then coalesce(baz_sel.deliverytimefact,0) else 0 end as delivery_vikonan_work_priconection ,
/*доставка работы КР  */
  case when (
          (
           (baz_sel.staterefcode in (1 /*, 8*/ ) )
        and  (
                (baz_sel.enp_typerefcode in (1 /*, 10*/)   and  ($P{Ppodrkod} <> 755) )
                or
                (baz_sel.enp_typerefcode in (1, 10)   and  ($P{Ppodrkod} = 755) )  /*если СПС дильниця з ремонту трансформ то учитываем и неплановые работы*/
                or
                ($P{Ppodrkod} = 605) /*если бригада кабельніх линий  ХГЕС то берем все кап ремонты в процент по кап ремонтам*/
              )
          )
          or /* NET-3595 если    Дільниця з ремонту  силових трансформаторів код = 755
                             или Майстерня з ремонту трансформаторів код 437  то в капремонты учитываем капремонты + доробку + демонтаж */
          (
           ( $P{Ppodrkod} = 755 or $P{Ppodrkod} = 437 )  and baz_sel.staterefcode in ( 1,8,19)
           )
            or /*NET-3838 если код подр 755 или 437 то в КР кидаем реконстр можернизац по неплановым работам*/
          (
           ( $P{Ppodrkod} = 755 or $P{Ppodrkod} = 437 )  and baz_sel.staterefcode = 2 and  baz_sel.enp_typerefcode = 10
           )
          or
           /*SUPP-22235  доробку тип акта и доробку под вид работ относим как КР для спс  */
            ( ( $P{Ppodrkod} = 73 )  and baz_sel.staterefcode in ( 1,8) )
        )
           then coalesce(baz_sel.deliverytimefact,0) else 0 end as delivery_work_kr ,

baz_sel.countfact as fact_zagr_all_work ,  baz_sel.deliverytimefact ,  baz_sel.humanitemcode

 From (
SELECT  enhumenitem.countfact ,
        enplanwork.typerefcode as enp_typerefcode ,
        enplanwork.staterefcode ,
        enelement.typerefcode as element_typerefcode ,
        finworker.name as fio_worker ,
        finworker.positionname ,
        sum( endeliverytime.deliverytimefact ) as deliverytimefact  ,
        enhumenitem.code as humanitemcode  ,
        enplanwork.datestart

           from enhumenitem   left join endeliverytime on (endeliverytime.humenitemrefcode = enhumenitem.code  )
           , enplanworkitem , finworker , enplanwork
           ,  enelement , tkposition
                   Where  enhumenitem.planitemrefcode = enplanworkitem.code
                     and  enplanworkitem.planrefcode = enplanwork.code
                     and  coalesce(enhumenitem.countfact  , 0 ) <> 0
                     and  enhumenitem.finworkercode = finworker.code


                     and  enplanwork.datestart between to_date($P{pdate1} , 'dd.mm.yyyy') and to_date($P{pdate2},'dd.mm.yyyy')
                     and  enplanwork.statuscode NOT IN (2,6)
                     and  enplanwork.kindcode = $P{plankind}   /* задание факт */
                     and  enplanwork.elementrefcode = enelement.code
                     and  enplanworkitem.countgen <> 0
                     and  finworker.tabnumber/* ::varchar */ = cast($P{tabn_fin} as double precision)
                     and  enhumenitem.positiongencode = tkposition.code

                     and  finworker.kindrefcode = 1

                     group by enhumenitem.countfact ,
        enplanwork.typerefcode  ,
        enplanwork.staterefcode ,
        enelement.typerefcode ,
        finworker.name  ,
        finworker.positionname ,
        enhumenitem.code ,
        enplanwork.datestart




) baz_sel
) baz_sum
group by baz_sum.fio_worker , baz_sum.positionname /*,  baz_sum.deliverytimefact*/



UNION
select
(select  case when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '01' then 'Січень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '02' then 'Лютий'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '03' then 'Березень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '04' then 'Квітень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '05' then 'Травень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '06' then 'Червень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '07' then 'Липень'
              when to_char(to_date($P{pdate2},'dd.mm.yyyy'),'mm') = '08' then 'Серпень'
              when to_char(to_date($P{pdate2},'dd.mm.yyyy'),'mm') = '09' then 'Вересень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '10' then 'Жовтень'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '11' then 'Листопад'
              when to_char(to_date($P{pdate2} ,'dd.mm.yyyy'),'mm') = '12' then 'Грудень'
            end  ) as period  ,
 $P{fio_fin} as fio_worker ,  $P{post_fin} as name_position_fin  ,


0 as realizac_invest_progr ,
0 as vikonan_work_priconection ,
0 as work_in_energozbut ,
0 as fact_zagr_all_work ,
0 as  deliverytimefact ,
0 as delivery_realizac_invest_progr ,
0 as delivery_vikonan_work_priconection ,
0 as delivery_work_kr

  where not exists ( select * from
             enhumenitem , finworker , enplanwork
                   Where  enhumenitem.planrefcode = enplanwork.code
                     and  coalesce(enhumenitem.countfact  , 0 ) <> 0
                     and  enhumenitem.finworkercode = finworker.code
                     and  enplanwork.datestart between to_date($P{pdate1} , 'dd.mm.yyyy') and to_date($P{pdate2}  ,'dd.mm.yyyy')
                     and  enplanwork.statuscode NOT IN (2,6)
                     and  enplanwork.kindcode = $P{plankind}  /* задание факт */
                     and  finworker.tabnumber/* ::varchar */ = cast($P{tabn_fin} as double precision)
                     and  finworker.kindrefcode = 1

               )


) sel_zag

group by period , fio_worker , name_position_fin


order by fio_worker , name_position_fin]]>
