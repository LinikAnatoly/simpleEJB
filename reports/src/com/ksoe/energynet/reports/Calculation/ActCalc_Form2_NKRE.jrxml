	<queryString>
		<![CDATA[select query.* from
(select chs.code,
chs.classificationtypenmbr as vidwork,
chs.positionname as name,
chs.timegenmonth as NORMHOURSMONTH,
chs.timegen as countmh,
chs.salarymonth as SALARYWORKER,
chs.pricehour as tariff1hour,
chs.salaryhour as basiczp,
chs.salarybonus as prem,
chs.salarytotalbonus as dopolntitzp,
chs.salarytotal as totalzp,
 tkp.salarysurcharge  || '/' || chs.salarysurcharge as salarysurcharge,
 tkp.salarypremium   || '/' || chs.salarypremium as salarypremium,
 tkp.salaryadditional  || '/' || chs.salaryadditional as salaryadditional

from encalchumensalary chs, tkpositionhistory tkp, enplanwork p
where
chs.positionrefcode = tkp.positioncode
and chs.planrefcode = p.code
and p.datestart between tkp.datefrom and coalesce(tkp.dateto,'01.01.2999')
and
(( chs.planrefcode = (  select pw2.code from enplanwork pw , enplanwork pw2
    where  pw.code =  $P{plancode}
      and pw.elementrefcode = pw2.elementrefcode
      and pw.kindcode = 5
      and pw2.kindcode = 6 limit 1 )  and 0 = $P{licenzed} ) or ( chs.planrefcode = $P{plancode} and $P{licenzed} <> 0 )
 )
and 1 = $P{reportType}

union all

select
chs.code
, ct.kod as vidwork
, chs.positionname as name
, chs.timegenmonth as NORMHOURSMONTH
, chs.timegen as countmh
, chs.salarymonth as SALARYWORKER
, chs.salaryhour as tariff1hour
, chs.salary as basiczp
, chs.salarybonus as prem
, chs.salarytotalbonus as dopolntitzp
, chs.salarytotal as totalzp
, chs.percentsurcharge  || '/' || chs.salarysurcharge as salarysurcharge
, chs.percentpremium   || '/' || chs.salarypremium as salarypremium
, chs.percentadditional  || '/' || chs.salaryadditional as salaryadditional
 from tkcalchumensalary chs
 inner join tkcalccost as cc on chs.calccostrefcode = cc.code
 inner join tkclassificationtype as ct on cc.classificationtyperfcd = ct.code
 where
 cc.code = $P{calcCostCode}
 and 2 = $P{reportType}
) as query
order by query.code]]>
	</queryString>
