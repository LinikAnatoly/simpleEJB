	</parameter>
	<parameter name="transportCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(-1)]]></defaultValueExpression>
	</parameter>
	<parameter name="reportType" class="java.lang.Integer">
		<parameterDescription><![CDATA[Тип отчет:
1 - отчет формируется по данным из планов сметы;
2 - отчет формируется по данным из расчетов по калькуляциям.]]></parameterDescription>
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="calcCostCode" class="java.lang.Integer">
		<parameterDescription><![CDATA[код расчета для калькуляции]]></parameterDescription>
		<defaultValueExpression><![CDATA[Integer.MIN_VALUE]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select  transpname,
TKSALARYDRIVER,
TKNORMHOURSMONTH,
TKSALARYDRIVERHOURS,
TKANCHARGEONSALARY,
vsogo,
TKTRANSPORTCOST,
TKTRANSPORTYEARAMORT,
TKTRANSPORTAMORT,
TKTRANSPORTFUELCHARGE,
cost1mh,

case when isnotlicensedactivity <> 1 then
  case when tktransporttypecode in (500000028, 75000010, 75000184) then
    round(cast((((coalesce(cost1mh,0)+coalesce(profitrate,0) )/ average_speed ) ) as decimal),2)
    else
    round(cast(((coalesce(cost1mh,0) + coalesce(profitrate,0) )/ average_speed ) as decimal),2) end

else /*для транспортных услуг  тип транспорта анализируется с марки*/
  case when transporttypecode_mark in (500000000 , 75000004 ,75000007 ) then
     (rashodprobeg/100)::numeric(15,2)
  else
      ((coalesce(cost1mh,0) + coalesce(profitrate,0))/average_speed)::numeric(15,2)
  end
 end    as cost1km,

productioncosts,
administrativecosts,
profitrate,

case when isnotlicensedactivity <> 1 then
    case when tktransporttypecode in (500000028, 75000010, 75000184)
    then 'Плата за пробіг = '||cast((coalesce(cost1mh,0) + coalesce(profitrate,0) )::numeric(15,2) as varchar)||'[Вартість 1 маш.год]'||
                                                    '/'||average_speed::varchar ||'[Середня швидкість авто] = '||

                                                    round(cast((((coalesce(cost1mh,0) + coalesce(profitrate,0) )/ average_speed )) as decimal),2)
    else 'Плата за пробіг = ('||cast((coalesce(cost1mh,0) + coalesce(profitrate,0) )::numeric(15,2) as varchar)||
    '[Вартість 1 маш.год] / '||
    average_speed::varchar||
    '[Середня швидкість авто] = '||
    round(cast(((coalesce(cost1mh,0) + coalesce(profitrate,0) )/ average_speed ) as decimal),2)
    end
 else  /*для транспортных услуг  тип транспорта анализируется с марки*/
    case when transporttypecode_mark in (500000000 , 75000004 ,75000007 ) then

    'Плата за пробіг = ' || rashodprobeg::numeric(15,2) || '[Витрата палива на 100км]/100*[Вартість літра палива] = ' || (rashodprobeg/100)::numeric(15,2)
    else

    'Плата за пробіг = (' || coalesce(cost1mh,0)::numeric(15,2) || '[Вартість 1 маш.год. Ф.№5] + ' || coalesce(profitrate,0)::numeric(15,2) || '['||percentprofitrate||'% від суми витрат Ф.№5 ])/' || average_speed::numeric(15,2) || '[Баз.швидк.]=' || ((coalesce(cost1mh,0) + coalesce(profitrate,0))/average_speed)::numeric(15,2)

    end
 end  as commentpricedistance

, isnotlicensedactivity
, average_speed
, production_expenses_percent
, administrative_expenses_percent
, repaircostsperhour
, percentprofitrate
from (
        select  tr.name as transpname,
        cthu.salarymonthdriver as TKSALARYDRIVER,
        cthu.normworktimemonth as TKNORMHOURSMONTH,
        cthu.salaryhourdriver as TKSALARYDRIVERHOURS,
        cthu.salarychargehourdriver as TKANCHARGEONSALARY,
        cthu.salarytotalhourdriver as vsogo,
        cthu.costmachine as TKTRANSPORTCOST,
        cthu.amortizationyearmachin as TKTRANSPORTYEARAMORT,
        cthu.amortizationhourmachin as TKTRANSPORTAMORT,
        0 as TKTRANSPORTFUELCHARGE,
        coalesce(cthu.salarytotalhourdriver,0) + coalesce(cthu.productioncosts,0) + coalesce(cthu.amortizationhourmachin,0) + coalesce(cthu.administrativecosts, 0) + coalesce(cthu.repaircostsperhour, 0)  as cost1mh,


         cthu.productioncosts,
         coalesce(cthu.administrativecosts, 0) as administrativecosts,
        case when /*если трансп услуги*/ tct.isnotlicensedactivity = 1 then
        ((coalesce(cthu.salarytotalhourdriver,0)
        + coalesce(cthu.productioncosts,0)
        + coalesce(cthu.amortizationhourmachin,0)
        + coalesce(cthu.administrativecosts, 0)
        + coalesce(cthu.repaircostsperhour, 0)) *(tct.percentprofitrate/100))::numeric(15,2)  else 0 end as profitrate,

         tct.isnotlicensedactivity ,
         tr.transportmarkcode /*tr.tktransporttypecode*/ as tktransporttypecode ,
         tm.transporttypecode as transporttypecode_mark ,
         tm.rashodprobeg  ,
-- скорость

 case when tct.isnotlicensedactivity <> 1 then 45 --средняя скорость для калькуляций кроме транспортных
     else
     case when tm.transporttypecode in (500000000,75000004,75000007) then 0 --- в логике нету для таких расчета по скорости
     else
       coalesce(tm.basicspeed,0)
     end
 end as average_speed
, coalesce(p2t.productionexpenssprcnt, 60) as production_expenses_percent
, coalesce(p2t.administratvxpnssprcnt, 0) as administrative_expenses_percent
, tct.percentprofitrate
, coalesce(cthu.repaircostsperhour, 0) as repaircostsperhour
 from encalctransportusagehr as cthu
 inner join tktransport as tr on cthu.tktransportrefcode = tr.code
 inner join encalctransportusage as ctu on (cthu.planrefcode = ctu.planrefcode and ctu.tktransportrefcode = cthu.tktransportrefcode)
 inner join tktransportmark as tm on tm.code = tr.transportmarkcode
 inner join enplanwork2classfctntp as p2t on ctu.plan2ctyperefcode = p2t.code
 inner join tkclassificationtype as tct on p2t.classificationtyperfcd = tct.code
        where cthu.planrefcode = $P{plancode}
          and tr.code = $P{transportCode}
          and 1 = $P{reportType}

union all

        select  cct.transportname as transpname,
        cct.salarymonthdriver as TKSALARYDRIVER,
        cct.normworktimemonth as TKNORMHOURSMONTH,
        cct.salaryhourdriver as TKSALARYDRIVERHOURS,
        cct.salarychargehourdriver as TKANCHARGEONSALARY,
        cct.salarytotalhourdriver as vsogo,
        cct.costmachine as TKTRANSPORTCOST,
        cct.amortizationyearmachin as TKTRANSPORTYEARAMORT,
        cct.amortizationhourmachin as TKTRANSPORTAMORT,
        0 as TKTRANSPORTFUELCHARGE,
        coalesce(cct.salarytotalhourdriver,0) + coalesce(cct.productioncosts,0) + coalesce(cct.amortizationhourmachin,0) + coalesce(cct.administrativecosts, 0) + coalesce(cct.repaircostsperhour, 0) as cost1mh,


         cct.productioncosts,
         coalesce(cct.administrativecosts, 0) as administrativecosts,

        case when ct.isnotlicensedactivity = 1 then
        ((coalesce(cct.salarytotalhourdriver,0)
        + coalesce(cct.productioncosts,0)
        + coalesce(cct.amortizationhourmachin,0)
        + coalesce(cct.administrativecosts, 0)
        + coalesce(cct.repaircostsperhour, 0)) *(ct.percentprofitrate/100))::numeric(15,2)  else 0 end as profitrate,

         ct.isnotlicensedactivity ,
         tr.transportmarkcode as tktransporttypecode ,
         tm.transporttypecode as transporttypecode_mark ,
         tm.rashodprobeg  ,
 case when ct.isnotlicensedactivity <> 1 then 45
     else
     case when tm.transporttypecode in (500000000,75000004,75000007) then 0
     else
       coalesce(tm.basicspeed,0)
     end
 end as average_speed
, coalesce(cc.productionexpenssprcnt, 60) as production_expenses_percent
, coalesce(cc.administratvxpnssprcnt, 0) as administrative_expenses_percent
, ct.percentprofitrate
, coalesce(cct.repaircostsperhour, 0) as repaircostsperhour
from tkcalctransport as cct
inner join tkcalccost as cc on cct.calccostrefcode = cc.code
inner join tkclassificationtype as ct on cc.classificationtyperfcd = ct.code
inner join tktransport as tr on cct.transportrefcode = tr.code
inner join tktransportmark as tm on tr.transportmarkcode = tm.code
where
cc.code = $P{calcCostCode}
and tr.code = $P{transportCode}
and 2 = $P{reportType}

  ) as q1]]>
	</queryString>
