	<parameter name="objNameFinExecutor" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[Select
case when $P{show_last_buy_partner} = 1 then ( select o."name" from rqbillitem bi , rqbill b , rqorg o
where bi.billrefcode =b.code
and b.orgcode=o.code
and bi.materialcode=dat.materialrefcode
order by b.dategen desc
limit 1
) else '' end as last_buy_partner ,
TRIM(objname) AS objname ,
trim(finexecutorname) as finexecutorname ,
mname, meas, cast(sum(countfact) as numeric(15,4)) as countfact,
cast(sum(cost) as numeric(15,4)) as cost,
cast(price as numeric(15,4)) as price,
deliverydate,
dname , budgetrefname , dat.kindrefcode , dat.materialrefcode ,
case when $P{pr_identid} = 1 then
     case when identid is null then 'Без класіфікатора' else identid end
     when $P{pr_identid} = 0 then '1' end
as identid ,


  --- start col group
(case when  $P{pr_par_mat} = 1 then (  select distinct(tmm.name) from tkmaterials tmm where tmm.code =
                   (select rootgrouprefcode from tkmaterials tmm2
                                            where tmm2.code = materialrefcode )
                 )
     when  $P{pr_par_mat} = 0 then '1' end )
as name_group
 , enresponsiblesname
  ,   (case when $P{withNN} = '1' then
  (select string_agg(tn.mat_id::text,',') from tempnomenclatures tn
  where tn.materialrefcode = dat.materialrefcode)
 when  $P{withNN} = '0' then '' end ) as nn_string,
 dat.meascode
   From
 (
Select
 case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end  as mname,
 es.countfact as countfact,
 ei.name as meas,
 p.monthgen as monthgen ,
 sm.deliverydate as deliverydate,
 coalesce(sm.cost, 0) as price,
 coalesce((sm.cost * es.countfact), 0) as cost,
(case when $P{objNameField} = 1 then '1' else ( ename||' , инв№ '||coalesce(invnumber,' ') ) end) as objname,
(case when $P{objNameFinExecutor} = 1 then '1' else (select f."name" from finexecutor f where f.code = p.finexecutorcode order by f.code desc limit 1 ) end) as finexecutorname,
 $P!{dNameField} as dname ,
 $P!{dbNameField} As budgetrefname
 , es.kindrefcode ,
 es.materialrefcode
 , cast(sm.identid as varchar ) as identid
 , rs.fio ||' ' || 'таб.№' ||' '|| rs.tabnumber as enresponsiblesname

, ei.code as meascode
 From enplanwork p ,
 enelementdata e ,
tkmeasurement ei,
enestimateitem es,
tkmaterials sm left join enresponsibles rs on sm.responsiblesrefcode = rs.code ,
endepartment d ,
endepartment db
Where p.code = es.planrefcode
And p.departmentrefcode = d.code
And p.budgetrefcode = db.code
And p.elementrefcode = e.ecode
And es.materialrefcode = sm.code
And ei.code=sm.measurementcode
And p.kindcode = $P{pKindCode}  /*годовой(1) или текущий(2)*/
And p.yeargen = $P{yearGen}
And ((p.monthgen = $P{monthGen}  and $P{wholeYear} = 0) or $P{wholeYear} <> 0)
and (p.departmentrefcode = $P{renCode} or $P{renCode} = 0)
/*And ((p.budgetrefcode = {budgCode}) or {budgCode} = 0)*/
And $P!{budgetstringcode}
And (e.ecode = $P{elementCode} or $P{elementCode} = 0)
And p.statuscode not in (2,6)
And es.countfact <> 0
And (( es.kindrefcode  = $P{kindrefcodemat}  AND $P{kindrefcodemat} = 1 ) OR
     ( es.kindrefcode  In (1,2) AND $P{kindrefcodemat}  = 2 ))

And ( p.staterefcode = $P{stateworkcode} or $P{stateworkcode} = 0 )
And ( e.etype = $P{elementtypecode} or $P{elementtypecode} = 0 )

-- and es.statusrefcode not in (5, 6 , 7 , 9 , 8 )
 $P!{estimatestatus}

/* группа материалов */
/*and sm.code in
(select tm.code from tkmaterials tm
where tm.rootgrouprefcode = {materialGroupCode} or {materialGroupCode} = 0) */
$P!{materialGroupCodes}

/*фильтр по Под вид работ заказ Щербины Саши21,12,2010*/

And ( p.typerefcode = $P{typerefcode} or $P{typerefcode} = 0 )


And ( (p.budgetrefcode <> 240000001 and $P{IsNotEnergozbyt} = 1 ) or $P{IsNotEnergozbyt} = 0 )


/*вид работ + статус по внеплановым работам */
and case when $P{planform} = 0 then /*(плановые и позаплановые) тогда накинем фильтр по внеплановым планам*/
          case when p.formrefcode = 2 and  $P{planStatusForPozaplanPlans} = 13  then p.statuscode in (1,3,7)
               when p.formrefcode = 2 and  $P{planStatusForPozaplanPlans} = 1    then p.statuscode  in (1,7)
               when p.formrefcode = 2 and  $P{planStatusForPozaplanPlans} = 3   then p.statuscode = 3
               when p.formrefcode = 2 and  $P{planStatusForPozaplanPlans} = 0    then 1 = 1
               when p.formrefcode = 1                                             then 1 = 1
          end
         when $P{planform} = 1 then /*(плановые только) */ p.formrefcode = 1
         when $P{planform} = 2 then /*(внеплановые только) */
          case when p.formrefcode = 2 and  $P{planStatusForPozaplanPlans} = 13  then p.statuscode in (1,3,7)
               when p.formrefcode = 2 and  $P{planStatusForPozaplanPlans} = 1    then p.statuscode  in (1,7)
               when p.formrefcode = 2 and  $P{planStatusForPozaplanPlans} = 3   then p.statuscode = 3
               when p.formrefcode = 2 and  $P{planStatusForPozaplanPlans} = 0    then 1 = 1
          end

    end

 /*тип работ + статус по инвест программе*/
 and
   case when $P{pBudgOrInv} = 0 then
         case  when p.typerefcode in (5, 100, 106,111,112) and  $P{planStatusForInvest} = 13  then p.statuscode in (1,3,7) /* инвест + статусы*/
               when p.typerefcode in (5, 100, 106,111,112) and  $P{planStatusForInvest} = 1    then p.statuscode  in (1,7)    /* инвест + статусы*/
               when p.typerefcode in (5, 100, 106,111,112) and  $P{planStatusForInvest} = 3   then p.statuscode = 3      /* инвест + статусы*/
               when p.typerefcode in (5, 100, 106,111,112) and  $P{planStatusForInvest} = 0    then 1 = 1                 /* инвест + статусы*/
               when p.typerefcode not in (5, 100, 106,111,112)                                  then 1 = 1      /* бюджет не учитывая статусов*/
          end
         when $P{pBudgOrInv} = 1  then p.typerefcode not in (5, 100, 106,111,112) /* бюджетные планы*/
         when $P{pBudgOrInv} = 2  then /*только инвест*/
         case  when p.typerefcode in (5, 100, 106,111,112) and  $P{planStatusForInvest} = 13  then p.statuscode in (1,3,7) /* инвест + статусы*/
               when p.typerefcode in (5, 100, 106,111,112) and  $P{planStatusForInvest} = 1    then p.statuscode  in (1,7)     /* инвест + статусы*/
               when p.typerefcode in (5, 100, 106,111,112) and  $P{planStatusForInvest} = 3   then p.statuscode = 3      /* инвест + статусы*/
               when p.typerefcode in (5, 100, 106,111,112) and  $P{planStatusForInvest} = 0    then 1 = 1                 /* инвест + статусы*/
          end
   end

and p.typerefcode <> 11 -- планы перевезення не берем
and (  ( es.planitemrefcode is null )
      or
      -- не берем если источник норматива "перевезення"
       ( 500000024 <> ( select tkd.techcardsourcecode from enplanworkitem pi , tktechcard tkd
                                                      where pi.kartarefcode = tkd.code
                                                        and pi.code = es.planitemrefcode limit 1  )
       )
    )

and case when $P{is_works_finished} = 0 then p.statuscode <> 8 else 1 =1 end
and case when $P{isOWN_PRODUCTION} = 0 then es.statusrefcode <> 10 else 1=1 end
) dat
group by  identid , objname, finexecutorname , mname, meas, /*cost, */ price, dname , budgetrefname , dat.kindrefcode ,  dat.materialrefcode  , enresponsiblesname, deliverydate, dat.meascode
order by  identid , name_group ,  dat.budgetrefname , dat.dname , trim(dat.objname) , trim(dat.finexecutorname) , mname , enresponsiblesname]]>
	</queryString>
