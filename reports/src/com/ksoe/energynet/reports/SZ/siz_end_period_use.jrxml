        , depname
from
(
Select  fio ,
        tabnumber ,
        materialname ,
        materialcode ,
        elementcode ,
        dateeditestim ,
        name ,
        countmonth ,
        useworktime_from_estim ,
        countfact ,
        case /*если срок исп на карточке не нулевой а срок на устимейт нулевой значит
             от макс даты редактирования строки материала в плате отсчитываем колл-во месяцев использования по норме и получаем
             дату конца использования материала */
             when coalesce(countmonth,0) <> 0 and coalesce(useworktime_from_estim,0) = 0
             then add_months(dateeditestim, countmonth::integer)
             /*если срок исп на карточке не нулевой и срок на устимейт не нулевой и срок на карточке больше срока на естимейте
               значит от макс даты редактирования строки материала в плате отсчитываем срок на  разницу месяцев срок по карточке минус срок из естимейта  */
             when ( coalesce(countmonth,0) <> 0 and coalesce(useworktime_from_estim,0) <>  0 ) and ( coalesce(countmonth,0) > coalesce(useworktime_from_estim,0))
             then add_months(dateeditestim, (countmonth-useworktime_from_estim)::integer)
             /*если сроки на карточке и естимейте совпадают то берем прибавляем срок использования указанный на карточке */
             when (( coalesce(countmonth,0) <> 0 and coalesce(useworktime_from_estim,0) <> 0 ) and  (countmonth::integer = useworktime_from_estim::integer ))
             then add_months(dateeditestim, countmonth::integer)
 end as periodend
 , depname
From  (

Select  so.fio ,
        so.tabnumber ,
        m.name as materialname ,
        m.code as materialcode ,
        so.elementcode ,
        max(i.dateedit) as dateeditestim ,
        tus.name ,
        tus.countmonth ,
        (select i2.useworktime from enestimateitem i2 , enplanwork p2
          where i2.materialrefcode = m.code
            and i2.planrefcode = p2.code
            and p2.elementrefcode = so.elementcode
           -- and i2.dateedit = i.dateedit
            order by i2.dateedit DESC
            limit 1   ) as useworktime_from_estim ,
        i.countfact as countfact
        , dep.name as depname
From ensizobject so , enplanwork p , enestimateitem i , tkmaterials m left join tktimeuse tus on tus.code = m.timeusecode
     , endepartment dep
where so.statuscode = 0 /*работающие*/
  and so.elementcode = p.elementrefcode
  and p.code = i.planrefcode
  and i.statusrefcode = 3 /*статус материала в наявности*/
  and p.typerefcode = 13 /* Засоби захисту*/
  and p.staterefcode = 17 /*Спецодяг*/
  and p.kindcode = 2 /*только по месячным планам*/
  and so.podrid = dep.code
  $P!{department}

  and i.materialrefcode = m.code
  and coalesce(tus.countmonth,0) < 100 /*берем только явное количество месяцов использования */
  /*проверка на списание материала
  если материал в наявности но он уже списан то мы его не берем*/
  and  not EXISTS (Select  f.estimateitemrefcode
                     from finmaterials f , enestimateitem ii , enplanwork pp
                     where f.estimateitemrefcode in (
                                                      select e2e.estimateitemoutrefcode from enestimateitem2nstmttm e2e
                                                       where e2e.typerefcode = 1
                                                         and e2e.estimateiteminrefcode in (
                                                     			 select e2e.estimateitemoutrefcode from enestimateitem2nstmttm e2e
                                                     			  where e2e.typerefcode = 1
                                                        		    and e2e.estimateiteminrefcode = i.code
                                                                                          )
                                                    )
                     and   f.estimateitemrefcode = ii.code
                     and   ii.planrefcode = pp.code
                     and   pp.kindcode = 4
                     and   pp.statuscode = 3    )

  group by so.fio , m.name , m.code ,  so.tabnumber , tus.name , tus.countmonth , so.elementcode , i.countfact , dep.name
  order by  so.fio , so.tabnumber , m.name , m.code
  ) sel_h


 ) sel_p

 ) sel_add

 ) sel_by_month

 Group by $P!{ffio} ,
          $P!{ftabnumber} ,
          materialname ,
          materialcode ,
          $P!{fdepname}
) sel_sum

 where  monthsum > 0

 Order by fio ,  materialname]]>
