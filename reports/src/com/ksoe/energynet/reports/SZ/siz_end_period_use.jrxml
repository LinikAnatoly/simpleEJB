        add_months(periodend,(countmonth+countmonth+countmonth+countmonth+countmonth+countmonth+countmonth+countmonth)::integer) as add_months8,
        add_months(periodend,(countmonth+countmonth+countmonth+countmonth+countmonth+countmonth+countmonth+countmonth+countmonth)::integer) as add_months9,
