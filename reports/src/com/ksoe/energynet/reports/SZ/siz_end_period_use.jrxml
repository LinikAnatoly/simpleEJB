        add_months(periodend,(countmonth+countmonth)::integer) as add_months2,
        add_months(periodend,(countmonth+countmonth+countmonth)::integer) as add_months3,
        add_months(periodend,(countmonth+countmonth+countmonth+countmonth)::integer) as add_months4,
        add_months(periodend,(countmonth+countmonth+countmonth+countmonth+countmonth)::integer) as add_months5,
        add_months(periodend,(countmonth+countmonth+countmonth+countmonth+countmonth+countmonth)::integer) as add_months6,
