	</parameter>
	<parameter name="renCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
  positionName,
  razryad,
  sum((norm_time / wcount) * wcount_position * countgen) as sumTime

from

(
select
k.id,

   (select sum(dth1.kolvo)
    from tehkart.datatehkart dth1
    where dth1.id_sprav = 2 and dth1.id_teh_kart = k.id ) as wcount,

dth.kolvo  as wcount_position,
/*k.num,
k.opisanie,*/

 kd.name_dolz as positionName , kd.razryad,
k.norm_time,
pi.countgen


 from
 enplanwork p, enplanworkitem pi, endepartment d,
 tehkart.karti k left join tehkart.datatehkart dth
    on dth.id_teh_kart = k.id and dth.id_sprav = 2
left join tehkart.karta_dolz kd on dth.id_str_sprav = kd.id

where
p.code = pi.planrefcode and k.id = pi.kartarefcode
and d.code = p.departmentrefcode
and p.yeargen = $P{yearGen}
and p.monthgen = $P{monthGen}
and d.code = $P{renCode}
and p.statuscode not in (2,6)
--group by
-- kd.name_dolz, kd.razryad
--order by k.opisanie, kd.name_dolz, kd.razryad
) as q
group by
  positionName , razryad
order by
  positionName, razryad]]>
	</queryString>
