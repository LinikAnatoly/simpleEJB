	<parameter name="planworkcode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
mat.code as matcode, mat.name as matname, mes.name as measurement,
sum(ei.countfact) as sumfact
from enestimateitem ei
inner join tkmaterials mat on ei.materialrefcode = mat.code
inner join tkmeasurement mes on mat.measurementcode = mes.code
where ei.countfact>0 and ei.planrefcode = $P{planworkcode}
and ei.statusrefcode in (1, 2, 3)
group by mat.code, mat.name, mes.name
order by mat.name, mat.code]]>
	</queryString>
