	</parameter>
	<queryString>
		<![CDATA[select
 /* SUPP-76878 значение загальновиробнич берем из справочника */
 (select net.get_standard_const_entry_on_date(5/*загальновиробн.*/, selda.datestart) ) as percent,
 /*case when selda.datestart > '01.02.2017' and isafterdate70 = 1 then 50 else 70 end as percent,*/
	numbergen,
	contractnumber,
	contractdate,
	dategen,
	selda.datestart,
	isafterdate70,
	selda.datefinal,
	2 as planworktype,
	7 as planworkstate,
	22 as etype,
        year,
        mm,
        selperiod.departmentname,
		(select
        	 distinct rencity
        	from
        		epren ep,
            		endepartment dep,
            		endepartment dep1,
            		endepartment2epren deep
 		where
        		ep.code = deep.renrefcode
            		and deep.departmentrefcode = dep1.code
            		and dep1.code = dep.rencode
      			and dep.code = depcode)  as rencity

from
	(
	select
		ai.contractnumber,
		ai.contractdate,
		ai.numbergen,
            	ai.dategen,
		ai.actdatestart as datestart
/* если дата акта больше чем 01,05,2012 значит признак делаем для того что бы можно было накидывать 70 процентов общепроизв*/
, case when  ai.actdatestart >= to_date('01.05.2012','dd.mm.yyyy')  then 1 else 0 end  as isafterdate70

		,ai.actdateend as datefinal,
            	to_char(ai.dategen,'yyyy') as year,
            	case
            		when to_char(ena.dategen,'mm') = '01' then 'Січень'
              		when to_char(ena.dategen,'mm') = '02' then 'Лютий'
              		when to_char(ena.dategen,'mm') = '03' then 'Березень'
              		when to_char(ena.dategen,'mm') = '04' then 'Квітень'
              		when to_char(ena.dategen,'mm') = '05' then 'Травень'
              		when to_char(ena.dategen,'mm') = '06' then 'Червень'
              		when to_char(ena.dategen,'mm') = '07' then 'Липень'
              		when to_char(ena.dategen,'mm') = '08' then 'Серпень'
              		when to_char(ena.dategen,'mm') = '09' then 'Вересень'
              		when to_char(ena.dategen,'mm') = '10' then 'Жовтень'
			when to_char(ena.dategen,'mm') = '11' then 'Листопад'
              		when to_char(ena.dategen,'mm') = '12' then 'Грудень'
            	end  as mm
      	from
        	enact ena,
            	enactincome as ai,
            	enactincome2enact as aiac,
            	enelement ene,
            	enelementdata enda
	where
        	ena.elementcode = ene.code
  		and enda.ecode = ene.code
  		and ena.code = aiac.actrefcode
            	and aiac.actincomerefcode = ai.code
            	and ai.code = $P{PcodeAkt}
    ) selda,

   	(
    	select
        	min(enworkorder.dategen ) as datestart,
                max(enworkorder.dategen ) as datefinal,
                max(endepartment.code) as depcode,
                max(endepartment.shortname) as departmentname
        from
        	enworkorder,
                enworkorder2enplanwork,
                enplanwork,
                endepartment
        where
        		enworkorder.code = enworkorder2enplanwork.workordercode
                and enplanwork.code = enworkorder2enplanwork.plancode
				and endepartment.code = enplanwork.departmentrefcode
                and enplanwork.code in
                     	(Select
                        	enp.code
                      	from
                        	enact ena,
                            enactincome as ai,
                            enactincome2enact as aiac,
                            enact2enplanwork ena2,
                            enplanwork enp
                      	where
                        	ai.code = $P{PcodeAkt}
                            and ena.code = aiac.actrefcode
                            and aiac.actincomerefcode = ai.code
                      		and ena.code = ena2.actrefcode
                      		and ena2.plancode = enp.code)
    ) selperiod
limit 1]]>
	</queryString>
