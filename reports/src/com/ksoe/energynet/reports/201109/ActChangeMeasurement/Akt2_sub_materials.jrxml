	<parameter name="FKOrderCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
mat_code,
next_mat_id,
materialname,
before_nn,
before_mu,
sum(before_qty) as before_qty,
before_price,
sum(before_cost) as before_cost,
coeff,
sum(after_qty) as  after_qty,
sum(after_cost) as after_cost,
after_price,
after_mu,
bal_sch
from
(select
mat_code,
next_mat_id,
materialname,
before_nn,
before_mu,
before_qty,
before_price,
before_cost,
coeff,
before_qty*coeff as after_qty,
before_cost as after_cost,
round(cast((before_cost/ (before_qty*coeff)) as DECIMAL),3) as after_price,
(select
	me.name
	from
	tkmeasurement as me
	inner join
	tkmaterials as ma
	on
	ma.measurementcode = me.code
	where
	ma.code = mat_code) as after_mu,
bal_sch
from
(select
	mat_code,
	next_mat_id,
	materialname,
	before_nn,
	before_mu,
	before_qty,
	before_price,
	before_cost,
	coeff,
    bal_sch
from
	(select
		ma.code as mat_code,
		ma.name as materialname,
		fi.next_mat_id as next_mat_id,
		fma.nn as before_nn,
		trim(fma.mu_name) as before_mu,
		fma.quantity as before_qty,
		fma.price as before_price,
		fma.cost as before_cost,
        fma.bal_sch as bal_sch,
		(select
			alme.coeff
		from
			tkmaterials as ma1
			inner join
			(tkaltermeasurement as alme
			inner join
			tkmeasurement as me
			on
			alme.measurementcode = me.code)
			on
			alme.materialrefcode = ma.code
			where
			ma1.code = ma.code
			and upper(trim(me.name)) = upper(trim(fma.mu_name))) as coeff
	from
		rqfkorderitem as fi
		inner join
		((rqfkorderitem2enstmttm as oies
		inner join
		finmaterials as fma
		on
		fma.code = oies.finmaterialsrefcode)
		inner join
		(enestimateitem as es
		inner join
		tkmaterials as ma
		on
		es.materialrefcode = ma.code)
		on
		es.code = oies.estimateitemcode)
		on
		fi.code = oies.fkorderitemrefcode
	where
		fi.fkorderrefcode = $P{FKOrderCode}) as order_mat) as order_mat_with_coeff) as order_mat_with_coeff_not_grouped
group by
mat_code,
next_mat_id,
materialname,
before_nn,
before_mu,
before_price,
coeff,
after_price,
after_mu,
bal_sch]]>
	</queryString>
