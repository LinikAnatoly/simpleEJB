	<queryString>
		<![CDATA[/*Запрос материалы  */
Select
   mat_name ,mu_name , sum(countgen) as countgen  ,
                       cost ,
                       sum(stoimost) as stoimost ,
                       bal_sch  , nn , kindrefcode
From (
Select
         Case When estimate.kindrefcode = 2 Then finmat.mat_name||('(ПММ)')
               Else finmat.mat_name end as mat_name
        , finmat.mu_name
        , finmat.quantity  as countgen
        , finmat.calc_price  as cost
        , coalesce(finmat.cost,0) as stoimost
        , finmat.bal_sch
        , finmat.nn
        , estimate.kindrefcode
  From
        enestimateitem estimate
       , finmaterials finmat
       , enact2enplanwork ena2
where
ena2.actrefcode = $P{PcodeAkt}
  and ena2.plancode = estimate.planrefcode
  and finmat.estimateitemrefcode = estimate.code
  and finmat.statusrefcode = 1
  and estimate.kindrefcode in (1,2)
  and finmat.cost<> 0

 /*материалы до доработки - не попадают в акт, будут показываться в
стоимости доработанного материала*/
  and estimate.code not in
    	(
	select
		es_beforerefined.code
	from
		enestimateitem as es_beforerefined inner join enestimateitem as es_refined
		on (es_beforerefined.planrefcode = es_refined.planrefcode
		and es_beforerefined.materialrefcode = es_refined.materialrefcode
    		and (es_beforerefined.planitemrefcode = es_refined.planitemrefcode
    		or es_refined.planitemrefcode is null))
	where es_refined.kindrefcode = 4
    		and es_beforerefined.kindrefcode = 1
		and es_beforerefined.planrefcode in
			(select acpw.plancode from enact2enplanwork as  acpw where acpw.actrefcode = $P{PcodeAkt})
	)

) sel1
 group by sel1.mat_name ,   sel1.mu_name ,
               sel1.bal_sch , sel1.nn,  sel1.cost  ,kindrefcode
  order by  nn , bal_sch , mat_name , mu_name , cost]]>
	</queryString>
