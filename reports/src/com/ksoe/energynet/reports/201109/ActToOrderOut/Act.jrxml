	</parameter>
	<queryString>
		<![CDATA[select
  txtgen,
  numberdoc,
  max(findoccode) as findoccode,
  molincode, molinname,
  moloutcode, moloutname,
  dategen, dateedit
   , commentgen
   , coalesce((select group_concat(fo1.numberdoc||' від '||to_char(fo1.dategen, 'dd.mm.yyyy'), ', ')
	     from rqfkorder2fkorder as fofo1
               inner join rqfkorder as fo1 on fofo1.fkorderoutrefcode = fo1.code
	     where fofo1.typerefcode = 4
                 and fofo1.fkorderinrefcode = sel1.code), '') as acts_services
   , orderinfo, isbyorder, orgname, actstring
 FROM (

SELECT
o.code,
ok.txtgen,
o.numberdoc,
f2f.findoccode,
o.molincode, o.molinname,
o.moloutcode, o.moloutname,
o.dategen, o.dateedit
, o.commentgen
, (case when coalesce(o.isbyorder, 0) = 1 then coalesce(o.orderinfo, '') else '' end) as orderinfo
, coalesce(o.isbyorder, 0) as isbyorder

, coalesce((select org.name
     from rqfkorder2fkorder oo, rqfkorder o1, rqorg org
    where oo.fkorderinrefcode = o.code and oo.fkorderoutrefcode = o1.code
      and oo.typerefcode = 4
      and o1.orgcode = org.code limit 1), '') as orgname

, coalesce((select o1.numberdoc || ' від ' || to_char(o1.dategen, 'dd.mm.yyyy')
     from rqfkorder2fkorder oo, rqfkorder o1
    where oo.fkorderinrefcode = o.code and oo.fkorderoutrefcode = o1.code
      and oo.typerefcode = 4 limit 1), '') as actstring
from
rqfkorder o left join rqfkorderkind ok on (o.kindcode = ok.code)
left join findoc2fkorder f2f on (f2f.fkorderrefcode = o.code)
where
o.code = $P{orderCode}
) sel1
group by
code,
txtgen,
numberdoc,
molincode, molinname,
moloutcode, moloutname,
dategen, dateedit, commentgen, orderinfo, isbyorder, orgname, actstring]]>
