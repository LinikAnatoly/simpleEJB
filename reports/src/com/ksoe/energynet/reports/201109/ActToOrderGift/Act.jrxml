	<queryString>
		<![CDATA[select
  txtgen,
  numberdoc,
  max(findoccode) as findoccode,
  molincode, molinname,
  moloutcode, moloutname,
  dategen, dateedit
   , commentgen



 FROM (

SELECT
ok.txtgen,
o.numberdoc,
f2f.findoccode,
o.molincode, o.molinname,
o.moloutcode, o.moloutname,
o.dategen, o.dateedit
, o.commentgen
from
rqfkorder o,
 rqfkorderkind ok
, findoc2fkorder f2f
where
o.code = $P{orderCode}
and o.kindcode = ok.code
and f2f.fkorderrefcode = o.code
and (case when o.statuscode = 3 then
       f2f.findoctyperefcode in (7)
     else
       f2f.findoctyperefcode in (11)
     end)
) sel1
group by
txtgen,
numberdoc,
molincode, molinname,
moloutcode, moloutname,
dategen, dateedit , commentgen]]>
