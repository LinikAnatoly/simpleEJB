	</parameter>
	<parameter name="calcCostCode" class="java.lang.Integer">
		<parameterDescription><![CDATA[код расчета для калькуляции]]></parameterDescription>
		<defaultValueExpression><![CDATA[Integer.MIN_VALUE]]></defaultValueExpression>
	</parameter>
	<parameter name="servicesCostCode" class="java.lang.Integer">
		<parameterDescription><![CDATA[код объекта ENServicesCost]]></parameterDescription>
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select cmu.code, cmu.classificationtypenmbr as clasificator,
cmu.materialname as matname,
cmu.measureunitname as measname,
cmu.pricegen as costnkre,
cmu.countgen as kolvo,
cmu.sumgen as sum
from encalcmaterialsusage cmu
where cmu.planrefcode = $P{plancode}/*500147773*/
and $P{reportType} in (1,4)

union all

select cm.code, ct.kod as clasificator
, cm.materialname as matname
, cm.measureunitname as measname
, cm.pricegen as costnkre
, cm.countgen as kolvo
, cm.sumgen as sum
from tkcalccost as cc
inner join tkclassificationtype as ct on cc.classificationtyperfcd = ct.code
inner join tkcalcmaterials as cm on cc.code = cm.calccostrefcode
where cc.code = $P{calcCostCode}
and $P{reportType} = 2

union all

select sm.code, ct.kod as clasificator
, sm.materialname as matname
, sm.measureunitname as measname
, sm.pricegen as costnkre
, sm.countgen as kolvo
, sm.sumgen as sum  from
enservicesmaterials as sm
left join tkcalcmaterials as cm on sm.calcmaterialsrefcode = cm.code
inner join enservicescost as sc on sm.servicescostrefcode = sc.code
inner join tkcalccost as cc on sc.tkcalccostrefcode = cc.code
inner join tkclassificationtype as ct on cc.classificationtyperfcd = ct.code
where
sc.code = $P{servicesCostCode}
and $P{reportType} = 3
order by code]]>
	</queryString>
