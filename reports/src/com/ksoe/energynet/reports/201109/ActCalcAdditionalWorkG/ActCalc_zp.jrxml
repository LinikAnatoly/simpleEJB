	<parameter name="reportType" class="java.lang.Integer">
		<parameterDescription><![CDATA[Тип отчет:
1 - отчет формируется по данным из планов сметы (по коду договора);
2 - отчет формируется по данным из расчетов по калькуляциям;
3 - отчет формируется по данным из расчетов по калькуляциям из договоров;
4 - отчет формируется по данным из планов сметы (по коду плана).]]></parameterDescription>
	</parameter>
	<parameter name="calcCostCode" class="java.lang.Integer">
		<parameterDescription><![CDATA[код расчета для калькуляции]]></parameterDescription>
		<defaultValueExpression><![CDATA[Integer.MIN_VALUE]]></defaultValueExpression>
	</parameter>
	<parameter name="servicesCostCode" class="java.lang.Integer">
		<parameterDescription><![CDATA[код объекта ENServicesCost]]></parameterDescription>
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
chs.code
, chs.classificationtypenmbr as vidwork
, chs.positionname as name
, chs.timegenmonth as NORMHOURSMONTH
, chs.timegen as countmh
, chs.salarymonth as SALARYWORKER
, chs.pricehour as tariff1hour
, chs.salaryhour as basiczp
, chs.salarybonus as prem
, chs.salarytotalbonus as dopolntitzp
, chs.salarytotal as totalzp
, chtp.calckindrefcode as calc_kind
, coalesce(chs.salarysurcharge,0) + coalesce(chs.salarypremium, 0) + coalesce(chs.salaryadditional, 0) as salary_total_surcharges
from encalchumensalary chs
inner join enplanwork2classfctntp as chtp on chtp.code = chs.plan2ctyperefcode
inner join tkclassificationtype as tp on tp.code = chtp.classificationtyperfcd
where chs.planrefcode = $P{plancode}
and tp.isnotlicensedactivity != 3
and $P{reportType} in (1,4)


union all

select ch.code
, ct.kod as vidwork
, ch.positionname as name
, ch.timegenmonth as NORMHOURSMONTH
, ch.timegen as countmh
, ch.salarymonth as SALARYWORKER
, ch.salaryhour as tariff1hour
, ch.salary as basiczp
, ch.salarybonus as prem
, ch.salarytotalbonus as dopolntitzp
, ch.salarytotal as totalzp
, cc.calckindrefcode as calc_kind
, coalesce(ch.salarysurcharge,0) + coalesce(ch.salarypremium, 0) + coalesce(ch.salaryadditional, 0) as salary_total_surcharges
from tkcalchumensalary as ch
inner join tkcalccost as cc on ch.calccostrefcode = cc.code
inner join tkclassificationtype as ct on cc.classificationtyperfcd = ct.code
where
cc.code = $P{calcCostCode}
and ct.isnotlicensedactivity != 3
and $P{reportType} = 2

union all

select ch.code
, ct.kod as vidwork
, ch.positionname as name
, ch.timegenmonth as NORMHOURSMONTH
, sh.timegen as countmh
, ch.salarymonth as SALARYWORKER
, ch.salaryhour as tariff1hour
, sh.salary as basiczp
, sh.salarybonus as prem
, sh.salarytotalbonus as dopolntitzp
, sh.salarytotal as totalzp
, cc.calckindrefcode as calc_kind
, coalesce(sh.salarysurcharge,0) + coalesce(sh.salarypremium, 0) + coalesce(sh.salaryadditional, 0) as salary_total_surcharges from
enserviceshumensalary as sh
inner join enservicescost as sc on sh.servicescostrefcode = sc.code
inner join tkcalccost as cc on sc.tkcalccostrefcode = cc.code
inner join tkclassificationtype as ct on cc.classificationtyperfcd = ct.code
inner join tkcalchumensalary as ch on sh.calchumensalaryrefcode = ch.code
where
sc.code = $P{servicesCostCode}
and ct.isnotlicensedactivity != 3
and $P{reportType} = 3

order by code]]>
	</queryString>
