	<queryString>
		<![CDATA[select ctu.classificationtypenmbr as clasificator,
ctu.transportname as transportname,
ctu.normmachinehours as countworkfact,
ctu.normdistance as distance,
ctu.pricemachinehours as tariff1mh,
ctu.pricedistance as tariffgrn1km,
ctu.costmachinehours as payservice,
ctu.costdistance as payrun,
ctu.costtotal as paysum ,
ctu.commentpricedistance
from encalctransportusage ctu
where ctu.planrefcode = $P{plancode} /*500147773*/
and $P{reportType} = 1

union all

select
cls.kod as clasificator
, ct.transportname
, 1 as normmachinehours
, 100 as normdistance
, ct.costmachinehours as tariff1mh
, ct.costperkilometer as tariffgrn1km
, ct.costmachinehours as payservice
, ct.costdistance as payrun
, ct.costtotal as paysum
, ct.commentperkilometer
from
tkcalccost as cc
inner join tkcalctransport as ct on cc.code = ct.calccostrefcode
inner join tkclassificationtype as cls on cc.classificationtyperfcd = cls.code
where
(cc.code = $P{calcCostCode}
and $P{reportType} = 2)

union all

select
cls.kod as clasificator
, ct.transportname
, st.machinehourscount as normmachinehours
, st.distance as normdistance
, ct.costmachinehours as tariff1mh
, ct.costperkilometer as tariffgrn1km
, st.costmachinehours as payservice
, st.costdistance as payrun
, st.costtotal as paysum
, ct.commentperkilometer
from
tkcalccost as cc
inner join tkcalctransport as ct on cc.code = ct.calccostrefcode
inner join tkclassificationtype as cls on cc.classificationtyperfcd = cls.code
inner join enservicescost as sc on sc.tkcalccostrefcode = cc.code
inner join enservicestransport as st on (st.servicescostrefcode = sc.code and st.calctransportrefcode = ct.code)
where
sc.code = $P{servicesCostCode}
and $P{reportType} = 3]]>
	</queryString>
