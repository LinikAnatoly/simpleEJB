		<variableExpression><![CDATA[$F{razomwithpdv}]]></variableExpression>
	</variable>
	<variable name="isGiveCounterToBalance" class="java.lang.Boolean">
		<variableExpression><![CDATA[$V{isGiveCounterToBalance}]]></variableExpression>
		<initialValueExpression><![CDATA[$F{isgivecounter} == 2]]></initialValueExpression>
	</variable>
	<variable name="invNumberCounterToBalance" class="java.lang.String">
		<variableExpression><![CDATA[$V{invNumberCounterToBalance}]]></variableExpression>
		<initialValueExpression><![CDATA[( $V{isGiveCounterToBalance} ? ((com.ksoe.energynet.reports.common.FINScriptlet)$P{REPORT_SCRIPTLET}).getInvNumberBySerialNumber($F{counter_serialnumber}, $F{servicesobject_code})
    : null )]]></initialValueExpression>
	</variable>
	<variable name="sumCostWithoutVAT" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{razom}]]></variableExpression>
		<initialValueExpression><![CDATA[new BigDecimal(0)]]></initialValueExpression>
	</variable>
	<variable name="sumCostVAT" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{pdv}]]></variableExpression>
		<initialValueExpression><![CDATA[new BigDecimal(0)]]></initialValueExpression>
	</variable>
	<variable name="sumCostWithVAT" class="java.math.BigDecimal" calculation="Sum">
		<variableExpression><![CDATA[$F{razomwithpdv}]]></variableExpression>
		<initialValueExpression><![CDATA[new BigDecimal(0)]]></initialValueExpression>
	</variable>
	<variable name="sumAdditionalItemsWithoutVAT" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{calc_additional_sum}]]></variableExpression>
		<initialValueExpression><![CDATA[new BigDecimal(0).setScale(2, BigDecimal.ROUND_HALF_UP)]]></initialValueExpression>
	</variable>
	<variable name="sumAdditionalItemsVAT" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{sumAdditionalItemsWithoutVAT}.multiply($F{vat_percent}.divide(new BigDecimal(100), 2, BigDecimal.ROUND_HALF_UP))]]></variableExpression>
		<initialValueExpression><![CDATA[new BigDecimal(0)]]></initialValueExpression>
	</variable>
	<variable name="sumAdditionalItemsWithVAT" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$V{sumAdditionalItemsWithoutVAT}.add($V{sumAdditionalItemsVAT})]]></variableExpression>
		<initialValueExpression><![CDATA[new BigDecimal(0)]]></initialValueExpression>
	</variable>
	<variable name="isContragentPerson" class="java.lang.Boolean">
		<variableExpression><![CDATA[$V{isContragentPerson}]]></variableExpression>
		<initialValueExpression><![CDATA[($F{contragenttyperefcode} != null
    && $F{contragenttyperefcode}.doubleValue() == 1)
        || $P{reportType} != 3]]></initialValueExpression>
	</variable>
