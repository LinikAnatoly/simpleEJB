	<parameter name="soCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="calcCostCode" class="java.lang.Integer">
		<parameterDescription><![CDATA[код расчета для калькуляции]]></parameterDescription>
		<defaultValueExpression><![CDATA[Integer.MIN_VALUE]]></defaultValueExpression>
	</parameter>
	<parameter name="reportType" class="java.lang.Integer">
		<parameterDescription><![CDATA[Тип отчет:
1 - отчет формируется по данным из планов сметы (по коду договора);
2 - отчет формируется по данным из расчетов по калькуляциям;
3 - отчет формируется по данным из расчетов по калькуляциям из договоров;
4 - отчет формируется по данным из планов сметы (по коду плана).]]></parameterDescription>
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="servicesCostCodes" class="java.lang.String">
		<parameterDescription><![CDATA[коды объектов ENServicesCost перечисленные через запятую]]></parameterDescription>
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<parameter name="plancode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[Integer.MIN_VALUE]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
calc.clasificator,
calc_single.calc_name,
calc.kalkulcost,
calc.materialvitrati,
calc.avtovitrati,
calc.inwayvitrati,
calc.razom,
calc.pdv,
calc.razomwithpdv,
calc.costwithoutvat +
coalesce((select sum(c.summa)::numeric(15,2) as sumcalc
from enservices2clcddtnltms c, encalcadditionalitemtp ct
where  c.calcadditionaltmtprfcd = ct.code
 and c.servicesobjectrefcode  = calc.servicesobject_code),0) as costwithoutvat,
calc.costvat +
coalesce((select sum(c.summa)*0.2::numeric(15,2) as sumcalc
from enservices2clcddtnltms c, encalcadditionalitemtp ct
where  c.calcadditionaltmtprfcd = ct.code
 and c.servicesobjectrefcode  = calc.servicesobject_code),0) as costvat,

calc.totalcost +
coalesce((select sum(c.summa)*1.2::numeric(15,2) as sumcalc
from enservices2clcddtnltms c, encalcadditionalitemtp ct
where  c.calcadditionaltmtprfcd = ct.code
 and c.servicesobjectrefcode  = calc.servicesobject_code),0) as totalcost,

(select sum(c.summa) as sumcalc
from enservices2clcddtnltms c, encalcadditionalitemtp ct
where  c.calcadditionaltmtprfcd = ct.code
 and c.servicesobjectrefcode  = calc.servicesobject_code) as calc_additional_sum,

calc.contragentname ,
calc.address ,
calc.depname ,
calc.warrantposition ,
calc.warrantshortfio  ,
calc.contragentaddress ,
calc.contragentokpo ,
calc.contragentbossname ,
calc.contragenttyperefcode ,
calc.calctyperefcode,
calc.isgivecounter,
calc.counter_type,
calc.counter_cost,
calc.counter_serialnumber,
calc.servicesobject_code,
countgen ,
calc_single.clasificator as clasificator_single,
calc_single.kalkulcost as kalkulcost_single,
calc_single.materialvitrati as materialvitrati_single,
calc_single.avtovitrati as avtovitrati_single,
calc_single.inwayvitrati as inwayvitrati_single,
calc_single.razom as razom_single,
calc_single.pdv as pdv_single,
calc_single.razomwithpdv as razomwithpdv_single,
calc_single.costvat as costvat_single,
calc_single.totalcost as totalcost_single,
calc.contracttyperefcode,
calc.calc_kind,

(select group_concat(ct.name  ,' ; ') as name
from
 enplanwork2classfctntp pw2ct, tkclassificationtype ct, enplanwork pw, enservicesobject so
  where pw2ct.planrefcode = pw.code
  and pw.elementrefcode = so.elementcode
  and pw.kindcode = 5
  and so.code =  calc.servicesobject_code
    and pw2ct.classificationtyperfcd = ct.code) as name,
 calc.contractdate,
 get_standard_const_entry_on_date(1, calc.contractdate) as vat_percent
, '' as measurement
, calc.contragentposition
from (
select ct.kod as clasificator,
ctc.calculationcost as kalkulcost,
ctc.materialexpenses as materialvitrati,
ctc.transportexpenses as avtovitrati,
ctc.deliverycost as inwayvitrati,
ctc.costwithoutvat as razom,
ctc.costvat as pdv,
ctc.totalcost as razomwithpdv,
cct.costvat, cct.costwithoutvat, cct.totalcost ,
so.contragentname ,
so.code as servicesobject_code,
wa.address,
dep.name as depname ,
wa.warrantposition ,
wa.warrantshortfio  ,
so.contragentaddress ,
so.contragentokpo /*окпо или Инд код*/ ,
so.contragentbossname ,
so.contragenttyperefcode /*1- фіз иначе не физ*/   ,
so.calctyperefcode,
pw2ct.countgen,
coalesce(ct.isgivecounter,0) as isgivecounter
, coalesce(gi.countertype,'') as counter_type
, coalesce(gi.cost, 0) as counter_cost
, coalesce(gi.serialnumber, '') as counter_serialnumber
, so.contracttyperefcode
, pw2ct.calckindrefcode as calc_kind
, so.contractdate
, coalesce(so.contragentposition, '') as contragentposition
 from encalctotalcost ctc inner join enplanwork2classfctntp pw2ct on ctc.plan2ctyperefcode = pw2ct.code
 inner join tkclassificationtype ct on pw2ct.classificationtyperfcd = ct.code
 inner join  encalccontracttotal cct on cct.planrefcode = ctc.planrefcode
 inner join enplanwork as pw on (pw.code = pw2ct.planrefcode and  pw.kindcode = 5)
 inner join enservicesobject as so on pw.elementrefcode = so.elementcode
 left join engivecounter as gi on so.code = gi.servicesobjectrefcode and gi.plan2cltyperefcode = pw2ct.code
 left join enwarrant as wa ON so.warrantrefcode = wa.code
 left join endepartment dep ON wa.departmentrefcode = dep.code
where ((so.code = $P{soCode} and $P{reportType} = 1) or (pw.code = $P{plancode} and $P{reportType} = 4))
) as calc

full join
(select ct.kod as clasificator,
ct.name as calc_name,
ctc.calculationcost as kalkulcost,
ctc.materialexpenses as materialvitrati,
ctc.transportexpenses as avtovitrati,
ctc.deliverycost as inwayvitrati,
ctc.costwithoutvat as razom,
ctc.costvat as pdv,
ctc.totalcost as razomwithpdv,
cct.costvat, cct.totalcost
 from enplanwork as pw_calc
inner join enservicesobject as so on pw_calc.elementrefcode = so.elementcode
inner join enplanwork2classfctntp as pwct_calc on pw_calc.code = pwct_calc.planrefcode
inner join tkclassificationtype as ct on pwct_calc.classificationtyperfcd = ct.code
inner join enplanwork as pw on so.elementcode = pw.elementrefcode
inner join enplanwork2classfctntp as pwct on (pw.code = pwct.planrefcode and ct.code = pwct.classificationtyperfcd)
inner join encalctotalcost as ctc on pwct.code = ctc.plan2ctyperefcode
inner join  encalccontracttotal as cct on ctc.planrefcode = cct.planrefcode

where ((so.code = $P{soCode} and $P{reportType} = 1) or (pw_calc.code = $P{plancode} and $P{reportType} = 4))
and pw.kindcode = 6
and pw_calc.kindcode = 5) calc_single on calc.clasificator = calc_single.clasificator

union all

select  ct.kod as clasificator
, ct.name as calc_name
, cc.calculationcost as kalkulcost
, cc.materialexpenses as materialvitrati
, cc.transportexpenses as avtovitrati
, cc.deliverycost as inwayvitrati
, cc.costwithoutvat as razom
, cc.costvat as pdv
, cc.costwithvat as razomwithpdv
, cc.costwithoutvat as costwithoutvat
, cc.costvat as costvat
, cc.costwithvat as totalcost
, 0 as calc_additional_sum
, '' as contragentname
, '' as address
, '' as depname
, '' as warrantposition
, '' as warrantshortfio
, '' as contragentaddress
, '' as contragentokpo
, '' as contragentbossname
, -1 as contragenttyperefcode
, 1 as calctyperefcode
, coalesce(ct.isgivecounter,0) as isgivecounter
, '' as counter_type
, 0 as counter_cost
, '' as counter_serialnumber
, -1 as servicesobject_code
, 1 as countgen
, ct.kod as clasificator_single
, cc.calculationcost as kalkulcost_single
, cc.materialexpenses as materialvitrati_single
, cc.transportexpenses as avtovitrati_single
, cc.deliverycost as inwayvitrati_single
, cc.costwithoutvat as razom_single
, cc.costvat as pdv_single
, cc.costwithvat as razomwithpdv_single
, cc.costvat as costvat_single
, cc.costwithvat as totalcost_single
, -1 as contracttyperefcode
, cc.calckindrefcode as calc_kind
, ct.name as name
, cc.dategen as contractdate
, get_standard_const_entry_on_date(1, cc.dategen) as vat_percent
, (select
	string_agg(distinct coalesce(me1.name,''), ',')
   from
	tktechcard as tc1
	left join tkmeasurement as me1 on tc1.measurementcode = me1.code
   where
	tc1.classificationtypecode = ct.code) as measurement
, '' as contragentposition
from tkclassificationtype as ct
inner join tkcalccost as cc on ct.code = cc.classificationtyperfcd
where
2 = $P{reportType}
and cc.code = $P{calcCostCode}

union all

select  ct.kod as clasificator
, ct.name as calc_name
, sc.calculationcost as kalkulcost
, sc.materialexpenses as materialvitrati
, sc.transportexpenses as avtovitrati
, sc.deliverycost as inwayvitrati
, sc.costwithoutvat as razom
, sc.costvat as pdv
, sc.costwithvat as razomwithpdv
, sc.costwithoutvat as costwithoutvat
, sc.costvat as costvat
, sc.costwithvat as totalcost
, coalesce((select sum(c.summa)
from enservices2clcddtnltms c inner join encalcadditionalitemtp ct on c.calcadditionaltmtprfcd = ct.code
where c.servicesobjectrefcode  = so.code),0) as calc_additional_sum
, so.contragentname as contragentname
, wa.address as address
, dep.name as depname
, wa.warrantposition as warrantposition
, wa.warrantshortfio as warrantshortfio
, so.contragentaddress as contragentaddress
, so.contragentokpo as contragentokpo
, so.contragentbossname as contragentbossname
, so.contragenttyperefcode as contragenttyperefcode
, so.calctyperefcode as calctyperefcode
, coalesce(ct.isgivecounter,0) as isgivecounter
, '' as counter_type
, 0 as counter_cost
, '' as counter_serialnumber
, so.code as servicesobject_code
, sc.countgen as countgen
, ct.kod as clasificator_single
, cc.calculationcost as kalkulcost_single
, cc.materialexpenses as materialvitrati_single
, cc.transportexpenses as avtovitrati_single
, cc.deliverycost as inwayvitrati_single
, cc.costwithoutvat as razom_single
, cc.costvat as pdv_single
, cc.costwithvat as razomwithpdv_single
, cc.costvat as costvat_single
, cc.costwithvat as totalcost_single
, so.contracttyperefcode as contracttyperefcode
, cc.calckindrefcode as calc_kind
, (select group_concat(coalesce(ct1.name,''), ',')
  from enservicescost as sc1
inner join tkcalccost as cc1 on sc1.tkcalccostrefcode = cc1.code
inner join tkclassificationtype as ct1 on cc1.classificationtyperfcd = ct1.code
where sc1.code in ($P!{servicesCostCodes}) ) as name
, sc.dategen as contractdate
, get_standard_const_entry_on_date(1, cc.dategen) as vat_percent
, (select
	string_agg(distinct coalesce(me1.name,''), ',')
   from
	tktechcard as tc1
	left join tkmeasurement as me1 on tc1.measurementcode = me1.code
   where
	tc1.classificationtypecode = ct.code) as measurement
, coalesce(so.contragentposition, '') as contragentposition
from
enservicescost as sc
inner join enservicesobject as so on sc.servicesobjectrefcode = so.code
inner join endepartment as dep on so.departmentcode = dep.code
inner join tkcalccost as cc on sc.tkcalccostrefcode = cc.code
inner join tkclassificationtype as ct on cc.classificationtyperfcd = ct.code
left join enwarrant as wa on so.warrantrefcode = wa.code
where
3 = $P{reportType}
and sc.code in ($P!{servicesCostCodes})]]>
	</queryString>
