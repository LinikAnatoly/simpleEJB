	<parameter name="plancode" class="java.lang.Integer"/>
	<parameter name="reportType" class="java.lang.Integer">
		<parameterDescription><![CDATA[Тип отчет:
1 - отчет формируется по данным из планов сметы (по коду договора);
2 - отчет формируется по данным из расчетов по калькуляциям;
3 - отчет формируется по данным из расчетов по калькуляциям из договоров;
4 - отчет формируется по данным из планов сметы (по коду плана).]]></parameterDescription>
	</parameter>
	<parameter name="calcCostCode" class="java.lang.Integer">
		<parameterDescription><![CDATA[код расчета для калькуляции]]></parameterDescription>
		<defaultValueExpression><![CDATA[Integer.MIN_VALUE]]></defaultValueExpression>
	</parameter>
	<parameter name="servicesCostCode" class="java.lang.Integer">
		<parameterDescription><![CDATA[код объекта ENServicesCost]]></parameterDescription>
		<defaultValueExpression><![CDATA["-1"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
sub_query.*
, (sub_query.kalkulcost * (vat_percent/100)) as costvat
, (sub_query.kalkulcost * (1 + vat_percent/100)) as totalcost
 from
(select
cc.directexpenses
, cc.salaryexpenses as oplatatruda
, cc.socialcharges as vidrahsoc
, cc.productionexpenses as virobnvitrati
, cc.totalexpenses as vsogovitrat
, cc.normincome as normapributku
, cc.calculationcost as kalkulcost
, group_concat( ct.name  , ';') as clasificatorname
, ct.kod as clasificator
, ct.percentprofitrate::numeric(15,2) as percentprofitrate
, coalesce(pw2ct.productionexpenssprcnt, 60) as production_expenses_percent
, pw2ct.calckindrefcode as calc_kind
, get_standard_const_entry_on_date(1, coalesce(so.contractdateservices, so.contractdate)) as vat_percent
from
 encalccost as cc
inner join enplanwork2classfctntp as pw2ct on cc.plan2ctyperefcode  = pw2ct.code
inner join enplanwork as pw on pw2ct.planrefcode = pw.code
inner join enservicesobject as so on pw.elementrefcode = so.elementcode
inner join tkclassificationtype as ct on pw2ct.classificationtyperfcd = ct.code
where
cc.planrefcode = $P{plancode}
and $P{reportType} in (1,4)
group by
cc.directexpenses
, cc.salaryexpenses
, cc.socialcharges
, cc.productionexpenses
, cc.totalexpenses
, cc.normincome
, cc.calculationcost
, ct.kod
, ct.percentprofitrate::numeric(15,2)
, coalesce(pw2ct.productionexpenssprcnt, 60)
, pw2ct.calckindrefcode
, so.code

union all

select
cc.directexpenses
, cc.salaryexpenses as oplatatruda
, cc.socialcharges as vidrahsoc
, cc.productionexpenses as virobnvitrati
, cc.totalexpenses as vsogovitrat
, cc.normincome as normapributku
, cc.calculationcost as kalkulcost
, ct.name as clasificatorname
, ct.kod as clasificator
, ct.percentprofitrate::numeric(15,2) as percentprofitrate
,cc.productionexpenssprcnt as production_expenses_percent
,cc.calckindrefcode as calc_kind
, get_standard_const_entry_on_date(1, cc.dategen) as vat_percent
 from tkcalccost as cc
inner join tkclassificationtype as ct on cc.classificationtyperfcd = ct.code
where
cc.code = $P{calcCostCode}
and $P{reportType} = 2

union all

select
sc.directexpenses
, sc.salaryexpenses as oplatatruda
, sc.socialcharges as vidrahsoc
, sc.productionexpenses as virobnvitrati
, sc.totalexpenses as vsogovitrat
, sc.normincome as normapributku
, sc.calculationcost as kalkulcost
, ct.name as clasificatorname
, ct.kod as clasificator
, ct.percentprofitrate::numeric(15,2) as percentprofitrate
,cc.productionexpenssprcnt as production_expenses_percent
,cc.calckindrefcode as calc_kind
, get_standard_const_entry_on_date(1, cc.dategen) as vat_percent
 from enservicescost as sc
inner join tkcalccost as cc on sc.tkcalccostrefcode = cc.code
inner join tkclassificationtype as ct on cc.classificationtyperfcd = ct.code
where
sc.code = $P{servicesCostCode}
and $P{reportType} = 3) as sub_query]]>
	</queryString>
