<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="rqOrderOborot" pageWidth="4000" pageHeight="595" orientation="Landscape" whenNoDataType="BlankPage" columnWidth="3940" leftMargin="30" rightMargin="30" topMargin="20" bottomMargin="20" scriptletClass="com.ksoe.energynet.reports.common.FINScriptlet" isIgnorePagination="true">
	<property name="ireport.scriptlethandling" value="2"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="startDate" class="java.lang.String">
		<parameterDescription><![CDATA[startDate]]></parameterDescription>
		<defaultValueExpression><![CDATA[new String("01.12.2011")]]></defaultValueExpression>
	</parameter>
	<parameter name="endDate" class="java.lang.String">
		<parameterDescription><![CDATA[endDate]]></parameterDescription>
		<defaultValueExpression><![CDATA[new String("01.12.2011")]]></defaultValueExpression>
	</parameter>
	<parameter name="materialGroupCode" class="java.lang.String">
		<parameterDescription><![CDATA[materialGroupCode]]></parameterDescription>
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<parameter name="contractNumber" class="java.lang.String">
		<parameterDescription><![CDATA[contractNumber]]></parameterDescription>
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<parameter name="budgCode" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<parameter name="orderform" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<parameter name="zayakind" class="java.lang.String"/>
	<parameter name="ordertype" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<parameter name="orderCode" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("500003330")]]></defaultValueExpression>
	</parameter>
	<parameter name="renCode" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<parameter name="elementCode" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<parameter name="elementName" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="renName" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("")]]></defaultValueExpression>
	</parameter>
	<parameter name="viewmotion" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="IsComplete" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="planCode" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<parameter name="paccount" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<parameter name="porgcode" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<parameter name="orderitemstatus" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="ddscode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="orderstatus" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(2)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[Select
*
from
(
select
(select name from tkmaterials tm
where tm.code in
(select rootgrouprefcode
from tkmaterials
where code = sm.code)) as parent_material,
/*case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end  as mname,*/
 tk.techkartnumber ,
 coalesce(pi.commentgen,'Невизначено') as mname ,
 ei.name as meas,
 sm.numkatalog ,
 ord.numberdoc as order_number,
 to_char(ord.orderperiod,'MM.yyyy') as order_period,
 ord.countfact as order_countfact,
 ord.pricewithoutnds as order_pricewithoutnds,
 ord.sumwithoutnds as  sumwithoutnds,
 ord.contractnumber as contractnumber,
 to_char(ord.planneddatedelivery,'dd.mm.yyyy') as planneddatedelivery,
 ord.planneddatepays,
 ord.oicode as oicode,
 ord.budgetrefcode ,
 to_char(ord.orderdategen,'dd.MM.yyyy') as order_dategen,
 tp.name as typename, kn.name as kindname,

(select coalesce(count(distinct b2o.code),0)
from rqbillitem2orderitem b2o
where b2o.orderitemrefcode = ord.oicode) as bcount
 /*количество строк для движения */
, (
SELECT count(countfact) from (
SELECT  rqfkorderitem2enstmttm.countgen as countfact
from rqfkorderitem ii
      left join rqfkorderitem2enstmttm
         on ( rqfkorderitem2enstmttm.fkorderitemrefcode = ii.code
              and coalesce(rqfkorderitem2enstmttm.countgen,0) <> 0 )
, rqfkorder iord left join  findoc2fkorder ffkord on (iord.code = ffkord.fkorderrefcode)  , rqfkorderstatus

where iord.code = ii.fkorderrefcode
and iord.statuscode = rqfkorderstatus.code
and rqfkorderstatus.code in (3,4,5)  /*НЕ равно черновым сказано Салыгиным 23,11,2010*/
and iord.kindcode = 14
and rqfkorderitem2enstmttm.estimateitemcode in ( select estimateitemcode from rqorderitem2enestimttm rrr where
                                                                                 rrr.orderitemcode = ord.oicode
                                                                         )

 UNION

SELECT  sum(rqfkorderitemremainder.countgen) as countfact
       from rqfkorderitemremainder , rqfkorderitem , rqfkorder , rqfkorderstatus
    where rqfkorderitemremainder.estimateitemrefcode in
                                              ( select    estimateitemcode from rqorderitem2enestimttm rrr where
                                                         rrr.orderitemcode = ord.oicode
                                              )
      and rqfkorderitemremainder.typerefcode = 2
      and rqfkorderitemremainder.fkorderitemrefcode = rqfkorderitem.code
      and rqfkorderitem.fkorderrefcode = rqfkorder.code
      and rqfkorder.kindcode = 14
      and rqfkorder.statuscode in (3,4,5) /*НЕ равно черновым сказано Салыгиным 23,11,2010*/
      and rqfkorder.statuscode = rqfkorderstatus.code

      ) selprihordercount
) as countmotion
, ord.rqorgname
, ord.contractdate ,
  ord.ddscodetext,
 ( select rqorderstatus.name from rqorder , rqorderitem , rqorderstatus
   where rqorder.code = rqorderitem.orderrefcode
     and rqorderitem.code = ord.oicode
     and rqorderstatus.code = rqorder.statusrefcode limit 1  ) as rqorderstatus ,
    (
select
totalcode
from
(select
mainoi.code as TotalCode,
/*Кол-во по заявке*/
coalesce( mainoi.countfact,0) as OrderQty,
/* Кол-во по счету - естимейты */
coalesce((select
Qty
from
(select
Bills1.TotalCode as TotalCode,
Sum(Bills1.Qty) as Qty
from
(select distinct
oi.code as TotalCode,
bies.code as biescode,
coalesce(bies.countgen,0) as Qty
from
(enestimateitem as es
left join
(rqbillitem2enestimattm as bies
left join
rqbillitem as bi
on
bi.code = bies.billitemcode)
on
es.code = bies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi
on
oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) )
on
oies.estimateitemcode = es.code
where oi.code = mainoi.code) as Bills1
group by TotalCode) as Bills),0) as BIESQty,
/* Количество по приходу - естимейты без остатков с типом 1 */
coalesce((select
Qty
from
(select
FKOrders1.TotalCode,
sum(FKOrders1.Qty+FKOrders1.ostQty) as Qty
from
(select distinct
oi.code as TotalCode,
fies.code as ficode,
(coalesce(fies.countgen,0)) as Qty,
(case when fir.fkorderitemrefcode = fi.code then coalesce(fir.countgen,0) else 0 end) as ostQty
from
((enestimateitem as es
left join
rqfkorderitemremainder as fir
on
fir.estimateitemrefcode = es.code)
left join
(rqfkorderitem2enstmttm as fies
left join
(rqfkorderitem as fi
left join
rqfkorder as fo
on
fo.code = fi.fkorderrefcode and fo.statuscode in (3,4,5)  )
on
fi.code = fies.fkorderitemrefcode)
on
es.code = fies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi on oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0)) on oies.estimateitemcode = es.code
where oi.code = mainoi.code
and fo.statuscode <> 1
and fo.kindcode = 14) as FKOrders1
group by TotalCode) as FKOrders),0) as FIESQty,
/* Проверка есть ли на эту строку остаток типа 1 */
(select
TotalCode from
(select distinct
oi.code as TotalCode
from
(enestimateitem as es
left join
(rqfkorderitem2enstmttm as fies
left join
((rqfkorderitem as fi
left join
rqfkorderitemremainder as fir
on
fir.fkorderitemrefcode = fi.code)
left join
rqfkorder as fo
on
fo.code = fi.fkorderrefcode and fo.statuscode in (3,4,5) )
on
fi.code = fies.fkorderitemrefcode)
on
es.code = fies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi on oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) ) on oies.estimateitemcode = es.code
where oi.code = mainoi.code
and fo.statuscode <> 1
and fo.kindcode = 14
and fir.typerefcode = 1) as FKOrders1) as IsThereTypeOne,
/* Сумма строк прихода где есть тип 1 */
coalesce((select
sum(FIQty)
from
(select distinct
oi.code as TotalCode,
fi.countgen as FIQty
from
(enestimateitem as es
left join
(rqfkorderitem2enstmttm as fies
left join
((rqfkorderitem as fi
left join
rqfkorderitemremainder as fir
on
fir.fkorderitemrefcode = fi.code)
left join
rqfkorder as fo
on
fo.code = fi.fkorderrefcode and fo.statuscode in (3,4,5)  )
on
fi.code = fies.fkorderitemrefcode)
on
es.code = fies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi on oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) ) on oies.estimateitemcode = es.code
where oi.code = mainoi.code
and fo.statuscode <> 1
and fo.kindcode = 14) as FKOrders1
group by TotalCode),0) as FIQty,
/* Сумма строк по счету для сравнения с суммой строк прихода где есть остатки типа 1 */
coalesce((select
Qty
from
(select
Bills1.TotalCode as TotalCode,
Sum(Bills1.Qty) as Qty
from
(select distinct
oi.code as TotalCode,
bi.code as Bicode,
bi.countfact as Qty
from
(enestimateitem as es
left join
(rqbillitem2enestimattm as bies
left join
rqbillitem as bi
on
bi.code = bies.billitemcode)
on
es.code = bies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi
on
oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) )
on
oies.estimateitemcode = es.code
where oi.code = mainoi.code) as Bills1
group by TotalCode) as Bills),0) as BIQty
from
rqorderitem as mainoi
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
Where  (mainoi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) ) as IncompleteDelivery
where
IncompleteDelivery.Totalcode = oicode
and
case when $P{IsComplete} > 3  then BIESQty is not null else 0=0 end
and
case
when $P{IsComplete} = 0
then 0 = 0
when $P{IsComplete} = 1
then
   case
       when IsThereTypeOne is not null
       then round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 2
then
   case
       when IsThereTypeOne is not null
       then round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 3
then
   case
       when IsThereTypeOne is not null
       then round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
           or round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
           or round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 4
then
   case
       when IsThereTypeOne is not null
       then round(cast(BIQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(BIESQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 5
then
   case
       when IsThereTypeOne is not null
       then round(cast(BIQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(BIESQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 6
then
   case
       when IsThereTypeOne is not null
       then round(cast(BIQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
           or round(cast(BIQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(BIESQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
           or round(cast(BIESQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
end
) as priznak
,
/*анализ условия по счетам ( все 0 ,  невыставленные 1 , выставленые 2 , выставлен и неоплачен 3, выставлен и  оплачен 4 )  */
( select
case when $P!{paccount} = 0 then 1 /*показываем все строки невзирая на счета  */
     when $P!{paccount} = 1 then   /*показываем строки с невыставлеными счетами*/
       case when (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                   where rqb2ordit.orderitemrefcode =  ord.oicode
                   limit 1 ) > 0 then 0 else 1 end
     when $P!{paccount} = 2 then   /*показываем строки с выставлеными счетами*/
       case when (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                   where rqb2ordit.orderitemrefcode =  ord.oicode
                   limit 1 ) > 0 then 1 else 0 end
     when $P!{paccount} = 3 then   /*показываем строки с выставлеными счетами и неоплачеными*/
       case when  (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                    where rqb2ordit.orderitemrefcode =  ord.oicode
                    limit 1 ) > 0
               and ( select coalesce(sum(rqp2bilit.summafact),0) from rqpaydocitem2billitem rqp2bilit
  where rqp2bilit.billitemcode in (select rqb2ordit.billitemrefcode from rqbillitem2orderitem rqb2ordit
                                    where rqb2ordit.orderitemrefcode =  ord.oicode  )
                    ) = 0     then 1 else 0 end
     when $P!{paccount} = 4 then   /*показываем строки с выставлеными счетами и оплачеными*/
       case when  (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                    where rqb2ordit.orderitemrefcode =  ord.oicode
                    limit 1 ) > 0
               and ( select coalesce(sum(rqp2bilit.summafact),0) from rqpaydocitem2billitem rqp2bilit
  where rqp2bilit.billitemcode in (select rqb2ordit.billitemrefcode from rqbillitem2orderitem rqb2ordit
                                    where rqb2ordit.orderitemrefcode =  ord.oicode  )
                    ) > 0     then 1 else 0 end

end /*если все счета */

) as priznak_account
 from
tkmeasurement ei,
tkmaterials sm,
enestimateitem es left join
(select rqddscodes.txtcode as ddscodetext,  rqorg.name as rqorgname , rqorg.id as rqorgcode ,   o2e.estimateitemcode, ro.code, ro.numberdoc, ro.orderperiod,
roi.countfact, roi.pricewithoutnds, ro.dategen  as orderdategen ,
 roi.sumwithoutnds, roi.planneddatepays, roi.planneddatedelivery, roi.findocid, roi.contractnumber,
 ro.rqorderformcode, ro.kindrefcode, ro.rqordertypecode, roi.code as oicode ,  roi.contractdate , ro.budgetrefcode
from rqorderitem2enestimttm o2e, rqorderitem roi left join rqorg on (roi.orgcode = rqorg.code )
                                     left join rqddscodes on (roi.ddscodescode = rqddscodes.code)          , rqorder ro
where o2e.orderitemcode = roi.code
and ro.code = roi.orderrefcode

/*статус заявки по умолчанию только "в работе ОМТС" , если угодно то берем еще и "попереді" */
and  (
      ( ro.statusrefcode = 2 and $P{orderstatus} = 2  ) /* только в работе ОМТС */
    or
      ( ro.statusrefcode in(2,3) and $P{orderstatus}  = 23  ) /*в работе ОМТС и попередні*/
    )

and ( rqddscodes.code = $P{ddscode} or $P{ddscode}  = 0 )

/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (roi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0)
) as ord on ord.estimateitemcode = es.code


,rqordertype tp, rqorderkind kn
,enplanwork p, enelementdata ed
,endepartment db, endepartment r , enplanworkitem pi  , tktechcard tk
where p.code = es.planrefcode
and p.elementrefcode = ed.ecode
and p.budgetrefcode = db.code
and p.departmentrefcode = r.code
and es.materialrefcode = sm.code
and ei.code = sm.measurementcode
and es.countfact <> 0
and es.kindrefcode  = 6 /*только услуги*/

and ord.kindrefcode = kn.code
and ord.rqordertypecode = tp.code
and es.planitemrefcode = pi.code
and pi.kartarefcode = tk.code
/* типы заявок */
/* ХОЕ планова = 4 , ХОЕ позапланова = 5 */
and  (ord.kindrefcode = $P!{orderform} or $P!{orderform} = 0)
/*1 - бюджетна заявка , 2 - инвестпрограмма*/
and  (ord.rqordertypecode = $P!{ordertype} or $P!{ordertype} = 0)

/* период заявки */
and ord.code in
(select rq.code
  from rqorder rq
where rq.orderperiod between to_date($P{startDate},'dd.MM.yyyy') and to_date($P{endDate},'dd.MM.yyyy'))

/* номер договора */
and (ord.findocid = $P!{contractNumber} or $P!{contractNumber} = 0)

/* бюджетодержатель */
and (db.code = $P!{budgCode} or $P!{budgCode} = 0)

/* подразделение */
and (r.code = $P!{renCode} or $P!{renCode} = 0)

/* заявка */
and (ord.code = $P!{orderCode}  or $P!{orderCode} = 0)

/* объект планирования */
/*and (ed.ecode = elementCode or elementCode = 0)*/

/*План */
and (p.code = $P!{planCode} or $P!{planCode} = 0)

/*постачальник */
 and (ord.rqorgcode = $P!{porgcode} or  $P!{porgcode} = 0 )


group by  (select name from tkmaterials tm
where tm.code in
(select rootgrouprefcode
from tkmaterials
where code = sm.code)),
/*case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end*/
 coalesce(pi.commentgen,'Невизначено') ,
 ei.name,
 sm.numkatalog ,
 ord.numberdoc, ord.orderperiod,
 ord.pricewithoutnds,
 ord.countfact, ord.sumwithoutnds,
 ord.planneddatedelivery, ord.planneddatepays,
 ord.oicode,
 ord.contractnumber, /*db.name, */ /*r.name, ed.ename,*/ tp.name, kn.name ,
 ord.rqorgname ,
 ord.contractdate ,
 ord.ddscodetext ,
 ord.budgetrefcode ,
 ord.orderdategen  ,
 tk.techkartnumber

order by  (select name from tkmaterials tm
where tm.code in
(select rootgrouprefcode
from tkmaterials
where code = sm.code)),
/*case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end,*/
coalesce(pi.commentgen,'Невизначено') ,
ei.name
) as q
where
q.priznak is not null
and q.priznak_account = 1
and q.bcount <> 0  /* сразу выбираются строки, на которые есть счета */

union all

select
*
from
(
select
(select name from tkmaterials tm
where tm.code in
(select rootgrouprefcode
from tkmaterials
where code = sm.code)) as parent_material,
/*case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end  as mname,*/
 tk.techkartnumber  ,
 coalesce(pi.commentgen,'Невизначено') as mname ,
 ei.name as meas,
 sm.numkatalog ,
 ord.numberdoc as order_number,
 to_char(ord.orderperiod,'MM.yyyy') as order_period,
 ord.countfact as order_countfact,
 ord.pricewithoutnds as order_pricewithoutnds,
 ord.sumwithoutnds as  sumwithoutnds,
 ord.contractnumber as contractnumber,
 to_char(ord.planneddatedelivery,'dd.mm.yyyy') as planneddatedelivery,
 ord.planneddatepays,
 ord.oicode as oicode,
 ord.budgetrefcode ,
 to_char(ord.orderdategen,'dd.MM.yyyy') as order_dategen,

 /*db.name as budname, *//*r.name as rname, ed.ename as ename,*/
 tp.name as typename, kn.name as kindname,

(select coalesce(count(distinct b2o.code),0)
from rqbillitem2orderitem b2o
where b2o.orderitemrefcode = ord.oicode) as bcount
 /*количество строк для движения */
 , (
SELECT count(countfact) from (
SELECT  rqfkorderitem2enstmttm.countgen as countfact
from rqfkorderitem ii
      left join rqfkorderitem2enstmttm
         on ( rqfkorderitem2enstmttm.fkorderitemrefcode = ii.code
              and coalesce(rqfkorderitem2enstmttm.countgen,0) <> 0 )
, rqfkorder iord left join  findoc2fkorder ffkord on (iord.code = ffkord.fkorderrefcode)  , rqfkorderstatus

where iord.code = ii.fkorderrefcode
and iord.statuscode = rqfkorderstatus.code
and rqfkorderstatus.code in (3,4,5)  /*НЕ равно черновым сказано Салыгиным 23,11,2010*/
and iord.kindcode = 14
and rqfkorderitem2enstmttm.estimateitemcode in ( select estimateitemcode from rqorderitem2enestimttm rrr where
                                                                                 rrr.orderitemcode = ord.oicode
                                                                         )

 UNION

SELECT  sum(rqfkorderitemremainder.countgen) as countfact
       from rqfkorderitemremainder , rqfkorderitem , rqfkorder , rqfkorderstatus
    where rqfkorderitemremainder.estimateitemrefcode in
                                              ( select    estimateitemcode from rqorderitem2enestimttm rrr where
                                                         rrr.orderitemcode = ord.oicode
                                              )
      and rqfkorderitemremainder.typerefcode = 2
      and rqfkorderitemremainder.fkorderitemrefcode = rqfkorderitem.code
      and rqfkorderitem.fkorderrefcode = rqfkorder.code
      and rqfkorder.kindcode = 14
      and rqfkorder.statuscode in (3,4,5) /*НЕ равно черновым сказано Салыгиным 23,11,2010*/
      and rqfkorder.statuscode = rqfkorderstatus.code

      ) selprihordercount
) as countmotion
, ord.rqorgname
, ord.contractdate ,
  ord.ddscodetext,
 ( select rqorderstatus.name from rqorder , rqorderitem , rqorderstatus
   where rqorder.code = rqorderitem.orderrefcode
     and rqorderitem.code = ord.oicode
     and rqorderstatus.code = rqorder.statusrefcode limit 1  ) as rqorderstatus ,
    (
select
totalcode
from
(select
mainoi.code as TotalCode,
/*Кол-во по заявке*/
coalesce(mainoi.countfact,0) as OrderQty,
/* Кол-во по счету - естимейты */
coalesce((select
Qty
from
(select
Bills1.TotalCode as TotalCode,
Sum(Bills1.Qty) as Qty
from
(select distinct
oi.code as TotalCode,
bies.code as biescode,
coalesce(bies.countgen,0) as Qty
from
(enestimateitem as es
left join
(rqbillitem2enestimattm as bies
left join
rqbillitem as bi
on
bi.code = bies.billitemcode)
on
es.code = bies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi
on
oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0))
on
oies.estimateitemcode = es.code
where oi.code = mainoi.code) as Bills1
group by TotalCode) as Bills),0) as BIESQty,
/* Количество по приходу - естимейты без остатков с типом 1 */
coalesce((select
Qty
from
(select
FKOrders1.TotalCode,
sum(FKOrders1.Qty+FKOrders1.ostQty) as Qty
from
(select distinct
oi.code as TotalCode,
fies.code as ficode,
(coalesce(fies.countgen,0)) as Qty,
(case when fir.fkorderitemrefcode = fi.code then coalesce(fir.countgen,0) else 0 end) as ostQty
from
((enestimateitem as es
left join
rqfkorderitemremainder as fir
on
fir.estimateitemrefcode = es.code)
left join
(rqfkorderitem2enstmttm as fies
left join
(rqfkorderitem as fi
left join
rqfkorder as fo
on
fo.code = fi.fkorderrefcode and fo.statuscode in (3,4,5)  )
on
fi.code = fies.fkorderitemrefcode)
on
es.code = fies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi on oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus}  or $P!{orderitemstatus} = 0) ) on oies.estimateitemcode = es.code
where oi.code = mainoi.code
and fo.statuscode <> 1
and fo.kindcode = 14) as FKOrders1
group by TotalCode) as FKOrders),0) as FIESQty,
/* Проверка есть ли на эту строку остаток типа 1 */
(select
TotalCode from
(select distinct
oi.code as TotalCode
from
(enestimateitem as es
left join
(rqfkorderitem2enstmttm as fies
left join
((rqfkorderitem as fi
left join
rqfkorderitemremainder as fir
on
fir.fkorderitemrefcode = fi.code)
left join
rqfkorder as fo
on
fo.code = fi.fkorderrefcode and fo.statuscode in (3,4,5) )
on
fi.code = fies.fkorderitemrefcode)
on
es.code = fies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi on oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) ) on oies.estimateitemcode = es.code
where oi.code = mainoi.code
and fo.statuscode <> 1
and fo.kindcode = 14
and fir.typerefcode = 1) as FKOrders1) as IsThereTypeOne,
/* Сумма строк прихода где есть тип 1 */
coalesce((select
sum(FIQty)
from
(select distinct
oi.code as TotalCode,
fi.countgen as FIQty
from
(enestimateitem as es
left join
(rqfkorderitem2enstmttm as fies
left join
((rqfkorderitem as fi
left join
rqfkorderitemremainder as fir
on
fir.fkorderitemrefcode = fi.code)
left join
rqfkorder as fo
on
fo.code = fi.fkorderrefcode and fo.statuscode in (3,4,5)  )
on
fi.code = fies.fkorderitemrefcode)
on
es.code = fies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi on oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus}  or $P!{orderitemstatus} = 0) ) on oies.estimateitemcode = es.code
where oi.code = mainoi.code
and fo.statuscode <> 1
and fo.kindcode = 14) as FKOrders1
group by TotalCode),0) as FIQty,
/* Сумма строк по счету для сравнения с суммой строк прихода где есть остатки типа 1 */
coalesce((select
Qty
from
(select
Bills1.TotalCode as TotalCode,
Sum(Bills1.Qty) as Qty
from
(select distinct
oi.code as TotalCode,
bi.code as Bicode,
bi.countfact as Qty
from
(enestimateitem as es
left join
(rqbillitem2enestimattm as bies
left join
rqbillitem as bi
on
bi.code = bies.billitemcode)
on
es.code = bies.estimateitemcode)
left join
(rqorderitem2enestimttm as oies
left join
rqorderitem as oi
on
oi.code = oies.orderitemcode
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (oi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0))
on
oies.estimateitemcode = es.code
where oi.code = mainoi.code) as Bills1
group by TotalCode) as Bills),0) as BIQty
from
rqorderitem as mainoi
/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
where  (mainoi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0) ) as IncompleteDelivery
where
IncompleteDelivery.Totalcode = oicode
and
case when $P{IsComplete} > 3  then BIESQty is not null else 0=0 end
and
case
when $P{IsComplete}  = 0
then 0 = 0
when $P{IsComplete} = 1
then
   case
       when IsThereTypeOne is not null
       then round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 2
then
   case
       when IsThereTypeOne is not null
       then round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 3
then
   case
       when IsThereTypeOne is not null
       then round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
           or round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(OrderQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
           or round(cast(OrderQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 4
then
   case
       when IsThereTypeOne is not null
       then round(cast(BIQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(BIESQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 5
then
   case
       when IsThereTypeOne is not null
       then round(cast(BIQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(BIESQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
   end
when $P{IsComplete} = 6
then
   case
       when IsThereTypeOne is not null
       then round(cast(BIQty as decimal),3) < round(cast(coalesce(FIQty,0) as decimal),3)
           or round(cast(BIQty as decimal),3) > round(cast(coalesce(FIQty,0) as decimal),3)
       else round(cast(BIESQty as decimal),3) < round(cast(coalesce(FIESQty,0) as decimal),3)
           or round(cast(BIESQty as decimal),3) > round(cast(coalesce(FIESQty,0) as decimal),3)
   end
end
) as priznak
,
/*анализ условия по счетам ( все 0 ,  невыставленные 1 , выставленые 2 , выставлен и неоплачен 3, выставлен и  оплачен 4 )  */
( select
case when $P!{paccount} = 0 then 1 /*показываем все строки невзирая на счета  */
     when $P!{paccount} = 1 then   /*показываем строки с невыставлеными счетами*/
       case when (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                   where rqb2ordit.orderitemrefcode =  ord.oicode
                   limit 1 ) > 0 then 0 else 1 end
     when $P!{paccount} = 2 then   /*показываем строки с выставлеными счетами*/
       case when (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                   where rqb2ordit.orderitemrefcode =  ord.oicode
                   limit 1 ) > 0 then 1 else 0 end
     when $P!{paccount} = 3 then   /*показываем строки с выставлеными счетами и неоплачеными*/
       case when  (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                    where rqb2ordit.orderitemrefcode =  ord.oicode
                    limit 1 ) > 0
               and ( select coalesce(sum(rqp2bilit.summafact),0) from rqpaydocitem2billitem rqp2bilit
  where rqp2bilit.billitemcode in (select rqb2ordit.billitemrefcode from rqbillitem2orderitem rqb2ordit
                                    where rqb2ordit.orderitemrefcode =  ord.oicode  )
                    ) = 0     then 1 else 0 end
     when $P!{paccount} = 4 then   /*показываем строки с выставлеными счетами и оплачеными*/
       case when  (select coalesce( code ,0 )  from rqbillitem2orderitem rqb2ordit
                    where rqb2ordit.orderitemrefcode =  ord.oicode
                    limit 1 ) > 0
               and ( select coalesce(sum(rqp2bilit.summafact),0) from rqpaydocitem2billitem rqp2bilit
  where rqp2bilit.billitemcode in (select rqb2ordit.billitemrefcode from rqbillitem2orderitem rqb2ordit
                                    where rqb2ordit.orderitemrefcode =  ord.oicode  )
                    ) > 0     then 1 else 0 end

end /*если все счета */

) as priznak_account
 from
tkmeasurement ei,
tkmaterials sm,
enestimateitem es left join
(select rqddscodes.txtcode as ddscodetext,  rqorg.name as rqorgname , rqorg.id as rqorgcode ,   o2e.estimateitemcode, ro.code, ro.numberdoc, ro.orderperiod,
roi.countfact, roi.pricewithoutnds, ro.dategen  as orderdategen ,
 roi.sumwithoutnds, roi.planneddatepays, roi.planneddatedelivery, roi.findocid, roi.contractnumber,
 ro.rqorderformcode, ro.kindrefcode, ro.rqordertypecode, roi.code as oicode ,  roi.contractdate , ro.budgetrefcode
from rqorderitem2enestimttm o2e, rqorderitem roi left join rqorg on (roi.orgcode = rqorg.code )
                                     left join rqddscodes on (roi.ddscodescode = rqddscodes.code)          , rqorder ro
where o2e.orderitemcode = roi.code
and ro.code = roi.orderrefcode
/*статус заявки по умолчанию только "в работе ОМТС" , если угодно то берем еще и "попереді" */
and  (
      ( ro.statusrefcode = 2 and $P{orderstatus} = 2  ) /* только в работе ОМТС */
    or
      ( ro.statusrefcode in(2,3) and $P{orderstatus}  = 23  ) /*в работе ОМТС и попередні*/
    )
and ( rqddscodes.code = $P{ddscode} or $P{ddscode}  = 0 )

/* 1  - показывать только необходимые строки заявки  , 0 - показывать необходимые строки и те которые утратили необходимость*/
and  (roi.statusrefcode = $P!{orderitemstatus} or $P!{orderitemstatus} = 0)
) as ord on ord.estimateitemcode = es.code


,rqordertype tp, rqorderkind kn
,enplanwork p, enelementdata ed
,endepartment db, endepartment r , enplanworkitem pi  , tktechcard tk
where p.code = es.planrefcode
and p.elementrefcode = ed.ecode
and p.budgetrefcode = db.code
and p.departmentrefcode = r.code
and es.materialrefcode = sm.code
and ei.code = sm.measurementcode
and es.countfact <> 0
and es.kindrefcode  = 6  /*только услуги */
and ord.kindrefcode = kn.code
and ord.rqordertypecode = tp.code
and es.planitemrefcode = pi.code
and pi.kartarefcode = tk.code
/* типы заявок */
/* ХОЕ планова = 4 , ХОЕ позапланова = 5 */
and  (ord.kindrefcode = $P!{orderform}  or $P!{orderform}  = 0)
/*1 - бюджетна заявка , 2 - инвестпрограмма*/
and  (ord.rqordertypecode = $P!{ordertype} or $P!{ordertype} = 0)

/* период заявки */
and ord.code in
(select rq.code
  from rqorder rq
where rq.orderperiod between to_date($P{startDate},'dd.MM.yyyy') and to_date($P{endDate},'dd.MM.yyyy'))

/* номер договора */
and (ord.findocid = $P!{contractNumber} or $P!{contractNumber} = 0)

/* бюджетодержатель */
and (db.code = $P!{budgCode}  or $P!{budgCode} = 0)

/* подразделение */
and (r.code = $P!{renCode} or $P!{renCode} = 0)

/* заявка */
and (ord.code = $P!{orderCode} or $P!{orderCode} = 0)

/* объект планирования */
/*and (ed.ecode = elementCode or elementCode = 0)*/

/*План */
and (p.code = $P!{planCode} or $P!{planCode} = 0)

/*постачальник */
 and (ord.rqorgcode = $P!{porgcode} or  $P!{porgcode} = 0 )


group by  (select name from tkmaterials tm
where tm.code in
(select rootgrouprefcode
from tkmaterials
where code = sm.code)),
/*case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end,*/
coalesce(pi.commentgen,'Невизначено')  ,
 ei.name,
 sm.numkatalog ,
  ord.numberdoc, ord.orderperiod,
 ord.pricewithoutnds,
 ord.countfact, ord.sumwithoutnds,
 ord.planneddatedelivery, ord.planneddatepays,
 ord.oicode,
 ord.contractnumber,  tp.name, kn.name ,
 ord.rqorgname ,
 ord.contractdate ,
 ord.ddscodetext ,
 ord.budgetrefcode ,
 ord.orderdategen  ,
 tk.techkartnumber

order by  (select name from tkmaterials tm
where tm.code in
(select rootgrouprefcode
from tkmaterials
where code = sm.code)),
/*case when es.kindrefcode = 2 then  sm.name||('(ПММ)') else sm.name end,*/
coalesce(pi.commentgen,'Невизначено') ,
ei.name
) as q
where
q.priznak is not null
and q.priznak_account = 1
and q.bcount = 0  /* без счетов */]]>
	</queryString>
	<field name="parent_material" class="java.lang.String"/>
	<field name="mname" class="java.lang.String"/>
	<field name="meas" class="java.lang.String"/>
	<field name="order_number" class="java.lang.String"/>
	<field name="order_period" class="java.lang.String"/>
	<field name="order_countfact" class="java.lang.Double"/>
	<field name="order_pricewithoutnds" class="java.lang.Double"/>
	<field name="sumwithoutnds" class="java.lang.Double"/>
	<field name="contractnumber" class="java.lang.String"/>
	<field name="planneddatedelivery" class="java.lang.String"/>
	<field name="oicode" class="java.lang.Integer"/>
	<field name="ddscodetext" class="java.lang.String"/>
	<field name="typename" class="java.lang.String"/>
	<field name="kindname" class="java.lang.String"/>
	<field name="planneddatepays" class="java.util.Date"/>
	<field name="bcount" class="java.lang.Integer"/>
	<field name="rqorgname" class="java.lang.String"/>
	<field name="contractdate" class="java.util.Date"/>
	<field name="order_dategen" class="java.lang.String"/>
	<field name="budgetrefcode" class="java.math.BigDecimal"/>
	<field name="numkatalog" class="java.lang.String"/>
	<field name="countmotion" class="java.lang.Integer"/>
	<field name="techkartnumber" class="java.lang.String"/>
	<field name="rqorderstatus" class="java.lang.String"/>
	<variable name="orderSum" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{sumwithoutnds}]]></variableExpression>
	</variable>
	<variable name="bsum" class="java.lang.Double" calculation="Sum"/>
	<variable name="bvatsum" class="java.lang.Double" calculation="Sum"/>
	<variable name="paysum" class="java.lang.Double" calculation="Sum"/>
	<variable name="payrestsum" class="java.lang.Double" calculation="Sum"/>
	<variable name="suminvsum" class="java.lang.Double" calculation="Sum"/>
	<variable name="vsumbillwithoutnds" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$V{vsumbillwithoutnds}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0.0)]]></initialValueExpression>
	</variable>
	<variable name="sumbillnds" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$V{sumbillnds}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0.0)]]></initialValueExpression>
	</variable>
	<variable name="sumpay" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$V{sumpay}]]></variableExpression>
		<initialValueExpression><![CDATA[new Double(0)]]></initialValueExpression>
