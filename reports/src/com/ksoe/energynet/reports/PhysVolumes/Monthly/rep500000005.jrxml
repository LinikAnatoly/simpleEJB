        	ELSE null
        END
	ELSE
    	(CASE
        	WHEN (PW.kindcode=4) THEN
            	CASE
                WHEN PW.statuscode = 3 THEN
				CASE
                	WHEN PW.datestart between to_date(cast('01.'||PW.monthgen||'.'||PW.yeargen as varchar),'dd.MM.yyyy') and to_date(cast('10.'||PW.monthgen||'.'||PW.yeargen as varchar),'dd.MM.yyyy') THEN cast((cast(EL.code as varchar) || '2') as numeric)
					WHEN PW.datestart between to_date(cast('11.'||PW.monthgen||'.'||PW.yeargen as varchar),'dd.MM.yyyy') and to_date(cast('20.'||PW.monthgen||'.'||PW.yeargen as varchar),'dd.MM.yyyy') THEN cast((cast(EL.code as varchar) || '3') as numeric)
					WHEN PW.datestart between to_date(cast('21.'||PW.monthgen||'.'||PW.yeargen as varchar),'dd.MM.yyyy') and last_day(to_date(cast('21.'||PW.monthgen||'.'||PW.yeargen as varchar),'dd.MM.yyyy')) THEN cast((cast(EL.code as varchar) || '4') as numeric) END
 				ELSE null
                END
                    ELSE
