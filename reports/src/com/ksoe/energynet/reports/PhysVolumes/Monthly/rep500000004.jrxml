<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="rep500000004" pageWidth="1994" pageHeight="595" orientation="Landscape" columnWidth="1834" leftMargin="30" rightMargin="30" topMargin="20" bottomMargin="20" isIgnorePagination="true">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="MonthGen" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="YearGen" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["D:\\Documents\\Reports\\ТО\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="DepartmentCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="ReportCode" class="java.lang.Integer" isForPrompting="false">
		<defaultValueExpression><![CDATA[new Integer(500000004)]]></defaultValueExpression>
	</parameter>
	<parameter name="geoDeptCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="geoDeptName" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
name.Totalcode,
elname,
(case
	when name.planfact = 0 then 'план'
    when name.planfact=1 then 'факт'
    when name.planfact=2 then 'річний план'
    when name.planfact=4 then 'план'
    when name.planfact=5 then 'факт'
    else ''
end) as plans,
depname,
p0_01, p1, p2, p3, p4, p5, p6, p7, p8, p8_02, p9, p8_01, p10, p11, p12, p13, p14, p15, p16, p17, p18,
p19, p20, p21, p22, p23, p24, p25

from

/* Наименование и планфакт */
(
/* Наименование річний план */
select distinct
cast((cast(EL.code as varchar) || '0') as numeric) as TotalCode,
(LI.name || ' № ' || LI.invnumber) as elname,
2 as planfact, 0 as sort,
de.name as depname
from
(((enplanwork as PW
inner join
endepartment as DE
on
DE.code = PW.departmentrefcode) inner join
(enelement as EL inner join
enline04 as LI
on EL.code=LI.elementcode)
on PW.elementrefcode=EL.code) inner join
enplanworkitem as PI on
PW.code=PI.planrefcode) inner join
(tktechcard as TC inner join
(tkreportitem2techcard as RITC inner join
(tkreportitem as RI inner join
tkreport as RE
on RI.reportrefcode=RE.code)
on RITC.reportitemcode=RI.code)
on TC.code=RITC.techcardcode)
on PI.kartarefcode=TC.code
where
RE.code = $P{ReportCode} /* номер отчета */
and PW.monthgen = $P{MonthGen} /* месяц */
and PW.yeargen = $P{YearGen} /* год */
and (case when $P{DepartmentCode} <> 0 then PW.departmentrefcode = $P{DepartmentCode} /* підрозділ */ when $P{DepartmentCode} = 0 then PW.departmentrefcode = PW.departmentrefcode end)
and (case when $P{geoDeptCode} <> 0 then el.geodepartmentrefcode = $P{geoDeptCode} /* geo підрозділ */ when $P{geoDeptCode} = 0 then 1=1 end)
and PW.kindcode in (1,2,4) /* річні, місячні, завдання факт */
and PW.typerefcode in (1,2,3,10) /* планові, авр, припис, непланові */
and PW.statuscode in (1,3,4,5,7,8) /* затверджені і чорнові */
 /*SUPP-61313 Планы со статусом работы завершены берутся только те, что с актами*/
and case when pw.statuscode = 8 then exists (select 1 from enplancorrecthistory as pco1
inner join enplancorrecthistory as pco2 on pco1.planrefcode = pco2.planrefcode
where
pw.code in (pco1.planoldrefcode, pco1.plannewrefcode)
and pco2.reasoncode = 5) else 1=1 end
and PW.budgetrefcode = 75000001 /* СРС */
and PW.staterefcode = 3 /* ТО */
and EL.typerefcode = 2 /* ПЛ 0.4 кВ */

union all

/* Наименование план */
select distinct
cast((cast(EL.code as varchar) || '1') as numeric) as TotalCode,
(LI.name || ' № ' || LI.invnumber) as elname,
0 as planfact, 0 as sort,
de.name as depname
from
(((enplanwork as PW
inner join
endepartment as DE
on
DE.code = PW.departmentrefcode) inner join
(enelement as EL inner join
enline04 as LI
on EL.code=LI.elementcode)
on PW.elementrefcode=EL.code) inner join
enplanworkitem as PI on
PW.code=PI.planrefcode) inner join
(tktechcard as TC inner join
(tkreportitem2techcard as RITC inner join
(tkreportitem as RI inner join
tkreport as RE
on RI.reportrefcode=RE.code)
on RITC.reportitemcode=RI.code)
on TC.code=RITC.techcardcode)
on PI.kartarefcode=TC.code
where
RE.code = $P{ReportCode} /* номер отчета */
and PW.monthgen = $P{MonthGen} /* месяц */
and PW.yeargen = $P{YearGen} /* год */
and (case when $P{DepartmentCode} <> 0 then PW.departmentrefcode = $P{DepartmentCode} /* підрозділ */ when $P{DepartmentCode} = 0 then PW.departmentrefcode = PW.departmentrefcode end)
and (case when $P{geoDeptCode} <> 0 then el.geodepartmentrefcode = $P{geoDeptCode} /* geo підрозділ */ when $P{geoDeptCode} = 0 then 1=1 end)
and PW.kindcode in (1,2,4) /* річні, місячні, завдання факт */
and PW.typerefcode in (1,2,3,10) /* планові, авр, припис, непланові */
and PW.statuscode in (1,3,4,5,7,8) /* затверджені і чорнові */
 /*SUPP-61313 Планы со статусом работы завершены берутся только те, что с актами*/
and case when pw.statuscode = 8 then exists (select 1 from enplancorrecthistory as pco1
inner join enplancorrecthistory as pco2 on pco1.planrefcode = pco2.planrefcode
where
pw.code in (pco1.planoldrefcode, pco1.plannewrefcode)
and pco2.reasoncode = 5) else 1=1 end
and PW.budgetrefcode = 75000001 /* СРС */
and PW.staterefcode = 3 /* ТО */
and EL.typerefcode = 2 /* ПЛ 0.4 кВ */

union all

/* Наименование факт */
select distinct
cast((cast(EL.code as varchar) || '2') as numeric) as TotalCode,
(LI.name || ' № ' || LI.invnumber) as elname,
1 as planfact, 0 as sort,
de.name as depname
from
(((enplanwork as PW
inner join
endepartment as DE
on
DE.code = PW.departmentrefcode) inner join
(enelement as EL inner join
enline04 as LI
on EL.code=LI.elementcode)
on PW.elementrefcode=EL.code) inner join
enplanworkitem as PI on
PW.code=PI.planrefcode) inner join
(tktechcard as TC inner join
(tkreportitem2techcard as RITC inner join
(tkreportitem as RI inner join
tkreport as RE
on RI.reportrefcode=RE.code)
on RITC.reportitemcode=RI.code)
on TC.code=RITC.techcardcode)
on PI.kartarefcode=TC.code
where
RE.code = $P{ReportCode} /* номер отчета */
and PW.monthgen = $P{MonthGen} /* месяц */
and PW.yeargen = $P{YearGen} /* год */
and (case when $P{DepartmentCode} <> 0 then PW.departmentrefcode = $P{DepartmentCode} /* підрозділ */ when $P{DepartmentCode} = 0 then PW.departmentrefcode = PW.departmentrefcode end)
and (case when $P{geoDeptCode} <> 0 then el.geodepartmentrefcode = $P{geoDeptCode} /* geo підрозділ */ when $P{geoDeptCode} = 0 then 1=1 end)
and PW.kindcode in (1,2,4) /* річні, місячні, завдання факт */
and PW.typerefcode in (1,2,3,10) /* планові, авр, припис, непланові */
and PW.statuscode in (1,3,4,5,7,8) /* затверджені і чорнові */
 /*SUPP-61313 Планы со статусом работы завершены берутся только те, что с актами*/
and case when pw.statuscode = 8 then exists (select 1 from enplancorrecthistory as pco1
inner join enplancorrecthistory as pco2 on pco1.planrefcode = pco2.planrefcode
where
pw.code in (pco1.planoldrefcode, pco1.plannewrefcode)
and pco2.reasoncode = 5) else 1=1 end
and PW.budgetrefcode = 75000001 /* СРС */
and PW.staterefcode = 3 /* ТО */
and EL.typerefcode = 2 /* ПЛ 0.4 кВ */

union all

/* Наименование Всього річний план */
select distinct
(-27) as TotalCode,
('Всього річний план') as elname,
3 as planfact, 1 as sort,
null as depname

union all

/* Наименование Всього місячний план */
select distinct
(-26) as TotalCode,
('Всього місячний план') as elname,
3 as planfact, 1 as sort,
null as depname

union all

/* Наименование Всього факт */
select distinct
(-25) as TotalCode,
('Всього факт') as elname,
3 as planfact, 1 as sort,
null as depname

union all

/* Наименование За рік */
select distinct
(-24) as TotalCode,
('За рік') as elname,
3 as planfact, 1 as sort,
null as depname

union all

/* Наименование З початку року план */
select distinct
(-23) as TotalCode,
('З початку року') as elname,
4 as planfact, 1 as sort,
null as depname

union all

/* Наименование З початку року факт */
select distinct
(-22) as TotalCode,
('З початку року') as elname,
5 as planfact, 1 as sort,
null as depname

) as name

left join

/* Даннные */
(select
(CASE
	WHEN PW.kindcode=1 THEN
    	CASE
        	WHEN PW.typerefcode=1 THEN cast((cast(EL.code as varchar) || '0') as numeric)
            ELSE null
        END
	ELSE
		(CASE
			WHEN (PW.kindcode=4) THEN
            	CASE
                	WHEN pw.statuscode = 3 THEN cast((cast(EL.code as varchar) || '2') as numeric)
                    ELSE null
    			END
            ELSE
    			CASE
        			WHEN PW.typerefcode = 1 THEN cast((cast(EL.code as varchar) || '1') as numeric)
            		ELSE null
        		END
		END)
END) as TotalCode,
/* 0.01. Комплекс робіт з технічного обслуговування */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000205)) as p0_01,
/* 1. Огляд по всій довжині електромонтерами */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000095)) as p1,
/* 2. Огляд ПЛ, які є в плані капремонту наступного року, інженерно-технічними працівниками */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000096)) as p2,
/* 3. Перевірка загнивання деталей дерев'яних опор */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000097)) as p3,
/* 4. Перевірка стану залізобетонних опор */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000098)) as p4,
/* 5. Вимірювання опору заземлення опор */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000099)) as p5,
/* 6. Вибіркова перевірка стану пристроїв заземлення опор з розкриттям грунту */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000100)) as p6,
/* 7. Перевірка опору петлі "фаза - нуль" */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000101)) as p7,
/* 8. Вирубування окремих дерев */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000102)) as p8,
/* 8.02 Обрізування крони дерева */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000213)) as p8_02,
/* 9. Вирубування кущів в охоронній зоні ПЛ, обрізування гілок */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000103)) as p9,
/*8.01. Розчищення траси ПЛ під порослі дерев*/
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000209)) as p8_01,
/* 10. Відновлення номерних знаків і плакатів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000104)) as p10,
/* 11. Вирівнювання опор */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000083)) as p11,
/* 12. Перетягування проводів в окремих прогонах */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000084)) as p12,
/* 13. Перетягування бандажів кріплення стойки опори до приставки */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000085)) as p13,
/* 14. Заміна обірваних заземлюючих спусків і встановлення затискачів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000105)) as p14,
/* 15. Заміна обірваних в'язок проводів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000106)) as p15,
/* 16. Перевірка стану ізоляції і заміна окремих пошкоджених ізоляторів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000107)) as p16,
/* 17. Встановлення захисних кутників для захисту кабелів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000108)) as p17,
/* 18. Розчищення розлинності для захисту опор від низових пожеж */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000109)) as p18,
/* 19. Вимірювання навантажень */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000110)) as p19,
/* 20. Вимірювання напруги в контрольних точках */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000111)) as p20,
/* 21. Заміна пошкоджених окремих опор */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000112)) as p21,
/* 22. Заміна пошкоджених проводів в окремих прогонах */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000113)) as p22,
/* 23. Заміна пошкоджених шлейфів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000114)) as p23,
/* 24. Вирівнювання траверси */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000115)) as p24,
/* 25. Ремонт окремих відгалуджень на вводах */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000116)) as p25

from

(enplanwork as PW inner join
(enelement as EL inner join
enline04 as LI
on EL.code=LI.elementcode)
on PW.elementrefcode=EL.code) inner join
enplanworkitem as PI on
PW.code=PI.planrefcode
where
PW.monthgen = $P{MonthGen} /* месяц */
and PW.yeargen = $P{YearGen} /* год */
and (case when $P{DepartmentCode} <> 0 then PW.departmentrefcode = $P{DepartmentCode} /* підрозділ */ when $P{DepartmentCode} = 0 then PW.departmentrefcode = PW.departmentrefcode end)
