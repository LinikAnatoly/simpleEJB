on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000103)) as p9,
/* 8.01. Розчищення траси ПЛ під порослі дерев*/
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000209)) as p8_01,
/* 10. Відновлення номерних знаків і плакатів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000104)) as p10,
/* 11. Вирівнювання опор */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000083)) as p11,
/* 12. Перетягування проводів в окремих прогонах */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000084)) as p12,
/* 13. Перетягування бандажів кріплення стойки опори до приставки */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000085)) as p13,
/* 14. Заміна обірваних заземлюючих спусків і встановлення затискачів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000105)) as p14,
/* 15. Заміна обірваних в'язок проводів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000106)) as p15,
/* 16. Перевірка стану ізоляції і заміна окремих пошкоджених ізоляторів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000107)) as p16,
/* 17. Встановлення захисних кутників для захисту кабелів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000108)) as p17,
/* 18. Розчищення розлинності для захисту опор від низових пожеж */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000109)) as p18,
/* 19. Вимірювання навантажень */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000110)) as p19,
/* 20. Вимірювання напруги в контрольних точках */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000111)) as p20,
/* 21. Заміна пошкоджених окремих опор */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000112)) as p21,
/* 22. Заміна пошкоджених проводів в окремих прогонах */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000113)) as p22,
/* 23. Заміна пошкоджених шлейфів */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000114)) as p23,
/* 24. Вирівнювання траверси */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000115)) as p24,
/* 25. Ремонт окремих відгалуджень на вводах */
sum((select sum(PI1.countgen)
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000116)) as p25

from

(enplanwork as PW inner join
(enelement as EL inner join
enline04 as LI
on EL.code=LI.elementcode)
on PW.elementrefcode=EL.code) inner join
enplanworkitem as PI on
PW.code=PI.planrefcode
where
PW.monthgen  <= $P{MonthGen} /* месяц */
and PW.yeargen = $P{YearGen} /* год */
and (case when $P{DepartmentCode} <> 0 then PW.departmentrefcode = $P{DepartmentCode} /* підрозділ */ when $P{DepartmentCode} = 0 then PW.departmentrefcode = PW.departmentrefcode end)
and (case when $P{geoDeptCode} <> 0 then el.geodepartmentrefcode = $P{geoDeptCode} /* geo підрозділ */ when $P{geoDeptCode} = 0 then 1=1 end)
and PW.kindcode in (4) /* завдання факт */
and PW.typerefcode in (1,2,3,10) /* планові, авр, припис, непланові */
and PW.statuscode = 3 /* затверджені */
and PW.budgetrefcode = 75000001 /* СРС */
and PW.staterefcode = 3 /* ТО */
and EL.typerefcode = 2 /* ПЛ 0.4 кВ */

) as summary

on name.totalcode=summary.totalcode

order by name.sort, name.totalcode asc]]>
