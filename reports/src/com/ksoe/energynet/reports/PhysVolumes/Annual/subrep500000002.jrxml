<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="subrep500000002" pageWidth="1684" pageHeight="645" orientation="Landscape" columnWidth="1534" leftMargin="0" rightMargin="0" topMargin="28" bottomMargin="0" isIgnorePagination="true">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="YearGen" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="SUBREPORT_DIR" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["D:\\Documents\\Reports\\ТО\\"]]></defaultValueExpression>
	</parameter>
	<parameter name="DepartmentCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(1)]]></defaultValueExpression>
	</parameter>
	<parameter name="ReportCode" class="java.lang.Integer" isForPrompting="false">
		<defaultValueExpression><![CDATA[new Integer(500000002)]]></defaultValueExpression>
	</parameter>
	<parameter name="planKindCode" class="java.lang.Integer">
		<parameterDescription><![CDATA[Вид планов:
1 - годовые
2 - месячные]]></parameterDescription>
	</parameter>
	<parameter name="geoDeptCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<parameter name="geoDeptName" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
name.Totalcode,
elname,
cast('' as varchar) as plans,
p0_01, p1, p2, p3, p4, p5, p6, p7, p7_02, p8, p7_01, p9, p10, p11, p12, p13, p14, p15, p16, p17, p18,
p19, p20, p21, p22, p23, p24

from

/* Наименование и планфакт */

/* Наименование план */
(
/* 1 січень */
select distinct
cast(101 as numeric) as TotalCode,
cast('січень' as varchar) as elname,
1 as planfact, 0 as sort

union all

/* 2 лютий */
select distinct
cast(102 as numeric) as TotalCode,
cast('лютий' as varchar) as elname,
2 as planfact, 0 as sort

union all

/* 3 березень */
select distinct
cast(103 as numeric) as TotalCode,
cast('березень' as varchar) as elname,
3 as planfact, 0 as sort

union all

/* 1 квартал */
select distinct
cast(103.5 as numeric) as TotalCode,
cast('1 квартал' as varchar) as elname,
13 as planfact, 0 as sort

union all

/* 4 квітень */
select distinct
cast(104 as numeric) as TotalCode,
cast('квітень' as varchar) as elname,
4 as planfact, 0 as sort

union all

/* 5 травень */
select distinct
cast(105 as numeric) as TotalCode,
cast('травень' as varchar) as elname,
5 as planfact, 0 as sort

union all

/* 6 червень */
select distinct
cast(106 as numeric) as TotalCode,
cast('червень' as varchar) as elname,
6 as planfact, 0 as sort

union all

/* 2 квартал */
select distinct
cast(106.5 as numeric) as TotalCode,
cast('2 квартал' as varchar) as elname,
13 as planfact, 0 as sort

union all

/* 7 липень */
select distinct
cast(107 as numeric) as TotalCode,
cast('липень' as varchar) as elname,
7 as planfact, 0 as sort

union all

/* 8 серпень */
select distinct
cast(108 as numeric) as TotalCode,
cast('серпень' as varchar) as elname,
8 as planfact, 0 as sort

union all

/* 9 вересень */
select distinct
cast(109 as numeric) as TotalCode,
cast('вересень' as varchar) as elname,
9 as planfact, 0 as sort

union all

/* 3 квартал */
select distinct
cast(109.5 as numeric) as TotalCode,
cast('3 квартал' as varchar) as elname,
13 as planfact, 0 as sort

union all

/* 10 жовтень */
select distinct
cast(110 as numeric) as TotalCode,
cast('жовтень' as varchar) as elname,
10 as planfact, 0 as sort

union all

/* 11 листопад */
select distinct
cast(111 as numeric) as TotalCode,
cast('листопад' as varchar) as elname,
11 as planfact, 0 as sort

union all

/* 12 грудень */
select distinct
cast(112 as numeric) as TotalCode,
cast('грудень' as varchar) as elname,
12 as planfact, 0 as sort

union all

/* 4 квартал */
select distinct
cast(112.5 as numeric) as TotalCode,
cast('4 квартал' as varchar) as elname,
13 as planfact, 0 as sort

union all

/* Наименование За рік */
select distinct
(113) as TotalCode,
('За рік') as elname,
13 as planfact, 1 as sort


) as name

left join

/*  Данные */
(select
(CASE
	WHEN pw.monthgen = 1 THEN cast(101 as numeric)
    WHEN pw.monthgen = 2 THEN cast(102 as numeric)
    WHEN pw.monthgen = 3 THEN cast(103 as numeric)
    WHEN pw.monthgen = 4 THEN cast(104 as numeric)
    WHEN pw.monthgen = 5 THEN cast(105 as numeric)
    WHEN pw.monthgen = 6 THEN cast(106 as numeric)
    WHEN pw.monthgen = 7 THEN cast(107 as numeric)
    WHEN pw.monthgen = 8 THEN cast(108 as numeric)
    WHEN pw.monthgen = 9 THEN cast(109 as numeric)
    WHEN pw.monthgen = 10 THEN cast(110 as numeric)
    WHEN pw.monthgen = 11 THEN cast(111 as numeric)
    WHEN pw.monthgen = 12 THEN cast(112 as numeric)
    ELSE null
END) as TotalCode,
/* 500000204 0.01 Комплекс робіт з технічного обслуговування */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000204)) as p0_01,
/*  500000031	1 Огляд ПЛ по всій довжині електромонтерами */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000031)) as p1,
/*  500000032	2 Огляд ПЛ, які є в плані капремонту наступного року, інженерно-технічними працівниками */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000032)) as p2,
/* 500000033	3 Перевірка загнивання деталей дерев'яних опор */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000033)) as p3,
/*  500000034	4 Перевірка стану залізобетонних опор */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000034)) as p4,
/* 500000035	5 Вимірювання опору заземлення опор */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000035)) as p5,
/*  500000036	6 Вибіркова перевірка стану пристроїв заземлення опор з розкриттям грунту */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000036)) as p6,
/*  500000037	7 Вирубування окремих дерев */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000037)) as p7,
/*  500000211	7.02 Обрізування крони дерев */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000211)) as p7_02,
/* 500000038	8 Вирубування кущів в охоронній зоні ПЛ, обрізування гілок */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000038)) as p8,
/*500000207 7,01 Розчищення траси ПЛ під порослі дерев*/
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000207)) as p7_01,
/* 500000039	9 Відновлення номерних знаків і плакатів на опорах */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000039)) as p9,
/* 500000040	10 Вирівнювання опор */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000040)) as p10,
/* 500000041	11 Перетягування проводів в прогонах опор */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000041)) as p11,
/* 500000042	12 Перетягування бандажів кріплення стойки опори до приставки */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000042)) as p12,
/*  500000043	13 Заміна обірваних заземлюючих спусків і встановлення затискачів */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000043)) as p13,
/* 500000044	14 Заміна обірваних в'язок проводів */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000044)) as p14,
/* 500000045	15 Перевірка стану ізоляції і заміна окремих пошкоджених ізоляторів */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000045)) as p15,
/* 500000046	16 Встановлення захисних кутників для захисту кабелів */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000046)) as p16,
/* 500000047	17 Розчищення рослинності для захисту опор від низових пожеж */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000047)) as p17,
/*  500000048	18 Вимірювання навантажень */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000048)) as p18,
/* 500000049	19 Заміна пошкоджених окремих опор */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000049)) as p19,
/*  500000050	20 Заміна пошкоджених проводів в окремих прогонах */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000050)) as p20,
/* 500000051	21 Заміна пошкоджених шлейфів */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000051)) as p21,
/* 500000052	22 Вирівнювання траверси */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000052)) as p22,
/* 500000053	23 Встановлення замків на приводах роз'єднувачів */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000053)) as p23,
/* 500000054	24 ТО роз'єднувача */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000054)) as p24

from

(enplanwork as PW inner join
(enelement as EL inner join
enline10 as LI
on EL.code=LI.elementcode)
on PW.elementrefcode=EL.code) inner join
enplanworkitem as PI on
PW.code=PI.planrefcode
where
PW.yeargen = $P{YearGen} /* год */
and (case when $P{DepartmentCode} <> 0 then PW.departmentrefcode = $P{DepartmentCode} /* підрозділ */ when $P{DepartmentCode} = 0 then PW.departmentrefcode = PW.departmentrefcode end)
and (case when $P{geoDeptCode} <> 0 then el.geodepartmentrefcode = $P{geoDeptCode} /* geo підрозділ */ when $P{geoDeptCode} = 0 then 1=1 end)
and pw.kindcode = $P{planKindCode} /* вид плана */
and PW.typerefcode in (1) /* планові */
and PW.statuscode in (1,3,4,5,7,8) /* затверджені і чорнові */
 /*SUPP-61313 Планы со статусом работы завершены берутся только те, что с актами*/
and case when pw.statuscode = 8 then exists (select 1 from enplancorrecthistory as pco1
inner join enplancorrecthistory as pco2 on pco1.planrefcode = pco2.planrefcode
where
pw.code in (pco1.planoldrefcode, pco1.plannewrefcode)
and pco2.reasoncode = 5) else 1=1 end
and PW.budgetrefcode = 75000001 /* СРС */
and PW.staterefcode = 3 /* ТО */
and EL.typerefcode = 1 /* ПЛ 10 кВ */

group by totalcode

union all

/* 1 квартал */
select
cast(103.5 as numeric) as TotalCode,
/* 500000204 0.01 Комплекс робіт з технічного обслуговування */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000204)) as p0_01,
/*  500000031	1 Огляд ПЛ по всій довжині електромонтерами */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000031)) as p1,
/*  500000032	2 Огляд ПЛ, які є в плані капремонту наступного року, інженерно-технічними працівниками */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000032)) as p2,
/* 500000033	3 Перевірка загнивання деталей дерев'яних опор */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000033)) as p3,
/*  500000034	4 Перевірка стану залізобетонних опор */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000034)) as p4,
/* 500000035	5 Вимірювання опору заземлення опор */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000035)) as p5,
/*  500000036	6 Вибіркова перевірка стану пристроїв заземлення опор з розкриттям грунту */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000036)) as p6,
/*  500000037	7 Вирубування окремих дерев */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000037)) as p7,
/*  500000211	7.02 Обрізування крони дерева */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000211)) as p7_02,
/* 500000038	8 Вирубування кущів в охоронній зоні ПЛ, обрізування гілок */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000038)) as p8,
/*500000207 7,01 Розчищення траси ПЛ під порослі дерев*/
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
on PI1.kartarefcode=TC1.code
where
PI1.code=PI.code
and RITC1.reportitemcode = 500000207)) as p7_01,
/* 500000039	9 Відновлення номерних знаків і плакатів на опорах */
sum((select PI1.countgen
from enplanworkitem as PI1 inner join
(tktechcard as TC1 inner join
tkreportitem2techcard as RITC1
on TC1.code=RITC1.techcardcode)
