	<parameter name="datestart" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("01.03.2012")]]></defaultValueExpression>
	</parameter>
	<parameter name="datefinal" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("31.03.2012")]]></defaultValueExpression>
	</parameter>
	<parameter name="tabnumber" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="typerefcode" class="java.lang.Integer">
		<parameterDescription><![CDATA[Код работы]]></parameterDescription>
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="podrcode" class="java.lang.Integer"/>
	<parameter name="podrname" class="java.lang.String"/>
	<parameter name="planstatus" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
trim(tabnumber) as tabnumber ,
trim(workername) as workername ,
trim(positionname)  as positionname,
trim(positionname_tk) as positionname_tk ,
trim(renname) as renname ,
--departmentname ,
trim(finexecutor_nameplan) as finexecutor_nameplan ,
objectcode,
trim(objectname) as objectname ,
sum(countfact) as countfact,
trim(enplanworktypename)  enplanworktypename ,
datestart as datestart
from (
	select
	( select axname from (
	  select ax.axname from axorgid2fkorgid ax where ax.axorgid = SUBSTRING(f.departmentcode,1,3)
	  union
	  select ax.axname from axorgid2fkorgid ax where ax.axorgid =
	   SUBSTRING((select ax.axorgid from axorgid2fkorgid ax where ax.fkorgid = f.departmentcode) ,1,3)
	   ) as selren limit 1
	 ) as renname ,
	f.tabnumber ,  f.name as workername ,
	f.positionname ,
	pos.name as positionname_tk,
	f.departmentcode ,
	f.departmentname ,
	p.elementrefcode as objectcode,
	( select 'Інв№ ' || eld.invnumber || ' ' || eld.ename  from enelementdata eld where eld.ecode = p.elementrefcode) as objectname  ,
	sum(hi.countfact) as countfact  	,
	fe.name as finexecutor_nameplan ,
	pt.shortname as enplanworktypename ,
	to_char(p.datestart,'dd.mm.yyyy')  as datestart
	from finworker f  , enhumenitem hi , enplanwork p  left join enact2enplanwork a2p on p.code = a2p.plancode
	 , tkposition pos  , finexecutor fe , enplanworktype pt  , enplanworkitem pi
	where p.code =pi.planrefcode
	and pi.code =hi.planitemrefcode
	and pt.code =p.typerefcode
	and p.finexecutorcode = fe.code
	and f.code = hi.finworkercode
	and hi.planrefcode = p.code
	and p.kindcode = 4
	and  coalesce(hi.countfact  , 0 ) <> 0
	and hi.positiongencode = pos.code
    and p.statuscode NOT IN (2,6)
    and pi.countgen <> 0
    and p.datestart between to_date($P{datestart},'dd.mm.yyyy') and to_date($P{datefinal},'dd.mm.yyyy')
    and (f.tabnumber = $P{tabnumber} or  $P{tabnumber} = '' )
    and ( p.typerefcode = $P{typerefcode} or $P{typerefcode} = 0 )
	and p.departmentrefcode in (select array2items(string_to_array((select * from getdepartmentcodesdown($P{podrcode}) ),','))::double precision)
	and (p.statuscode = $P{planstatus} or $P{planstatus} = 0 )
	group by p.datestart, pt.shortname , f.tabnumber , f.name  , f.departmentcode ,f.departmentname , p.elementrefcode , pos.name , f.positionname ,fe.name
UNION ALL
select
	( select axname from (
	  select ax.axname from axorgid2fkorgid ax where ax.axorgid = SUBSTRING(f.departmentcode,1,3)
	  union
	  select ax.axname from axorgid2fkorgid ax where ax.axorgid =
	   SUBSTRING((select ax.axorgid from axorgid2fkorgid ax where ax.fkorgid = f.departmentcode) ,1,3)
	   ) as selren limit 1
	 ) as renname ,
	f.tabnumber ,  f.name as workername ,
	f.positionname ,
	'водій' as positionname_tk,
	f.departmentcode ,
	f.departmentname ,
	p.elementrefcode as objectcode,
	( select 'Інв№ ' || eld.invnumber || ' ' || eld.ename  from enelementdata eld where eld.ecode = p.elementrefcode) as objectname  ,
	sum(ti.countworkfact) as countfact  	,
	fe.name as finexecutor_nameplan ,
	pt.shortname as enplanworktypename ,
	to_char(p.datestart,'dd.mm.yyyy')  as datestart
	from finworker f  , entransportitem ti , enplanwork p  left join enact2enplanwork a2p on p.code = a2p.plancode
	   , finexecutor fe , enplanworktype pt  , enplanworkitem pi
	where p.code =pi.planrefcode
	and pi.code =ti.planitemrefcode
	and pt.code =p.typerefcode
	and p.finexecutorcode = fe.code
	and f.code = ti.finworkercode
	and ti.planrefcode = p.code
	and p.kindcode = 4
	and  coalesce(ti.countworkfact  , 0 ) <> 0
    and p.statuscode NOT IN (2,6)
    and pi.countgen <> 0
     and p.datestart between to_date($P{datestart},'dd.mm.yyyy') and to_date($P{datefinal},'dd.mm.yyyy')
    and (f.tabnumber = $P{tabnumber} or  $P{tabnumber} = '' )
    and ( p.typerefcode = $P{typerefcode} or $P{typerefcode} = 0 )
	and p.departmentrefcode in (select array2items(string_to_array((select * from getdepartmentcodesdown($P{podrcode}) ),','))::double precision)
	and (p.statuscode = $P{planstatus} or $P{planstatus} = 0 )
	group by p.datestart, pt.shortname , f.tabnumber , f.name  , f.departmentcode ,f.departmentname , p.elementrefcode , f.positionname ,fe.name
	) as sel1
group by	trim(tabnumber)  ,
trim(workername) ,
trim(positionname)  ,
trim(positionname_tk),
trim(renname) ,
trim(finexecutor_nameplan)  ,
objectcode,
trim(objectname) ,
trim(enplanworktypename)  ,
datestart
order by tabnumber ,datestart  , enplanworktypename , objectname]]>
	</queryString>
