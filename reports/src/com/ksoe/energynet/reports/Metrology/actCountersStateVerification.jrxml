	<parameter name="fkOrderCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1017800744]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
  sv.invnumber
  , sv.buildnumber
  , sv.name
  , sv.pricegen
  , o.numberdoc
  , o.dategen
  , (SELECT oi.contractnumber
   FROM rqfkorderitem oi
   WHERE oi.fkorderrefcode = o.code
   LIMIT 1) AS contractnumber
  , (SELECT oi.contractdate
   FROM rqfkorderitem oi
   WHERE oi.fkorderrefcode = o.code
   LIMIT 1) AS contractdate
  , o.sumwithoutnds
  , o.sumnds
  , (o.sumwithoutnds + o.sumnds) AS totalsum
  , textgrivna(cast(o.sumwithoutnds + o.sumnds as numeric)) AS totalsumtext
  , (SELECT count(svv.code)
   FROM encountersstateverfctn svv
   WHERE svv.fkorderrefcode = o.code) AS totalcount
  , mo.is3phase
  , a.acttyperefcode as acttypecode
FROM encountersstateverfctn AS sv
  INNER JOIN rqfkorder AS o ON sv.fkorderrefcode = o.code
  INNER JOIN enact AS a ON sv.actrefcode = a.code
  INNER JOIN enmetrologyobject AS mo ON a.elementcode = mo.elementcode
WHERE o.code = $P{fkOrderCode}
ORDER BY sv.invnumber]]>
	</queryString>
