	<queryString>
		<![CDATA[select
case when oz.dategen < '18.03.2020' then ' в.о. Головного бухгалтера' else ' Головний бухгалтер ' end as buhposition ,
' Директор фінансовий ' as finposition ,
case when oz.dategen < '10.03.2020' then ' Ю.Д. Дучев ' else ' Н.В. Гавриленко ' end as finfio ,
oz.numbergen  ,
to_char(oz.dategen,'dd.mm.yyyy') as dategen ,
oz.codemol ,
oz.codepodr ,
dep.name as depname  ,
buhdat.kod_ist  ,
oz.summagen::numeric(15,2) as summagen ,
' '||oz.invnumberoz as invnumberoz  ,
oz.nameoz ,
-- case when coalesce(oz.fincontractnumber,'') = '' then 'Інвестиційна программа' else '' end as showInvProgr ,
case when oz.isinvestprogram = 1 then 'Інвестиційна програма ' ||
COALESCE(oz.yearinvestprogram,'') ||' рік ' ||
COALESCE(invprogr.name,'') || ' ' ||
COALESCE(oz.iteminvestprogram,'') ||' пункт' else '' end  showInvProgr ,
/*выводим уникальные даты наряд заданий по этому объекту по возрастанию*/
(select group_concat(to_char(daysnaryd_day,'dd.mm.yyyy'),', ') from (
    /*select distinct pw.datestart as  daysnaryd_day from enreconstrmodernoz2nct oz2zct  , enact2enplanwork a2p  , enplanwork pw
    where oz2zct.enreconstrmodernozrfcd = oz.code
    and oz2zct.actrefcode = a2p.actrefcode
    and a2p.plancode = pw.code
    order by pw.datestart*/
    select distinct(daysnaryd_day) as daysnaryd_day
      from
    (
    select distinct pw.datestart as daysnaryd_day from enreconstrmodernoz2nct oz2zct  , enact2enplanwork a2p  , enplanwork pw
        where oz2zct.enreconstrmodernozrfcd = oz.code
        and oz2zct.actrefcode = a2p.actrefcode
        and a2p.plancode = pw.code
        and oz2zct.iscalculationdate = 1
        group by pw.datestart
        /*учтем даты работ по актам проектирования (те которые без EnergyNET) */
        UNION ALL
        Select distinct enactproj2oz2date.dategen as daysnaryd_day from enactproj2oz2 , enactproj2oz2date
        where  enactproj2oz2.enreconstrmodernozrfcd = oz.code
        and enactproj2oz2.code = enactproj2oz2date.enactprojrefcode
        group by enactproj2oz2date.dategen
        order by daysnaryd_day
    )  as a
 ) qqq ) as daysnaryd_day ,
/*кол-во дней численно */
(   select count(distinct countday) as countday from (
    select distinct pw.datestart as countday from enreconstrmodernoz2nct oz2zct  , enact2enplanwork a2p  , enplanwork pw
    where oz2zct.enreconstrmodernozrfcd = oz.code
    and oz2zct.actrefcode = a2p.actrefcode
    and a2p.plancode = pw.code
    and oz2zct.iscalculationdate = 1
    group by pw.datestart
    /*учтем даты работ по актам проектирования (те которые без EnergyNET) */
    UNION ALL
    Select distinct enactproj2oz2date.dategen as countday from enactproj2oz2 , enactproj2oz2date
    where  enactproj2oz2.enreconstrmodernozrfcd = oz.code
    and enactproj2oz2.code = enactproj2oz2date.enactprojrefcode
    group by enactproj2oz2date.dategen
    order by countday
 ) as qqq ) as day_count_number ,
/*название которое клеим к числу дней */
(select * from  net.decline_day((
      select count(distinct countday) as countday from (
       /*select distinct pw.datestart as countday from enreconstrmodernoz2nct oz2zct  , enact2enplanwork a2p  , enplanwork pw
      where oz2zct.enreconstrmodernozrfcd = oz.code
      and oz2zct.actrefcode = a2p.actrefcode
      and a2p.plancode = pw.code
      group by pw.datestart
      order by pw.datestart */
      select distinct pw.datestart as countday from enreconstrmodernoz2nct oz2zct  , enact2enplanwork a2p  , enplanwork pw
    where oz2zct.enreconstrmodernozrfcd = oz.code
    and oz2zct.actrefcode = a2p.actrefcode
    and a2p.plancode = pw.code
    and oz2zct.iscalculationdate = 1
    group by pw.datestart
    /*учтем даты работ по актам проектирования (те которые без EnergyNET) */
    UNION ALL
    Select distinct enactproj2oz2date.dategen as countday from enactproj2oz2 , enactproj2oz2date
    where  enactproj2oz2.enreconstrmodernozrfcd = oz.code
    and enactproj2oz2.code = enactproj2oz2date.enactprojrefcode
    group by enactproj2oz2date.dategen
    order by countday

      ) as qqq )::INTEGER,'д'::text, 'ень'::text, 'ня'::text, 'нів'::text)
  ) as day_count_numbe_text ,
/*дата последнего наряда */
(/*select to_char(max(pw.datestart),'dd.mm.yyyy') daysnaryd_day from enreconstrmodernoz2nct oz2zct  , enact2enplanwork a2p  , enplanwork pw
where oz2zct.enreconstrmodernozrfcd = oz.code
and oz2zct.actrefcode = a2p.actrefcode
and a2p.plancode = pw.code*/
select to_char(max(countday),'dd.mm.yyyy') daysnaryd_day from (
    select distinct pw.datestart as countday from enreconstrmodernoz2nct oz2zct  , enact2enplanwork a2p  , enplanwork pw
        where oz2zct.enreconstrmodernozrfcd = oz.code
        and oz2zct.actrefcode = a2p.actrefcode
        and a2p.plancode = pw.code
        and oz2zct.iscalculationdate = 1
        group by pw.datestart
        /*учтем даты работ по актам проектирования (те которые без EnergyNET) */
        UNION ALL
        Select distinct enactproj2oz2date.dategen as countday from enactproj2oz2 , enactproj2oz2date
        where  enactproj2oz2.enreconstrmodernozrfcd = oz.code
        and enactproj2oz2.code = enactproj2oz2date.enactprojrefcode
        group by enactproj2oz2date.dategen
        order by countday ) as hhh


) as maxnaryad_day
, oz.typerm /*1- реконструкция , 2-модернизация*/
 , oz.characteristic  ,
oz.contractprice::numeric(15,2) as contractprice ,
oz.executedname ,
oz.executedposition ,
oz.acceptedname ,
oz.acceptedposition
------------------ определение есть ли в актах по ОЗ_2 работы по присоединению----------------
      ,  COALESCE(
      ( select sum(code) from (
      Select sum(tc2pl.code) as code  from enreconstrmodernoz2nct oz2act  , entechcond2planwork tc2pl , enact2enplanwork a2p
      where oz2act.enreconstrmodernozrfcd = oz.code
      and oz2act.actrefcode = a2p.actrefcode
      and a2p.plancode = tc2pl.planrefcode
      union all
      select sum(so2tc.code) as code from enservicesobject2techcondtnsservices so2tc , enreconstrmodernoz2nct oz2act ,
      enservicesobject so  , enact a
      where so2tc.servicesobjectrefcode = so.code
      and so.elementcode = a.elementcode
      and oz2act.enreconstrmodernozrfcd = oz.code
      and oz2act.actrefcode = a.code
      union all
      select sum(so.code) as code from  enreconstrmodernoz2nct oz2act , enservicesobject so  , enact a
      where so.elementcode = a.elementcode
      and oz2act.enreconstrmodernozrfcd = oz.code
      and oz2act.actrefcode = a.code
      and so.contracttyperefcode = 5 /*присоединения*/
      ) as sel_priconn ),
      null, 0) as  is_priconn
      , case when oz.dategen <'01.02.2020' then 1 else 2 end as var_print
    , oz.termuseful
    , coalesce(to_char(oz.dateexploitationout,'dd.mm.yyyy'),'') as dateexploitationout
    , coalesce(to_char(oz.dateexploitationin,'dd.mm.yyyy'),'') as dateexploitationin
    , (select rm2d.dfdoccode from enreconstrmodernz2dfdc rm2d where rm2d.enreconstrmodernozrfcd = oz.code order by rm2d.code desc limit 1) as doccode
from  enreconstrmodernoz oz left join  endepartment dep on oz.departmentrefcode = dep.code
                            left join  enreconstrmodern2osdat buhdat on oz.code = buhdat.enreconstrmodernozrfcd
                            left join  eninvestprogramgroups invprogr on oz.invgrouprefcode = invprogr.code
where oz.code = $P{ozCode}]]>
	</queryString>
