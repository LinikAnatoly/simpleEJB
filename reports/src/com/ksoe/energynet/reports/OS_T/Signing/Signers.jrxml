	</parameter>
	<queryString>
		<![CDATA[SELECT fiofulltoshort(fio) as fio, postinfo, signertyperefname
FROM (
 select dfs.fio, dfs.postinfo, st.name as signertyperefname
 from docflow.dfdocsigner dfs, docflow.dfdocsignertype st
 where dfs.signertyperefcode = st.code
 and st.code in
	(
	select stt.signertyperefcode
	from docflow.dfdocsubtype2signertyp stt, docflow.dfdocsubtype dst, docflow.dfdoc d
	where stt.docsubtyperefcode = dst.code
	  and d.code = dfs.docrefcode
	  and d.docsubtyperefcode = dst.code
	  and stt.signaturetyperefcode is not null
	)
 and ($P{dfDocSignerTypes} = '-1' or st.code in ($P!{dfDocSignerTypes}))
 and dfs.docrefcode = $P{docCode} order by dfs.code
) q]]>
	</queryString>
