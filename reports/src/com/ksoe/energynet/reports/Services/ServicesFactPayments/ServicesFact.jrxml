	<parameter name="tkclassificationtypecodes" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("0")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
so.elementcode
,to_char(pso.dategen, 'dd.mm.yyyy') as pay_date
, round(cast(sum(pso.sumtotal) as decimal), 2) as pay_sum
, coalesce((select ai.numbergen from enservicesobject2techcondtnsservices as sotc inner join enactincometechcondtns as ai on sotc.techcondservrefcode = ai.techcondservicesrefcod where sotc.servicesobjectrefcode = so.code), so.contractnumberservices||'/'||so.contractnumber) as act_income_number
, so.contractnumberservices
, de.name as dep_name
, sta.name as status
, to_char(coalesce((select ai.dategen from enservicesobject2techcondtnsservices as sotc inner join enactincometechcondtns as ai on sotc.techcondservrefcode = ai.techcondservicesrefcod where sotc.servicesobjectrefcode = so.code), ac_sub.dategen), 'dd.mm.yyyy') as act_income_date,
so.contragentname as contragentname,
' '||so.partnercode as partnercode
 from
enservicesobject as so
inner join enpayment2so as pso on so.code = pso.servicesobjectrefcode
inner join endepartment as de on de.code = so.departmentcode
inner join enservicesobjectstatus as sta on so.statusrefcode = sta.code
left join (select min(ac1.dategen) as dategen, ac1.elementcode from enact as ac1 group by ac1.elementcode) as ac_sub
on so.elementcode = ac_sub.elementcode
where
pso.sumtotal > 0
and pso.dategen between $P{dateStart} and $P{dateFinal}
and so.contractkindrefcode = 1
and so.contracttyperefcode <> 5

/*фильтр по калькуляциям в договоре */
        and case when $P{tkclassificationtypecodes} <> '' then  so.code in
           (select distinct so.code from tkclassificationtype tcl  , enplanwork2classfctntp p2clt , enplanwork p           , enservicesobject so
          where tcl.kod like replace($P{tkclassificationtypecodes},'*','%')
               and p2clt.classificationtyperfcd = tcl.code
               and p2clt.planrefcode = p.code
               and p.elementrefcode = so.elementcode
               and p.kindcode = 5
         ) else 1 = 1 end

group by so.elementcode, so.contractnumberservices, so.contractnumber, pso.dategen, de.name, sta.name, so.code, ac_sub.dategen
order by
pso.dategen
, sum(pso.sumtotal)
, coalesce((select ai.numbergen from enservicesobject2techcondtnsservices as sotc inner join enactincometechcondtns as ai on sotc.techcondservrefcode = ai.techcondservicesrefcod where sotc.servicesobjectrefcode = so.code), so.contractnumberservices||'/'||so.contractnumber)
, to_char(coalesce((select ai.dategen from enservicesobject2techcondtnsservices as sotc inner join enactincometechcondtns as ai on sotc.techcondservrefcode = ai.techcondservicesrefcod where sotc.servicesobjectrefcode = so.code), ac_sub.dategen), 'dd.mm.yyyy')
, so.contractnumberservices
, de.name
, sta.name]]>
	</queryString>
