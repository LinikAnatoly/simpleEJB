	<parameter name="servicesobjectcode" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
distinct
so.contragentokpo as okpo,
 coalesce(so.contragentname,'') as contragentname
, coalesce(bd.bankname, bd1.bankname) as bankname
, coalesce(bd.bankokpo, bd1.bankokpo) as bankokpo
, coalesce(bd.bankmfo, bd1.bankmfo) as bankmfo
, coalesce(bd.bankaccount, bd1.bankaccount) as bankaccount
, dep.name as depname
, dep.code as department_code
, so.contractdateservices
, orditem.nomenclaturename as name, orditem.nomenclatureunitname as unit_name, orditem.countgen::numeric(15,4) as countgen,
orditem.sellingpricewithoutnds::numeric(15,3) as sell_price,
orditem.sellingcostwithoutnds::numeric(15,3) as sell_cost,
ord.sumwithoutnds as itogo, ord.sumnds as vat, ord.sumwithoutnds + ord.sumnds as total,
textgrivna(cast (ord.sumwithoutnds + ord.sumnds as numeric)) as totaltext
from
enservicesobject so
inner join endepartment dep on dep.code = so.departmentcode

/* Тип оплат со счета  ||  6 - Плата за реалізацію */
left join enbankingdetails bd on /*bd.departmentrefcode = dep.rencode and*/ bd.billtyperefcode = 6
left join enbankingdetails bd1 on bd1.departmentrefcode = dep.code

inner join enservicesbjct2rqfkrdr so2ord on so.code = so2ord.servicesobjectrefcode
inner join rqfkorder ord on so2ord.fkorderrefcode = ord.code
inner join rqfkorderitem orditem on orditem.fkorderrefcode = ord.code
where
so.code = $P{servicesobjectcode}]]>
	</queryString>
