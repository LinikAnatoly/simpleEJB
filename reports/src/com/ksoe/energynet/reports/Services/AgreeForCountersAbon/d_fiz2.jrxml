  so.contragentokpo ,
  so.contragentbankaccount ,
  so.contragentbankname ,
  so.contragentbankmfo ,
  so.contragentbossname ,
  so.contragentpassport ,
  so.contractstatusrefcode ,
  so.contracttyperefcode ,
  so.contragenttyperefcode ,
  so.billstatusrefcode ,
  so.contractdateservices ,
  so.contractservicessumma ,
  so.contractservicespower ,
  so.commentservicesgen ,
  so.contractservicesdistnc,
  so.contractservicesday ,
  so.warrantrefcode ,
  dep.name , dep.code,
  enw.warrantfio, enw.numbergen, to_char(enw.datestart, 'dd.MM.yyyy'),
  enw.warrantposition
  , enw.warrantshortfio
  , so.basistype,
   bd.bankname
 , bd.bankokpo
 , bd.bankmfo
 , bd.bankaccount
 , case when coalesce(tcl.isnotlicensedactivity,0) = 1 then 1 else 0 end
 , enw.genitivefio
 , enw.genitiveposition
 , so.isnopay
 , fcalc.percentprepay
 , fcalc.sumprepay
 , fcalc.vatsumprepay
 , fcalc.totalsumprepay
 ,fcalc.totalsum
 ,fcalc.vatsum
 , fcalc.TOTALSUMCALC
 , fcalc.VATSUMCALC
 , so.executeworkdate
 , so.timestart
 , so.timefinal
 , coalesce(EXTRACT(HOUR FROM so.timestart),0)
 , EXTRACT(MINUTE FROM so.timestart)
 , EXTRACT(HOUR FROM so.timefinal)
 , EXTRACT(MINUTE FROM so.timefinal)
 , t_d.transport_cost
 , c_m.sum_calc_plus_materials
 , c_c.counter_cost
, gi.countertype
, gi.cost
, gi.phasity]]>
