	<parameter name="plancode" class="java.lang.String">
		<defaultValueExpression><![CDATA["500146221"]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[Select so.code  ,
       so.contractnumber ,
  so.contractdate ,
  so.name ,
  so.partnercode ,
  so.findoccode ,
  so.findocid ,
  so.commentgen ,
  so.usergen ,
  so.dateedit ,
  so.domain_info ,
  so.modify_time ,
  so.departmentcode ,
  so.elementcode ,
  so.contractnumberservices ,
  so.contragentname ,
  so.contragentaddress ,
  so.contragentokpo ,
  so.contragentbankaccount ,
  so.contragentbankname ,
  so.contragentbankmfo ,
  so.contragentbossname ,
  so.contragentpassport ,
  so.contractstatusrefcode ,
  so.contracttyperefcode ,
  so.contragenttyperefcode ,
  so.billstatusrefcode ,
  so.contractdateservices ,
  so.contractservicessumma ,
  so.contractservicespower ,
  so.commentservicesgen ,
  so.contractservicesdistnc,
  so.contractservicesday ,
  so.warrantrefcode ,
       dep.name as depname
        ,    group_concat(tcl.name ,';  ') as workname
        , trunc(fcalc.TOTALSUMCALC ) as summaGRN
        , textgrivna(cast (fcalc.TOTALSUMCALC as numeric)) as summaGRNKOPtxt
        , (fcalc.TOTALSUMCALC - trunc(fcalc.TOTALSUMCALC ))::numeric(15,2) * 100   as summaKOP
        , trunc(fcalc.VATSUMCALC) as ndsGRN
        , (fcalc.VATSUMCALC -   trunc(fcalc.VATSUMCALC))::numeric(15,2) * 100 as ndsKOP
        , textgrivna(cast (fcalc.VATSUMCALC as numeric)) as summaNDStxt

        , enw.warrantfio , enw.numbergen, to_char(enw.datestart, 'dd.MM.yyyy') as datestart, enw.warrantposition, dep.code as depcode
 , to_char(so.warrantdate,'dd.mm.yyyy') as cntrwarrantdate
 , so.warrantnumber as cntrwarrantnumber
 , so.warrantfio as cntrwarrantfio
 , enw.warrantshortfio
 , coalesce(so.basistype,0) as basistype, bd.bankname
 , bd.bankokpo
 , bd.bankmfo
 , bd.bankaccount
 , case when coalesce(tcl.isnotlicensedactivity,0) = 1 then 1 else 0 end as isnotlicensedactivity
 , enw.genitivefio
 , enw.genitiveposition
 , coalesce(so.isnopay,0) as isnopay
 , fcalc.percentprepay
 , fcalc.sumprepay /*Сума часткової оплати без ПДВ, грн.*/
 , fcalc.vatsumprepay /*ПДВ для часткової оплати, грн.*/
 , fcalc.totalsumprepay /*Сума часткової оплати з ПДВ, грн.*/
 , trunc(fcalc.totalsumprepay ) AS totalsumprepay_grn
 , (fcalc.totalsumprepay - trunc(fcalc.totalsumprepay ))::numeric(15,2) * 100   as totalsumprepay_kop
 , textgrivna(cast (fcalc.totalsumprepay as numeric)) as totalsumprepay_txt
 , trunc(fcalc.vatsumprepay ) AS vatsumprepay_grn
 , (fcalc.vatsumprepay - trunc(fcalc.vatsumprepay ))::numeric(15,2) * 100   as vatsumprepay_kop
 , textgrivna(cast (fcalc.vatsumprepay as numeric)) as vatsumprepay_txt
 , so.executeworkdate
 , so.timestart
 , so.timefinal
 , coalesce(EXTRACT(HOUR FROM so.timestart),0) as hourstart
 , EXTRACT(MINUTE FROM so.timestart) as minutestart
 , EXTRACT(HOUR FROM so.timefinal) as hourfinal
 , EXTRACT(MINUTE FROM so.timefinal) as minutefinal


 --- стоимость прибора учета с учетом НДС целочисленная часть (грн)
 , trunc(c_c.counter_cost*1.2) as counter_cost_grn
 --- стоимость прибора учета с учетом НДС дробная часть (коп)
 , (c_c.counter_cost*1.2 - trunc(c_c.counter_cost*1.2))::numeric(15,2) * 100 as counter_cost_kop
 --- стоимость прибора учета с учетом НДС прописью
 , textgrivna(cast (c_c.counter_cost*1.2 as numeric)) as counter_cost_txt
 --- НДС на стоимость прибора учета целочисленная часть (грн)
 , trunc(c_c.counter_cost*1.2 - c_c.counter_cost) as counter_cost_nds_grn
 --- НДС на стоимость прибора учета дробная часть (коп)
 , ((c_c.counter_cost*1.2 - c_c.counter_cost) -
 trunc(c_c.counter_cost*1.2 - c_c.counter_cost))::numeric(15,2) * 100   as counter_cost_nds_kop
  --- НДС на стоимость прибора учета прописью
 , textgrivna(cast ((c_c.counter_cost*1.2 - c_c.counter_cost) as numeric(15,2) )) as counter_cost_nds_txt

 --- стоимость работ по калькуляции + стоимость материалов не включая прибор учета
 --- целочисленная часть (грн)
, trunc((c_m.sum_calc_plus_materials*1.2)::numeric(15,2)) as calc_plus_materials_cost_grn
 --- дробная часть (коп)
, ((c_m.sum_calc_plus_materials*1.2)::numeric(15,2) -
trunc((c_m.sum_calc_plus_materials*1.2)::numeric(15,2)))::numeric(15,2) * 100  as calc_plus_materials_cost_kop
 --- прописью
, textgrivna((c_m.sum_calc_plus_materials*1.2)::numeric(15,2))
         as calc_plus_materials_cost_txt
 --- НДС на стоимость работ по калькуляции + стоимость материалов не включая прибор учета
 --- целочисленная часть (грн)
 , trunc((c_m.sum_calc_plus_materials*1.2 -
        c_m.sum_calc_plus_materials)::numeric(15,2)) as calc_plus_materials_nds_grn
  --- дробная часть (грн)
 , ((c_m.sum_calc_plus_materials*1.2 -
   c_m.sum_calc_plus_materials)::numeric(15,2) -
   trunc((c_m.sum_calc_plus_materials*1.2 -
          c_m.sum_calc_plus_materials)::numeric(15,2)))::numeric(15,2) * 100
          as calc_plus_materials_nds_kop
 --- прописью
 , textgrivna((c_m.sum_calc_plus_materials*1.2 -
             c_m.sum_calc_plus_materials)::numeric(15,2))  as calc_plus_materials_nds_txt

 --- стоимость транспортных расходов и доставки персонала
 --- целочисленная часть (грн)
 , trunc((t_d.transport_cost*1.2)::numeric(15,2)) as transport_cost_grn
 --- дробная часть (грн)
 , (t_d.transport_cost*1.2::numeric(15,2) -
 trunc((t_d.transport_cost*1.2)::numeric(15,2)))::numeric(15,2) * 100 as transport_cost_kop
 --- прописью
 , textgrivna((t_d.transport_cost*1.2)::numeric(15,2))  as transport_cost_txt
 --- НДС на стоимость транспортных расходов и доставки персонала
 --- целочисленная часть (грн)
 , trunc((t_d.transport_cost*1.2 - t_d.transport_cost)::numeric(15,2))  as transport_cost_nds_grn
 --- дробная часть (грн)
 , ((t_d.transport_cost*1.2 - t_d.transport_cost)::numeric(15,2) -
 trunc((t_d.transport_cost*1.2 - t_d.transport_cost)::numeric(15,2)))::numeric(15,2) * 100
 as transport_cost_nds_kop
 --- прописью
 , textgrivna((t_d.transport_cost*1.2 - t_d.transport_cost)::numeric(15,2))  as transport_cost_nds_txt
, gi.countertype as counter_type
, gi.cost as counter_cost
, gi.phasity as counter_phasity
from enservicesobject as so left join enwarrant as enw on so.warrantrefcode = enw.code
inner join enelement as e on so.elementcode = e.code
inner join enplanwork as pw on e.code = pw.elementrefcode
inner join endepartment as dep on dep.code = so.departmentcode
inner join enplanwork2classfctntp as p2c on pw.code = p2c.planrefcode
inner join tkclassificationtype as tcl on p2c.classificationtyperfcd = tcl.code
inner join encalccontracttotal as cct on cct.planrefcode = pw.code
inner join enbankingdetails as bd on so.departmentcode = bd.departmentrefcode
inner join enservicesfactcalc as fcalc on so.code = fcalc.servicesobjectrefcode
left join engivecounter as gi on so.code = gi.servicesobjectrefcode
left join       ( select c.planrefcode, c.sumgen as counter_cost
      from encalcmaterialsusage c
      where c.planrefcode = $P!{plancode}
      and upper(c.materialname) like upper('%лічильник%')) as c_c on pw.code = c_c.planrefcode,
      ---
     ( select sum(t.transportexpenses + t.deliverycost) as transport_cost
      from encalctotalcost t
      where t.planrefcode = $P!{plancode}) as t_d,
     (select cc.calculationcost + (select sum(c.sumgen) from encalcmaterialsusage c
      where c.planrefcode = cc.planrefcode and upper(c.materialname) not like upper('%лічильник%'))
      as sum_calc_plus_materials
      from  encalccost cc
      where cc.planrefcode = $P!{plancode}) as c_m
Where pw.code = $P!{plancode}
group by so.code  ,
  so.warrantdate  , so.warrantnumber , so.warrantfio , cct.totalcost ,  cct.costvat ,
  so.contractnumber ,
  so.contractdate ,
  so.name ,
  so.partnercode ,
  so.findoccode ,
  so.findocid ,
  so.commentgen ,
  so.usergen ,
  so.dateedit ,
