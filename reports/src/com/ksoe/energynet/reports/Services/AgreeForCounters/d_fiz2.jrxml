				<textFieldExpression><![CDATA[($F{depcode}.intValue() != 500000019 ?  $F{depname} + "  " + $V{companyShortName} + " в особі " : "  " + $V{companyShortName} + " в особі  "  ) + $F{genitiveposition} + " " +
( ($F{genitivefio} != "") ? $F{genitivefio} : "______________________________" )
