	<parameter name="soCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[1017006179]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
textgrivna(coalesce(so.contractservicessumma::numeric(15,2),0) + coalesce(so.contractservicessummvt::numeric(15,2),0)) as txt_costworks,
textgrivna(coalesce(so.contractservicessummvt::numeric(15,2),0)) as txt_costworks_vat,
coalesce(so.contractservicessummvt::numeric(15,2),0) as costworks_vat,
coalesce(so.contractservicessumma::numeric(15,2),0) + coalesce(so.contractservicessummvt::numeric(15,2),0) as costworks,
agreewarrant.code as warrantrefcode,
agreewarrant.warrantshortfio as enwarrant_warrantshortfio,
agreewarrant.genitivefio,
agreewarrant.genitiveposition,
trim(replace(agreewarrant.address,chr(10),'')) as enwarrant_address,
agreewarrant.warrantfio as enwarrant_warrantfio,
agreewarrant.warrantposition as enwarrant_warrantposition,
agreewarrant.numbergen as enwarrant_numbergen,
to_char(agreewarrant.datestart,'dd.mm.yyyy') as enwarrant_datestart,
so.contractnumberservices || '/' || so.contractnumber as fincontractnumber,
so.contractdate as fincontractdate,
so.contragentname,
so.contragentaddresswork,
so.contragentpassport,
 so.departmentcode as depcode
 , to_char(so.warrantdate,'dd.mm.yyyy') as cntrwarrantdate
 , so.warrantnumber as cntrwarrantnumber
 , so.warrantfio as cntrwarrantfio
 , coalesce(so.basistype,0) as basistype
 , so.contragentbankaccount  as bankrschet
 , so.contragentbankname as bankname
 , so.contragentbankmfo as bankmfo
 , so.contragentokpo as partnerokpo

, so.contragentposition as cntr_pos
, so.contragentbossname as cntr_name

 ,so.commentservicesgen
 ,so.contractservicesday
 ,so.projectcode
 ,so.paydetail
 , so.contractdateservices

from  enwarrant as agreewarrant,
 enservicesobject so
where  agreewarrant.code = so.warrantrefcode
and SO.code =  $P{soCode}]]>
	</queryString>
