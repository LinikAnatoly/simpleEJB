	<queryString>
		<![CDATA[select
so.contractservicessumma as kalkulcost,
dep.code as department_code,
so.contractdateservices,
bill.sumgen as itogo,
bill.sumvat as vat,
bill.sumtotal as total,

'Оплата згідно договору № ' || so.contractnumber || ' від ' || to_char(so.contractdate, 'dd.mm.yyyy') ||
' за надання послуг з ' || so.commentservicesgen   as name,
1 as countgen,
textgrivna(cast (bill.sumtotal as numeric)) as totaltext,
coalesce(so.contragentokpo,'') as okpo, so.contragentname
,  bd.bankname  as bankname
  , bd.bankokpo
  ,  bd.bankmfo  as bankmfo
  ,   bd.bankaccount  as bankaccount
 , dep.name as depname
from  enservicesobject so, enbankingdetails bd, endepartment dep,
ensobill bill
where
dep.code = so.departmentcode
and dep.code = bd.departmentrefcode
and so.code = bill.servicesobjectrefcode
and bill.code = $P!{billCode}]]>
