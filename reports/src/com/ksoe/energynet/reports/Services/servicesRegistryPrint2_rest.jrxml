	<parameter name="str_materials_code_counters" class="java.lang.String"/>
	<parameter name="str_materials_code_counters_1f" class="java.lang.String"/>
	<parameter name="str_materials_code_counters_3f" class="java.lang.String"/>
	<queryString>
		<![CDATA[select countername , rest_count , kod_mol , kod_subsch_b , phasity , zones , materialcode ,  phasity_en
 from (
        select a.str_name as countername , count(a.num_un) as rest_count , a.kod_mol , a.kod_subsch_b
        , a.phasity , a.zones , a.materialcode ,
          case when a.materialcode in ($P!{str_materials_code_counters_1f}) then 1
               when a.materialcode in ($P!{str_materials_code_counters_3f}) then 3
               else 0 end as phasity_en
          FROM countersread.ostable a
        where a.materialcode in($P!{str_materials_code_counters})
        and a.show_ = 'Y'
        and a.kod_podr in (18,27,42)
        and nvl(a.energy_lock,0) <= 0
        and nvl(a.is_avar, 0) <= 0
        and substr(a.kod_subsch_b,1,2) = '15'
        and NVL ( a.is_vrtu , 0) = 0
        and NVL ( a.is_avar , 0) = 0
        and (a.prizn_prih is null or a.prizn_prih <> '0')
        group by a.str_name  , a.kod_mol , a.kod_subsch_b , a.phasity , a.zones , a.materialcode
)
order by phasity_en ,  countername , kod_mol]]>
	</queryString>
