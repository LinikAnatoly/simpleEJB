	<parameter name="soCode" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
so.code as socode
, so.departmentcode
, so.contractnumberservices
, so.contractdateservices
, cc.costwithoutvat as kalkulcost
, cc.costvat
, cc.totalcost
, case when fcalc.code is null
then cct.costwithoutvat else fcalc.sumcalc end  + get_calc_additional_sum(pw.code) as itogo
, case when fcalc.code is null
then cct.costvat else fcalc.vatsumcalc end + get_calc_additional_sum(pw.code)*0.2  as vat
, case when fcalc.code is null
then cct.totalcost else fcalc.totalsumcalc end + get_calc_additional_sum(pw.code)*1.2 as total
, get_calc_additional_sum(pw.code) as sumCalcItems
, case when fcalc.code is null then
textgrivna(cast ((cct.totalcost + get_calc_additional_sum(pw.code)*1.2) as numeric))
else textgrivna(cast (fcalc.totalsumcalc as numeric) + get_calc_additional_sum(pw.code)*1.2) end as totaltext
, ct.name
, ct.kod as clasificator
, pw2ct.countgen
, coalesce(so.contragentokpo,'') as okpo, so.contragentname
, bd.bankname as bankname
, bd.bankokpo
, bd.bankmfo as bankmfo
, bd.bankaccount as bankaccount
, dep.name as depname
, so.contragenttyperefcode
, coalesce(so.contractservicesdistnc, 0) as distance
from
enservicesobject as so
inner join enplanwork as pw on so.elementcode = pw.elementrefcode
inner join endepartment as dep on so.departmentcode = dep.code
left join enbankingdetails as bd on dep.code = bd.departmentrefcode
inner join enplanwork2classfctntp as pw2ct on pw.code = pw2ct.planrefcode
inner join tkclassificationtype as ct on pw2ct.classificationtyperfcd = ct.code
inner join encalctotalcost as cc on pw2ct.code = cc.plan2ctyperefcode
inner join encalccontracttotal as cct on (cc.planrefcode = cct.planrefcode and pw.code = cc.planrefcode)
left join  enservicesfactcalc fcalc on so.code = fcalc.servicesobjectrefcode
where so.code = $P{soCode}
and pw.kindcode = 5]]>
	</queryString>
