	<queryString>
		<![CDATA[select
so.code as socode,
so.contractdateservices,
cc.totalcost as kalkulcost,
ai.summagen - ai.summavat as itogo,
ai.summavat as vat,
ai.summagen as total,
---
get_calc_additional_sum(pw.code) as sumCalcItems,
---
ct.name,
ct.kod as clasificator,
pi.countgen,
textgrivna(cast (ai.summagen as numeric)) as totaltext,

coalesce(so.contragentokpo,'') as okpo, so.contragentname
, bd.bankname as bankname
  , bd.bankokpo
  , bd.bankmfo as bankmfo
  , bd.bankaccount as bankaccount
, dep.code as department_code
, dep.name as depname

from encalccontracttotal cct, encalctotalcost cc,
	enplanwork2classfctntp pw2ct, tkclassificationtype ct, enplanwork pw,
	enservicesobject so, enbankingdetails bd, endepartment dep,
    enactincomeservices ai, enactincomserv2enact a2s, enact a, enact2enplanwork ap, enplanworkitem pi, tktechcard tk

where so.code = ai.servicesobjectrefcode
and cc.plan2ctyperefcode  = pw2ct.code
and pw2ct.classificationtyperfcd = ct.code
and cct.planrefcode = cc.planrefcode
and pw.code = cc.planrefcode
and so.elementcode = pw.elementrefcode
and dep.code = so.departmentcode
and dep.code = bd.departmentrefcode
and pw.kindcode = 5

 and pw2ct.classificationtyperfcd = ct.code
 and pw2ct.planrefcode = pw.code
 and a2s.actrefcode = a.code
 and ai.code = a2s.actincomerefcode
 and ap.actrefcode = a.code
 and ap.plancode = pi.planrefcode
 and pi.countgen > 0
 and a2s.actincomerefcode = $P{actIncomeServicesCode}
 and tk.code = pi.kartarefcode
 and tk.classificationtypecode = ct.code]]>
	</queryString>
