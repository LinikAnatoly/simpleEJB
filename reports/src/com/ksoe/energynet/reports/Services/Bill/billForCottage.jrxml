		<scriptletDescription><![CDATA[finScriptlet]]></scriptletDescription>
	</scriptlet>
	<parameter name="plancode" class="java.lang.String"/>
	<queryString>
		<![CDATA[Select
socode,
price ,
vartist,
(vartist*1.2)::numeric(15,2) as vartistwithpdv,
((vartist*1.2)::numeric(15,2) - vartist )::numeric(15,2) as pdv ,

countfamily	,
numberbeds,
textgrivna((vartist*1.2)::numeric(15,2) ) as totaltext,
kalkulcost,
itogo,
vat	total,
name,
clasificator,
countgen,
okpo,
contragentname,
bankname,
bankokpo,
bankmfo,
bankaccount,
depname,
department_code,
contractdateservices
from (

      Select socode,
      (((kalkulcost/countgen)/numberbeds)*countfamily)::numeric(15,2) as price ,
      ((((kalkulcost/countgen)/numberbeds)*countfamily)*countgen)::numeric(15,2)  as vartist,

      countfamily	,
      numberbeds,
      kalkulcost,
      itogo,
      vat,
      total,
      name,
      clasificator,
      countgen,
      okpo,
      contragentname,
      bankname,
      bankokpo,
      bankmfo,
      bankaccount,
      depname,
      department_code,
      contractdateservices
       from (
              select so.code as socode,
              coalesce((select coalesce(count(f2s.code) ,0)
              from enfamilysize2srvcsbjct f2s
              where f2s.relationrefcode = 5
              and f2s.servicesobjectrefcode = so.code), 0) as countfamily,

              (select c.numberbeds
              from enrentperiod2services r2s, encottage c
              where c.code = r2s.cottagerefcode
              and r2s.servicesobjectrefcode = so.code) as numberbeds,


              cc.totalcost as kalkulcost,
              cct.costwithoutvat as itogo,
              cct.costvat as vat,
              cct.totalcost as total,
              ct.name,
              ct.kod as clasificator,
              pw2ct.countgen,

              coalesce(so.contragentokpo,'') as okpo, so.contragentname
              , bd.bankname as bankname
                , bd.bankokpo
                , bd.bankmfo as bankmfo
                , bd.bankaccount as bankaccount,
              dep.name as depname,
              dep.code as department_code,
              so.contractdateservices
              from encalccontracttotal cct, encalctotalcost cc,
              enplanwork2classfctntp pw2ct, tkclassificationtype ct, enplanwork pw, enservicesobject so, enbankingdetails bd, endepartment dep
              where cc.planrefcode =  $P!{plancode}
              and cc.plan2ctyperefcode  = pw2ct.code
              and pw2ct.classificationtyperfcd = ct.code
              and cct.planrefcode = cc.planrefcode
              and pw.code = cc.planrefcode
              and so.elementcode = pw.elementrefcode
              and dep.code = so.departmentcode
              and dep.code = bd.departmentrefcode
      ) as qqq
) as qqqq]]>
	</queryString>
