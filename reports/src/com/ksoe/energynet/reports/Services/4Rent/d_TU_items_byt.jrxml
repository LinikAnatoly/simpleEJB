	<parameter name="axpartnercode" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="streetcode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="contractnumberservices" class="java.lang.String"/>
	<queryString>
		<![CDATA[select st.name || ' ' || s.name as streetname, ct.name || ' ' || c.name as cityname,
 case when f.voltage > 1 then 'ПЛ-10кВ ' || f.name
 else f.name end as linename,
 string_agg(distinct t.name::text,',') as postnumbers, tk.name as posttype,
 count(distinct t.code) as postcount
from cctower2jlc t2j ,  ccjointlinecontragents jlc, cctower t, cctowerkind tk,
ccfeeder f, address a,  street s, city c, streettype st, citytype ct
where t2j.jlcrefcode  = jlc.code
and t2j.towerrefcode = t.code
and t.kindrefcode = tk.code
and t.feederrefcode = f.code
and jlc.axpartnercode = $P{axpartnercode} /*'040428_055'*/
 and (coalesce(t2j.contractnumberservices,'') = $P{contractnumberservices} or coalesce(t2j.contractnumberservices,'') = '')
and t.addressrefcode = a.code
and a.streetcode = s.code
and s.citycode = c.code
and s.streettypecode = st.code
and c.citytypecode = ct.code
 and s.code = $P{streetcode} /*10500*/
group by st.name || ' ' || s.name, ct.name || ' ' || c.name,
  f.name,  f.voltage, tk.name]]>
	</queryString>
