	<queryString>
		<![CDATA[select  c.axpartnercode, c.axpartnername,
 so.contractnumberservices as contractnumber_6,
 to_char(so.contractdateservices,'dd.MM.yyyy') as contractdate_6,


  (select string_agg(so1.contractnumberservices || ' від  '  || to_char(so1.contractdateservices,'dd.MM.yyyy'), ',')
 from enservicesobject so1, enelement2enelement e2e
 where e2e.elementoutrefcode = so.elementcode
 and e2e.typerefcode = 2
 and so1.elementcode = e2e.elementinrefcode ) as contract_7,


  (select string_agg(so1.contractnumberservices || ' від  '  || to_char(so1.contractdateservices,'dd.MM.yyyy'), ',')
 from enservicesobject so1, enelement2enelement e2e
 where e2e.elementoutrefcode = so.elementcode
 and e2e.typerefcode = 3
 and so1.elementcode = e2e.elementinrefcode ) as contract_5


 from enservicesobject so, engeneralcontracts c
 where so.contractkindrefcode = 6
 and c.code = so.generalcontractrefcode

order by c.axpartnername]]>
	</queryString>
