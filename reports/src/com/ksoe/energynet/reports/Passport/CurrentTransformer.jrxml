	<parameter name="HighVoltCellCode" class="java.lang.Double">
		<defaultValueExpression><![CDATA[new Double(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
  coalesce(tkmaterials.name, '') as CurTransMat,
  cast(CurTrans.quantity as varchar) as CurTransQ,
  coalesce(cast(encurrenttransformer.accruracyclass as varchar), '')
    as AccuracyClass,
  coalesce(cast(encurrenttransformer.coeftransformation as varchar), '')
    as CoefTransformation,
  coalesce(cast(encurrenttransformer.secondarywindingsnumbr as varchar), '')
    as SecondaryWindingsNumbr
from encurrenttransformer
left join
  (select
    avg(encurrenttransformer.code) as CurTransCode,
    avg(encurrenttransformer.materialrefcode) as CurTransMatCode,
    sum(
      case coalesce(encurrenttransformer.numbergen, 1)
        when 0 then 1
        else coalesce(encurrenttransformer.numbergen, 1)
      end) as quantity
  from encurrenttransformer
  group by(' материал ' || encurrenttransformer.materialrefcode ||
    ' класс точности ' || encurrenttransformer.accruracyclass ||
    ' коэффициент трансформации ' || encurrenttransformer.coeftransformation ||
    ' количство вторичных обмоток ' ||
      encurrenttransformer.secondarywindingsnumbr)
  ) CurTrans on encurrenttransformer.code = CurTrans.CurTransCode
left join tkmaterials
  on tkmaterials.code = encurrenttransformer.materialrefcode
where encurrenttransformer.highvoltagesellcode = $P{HighVoltCellCode}]]>
	</queryString>
