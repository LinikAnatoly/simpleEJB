	<parameter name="SubStCode" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
  coalesce(mdm.name, '') as MeasurDeviceMat,
  coalesce(enmeasurdevicetype.name, '') as MeasurDeviceType,
  coalesce(enscale.name, '') as MeasurDeviceScale,
  coalesce(enmeasurdevice.worknumber, '') as MeasurDeviceWorkNum,
  case coalesce(enmeasurdevice.highvoltagesellcode, 0)
    when 0 then
      case coalesce(enmeasurdevice.branchcode, 0)
        when 0 then 'Місце встановлення не визначено'
        else enbranch.numbergen || '; ' || pnlm.name
      end
    else 'Високовольтна ланка № ' || enhighvoltagesell.numbergen
  end as MeasurDeviceSite
from enmeasurdevice
left join tkmaterials mdm on mdm.code = enmeasurdevice.materialrefcode
left join enmeasurdevicetype
  on enmeasurdevicetype.code = enmeasurdevice.measurdevicetypecode
left join enscale on enscale.code = enmeasurdevice.scalecode
left join enhighvoltagesell
  on enhighvoltagesell.code = enmeasurdevice.highvoltagesellcode
left join enbranch on enbranch.code = enmeasurdevice.branchcode
left join enpanel on enpanel.code = enbranch.panelcode
left join tkmaterials pnlm on pnlm.code = enpanel.materialrefcode
where enmeasurdevice.elementcode in
  (select enelement.code from enelement where enelement.elementinrefcode in
    (select ensubstation04.elementcode from ensubstation04
      where ensubstation04.code = $P{SubStCode}))]]>
	</queryString>
