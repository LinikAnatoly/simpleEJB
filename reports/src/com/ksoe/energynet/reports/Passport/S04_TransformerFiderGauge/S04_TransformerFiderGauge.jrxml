		<![CDATA[select
  (entransformertype.name || ', ' || entransformer.name) as transformername,
  cast (entransformer.nominalpower as varchar),
  cast (entransformer.lowcurrent as varchar),
  coalesce(enantsapf.name, 'не визначено') antsapf,
  cast(entransformer.ukz as varchar),
  epren.name as renname
from entransformer
left join entransformertype on entransformertype.code = entransformer.transformertypecode
left join enantsapf on enantsapf.code = entransformer.antsapfrefcode
left join enelement on enelement.code = entransformer.elementcode
left join epren on epren.code = enelement.renrefcode
where entransformer.code = $P{transformerCode}]]>
