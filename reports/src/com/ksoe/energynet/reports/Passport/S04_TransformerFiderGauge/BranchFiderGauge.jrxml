	<parameter name="transformerCode" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
  cast(enbranch.code as integer),
  replace(enbranch.numbergen, 'Присоединение № ', '') as numbergen,
  (enbranchtype.name || ', ' || enbranch.name
  ) as branch,
  (coalesce(tkmaterials.name, '') || ', ' || coalesce(tkmaterials.numkatalog, '')
  ) as material
from enbranch
left join enbranchtype on enbranchtype.code = enbranch.branchtyperefcode
left join tkmaterials on tkmaterials.code = enbranch.materialrefcode
where enbranch.panelcode in (
  select code from enpanel where enpanel.transformercode = $P{transformerCode})]]>
	</queryString>
