	<parameter name="trCode" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[--Предохранители высоковольтной стороны
select q.currentFuse from (
  select
    cast(min(coalesce(enfuse.currentfuse)) as varchar) as currentFuse
  from enfuse
  where enfuse.highvoltagesellcode in (
    select enhighvoltagesell.code from enhighvoltagesell
      where enhighvoltagesell.transformerrefcode = $P{trCode})) q
where q.currentFuse is not Null]]>
	</queryString>
