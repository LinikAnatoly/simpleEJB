	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="HighVoltCellCode" class="java.lang.Double">
		<defaultValueExpression><![CDATA[new Double(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
  endisconnector.ratedvoltage as DisconLoadSwU,
  endisconnector.ratedcurrent as DisconLoadSwI,
  disconnector_mat.name as DisconLoadSwMat,
  dis_drive_mat.name as DisconLoadSwDrive
from endisconnector
  left join tkmaterials disconnector_mat
    on disconnector_mat.code = endisconnector.materialrefcode
  left join tkmaterials dis_drive_mat
    on dis_drive_mat.code = endisconnector.matdriverefcode
where endisconnector.highvoltagesellcode = $P{HighVoltCellCode}

union all select
  enloadswitch.ratedvoltage as DisconLoadSwU,
  enloadswitch.ratedcurrent as DisconLoadSwI,
  load_switch_mat.name as DisconLoadSwMat,
  load_sw_drive_mat.name as DisconLoadSwDrive
from enloadswitch
  left join tkmaterials load_switch_mat
    on load_switch_mat.code = enloadswitch.materialrefcode
  left join tkmaterials load_sw_drive_mat
    on load_sw_drive_mat.code = enloadswitch.matdriverefcode
where enloadswitch.highvoltagesellcode = $P{HighVoltCellCode}]]>
	</queryString>
