<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="Insulator" pageWidth="284" pageHeight="30" orientation="Landscape" columnWidth="284" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0">
	<property name="ireport.scriptlethandling" value="0"/>
	<property name="ireport.encoding" value="UTF-8"/>
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<import value="net.sf.jasperreports.engine.*"/>
	<import value="java.util.*"/>
	<import value="net.sf.jasperreports.engine.data.*"/>
	<parameter name="HighVoltCellCode" class="java.lang.Double">
		<defaultValueExpression><![CDATA[new Double(0)]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
  tkmaterials.name,
  insulator.InsulatorU,
  insulator.InsulatorQ
from
  (select
    avg(eninsulator.materialrefcode) as InsulatorMatCode,
    avg(eninsulator.voltage) as InsulatorU,
    sum(
      case coalesce(eninsulator.numbergen, 1)
        when 0 then 1
        else coalesce(eninsulator.numbergen, 1)
      end) as InsulatorQ
  from eninsulator
  where eninsulator.highvoltagesellcode = $P{HighVoltCellCode}
  group by('материал ' || eninsulator.materialrefcode ||
           'напряжение ' || eninsulator.voltage)
  ) insulator
left join tkmaterials on tkmaterials.code = insulator.InsulatorMatCode]]>
	</queryString>
	<field name="name" class="java.lang.String"/>
	<field name="InsulatorU" class="java.lang.Double"/>
	<field name="InsulatorQ" class="java.lang.Double"/>
