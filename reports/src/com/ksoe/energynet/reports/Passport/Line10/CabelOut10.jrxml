	<queryString>
		<![CDATA[select
  encabelout10.code as cabelOut10Code,
  coalesce(cast(tkmaterials.name as varchar), '') as matName,
  coalesce(cast(encabelout10.cabellength as varchar), '') as cabelOut10Length,
  coalesce(cast(encabelmuftatype.name as varchar), '') as cabelMuftaType,
  coalesce(cast(encabelouttype10.name as varchar), '') as cabelOutType10,
  coalesce(cast(encabelout10.name as varchar), '') as cabelOut10Name

from encabelout10

left join tkmaterials on tkmaterials.code = encabelout10.materialrefcode
left join encabelmuftatype on encabelmuftatype.code = encabelout10.muftarefcode
left join encabelouttype10 on encabelouttype10.code = encabelout10.typerefcode

where encabelout10.line10refcode = $P{Line10Code}]]>
