	<parameter name="Line10Code" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
  (case
    when endisconnector.postrefcode is not Null
      then
        'Опора № ' || coalesce(cast(enpost.postnumbergen as varchar), '')
      else
        ''
  end) ||
  (case
    when coalesce(cast(endisconnector.name as varchar), '') <> ''
      then
        case
          when endisconnector.postrefcode is not Null
            then
              ', ' || coalesce(cast(endisconnector.name as varchar), '')
          else
            coalesce(cast(endisconnector.name as varchar), '')
        end
      else
        ''
  end) as postNumberGenDisconnectorName,
  coalesce(cast(tkmaterials.name as varchar), '') as matName,
  coalesce(cast(drM.name as varchar), '') as matDriveName,
  coalesce(cast((select max(enplanwork.datestart) from enplanwork
    where enplanwork.statuscode = 1 and enplanwork.kindcode <>  6
    and enplanwork.elementrefcode = endisconnector.elementcode) as varchar), ''
  ) as planWorkDateStart

from endisconnector

left join enpost on enpost.code = endisconnector.postrefcode
left join tkmaterials on tkmaterials.code = endisconnector.materialrefcode
left join tkmaterials drM on drM.code = endisconnector.matdriverefcode
left join enelement on enelement.code = endisconnector.elementcode

where endisconnector.line10refcode = $P{Line10Code}]]>
	</queryString>
