	<parameter name="Line10Code" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
  (case
    when enlineroute.previouspostrefcode is not Null
      then
        prevPost.postnumbergen
      else
        ''
  end ||
  case
    when enlineroute.postrefcode is not Null
      then
        case
          when enlineroute.previouspostrefcode is not Null
            then
              ' - ' || nextpost.postnumbergen
            else
              nextpost.postnumbergen
        end
      else
        ''
  end) as postNubmers,
  coalesce(cast(enlandscape.name as varchar), '') as landscapeName,
  coalesce(cast(enlineroute.distancepost as varchar), '') as lineRouteDistancePost

from enlineroute

left join enlandscape on enlandscape.code = enlineroute.landscaperefcode
left join enpost prevPost on prevPost.code = enlineroute.previouspostrefcode
left join enpost nextPost on nextPost.code = enlineroute.postrefcode

where enlineroute.line10refcode = $P{Line10Code}]]>
	</queryString>
