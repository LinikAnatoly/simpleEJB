	</parameter>
	<parameter name="pmonthstart" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(8)]]></defaultValueExpression>
	</parameter>
	<parameter name="pmonthend" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer(12)]]></defaultValueExpression>
	</parameter>
	<parameter name="pmonthstartstr" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("-")]]></defaultValueExpression>
	</parameter>
	<parameter name="pmonthendstr" class="java.lang.String">
		<defaultValueExpression><![CDATA[new String("--")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select materialcode, materialname, meas, price,
       sum(month1) as month1, sum(month2) as month2, sum(month3) as month3, sum(month4) as month4,
       sum(month5) as month5, sum(month6) as month6, sum(month7) as month7, sum(month8) as month8,
       sum(month9) as month9, sum(month10) as month10, sum(month11) as month11, sum(month12) as month12,

	   (sum(month1)+sum(month2)+sum(month3)+sum(month4)+sum(month5)+sum(month6)+
	   sum(month7)+sum(month8)+sum(month9)+sum(month10)+sum(month11)+sum(month12)) as countitogo,
	   (sum(month1)+sum(month2)+sum(month3)+sum(month4)+sum(month5)+sum(month6)+
	   sum(month7)+sum(month8)+sum(month9)+sum(month10)+sum(month11)+sum(month12))*price as sumitogo
from (
select z.materialcode, z.materialname, z.meas, z.potencial, z.enddate, z.price,

       case when to_char(z.enddate,'mm.yyyy') = '01.'||$P!{year} then count(z.tires) else 0 end as month1,
       case when to_char(z.enddate,'mm.yyyy') = '02.'||$P!{year} then count(z.tires) else 0 end as month2,
       case when to_char(z.enddate,'mm.yyyy') = '03.'||$P!{year} then count(z.tires) else 0 end as month3,
       case when to_char(z.enddate,'mm.yyyy') = '04.'||$P!{year} then count(z.tires) else 0 end as month4,
       case when to_char(z.enddate,'mm.yyyy') = '05.'||$P!{year} then count(z.tires) else 0 end as month5,
       case when to_char(z.enddate,'mm.yyyy') = '06.'||$P!{year} then count(z.tires) else 0 end as month6,
       case when to_char(z.enddate,'mm.yyyy') = '07.'||$P!{year} then count(z.tires) else 0 end as month7,
       case when to_char(z.enddate,'mm.yyyy') = '08.'||$P!{year} then count(z.tires) else 0 end as month8,
       case when to_char(z.enddate,'mm.yyyy') = '09.'||$P!{year} then count(z.tires) else 0 end as month9,
       case when to_char(z.enddate,'mm.yyyy') = '10.'||$P!{year} then count(z.tires) else 0 end as month10,
       case when to_char(z.enddate,'mm.yyyy') = '11.'||$P!{year} then count(z.tires) else 0 end as month11,
       case when to_char(z.enddate,'mm.yyyy') = '12.'||$P!{year} then count(z.tires) else 0 end as month12

from (
select t.code as tires, t.potencial, t.distanceall,
(select coalesce(sum(d.recorddistance),0) from enautotiresdistance d
 where d.tiresrefcode = t.code) as dist,
 t.distanceall - (select coalesce(sum(d.recorddistance),0) from enautotiresdistance d
 where d.tiresrefcode = t.code) as realdist,
 inst.instdate, inst.modt,

(select ('01.01.1970'::timestamp)::date + ((7200000+inst.modt)/(24*60*60*1000))::integer) as datavvoda,

((select ('01.01.1970'::timestamp)::date + ((7200000+inst.modt)/(24*60*60*1000))::integer)
- inst.instdate) as koldays,

(t.distanceall - (select coalesce(sum(d.recorddistance),0) from enautotiresdistance d
 where d.tiresrefcode = t.code)) /
 ((select ('01.01.1970'::timestamp)::date + ((7200000+inst.modt)/(24*60*60*1000))::integer)
- inst.instdate) as kmoneday,

round(t.potencial/((t.distanceall - (select coalesce(sum(d.recorddistance),0) from enautotiresdistance d
 where d.tiresrefcode = t.code)) /
 ((select ('01.01.1970'::timestamp)::date + ((7200000+inst.modt)/(24*60*60*1000))::integer)
- inst.instdate))::numeric) as allday,

inst.instdate+round(t.potencial/((t.distanceall - (select coalesce(sum(d.recorddistance),0) from enautotiresdistance d
 where d.tiresrefcode = t.code)) /
 ((select ('01.01.1970'::timestamp)::date + ((7200000+inst.modt)/(24*60*60*1000))::integer)
- inst.instdate))::numeric)::integer as enddate,

m.code as materialcode, m.name as materialname, coalesce(m.cost,0) as price,
me.name as meas

from tkmaterials m left join tkmeasurement me on me.code = m.measurementcode,  enautotires t

left join
 (select h.tiresrefcode as tcode, h.installdate as instdate, h.modify_time as modt
    from enautotireshistory h
   where h.code = (select min(code) from enautotireshistory
                    where enautotireshistory.tiresrefcode = h.tiresrefcode)) inst on inst.tcode = t.code

where m.code = t.materialrefcode

and t.code in (select distinct(th.tiresrefcode) from enautotireshistory th)
and ((select ('01.01.1970'::timestamp)::date + ((7200000+inst.modt)/(24*60*60*1000))::integer)- inst.instdate)>0
and (t.distanceall - (select coalesce(sum(d.recorddistance),0)
from enautotiresdistance d where d.tiresrefcode = t.code))>0
) z
 where z.enddate between to_date('01.'||$P{pmonthstart}||'.'||$P!{year},'dd.mm.yyyy') and to_date('30.'||$P{pmonthend}||'.'||$P!{year},'dd.mm.yyyy')

group by z.materialcode, z.materialname, z.meas, z.enddate, z.price, z.potencial
) w
group by materialcode, materialname, meas, price
order by materialcode, materialname, price]]>
	</queryString>
