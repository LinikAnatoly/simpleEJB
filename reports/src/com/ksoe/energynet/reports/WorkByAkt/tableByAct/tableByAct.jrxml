,
/*меняем услуги на сторону федорчак выбирает из договрв на сторону по бух статусу "проевденный в финколекции" */
-- если дата проведения проводк в фиснансы в текущем периоде,
 -- и дата приходного акта (она такая же как и у расходного (если неск расходных то дата самого первого акта)) тоже в текущем отчетном периоде
 -- то должно попасть в колонку проведено в фин
 -- !!!КОЛ-ВО ПРЕВЕДЕННЫХ АКТОВ ПО УСЛУГАМ НА СТОРОНУ ЭТО НЕ КОЛ-ВО РАСХОДНЫХ АКТОВ А СЧИТАТЬ КОЛ-ВО ДОХОДНЫХ (ОДНА УСЛУГА - ТОЛЬКО ОДИН ДОХОДНЫХ АКТ)

(select count(ENSERVICESOBJECT.code)
FROM ENSERVICESOBJECT  LEFT JOIN ENSERVICESCONTRACTSTTS ON ENSERVICESCONTRACTSTTS.CODE = ENSERVICESOBJECT.CONTRACTSTATUSREFCODE
LEFT JOIN ENSERVICESCONTRACTTYPE ON ENSERVICESCONTRACTTYPE.CODE = ENSERVICESOBJECT.CONTRACTTYPEREFCODE
LEFT JOIN ENSERVICESCONTRAGENTTP ON ENSERVICESCONTRAGENTTP.CODE = ENSERVICESOBJECT.CONTRAGENTTYPEREFCODE
LEFT JOIN ENSERVICESBILLSTATUS ON ENSERVICESBILLSTATUS.CODE = ENSERVICESOBJECT.BILLSTATUSREFCODE
LEFT JOIN ENWARRANT ON ENWARRANT.CODE = ENSERVICESOBJECT.WARRANTREFCODE
LEFT JOIN ENSERVICESOBJECTSTATUS ON ENSERVICESOBJECTSTATUS.CODE = ENSERVICESOBJECT.STATUSREFCODE ,
ENDEPARTMENT , ENELEMENT , enservicesobject2prov s2p
WHERE   ENDEPARTMENT.CODE = ENSERVICESOBJECT.DEPARTMENTCODE
AND ENELEMENT.CODE = ENSERVICESOBJECT.ELEMENTCODE
AND ENELEMENT.typerefcode = 23
AND ENSERVICESOBJECT.contracttyperefcode IN (1,2) -- SUPP-8134 НЕ БЕРЕМ ПРОДАЖИ )
/* в подразделение "Відділ реалізації технічних умов і видачі договорів про приєднання електроустановок замовників
" кидаем договора которые заводились на "ПрАТ Херсонобленерго" и ТОЛЬКО ТУ */
AND (
   (ENSERVICESOBJECT.DEPARTMENTCODE = depcode  and depcode <> 745 ) or
   (ENSERVICESOBJECT.DEPARTMENTCODE IN ( 745 , 500000019 ) and depcode = 745 and ENSERVICESOBJECT.contracttyperefcode = 1 /*ТУ*/ )
    )
AND ENSERVICESOBJECT.STATUSREFCODE = 2 /*проведенные */
and ENSERVICESOBJECT.code = s2p.servicesobjectrefcode
AND  (
      ( -- дата прих акта по договору (он же расходник(enact) на мин дату ) в текущем отчетном периоде
        (select a.dategen from enact a where a.elementcode = ENSERVICESOBJECT.elementcode order by a.dategen limit 1 ) between to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' ) and last_day(to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' ))
       and
       -- и дата проведения проводк в текущем отчетном периоде
        s2p.dateposting  between to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' ) and last_day(to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' ))
      )
AND ENSERVICESOBJECT.CONTRACTNUMBERSERVICES is not null ) )::integer as act_move_workside

,
-- SUPP-26355 колонка с колич проведен актов в периоде выборки и договора предыдущего периода
-- если дата проведения проводк в фиснансы в текущем периоде,
 -- а дата приходного акта (она такая же как и у расходного (если неск расходных то дата самого первого акта))
 -- то должно попасть в колонку проведено за предыдущ период
 -- !!!КОЛ-ВО ПРЕВЕДЕННЫХ АКТОВ ПО УСЛУГАМ НА СТОРОНУ ЭТО НЕ КОЛ-ВО РАСХОДНЫХ АКТОВ А СЧИТАТЬ КОЛ-ВО ДОХОДНЫХ (ОДНА УСЛУГА - ТОЛЬКО ОДИН ДОХОДНЫХ АКТ)
(select count(ENSERVICESOBJECT.code)
FROM ENSERVICESOBJECT  LEFT JOIN ENSERVICESCONTRACTSTTS ON ENSERVICESCONTRACTSTTS.CODE = ENSERVICESOBJECT.CONTRACTSTATUSREFCODE
LEFT JOIN ENSERVICESCONTRACTTYPE ON ENSERVICESCONTRACTTYPE.CODE = ENSERVICESOBJECT.CONTRACTTYPEREFCODE
LEFT JOIN ENSERVICESCONTRAGENTTP ON ENSERVICESCONTRAGENTTP.CODE = ENSERVICESOBJECT.CONTRAGENTTYPEREFCODE
LEFT JOIN ENSERVICESBILLSTATUS ON ENSERVICESBILLSTATUS.CODE = ENSERVICESOBJECT.BILLSTATUSREFCODE
LEFT JOIN ENWARRANT ON ENWARRANT.CODE = ENSERVICESOBJECT.WARRANTREFCODE
LEFT JOIN ENSERVICESOBJECTSTATUS ON ENSERVICESOBJECTSTATUS.CODE = ENSERVICESOBJECT.STATUSREFCODE ,
ENDEPARTMENT , ENELEMENT , enservicesobject2prov s2p
WHERE   ENDEPARTMENT.CODE = ENSERVICESOBJECT.DEPARTMENTCODE
AND ENELEMENT.CODE = ENSERVICESOBJECT.ELEMENTCODE
AND ENELEMENT.typerefcode = 23
AND ENSERVICESOBJECT.contracttyperefcode IN (1,2) -- SUPP-8134 НЕ БЕРЕМ ПРОДАЖИ )
/* в подразделение "Відділ реалізації технічних умов і видачі договорів про приєднання електроустановок замовників
" кидаем договора которые заводились на "ПрАТ Херсонобленерго" и ТОЛЬКО ТУ */
 AND (
   (ENSERVICESOBJECT.DEPARTMENTCODE = depcode  and depcode <> 745 ) or
   (ENSERVICESOBJECT.DEPARTMENTCODE IN ( 745 , 500000019 ) and depcode = 745 and ENSERVICESOBJECT.contracttyperefcode = 1 /*ТУ*/ )
    )
AND ENSERVICESOBJECT.STATUSREFCODE = 2 /*проведенные */
and ENSERVICESOBJECT.code = s2p.servicesobjectrefcode
AND  (
      ( -- дата прих акта по договору (он же расходник(enact) на мин дату ) в предыдущем периоде
        (select a.dategen from enact a where a.elementcode = ENSERVICESOBJECT.elementcode order by a.dategen limit 1 ) < to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' )
       and
       -- и дата проведения проводк в текущем отчетном периоде
        s2p.dateposting  between to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' ) and last_day(to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' ))
      )
AND ENSERVICESOBJECT.CONTRACTNUMBERSERVICES is not null )

)::integer as act_move_workside_prev_period

-- , sum(act_draft_workside)::integer as act_draft_workside /*черновые акты по услугам на сторону */
/*меняем услуги на сторону федорчак выбирает из договрв на сторону по бух статусу " чорновой " и подписаные договора  */
,(SELECT count(*)
FROM ENSERVICESOBJECT  LEFT JOIN ENSERVICESCONTRACTSTTS ON ENSERVICESCONTRACTSTTS.CODE = ENSERVICESOBJECT.CONTRACTSTATUSREFCODE
LEFT JOIN ENSERVICESCONTRACTTYPE ON ENSERVICESCONTRACTTYPE.CODE = ENSERVICESOBJECT.CONTRACTTYPEREFCODE
LEFT JOIN ENSERVICESCONTRAGENTTP ON ENSERVICESCONTRAGENTTP.CODE = ENSERVICESOBJECT.CONTRAGENTTYPEREFCODE
LEFT JOIN ENSERVICESBILLSTATUS ON ENSERVICESBILLSTATUS.CODE = ENSERVICESOBJECT.BILLSTATUSREFCODE
LEFT JOIN ENWARRANT ON ENWARRANT.CODE = ENSERVICESOBJECT.WARRANTREFCODE
LEFT JOIN ENSERVICESOBJECTSTATUS ON ENSERVICESOBJECTSTATUS.CODE = ENSERVICESOBJECT.STATUSREFCODE ,
ENDEPARTMENT , ENELEMENT
WHERE   ENDEPARTMENT.CODE = ENSERVICESOBJECT.DEPARTMENTCODE
AND ENELEMENT.CODE = ENSERVICESOBJECT.ELEMENTCODE
AND ENELEMENT.typerefcode = 23
AND ENSERVICESOBJECT.contracttyperefcode NOT IN (3,4) -- SUPP-8134 НЕ БЕРЕМ ПРОДАЖИ )
/* в подразделение "Відділ реалізації технічних умов і видачі договорів про приєднання електроустановок замовників
" кидаем договора которые заводились на "ПрАТ Херсонобленерго" и ТОЛЬКО ТУ */
AND (
   (ENSERVICESOBJECT.DEPARTMENTCODE = /*481*/  depcode  and depcode <> 745 ) or
   (ENSERVICESOBJECT.DEPARTMENTCODE IN ( 745 , 500000019 ) and depcode = 745 and ENSERVICESOBJECT.contracttyperefcode = 1 /*ТУ*/ )
    )
AND ENSERVICESOBJECT.CONTRACTSTATUSREFCODE = 5
AND ENSERVICESOBJECT.STATUSREFCODE =  1
AND  (
  case when ENSERVICESOBJECT.executeworkdate is not null then
 (  ENSERVICESOBJECT.executeworkdate >= to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' )
AND ENSERVICESOBJECT.executeworkdate <= last_day(to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' ))
  )
 else  (  ENSERVICESOBJECT.CONTRACTDATESERVICES >= to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' )
      AND ENSERVICESOBJECT.CONTRACTDATESERVICES <= last_day(to_date('01.'||$P{monthGen}||'.'|| $P{yearGen} , 'dd.mm.yyyy' ))
  ) end
-- если есть дата выполнения работ по договору то берем по этой дате иначе по дате договора так нужно федорчак

AND ENSERVICESOBJECT.CONTRACTNUMBERSERVICES is not null) )::integer as act_draft_workside
--, sum(act_podpisanie_workside)::integer as act_podpisanie_workside /*на подписании  акты по услугам на сторону */
/*на подписании услуги на сторону также условие по ВРТУВД и ПрАТ Хeрсонобленерго если ВРТУВД то акті берем "подготовка ТУ "  */
, (select count(*) from (
 Select distinct a.code as codeact , p.departmentrefcode  , a.acttyperefcode , p.typerefcode as plantyperefcode , p.budgetrefcode  , a.statusrefcode
 from enact a  , enact2enplanwork a2p  , enplanwork p
Where to_char(a.dategen,'yyyy')::numeric = $P{yearGen}
and  to_char(a.dategen,'mm')::numeric = $P{monthGen}
and a.code = a2p.actrefcode
and a.statusrefcode = 4 /*на подписании */
and a2p.plancode = p.code
AND (
   (p.DEPARTMENTrefCODE = /*481*/  depcode  and depcode <> 745  and a.acttyperefcode = 7 /*роботти на сторону*/  ) or
   (p.DEPARTMENTrefCODE IN ( 745 , 500000019 ) and depcode = 745 and a.acttyperefcode =  9 /*подготовка ту*/ )
    )
and p.typerefcode not in (19 /*Послуги зі сторони*/ , 20 /*Послуги зі сторони (Інвест Програма)*/)  )as  s1)::integer as act_podpisanie_workside /*на подписании  акты по услугам на сторону */


, sum(act_move_techobsl)::integer as act_move_techobsl  /*проведенные акты по тех. обслуживанию */
, sum(act_draft_techobsl)::integer as act_draft_techobsl  /*черновые акты по тех. обслуживанию */
