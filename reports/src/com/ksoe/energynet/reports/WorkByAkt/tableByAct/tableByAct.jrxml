, sum(act_podpisanie_rekostr_modern_sbyt)::integer as act_podpisanie_rekostr_modern_sbyt  /*на подписании акты по Реконструкции модернизации  енергосбыт */
, sum(act_move_techobsl_sbyt)::integer as act_move_techobsl_sbyt  /*проведенные акты по техобслуживание  (енергосбыт) */
, sum(act_draft_techobsl_sbyt)::integer as act_draft_techobsl_sbyt /*черновые акты по техобслуживание  (енергосбыт) */
, sum(act_podpisanie_techobsl_sbyt)::integer as act_podpisanie_techobsl_sbyt /*на подписании акты по техобслуживание  (енергосбыт) */
, sum(act_move_repair_sbyt)::integer as act_move_repair_sbyt  /*проведенные акты по ремонтам  (енергосбыт) */
, sum(act_draft_repair_sbyt)::integer as act_draft_repair_sbyt  /*черновые акты по ремонтам  (енергосбыт) */
, sum(act_podpisanie_repair_sbyt)::integer as act_podpisanie_repair_sbyt  /*на подписании акты по ремонтам  (енергосбыт) */


from (

select depcode , depname , rencode
,plan_repair
,plan_repair_fact
,plan_repair_fact_not_act

,plan_techservice
,plan_techservice_fact
,plan_techservice_fact_not_act

,plan_prisoed_modernizac
,plan_prisoed_modernizac_fact
,plan_prisoed_modernizac_fact_not_act

,plan_prisoed_stroitelstvo
,plan_prisoed_stroitelstvo_fact
