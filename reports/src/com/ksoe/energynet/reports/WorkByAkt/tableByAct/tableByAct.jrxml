, sum(act_podpisanie_prisoed_modern)::integer as act_podpisanie_prisoed_modern /*на подписании акты по Присоединению / Модернизации */
, sum(act_move_prisoed_build)::integer as  act_move_prisoed_build  /*проведенные акты по Присоединению / Строительство */
, sum(act_draft_prisoed_build)::integer as  act_draft_prisoed_build /*черновые акты по Присоединению / Строительство */
, sum(act_podpisanie_prisoed_build)::integer as act_podpisanie_prisoed_build  /*на подписании акты по Присоединению / Строительство */
, sum(act_move_rekostr_modern)::integer as act_move_rekostr_modern  /*проведенные акты по Реконструкции модернизации  */
, sum(act_draft_rekostr_modern)::integer as act_draft_rekostr_modern /*черновые акты по Реконструкции модернизации  */
, sum(act_podpisanie_rekostr_modern)::integer as act_podpisanie_rekostr_modern  /*на подписании акты по Реконструкции модернизации  */
, sum(act_move_rekostr_modern_sbyt)::integer as act_move_rekostr_modern_sbyt /*проведенные акты по Реконструкции модернизации  енергосбыт */
