              case when to_date('25.'||p.monthgen||'.'||p.yeargen, 'dd.MM.yyyy') between p.datestart and p.datefinal then 1 else 0 end as d25,
              case when to_date('26.'||p.monthgen||'.'||p.yeargen, 'dd.MM.yyyy') between p.datestart and p.datefinal then 1 else 0 end as d26,
              case when to_date('27.'||p.monthgen||'.'||p.yeargen, 'dd.MM.yyyy') between p.datestart and p.datefinal then 1 else 0 end as d27,
              case when to_date('28.'||p.monthgen||'.'||p.yeargen, 'dd.MM.yyyy') between p.datestart and p.datefinal then 1 else 0 end as d28,
              case when to_date('29.'||p.monthgen||'.'||p.yeargen, 'dd.MM.yyyy') between p.datestart and p.datefinal then 1 else 0 end as d29,
              case when to_date('30.'||p.monthgen||'.'||p.yeargen, 'dd.MM.yyyy') between p.datestart and p.datefinal then 1 else 0 end as d30,
              case when to_date('31.'||p.monthgen||'.'||p.yeargen, 'dd.MM.yyyy') between p.datestart and p.datefinal then 1 else 0 end as d31 ,
              coalesce((select 'Припис № ' || bo.numbergen || ' від ' || bo.dategen || ' ' || bo.itemtext || ' ' || borg.name as binding from enbindingover bo, enbindingoverorganiztn borg
               where  p.code = bo.planrefcode and
                bo.organizationcode = borg.code),'Припис не введено') as binding
           ,
             (select * from net.get_codemonthplan_byanycodeplan(p.code::numeric) ) as codemonthplan
           from enplanwork p left join finexecutor f on p.finexecutorcode = f.code,
            enelement e, enplanworktype pwt, enplanworkstate pws, enelementtype et
            where  p.yeargen = $P{yearGen} and p.monthgen = $P{monthGen}

            and p.elementrefcode = e.code
            and e.typerefcode = et.code
            and p.kindcode in (2,3,4) and p.statuscode not in (2,6)
            and p.staterefcode = pws.code
            and p.typerefcode = pwt.code
           -- and et.managementrefcode  in ( 1 ,2, 3)
            and ((et.managementrefcode = 1) or (et.managementrefcode = 3 and p.budgetrefcode = 240000001))
            --and p.budgetrefcode <> 240000001
            and p.typerefcode not in (19 /*Послуги зі сторони*/ , 20 /*Послуги зі сторони (Інвест Програма)*/ )

            -- SUPP-6699 не брать в отчет те планы в которых указаны работы
            and p.code not in ( select pw.code from enplanwork pw , enplanworkitem pwi --, enplanworkstate pws
                    where pw.code = pwi.planrefcode
                   -- and pw.staterefcode = pws.code
                    and pw.yeargen = $P{yearGen}
                    and pw.monthgen = $P{monthGen}
                    and pw.code = p.code
                    and pwi.kartarefcode in (75001805,
                    75001794,
                    75001791,
                    75001798,
                    75001795,
                    75001797,
                    75001802,
                    75001800,
                    75001790,
                    75001801) )

            ) as q
            ) as q1 ) as qq, enelement ed, endepartment edn

            where qq.elcode = ed.code and qq.department = edn.code

            group by qq.finexecutorname, qq.planworktypename, qq.planworkstatename,qq.formrefcode,  edn.name, ed.code , qq.cnn, qq.elcode, qq.fincode, qq.binding , qq.codemonthplan
            ) as qqq

) as sel_plan
ON  sel_dep.depcode = sel_plan.department

-------------- конец планы

) sel_dep_plan
group by depname ,rencode  ,depcode

) as rezult_sel_dep_plan
