	<queryString>
		<![CDATA[select  departmentname
       , budgname
       , numbergen
       , dategen
       , codeact
       , typeact
       , podvidwork
       , obj
       , codeobject
       , priznwork ,
       actstatus  From (

select  distinct  dep.name as departmentname
       , budg.name as budgname
       , ' '|| act.numbergen  as  numbergen
       , act.dategen
       , act.code as codeact
       , enps.shortname as typeact
       , enpt.shortname as podvidwork
       , eld.ename ||' Інв.№ '|| eld.invnumber as obj
       , eld.ecode as codeobject
, /*признак есть ли в акте невыполненые работы 0 нет 1 - есть */
        (select count(kartarefcode)  from (
                /*работы с месячного плана*/
                Select distinct enpi.kartarefcode  From enplanworkitem enpi , enplanwork enp , finexecutor fex
                 Where enpi.countgen <> 0
                   And enp.kindcode = 2
                   And enpi.planrefcode = enp.code
                   And enp.finexecutorcode = fex.code
                   And enp.statuscode not in (2 , 6)
                   And enp.elementrefcode =  eld.ecode /* from parent query */
                   And enp.code in ( select distinct  h1.planoldrefcode from enplancorrecthistory h1
                                      where h1.reasoncode = 4
                                        and h1.plannewrefcode in
                                               (select  h1.planoldrefcode from enplancorrecthistory h1
                                                 where h1.reasoncode = 5
                                                   and h1.plannewrefcode in
                                                     ( select a2p.plancode from  enact2enplanwork a2p
                                                        where a2p.actrefcode =   act.code /* from parent query */
                                                     )
                                               )
                                   )

                 and enpi.kartarefcode not in ( /*не равно работам из факта */
                                               Select distinct enpi.kartarefcode From enplanworkitem enpi , enact2enplanwork a2p ,
                                                                                      enplanwork enp , finexecutor fex
                                               Where enpi.planrefcode = a2p.plancode
                                                 And a2p.actrefcode = act.code /* from parent query */
                                                 And enpi.countgen <> 0
                                                 And enp.kindcode = 4
                                                 And enpi.planrefcode = enp.code
                                                 And enp.finexecutorcode = fex.code
                                                 And enp.elementrefcode = eld.ecode /* from parent query*/
                                                 And enp.statuscode not in (2 , 6)
                                              )
                ) sel_prizn_work ) as priznwork ,
                acs.name as actstatus
        from  enplanwork enpf left join finexecutor finex on (enpf.finexecutorcode = finex.code
                                                          and (
                                                               (finex.fincode = $P{finexecutorfincode} and $P{finexecutorfincode} <> 0 )
                                                              or
                                                               ( 0 = 0 )
                                                              )
                                                             ) ,
              enact act ,
              enact2enplanwork e2p ,
              endepartment dep ,
              endepartment budg ,
              enplanworkstate enps ,
              enplanworktype enpt ,
              enelementdata eld ,
              enplanworkitem enpfit ,
              enactstatus acs
where act.statusrefcode = acs.code
  and eld.ecode = act.elementcode
  and enpf.code = e2p.plancode
  and act.code = e2p.actrefcode
  and enpf.departmentrefcode = dep.code
  and enpf.budgetrefcode = budg.code
  and enps.code = enpf.staterefcode
  and enpf.typerefcode = enpt.code
  and enpfit.planrefcode = enpf.code
  and ( act.statusrefcode = $P{actstatus} or ( $P{actstatus} = 0 and act.statusrefcode <> 2 ) )
  and ( act.code = $P{enactcode} or $P{enactcode}  = 0 )
  and act.dategen between to_date($P{datestart} ,'dd.mm.yyyy') and to_date($P{datefinal} ,'dd.mm.yyyy')
  and enpf.kindcode = 4 /* берем только задание факт */
  and ( enpf.departmentrefcode = $P{podr} or $P{podr} = 0 )
  and ( enpf.budgetrefcode = $P{budj} or $P{budj} = 0 )
  and ( enpf.elementrefcode = $P{obj} or $P{obj} = 0 )
  and ( act.acttyperefcode = $P{typeact} or $P{typeact} = 0 )
  and enpf.finexecutorcode in (
     Select finex.code from finexecutor finex
      Where
      (
       ( finex.fincode = $P{finexecutorfincode} and $P{finexecutorfincode} <> 0 )
       or
       ( $P{finexecutorfincode} =  0 )
      )
                              )
  ) sel_act
  /* shownullfact :  1 - показываем только акты с невыполнеными работами */
  Where ( ($P{shownullfact} = 0 /*показ все акты */ ) or (priznwork = 1 and $P{shownullfact} = 1 /*показ только актов с невып работами*/ ) )
  order by departmentname  , budgname ,  obj , numbergen , dategen]]>
