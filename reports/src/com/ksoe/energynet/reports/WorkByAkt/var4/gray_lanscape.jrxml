	<queryString>
		<![CDATA[Select obj ,
       elementcode ,
       typeact ,
       budjname ,
       depname ,
        ( Select ffe.name  from finexecutor ffe where ffe.fincode = finexecutorfincode
           order by ffe.modify_time desc
           limit 1
          ) as  finexecutorname  ,
       finexecutorfincode ,
       planrefcodemonth  ,
       tkworkname ,
       workcode , workcountmonth , worktimemonth ,
       workccountfact , worktimefact

FROM (
Select eld.ename ||' Інв.№ '|| eld.invnumber as obj ,
       elementcode ,
       typeact ,
       budjname ,
       depname ,
        /*название бригады пока из плана , нада переделать как то что б из факта */
       (select distinct f.name from enplanwork p , finexecutor f where p.code = planrefcode
                                                            and p.finexecutorcode = f.code   ) as finexecutorname  ,
       /*код бригады пока из плана , нада переделать как то что б из факта */
       (select distinct f.fincode from enplanwork p , finexecutor f where p.code = planrefcode
                                                            and p.finexecutorcode = f.code   ) as finexecutorfincode ,
       planrefcode as planrefcodemonth  ,
       tkworkname ,
       workcode as workcode , sum(workcountmonth) as workcountmonth , sum(worktimemonth) as worktimemonth ,
       coalesce(sum(workccountfact),0) as workccountfact , sum(worktimefact) as worktimefact

FROM
(
Select distinct
       enps.shortname as typeact ,
       enp.elementrefcode as elementcode ,
       budg.name as budjname ,
       dep.name as depname ,
       enim.planrefcode ,
       tk.name as tkworkname , enim.kartarefcode  as workcode , enim.countgen as workcountmonth , enim.timegen as worktimemonth ,
       cast( 0 as numeric ) as workccountfact , cast( 0 as numeric ) as worktimefact from
  /*выборка кодов месячного плана под ПЛАНЫ ФАКТ*/
           (     Select phpl.planoldrefcode as planmonthcode /*код месячного плана  */ , phfa.plannewrefcode as planfactcode
                  From enplancorrecthistory  phfa  , enplancorrecthistory  phpl
                 Where phfa.plannewrefcode in ( Select a2pl.plancode From enact2enplanwork a2pl , enplanwork p
                                                 Where a2pl.actrefcode IN (/*182002889 , 182002913*/
                                                 select distinct actt.code from enact actt
                                                 where ( actt.statusrefcode = $P{actstatus} or ( $P{actstatus} = 0 and actt.statusrefcode <> 2 )  )
                                                   and ( actt.acttyperefcode = $P{typeact} or $P{typeact} = 0 )
                                                   and ( actt.code = $P{enactcode} or $P{enactcode} = 0 )
                                                   and actt.dategen between to_date( $P{datestart},'dd.mm.yyyy') and to_date($P{datefinal} ,'dd.mm.yyyy')
                                                                          )
                                                 and a2pl.plancode = p.code
                                                 and ( p.departmentrefcode = $P{podr} or $P{podr} = 0 )
                                                 and ( p.budgetrefcode = $P{budj} or $P{budj} = 0 )
                                                 and ( p.elementrefcode = $P{obj} or $P{obj} = 0 )


                                                     )
                   And phfa.reasoncode = 5 /*Перенос у ФАКТ*/

                   And phfa.planoldrefcode  = phpl.plannewrefcode
                   And phpl.reasoncode = 4 /*Перенос у ПЛАН*/  )  spl  , enplanworkitem enim , tktechcard tk , enplanwork enp  ,
                                                                         endepartment dep , endepartment budg , enplanworkstate enps
Where enim.planrefcode = spl.planmonthcode
  and enim.kartarefcode = tk.code
  and enp.departmentrefcode = dep.code
  and enim.planrefcode = enp.code
  and spl.planmonthcode = enp.code
  and enp.budgetrefcode = budg.code
  and enps.code = enp.staterefcode
  and enp.kindcode = 2 /*берем только месячные планы*/
  and enim.countgen <> 0

UNION  all

Select distinct

       enps.shortname as typeact ,
       enpf.elementrefcode as elementcode ,
       budg.name as budjname ,
       depf.name as depname ,
         ( select h1.planoldrefcode from enplancorrecthistory h1 where h1.reasoncode = 4 and h1.plannewrefcode IN
          (select h2.planoldrefcode from enplancorrecthistory h2 where h2.reasoncode = 5 and h2.plannewrefcode IN (enpf.code) )
         ) as planrefcode  ,
       tk.name as tkworkname , enif.kartarefcode as workcode , cast(0 as numeric ) as workcountmonth , cast(0 as numeric) as worktimemonth ,
       enif.countgen as workccountfact , enif.timegen as worktimefact from
             enplanworkitem enif , tktechcard tk , enplanwork enpf ,
              endepartment depf ,  endepartment budg , enplanworkstate enps   ,  enact2enplanwork e2p , enact act

Where enif.kartarefcode = tk.code
  and enpf.departmentrefcode = depf.code
  and enif.planrefcode = enpf.code
  and enpf.budgetrefcode = budg.code
  and enps.code = enpf.staterefcode
  and enpf.code = e2p.plancode
  and act.code = e2p.actrefcode
  and ( act.statusrefcode = $P{actstatus} or ( $P{actstatus} = 0 and act.statusrefcode <> 2 )  )
  and ( act.code = $P{enactcode} or $P{enactcode} = 0 )
  and act.dategen between to_date($P{datestart} ,'dd.mm.yyyy') and to_date($P{datefinal} ,'dd.mm.yyyy')
  and enpf.kindcode = 4 /*берем только задание факт*/
  and ( enpf.departmentrefcode = $P{podr} or $P{podr} = 0 )
  and ( enpf.budgetrefcode = $P{budj} or $P{budj} = 0 )
  and ( enpf.elementrefcode = $P{obj} or $P{obj} = 0 )
  and ( act.acttyperefcode = $P{typeact} or $P{typeact} = 0 )
) s_wormonthfact , enelementdata eld
Where  s_wormonthfact.elementcode = eld.ecode


GROUP BY  planrefcode ,  tkworkname , depname , budjname  , elementcode , eld.ename , eld.invnumber , typeact , workcode
) sel_alll
where ( (workccountfact <> 0 and $P{shownullfact} = 0 ) or ( $P{shownullfact} = 1 ) or (workccountfact = 0 and $P{shownullfact} = 2 ) )
  and ( finexecutorfincode = $P{finexecutorfincode} or $P{finexecutorfincode}  = 0 )
Order by  depname , budjname , elementcode , typeact , workcode]]>
	</queryString>
